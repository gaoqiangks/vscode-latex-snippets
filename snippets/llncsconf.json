	},


	"llncs{}{}":{
		"prefix":"llncs",
		"body":[
			"\\llncs{${1:text}}{${2:page number}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"llncsdoi{}":{
		"prefix":"llncsdoi",
		"body":[
			"\\llncsdoi{${1:DOI}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"copyrightnote":{
		"prefix":"copyrightnote",
		"body":[
			"\\copyrightnote"
		],
		"luasnip": {"priority": 3000} 
	}
}
