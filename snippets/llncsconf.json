{


	"conference{}":{
		"prefix":"conference",
		"body":[
			"\\conference{${1:conference name%text}}"
		]
	},


	"llncs{}{}":{
		"prefix":"llncs",
		"body":[
			"\\llncs{${1:text}}{${2:page number}}"
		]
	},


	"llncsdoi{}":{
		"prefix":"llncsdoi",
		"body":[
			"\\llncsdoi{${1:DOI}}"
		]
	},


	"copyrightnote":{
		"prefix":"copyrightnote",
		"body":[
			"\\copyrightnote"
		]
	}
}
