{


	"includeversion{}":{
		"prefix":"includeversion",
		"body":[
			"\\includeversion{${1:envname}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"excludeversion{}":{
		"prefix":"excludeversion",
		"body":[
			"\\excludeversion{${1:envname}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"excludeversion[]{}":{
		"prefix":"excludeversion",
		"body":[
			"\\excludeversion[${2:text}]{${1:envname}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"comment":{
		"prefix":"comment",
		"body":[
			"\\begin{comment}",
				"\t$1",
			"\\end{comment}"
		],
		"luasnip": {"priority": 3000} 
	}
}
