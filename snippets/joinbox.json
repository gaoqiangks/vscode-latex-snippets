{


	"joinbox{}{}":{
		"prefix":"joinbox",
		"body":[
			"\\joinbox{${1:content1%text}}{${2:content2%text}}"
		]
	},


	"joinboxes{}":{
		"prefix":"joinboxes",
		"body":[
			"\\joinboxes{${1:content1,content2,...%text}}"
		]
	},


	"joinfigs{}":{
		"prefix":"joinfigs",
		"body":[
			"\\joinfigs{${1:imagefile1,imagefile2,...%definition}}"
		]
	},


	"joinset{}":{
		"prefix":"joinset",
		"body":[
			"\\joinset{${1:options%keyvals}}"
		]
	},


	"joinbox*{}{}":{
		"prefix":"*joinbox",
		"body":[
			"\\joinbox*{${1:content1%text}}{${2:content2%text}}"
		]
	},


	"joinbox*[]{}{}":{
		"prefix":"*joinbox",
		"body":[
			"\\joinbox*[${3:options%keyvals}]{${1:content1%text}}{${2:content2%text}}"
		]
	},


	"joinboxes*{}":{
		"prefix":"*joinboxes",
		"body":[
			"\\joinboxes*{${1:content1,content2,...%text}}"
		]
	},


	"joinboxes*[]{}":{
		"prefix":"*joinboxes",
		"body":[
			"\\joinboxes*[${2:options%keyvals}]{${1:content1,content2,...%text}}"
		]
	},


	"joinfigs*{}":{
		"prefix":"*joinfigs",
		"body":[
			"\\joinfigs*{${1:imagefile1,imagefile2,...%definition}}"
		]
	},


	"joinfigs*[]{}":{
		"prefix":"*joinfigs",
		"body":[
			"\\joinfigs*[${2:options%keyvals}]{${1:imagefile1,imagefile2,...%definition}}"
		]
	},


	"joinbox[]{}{}":{
		"prefix":"joinbox",
		"body":[
			"\\joinbox[${3:options%keyvals}]{${1:content1%text}}{${2:content2%text}}"
		]
	},


	"joinboxes[]{}":{
		"prefix":"joinboxes",
		"body":[
			"\\joinboxes[${2:options%keyvals}]{${1:content1,content2,...%text}}"
		]
	},


	"joinfigs[]{}":{
		"prefix":"joinfigs",
		"body":[
			"\\joinfigs[${2:options%keyvals}]{${1:imagefile1,imagefile2,...%definition}}"
		]
	}
}
