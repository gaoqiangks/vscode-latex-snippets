{


	"tikzinput":{
		"prefix":"tikzinput",
		"body":[
			"\\tikzinput{${1:file}}"
		]
	},


	"tikzinput":{
		"prefix":"tikzinput",
		"body":[
			"\\tikzinput[${2:options%keyvals}]{${1:file}}"
		]
	},


	"ctikzinput":{
		"prefix":"ctikzinput",
		"body":[
			"\\ctikzinput{${1:file}}"
		]
	},


	"ctikzinput":{
		"prefix":"ctikzinput",
		"body":[
			"\\ctikzinput[${2:options%keyvals}]{${1:file}}"
		]
	}
}
