{


	"setsampletext{}":{
		"prefix":"setsampletext",
		"body":[
			"\\setsampletext{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"printfont{}{}":{
		"prefix":"printfont",
		"body":[
			"\\printfont{${1:font}}{${2:license}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"printfont[]{}{}":{
		"prefix":"printfont",
		"body":[
			"\\printfont[${3:options%keyvals}]{${1:font}}{${2:license}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"sampletext{}":{
		"prefix":"sampletext",
		"body":[
			"\\sampletext{${1:arg}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"sampletext[]{}":{
		"prefix":"sampletext",
		"body":[
			"\\sampletext[${2:opt}]{${1:arg}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"samplefeature{}":{
		"prefix":"samplefeature",
		"body":[
			"\\samplefeature{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
