{


	"Footnotemark{}":{
		"prefix":"Footnotemark",
		"body":[
			"\\Footnotemark{${1:mark}}"
		]
	},


	"Footnotetext{}{}":{
		"prefix":"Footnotetext",
		"body":[
			"\\Footnotetext{${1:mark}}{${2:text}}"
		]
	},


	"Footnote{}{}":{
		"prefix":"Footnote",
		"body":[
			"\\Footnote{${1:mark}}{${2:text}}"
		]
	}
}
