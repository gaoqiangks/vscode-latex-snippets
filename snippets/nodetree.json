{


	"NodetreeSet":{
		"prefix":"NodetreeSet",
		"body":[
			"\\NodetreeSet{${1:options%keyvals}}"
		]
	},


	"NodetreeSetOption":{
		"prefix":"NodetreeSetOption",
		"body":[
			"\\NodetreeSetOption[${2:option%keyvals}]{${1:value}}"
		]
	},


	"NodetreeResetOption":{
		"prefix":"NodetreeResetOption",
		"body":[
			"\\NodetreeResetOption{${1:option%keyvals}}"
		]
	},


	"NodetreeReset":{
		"prefix":"NodetreeReset",
		"body":[
			"\\NodetreeReset"
		]
	},


	"NodetreeRegisterCallback":{
		"prefix":"NodetreeRegisterCallback",
		"body":[
			"\\NodetreeRegisterCallback{${1:callbacks}}"
		]
	},


	"NodetreeUnregisterCallback":{
		"prefix":"NodetreeUnregisterCallback",
		"body":[
			"\\NodetreeUnregisterCallback{${1:callbacks}}"
		]
	}
}
