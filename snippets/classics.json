{


	"newclassic{}{}":{
		"prefix":"newclassic",
		"body":[
			"\\newclassic{${1:classic}}{${2:formatting directives}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"newclassic[]{}{}":{
		"prefix":"newclassic",
		"body":[
			"\\newclassic[${3:range sep}]{${1:classic}}{${2:formatting directives}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"newpagination{}{}{}{}":{
		"prefix":"newpagination",
		"body":[
			"\\newpagination{${1:pagination%cmd}}{${2:singular}}{${3:plural}}{${4:conjunction}}"
		],
		"luasnip": {"priority": 3000} 
	},


