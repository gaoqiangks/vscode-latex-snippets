{


	"newproof{}{}{}":{
		"prefix":"newproof",
		"body":[
			"\\newproof{${1:envname}}{${2:start text}}{${3:finish text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"qed":{
		"prefix":"qed",
		"body":[
			"\\qed"
		],
		"luasnip": {"priority": 3000} 
	},


	"qedtext":{
		"prefix":"qedtext",
		"body":[
			"\\qedtext"
		],
		"luasnip": {"priority": 3000} 
	},


	"proof":{
		"prefix":"proof",
		"body":[
			"\\begin{proof}",
				"\t$1",
			"\\end{proof}"
		],
		"luasnip": {"priority": 3000} 
	},


	"proof[]":{
		"prefix":"proof",
		"body":[
			"\\begin{proof}",
				"\t[${1:label}]",
			"\\end{proof}"
		],
		"luasnip": {"priority": 1000} 
	}
}
