{


	"medmath{}":{
		"prefix":"medmath",
		"body":[
			"\\medmath{${1:formula}}"
		]
	},


	"medop{}":{
		"prefix":"medop",
		"body":[
			"\\medop{${1:operator}}"
		]
	},


	"medint{}":{
		"prefix":"medint",
		"body":[
			"\\medint{${1:operator}}"
		]
	},


	"medintcorr{}":{
		"prefix":"medintcorr",
		"body":[
			"\\medintcorr{${1:length}}"
		]
	},


	"mfrac{}{}":{
		"prefix":"mfrac",
		"body":[
			"\\mfrac{${1:num}}{${2:den}}"
		]
	},


	"mbinom{}{}":{
		"prefix":"mbinom",
		"body":[
			"\\mbinom{${1:above}}{${2:below}}"
		]
	},


	"medsize":{
		"prefix":"medsize",
		"body":[
			"\\begin{medsize}",
				"\t$1",
			"\\end{medsize}"
		]
	},


	"mmatrix":{
		"prefix":"mmatrix",
		"body":[
			"\\begin{mmatrix}",
				"\t$1",
			"\\end{mmatrix}"
		]
	}
}
