{


	"kswrapfig{}{}":{
		"prefix":"kswrapfig",
		"body":[
			"\\kswrapfig{${1:imagefile}}{${2:text}}"
		]
	},


	"tightlist":{
		"prefix":"tightlist",
		"body":[
			"\\tightlist"
		]
	},


	"kswrapfig[]{}{}":{
		"prefix":"kswrapfig",
		"body":[
			"\\kswrapfig[${3:options%keyvals}]{${1:imagefile}}{${2:text}}"
		]
	},


	"kswrapfigline[]{}{}":{
		"prefix":"kswrapfigline",
		"body":[
			"\\kswrapfigline[${3:options%keyvals}]{${1:imagefile}}{${2:text}}"
		]
	}
}
