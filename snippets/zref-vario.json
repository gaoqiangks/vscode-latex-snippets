{


	"zvsetup{}":{
		"prefix":"zvsetup",
		"body":[
			"\\zvsetup{${1:options%keyvals}}"
		]
	},


	"zvref{}":{
		"prefix":"zvref",
		"body":[
			"\\zvref{${1:label}}"
		]
	},


	"zvpageref{}":{
		"prefix":"zvpageref",
		"body":[
			"\\zvpageref{${1:label}}"
		]
	},


	"zvrefrange{}{}":{
		"prefix":"zvrefrange",
		"body":[
			"\\zvrefrange{${1:fromlabel%ref}}{${2:tolabel%ref}}"
		]
	},


	"zvpagerefrange{}{}":{
		"prefix":"zvpagerefrange",
		"body":[
			"\\zvpagerefrange{${1:fromlabel%ref}}{${2:tolabel%ref}}"
