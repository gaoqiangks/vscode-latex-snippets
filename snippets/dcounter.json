{


	"DeclareDynamicCounter{}":{
		"prefix":"DeclareDynamicCounter",
		"body":[
			"\\DeclareDynamicCounter{${1:counter}}"
		]
	},


	"countstyle{}":{
		"prefix":"countstyle",
		"body":[
			"\\countstyle{${1:counter}}"
		]
	},


	"DynamicCount{}":{
		"prefix":"DynamicCount",
		"body":[
			"\\DynamicCount{${1:counter}}"
		]
	},


	"countstyle[]{}":{
		"prefix":"countstyle",
		"body":[
			"\\countstyle[${2:list of counters}]{${1:counter}}"
		]
	}
}
