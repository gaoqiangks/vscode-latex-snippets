{


	"punctstyle{}":{
		"prefix":"punctstyle",
		"body":[
			"\\punctstyle{${1:style%keyvals}}"
		]
	},


	"CJKpunctallowbreakbetweenpuncts":{
		"prefix":"CJKpunctallowbreakbetweenpuncts",
		"body":[
			"\\CJKpunctallowbreakbetweenpuncts"
		]
	},


	"CJKpunctnobreakbetweenpuncts":{
		"prefix":"CJKpunctnobreakbetweenpuncts",
		"body":[
			"\\CJKpunctnobreakbetweenpuncts"
		]
	},


	"CJKpunctsetkern{}{}{}":{
		"prefix":"CJKpunctsetkern",
		"body":[
			"\\CJKpunctsetkern{${1:标点1}}{${2:标点2}}{${3:间距}}"
		]
	},


	"CJKpunctmapfamily{}{}{}{}{}":{
		"prefix":"CJKpunctmapfamily",
		"body":[
			"\\CJKpunctmapfamily{${1:encoding}}{${2:family}}{${3:series}}{${4:shape}}{${5:punct family}}"
		]
	}
}
