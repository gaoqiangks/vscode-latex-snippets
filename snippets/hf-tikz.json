{


	"tikzmarkin{}":{
		"prefix":"tikzmarkin",
		"body":[
			"\\tikzmarkin{${1:marker-id}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tikzmarkin{}()()":{
		"prefix":"tikzmarkin",
		"body":[
			"\\tikzmarkin{${1:marker-id}}(${2:x1},${3:y1})(${4:x2},${5:y2})"
		],
		"luasnip": {"priority": 3000} 
	},


	"tikzmarkin[]{}":{
		"prefix":"tikzmarkin",
		"body":[
			"\\tikzmarkin[${2:options%keyvals}]{${1:marker-id}}"
		],
		"luasnip": {"priority": 1000} 
	},


