{


	"tikzmarkin":{
		"prefix":"tikzmarkin",
		"body":[
			"\\tikzmarkin{${1:marker-id}}"
		]
	},


	"tikzmarkin":{
		"prefix":"tikzmarkin",
		"body":[
			"\\tikzmarkin{${1:marker-id}}(${2:x1},${3:y1})(${4:x2},${5:y2})"
