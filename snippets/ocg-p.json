{


	"setocgtabularheader{}{}":{
		"prefix":"setocgtabularheader",
		"body":[
			"\\setocgtabularheader{${1:column name}}{${2:header%text}}"
		]
	},


	"toggleocgs{}{}":{
		"prefix":"toggleocgs",
		"body":[
			"\\toggleocgs{${1:layerid1 layerid2 ...}}{${2:display}}"
		]
	},


	"showocgs{}{}":{
		"prefix":"showocgs",
		"body":[
			"\\showocgs{${1:layerid1 layerid2 ...}}{${2:display}}"
		]
	},


	"hideocgs{}{}":{
		"prefix":"hideocgs",
		"body":[
			"\\hideocgs{${1:layerid1 layerid2 ...}}{${2:display}}"
		]
	},


	"setocgs{}{}{}{}":{
		"prefix":"setocgs",
		"body":[
			"\\setocgs{${1:tlayerid1 ...}}{${2:slayerid1 ...}}{${3:hlayerid1 ...}}{${4:display}}"
		]
	},


	"toggleocgs[]{}{}":{
		"prefix":"toggleocgs",
		"body":[
			"\\toggleocgs[${3:option%keyvals}]{${1:layerid1 layerid2 ...}}{${2:display}}"
		]
	},


	"showocgs[]{}{}":{
		"prefix":"showocgs",
		"body":[
			"\\showocgs[${3:option%keyvals}]{${1:layerid1 layerid2 ...}}{${2:display}}"
		]
	},


	"hideocgs[]{}{}":{
		"prefix":"hideocgs",
		"body":[
			"\\hideocgs[${3:option%keyvals}]{${1:layerid1 layerid2 ...}}{${2:display}}"
		]
	},


	"setocgs[]{}{}{}{}":{
		"prefix":"setocgs",
		"body":[
			"\\setocgs[${5:option%keyvals}]{${1:tlayerid1 ...}}{${2:slayerid1 ...}}{${3:hlayerid1 ...}}{${4:display}}"
		]
	},


	"ocgtabular":{
		"prefix":"ocgtabular",
		"body":[
			"\\begin{ocgtabular}",
				"\t$1",
			"\\end{ocgtabular}"
		]
	},


	"ocg":{
