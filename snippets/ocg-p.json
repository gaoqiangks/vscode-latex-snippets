{


	"setocgtabularheader{}{}":{
		"prefix":"setocgtabularheader",
		"body":[
			"\\setocgtabularheader{${1:column name}}{${2:header%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"toggleocgs{}{}":{
		"prefix":"toggleocgs",
		"body":[
			"\\toggleocgs{${1:layerid1 layerid2 ...}}{${2:display}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"toggleocgs[]{}{}":{
		"prefix":"toggleocgs",
		"body":[
			"\\toggleocgs[${3:option%keyvals}]{${1:layerid1 layerid2 ...}}{${2:display}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"showocgs{}{}":{
		"prefix":"showocgs",
		"body":[
			"\\showocgs{${1:layerid1 layerid2 ...}}{${2:display}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"showocgs[]{}{}":{
		"prefix":"showocgs",
		"body":[
			"\\showocgs[${3:option%keyvals}]{${1:layerid1 layerid2 ...}}{${2:display}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"hideocgs{}{}":{
		"prefix":"hideocgs",
		"body":[
			"\\hideocgs{${1:layerid1 layerid2 ...}}{${2:display}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"hideocgs[]{}{}":{
		"prefix":"hideocgs",
		"body":[
			"\\hideocgs[${3:option%keyvals}]{${1:layerid1 layerid2 ...}}{${2:display}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"setocgs{}{}{}{}":{
		"prefix":"setocgs",
		"body":[
			"\\setocgs{${1:tlayerid1 ...}}{${2:slayerid1 ...}}{${3:hlayerid1 ...}}{${4:display}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"setocgs[]{}{}{}{}":{
		"prefix":"setocgs",
		"body":[
			"\\setocgs[${5:option%keyvals}]{${1:tlayerid1 ...}}{${2:slayerid1 ...}}{${3:hlayerid1 ...}}{${4:display}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"ocgtabular{}{}{}":{
		"prefix":"ocgtabular",
		"body":[
			"\\begin{ocgtabular}",
				"\t{${1:cols}}{${2:database}}{${3:options}}",
			"\\end{ocgtabular}"
		],
		"luasnip": {"priority": 3000} 
	},


	"ocgtabular[]{}{}{}":{
		"prefix":"ocgtabular",
		"body":[
			"\\begin{ocgtabular}",
				"\t[${4:pos}]{${1:cols}}{${2:database}}{${3:options}}",
			"\\end{ocgtabular}"
		],
		"luasnip": {"priority": 1000} 
	},


	"ocg{}{}{}":{
		"prefix":"ocg",
		"body":[
			"\\begin{ocg}",
				"\t{${1:layer name}}{${2:layer id}}{${3:initial visibility}}",
			"\\end{ocg}"
		],
		"luasnip": {"priority": 3000} 
	},


	"ocg[]{}{}{}":{
		"prefix":"ocg",
		"body":[
			"\\begin{ocg}",
				"\t[${4:options%keyvals}]{${1:layer name}}{${2:layer id}}{${3:initial visibility}}",
			"\\end{ocg}"
		],
		"luasnip": {"priority": 1000} 
	}
}
