{


	"vwcolsetup{}":{
		"prefix":"vwcolsetup",
		"body":[
			"\\vwcolsetup{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"vwcol":{
		"prefix":"vwcol",
		"body":[
			"\\begin{vwcol}",
				"\t$1",
			"\\end{vwcol}"
		],
		"luasnip": {"priority": 3000} 
	},


	"vwcol[]":{
		"prefix":"vwcol",
		"body":[
			"\\begin{vwcol}",
				"\t[${1:options%keyvals}]",
			"\\end{vwcol}"
		],
		"luasnip": {"priority": 1000} 
	}
}
