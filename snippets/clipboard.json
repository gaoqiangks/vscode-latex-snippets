{


	"newclipboard{}":{
		"prefix":"newclipboard",
		"body":[
			"\\newclipboard{${1:basename}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"openclipboard{}":{
		"prefix":"openclipboard",
		"body":[
			"\\openclipboard{${1:basename}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"clipboard{}{}":{
		"prefix":"clipboard",
		"body":[
			"\\clipboard{${1:key%plain}}{${2:content%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"Copy{}{}":{
		"prefix":"Copy",
		"body":[
			"\\Copy{${1:key%plain}}{${2:content%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"Paste{}":{
		"prefix":"Paste",
		"body":[
			"\\Paste{${1:key%plain}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
