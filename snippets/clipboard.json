{


	"newclipboard":{
		"prefix":"newclipboard",
		"body":[
			"\\newclipboard{${1:basename}}"
		]
	},


	"openclipboard":{
		"prefix":"openclipboard",
		"body":[
			"\\openclipboard{${1:basename}}"
		]
	},


	"clipboard":{
		"prefix":"clipboard",
		"body":[
			"\\clipboard{${1:key%plain}}{${2:content%text}}"
		]
	},


	"Copy":{
		"prefix":"Copy",
		"body":[
			"\\Copy{${1:key%plain}}{${2:content%text}}"
		]
	},


	"Paste":{
		"prefix":"Paste",
		"body":[
			"\\Paste{${1:key%plain}}"
		]
	}
}
