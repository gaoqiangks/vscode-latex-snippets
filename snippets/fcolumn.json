{


	"sumline":{
		"prefix":"sumline",
		"body":[
			"\\sumline"
		]
	},


	"resetsumline":{
		"prefix":"resetsumline",
		"body":[
			"\\resetsumline"
		]
	},


	"leeg{}":{
		"prefix":"leeg",
		"body":[
			"\\leeg{${1:text}}"
		]
	},


	"checkfcolumns{}{}":{
		"prefix":"checkfcolumns",
		"body":[
			"\\checkfcolumns{${1:num1}}{${2:num2}}"
		]
	},


	"sumline[]":{
		"prefix":"sumline",
		"body":[
			"\\sumline[${1:space-above%l}]"
		]
	},


	"sumline[][]":{
		"prefix":"sumline",
		"body":[
			"\\sumline[${1:space-above%l}][${2:space-below%l}]"
		]
	}
}
