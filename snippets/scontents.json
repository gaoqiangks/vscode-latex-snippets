			"\\Scontents*{${1:argument}}"
		]
	},


	"Scontents*[]{}":{
		"prefix":"*Scontents",
		"body":[
			"\\Scontents*[${2:options%keyvals}]{${1:argument}}"
		]
	},


	"newenvsc{}[]":{
		"prefix":"newenvsc",
		"body":[
			"\\newenvsc{${1:envname}}[${2:initial keys%keyvals}]"
		]
	},


	"Scontents[]{}":{
		"prefix":"Scontents",
		"body":[
			"\\Scontents[${2:options%keyvals}]{${1:argument}}"
		]
	},


	"getstored[]{}":{
		"prefix":"getstored",
		"body":[
			"\\getstored[${2:index}]{${1:seq name}}"
		]
	},


	"foreachsc[]{}":{
		"prefix":"foreachsc",
		"body":[
			"\\foreachsc[${2:options%keyvals}]{${1:seq name}}"
		]
	},


	"typestored[]{}":{
		"prefix":"typestored",
		"body":[
			"\\typestored[${2:options%keyvals}]{${1:seq name}}"
		]
	},


	"meaningsc[]{}":{
		"prefix":"meaningsc",
		"body":[
			"\\meaningsc[${2:options%keyvals}]{${1:seq name}}"
		]
	},


	"scontents":{
		"prefix":"scontents",
		"body":[
			"\\begin{scontents}",
				"\t$1",
			"\\end{scontents}"
		]
	},


	"verbatimsc":{
		"prefix":"verbatimsc",
		"body":[
			"\\begin{verbatimsc}",
				"\t$1",
			"\\end{verbatimsc}"
		]
	},


	"scontents":{
