{


	"setupsc{}":{
		"prefix":"setupsc",
		"body":[
			"\\setupsc{${1:options%keyvals}}"
		]
	},


	"newenvsc{}":{
		"prefix":"newenvsc",
		"body":[
			"\\newenvsc{${1:envname}}"
		]
	},


	"Scontents{}":{
		"prefix":"Scontents",
		"body":[
			"\\Scontents{${1:argument}}"
		]
	},


	"getstored{}":{
		"prefix":"getstored",
		"body":[
			"\\getstored{${1:seq name}}"
		]
	},


	"foreachsc{}":{
		"prefix":"foreachsc",
		"body":[
			"\\foreachsc{${1:seq name}}"
		]
	},


	"typestored{}":{
		"prefix":"typestored",
		"body":[
			"\\typestored{${1:seq name}}"
		]
	},


	"meaningsc{}":{
		"prefix":"meaningsc",
		"body":[
			"\\meaningsc{${1:seq name}}"
		]
	},


	"countsc{}":{
		"prefix":"countsc",
		"body":[
			"\\countsc{${1:seq name}}"
		]
	},


