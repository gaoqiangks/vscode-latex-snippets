	"epigraph[]{}":{
		"prefix":"epigraph",
		"body":[
			"\\epigraph[${2:keyvals}]{${1:text}}"
		]
	},


	"qitem[]{}":{
		"prefix":"qitem",
		"body":[
			"\\qitem[${2:keyvals}]{${1:text}}"
		]
	},


	"epigraphs":{
		"prefix":"epigraphs",
		"body":[
			"\\begin{epigraphs}",
				"\t$1",
			"\\end{epigraphs}"
		]
	}
