	"epigraphtextindent":{
		"prefix":"epigraphtextindent",
		"body":[
			"\\epigraphtextindent"
		]
	},


	"epigraphauthorsourceindent":{
		"prefix":"epigraphauthorsourceindent",
		"body":[
			"\\epigraphauthorsourceindent"
		]
	},


	"epigraphtextwidth":{
		"prefix":"epigraphtextwidth",
		"body":[
			"\\epigraphtextwidth"
		]
	},


	"epigraphstyle":{
		"prefix":"epigraphstyle",
		"body":[
			"\\epigraphstyle"
		]
	},


	"epigraphdash":{
		"prefix":"epigraphdash",
		"body":[
			"\\epigraphdash"
		]
