{


	"point{}{}":{
		"prefix":"point",
		"body":[
			"\\point{${1:(${3:x},${4:y},${5:z})}}{${2:name}}"
		]
	},


	"axes{}":{
		"prefix":"axes",
		"body":[
			"\\axes{${1:keyvals}}"
		]
	},


	"vertex{}":{
		"prefix":"vertex",
		"body":[
			"\\vertex{${1:keyvals}}"
		]
	},


	"edge{}":{
		"prefix":"edge",
		"body":[
			"\\edge{${1:keyvals}}"
		]
	},


	"ray{}":{
		"prefix":"ray",
		"body":[
			"\\ray{${1:keyvals}}"
		]
	},


	"cone{}":{
		"prefix":"cone",
		"body":[
			"\\cone{${1:keyvals}}"
		]
	},


	"polygon{}":{
		"prefix":"polygon",
		"body":[
			"\\polygon{${1:keyvals}}"
		]
	},


	"halfspace{}":{
		"prefix":"halfspace",
		"body":[
			"\\halfspace{${1:keyvals}}"
		]
	},


	"hyperplane{}":{
		"prefix":"hyperplane",
		"body":[
			"\\hyperplane{${1:keyvals}}"
		]
	},


	"polyhedron":{
		"prefix":"polyhedron",
		"body":[
			"\\begin{polyhedron}",
				"\t$1",
			"\\end{polyhedron}"
		]
	}
}
