{


	"bibliographyunit":{
		"prefix":"bibliographyunit",
		"body":[
			"\\bibliographyunit"
		]
	},


	"putbib":{
		"prefix":"putbib",
		"body":[
			"\\putbib"
		]
	},


	"defaultbibliography{}":{
		"prefix":"defaultbibliography",
		"body":[
			"\\defaultbibliography{${1:bib file}}"
		]
	},


	"defaultbibliographystyle{}":{
		"prefix":"defaultbibliographystyle",
		"body":[
			"\\defaultbibliographystyle{${1:style%keyvals}}"
		]
	},


	"cite*{}":{
		"prefix":"*cite",
		"body":[
			"\\cite*{${1:bibid}}"
		]
	},


	"nocite*{}":{
		"prefix":"*nocite",
		"body":[
			"\\nocite*{${1:bibid}}"
		]
	},


	"bibliography*{}":{
		"prefix":"*bibliography",
		"body":[
			"\\bibliography*{${1:bib file}}"
		]
	},


	"bibliographystyle*{}":{
		"prefix":"*bibliographystyle",
		"body":[
			"\\bibliographystyle*{${1:style%keyvals}}"
		]
	},


	"bibliographyunit[]":{
		"prefix":"bibliographyunit",
		"body":[
			"\\bibliographyunit[${1:unit}]"
		]
	},


	"putbib[]":{
		"prefix":"putbib",
		"body":[
			"\\putbib[${1:bib file}]"
		]
	},


	"bibunit":{
		"prefix":"bibunit",
		"body":[
			"\\begin{bibunit}",
				"\t$1",
			"\\end{bibunit}"
		]
	},


	"bibunit":{
		"prefix":"bibunit",
		"body":[
			"\\begin{bibunit}",
				"\t$1",
			"\\end{bibunit}"
		]
	}
}
