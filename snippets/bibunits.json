{


	"bibliographyunit":{
		"prefix":"bibliographyunit",
		"body":[
			"\\bibliographyunit"
		]
	},


	"bibliographyunit":{
		"prefix":"bibliographyunit",
		"body":[
			"\\bibliographyunit[${1:unit}]"
		]
	},


	"cite*":{
		"prefix":"cite*",
		"body":[
			"\\cite*{${1:bibid}}"
		]
	},


	"nocite*":{
		"prefix":"nocite*",
		"body":[
			"\\nocite*{${1:bibid}}"
		]
	},


	"putbib":{
		"prefix":"putbib",
		"body":[
			"\\putbib"
		]
	},


	"putbib":{
		"prefix":"putbib",
		"body":[
			"\\putbib[${1:bib file}]"
		]
	},


	"defaultbibliography":{
		"prefix":"defaultbibliography",
		"body":[
			"\\defaultbibliography{${1:bib file}}"
		]
	},


	"defaultbibliographystyle":{
		"prefix":"defaultbibliographystyle",
		"body":[
			"\\defaultbibliographystyle{${1:style%keyvals}}"
		]
	},


	"bibliography*":{
		"prefix":"bibliography*",
		"body":[
			"\\bibliography*{${1:bib file}}"
		]
	},


	"bibliographystyle*":{
		"prefix":"bibliographystyle*",
		"body":[
			"\\bibliographystyle*{${1:style%keyvals}}"
		]
	},


	"bibunit":{
		"prefix":"bibunit",
		"body":[
			"\\begin{bibunit}",
			"\t$1",
			"\\end{bibunit}"
		]
	}
}
