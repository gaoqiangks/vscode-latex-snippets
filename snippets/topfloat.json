{


	"topI":{
		"prefix":"topI",
		"body":[
			"\\topI"
		]
	},


	"endtopI":{
		"prefix":"endtopI",
		"body":[
			"\\endtopI"
		]
	},


	"topII":{
		"prefix":"topII",
		"body":[
			"\\topII"
		]
	},


	"endtopII":{
		"prefix":"endtopII",
		"body":[
			"\\endtopII"
		]
	},


	"tabcap{}":{
		"prefix":"tabcap",
		"body":[
			"\\tabcap{${1:text}}"
		]
	},


	"figcap{}":{
		"prefix":"figcap",
		"body":[
			"\\figcap{${1:text}}"
		]
	},


	"tabcap[]{}":{
		"prefix":"tabcap",
		"body":[
			"\\tabcap[${2:width}]{${1:text}}"
		]
	},


	"figcap[]{}":{
		"prefix":"figcap",
		"body":[
			"\\figcap[${2:width}]{${1:text}}"
		]
	},


	"topfloat":{
		"prefix":"topfloat",
		"body":[
			"\\begin{topfloat}",
				"\t$1",
			"\\end{topfloat}"
		]
	}
}
