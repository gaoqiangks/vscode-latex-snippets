{


	"countword{}":{
		"prefix":"countword",
		"body":[
			"\\countword{${1:word%text}}"
		]
	},


	"CWClosePar":{
		"prefix":"CWClosePar",
		"body":[
			"\\CWClosePar"
		]
	},


	"CWtextscript{}":{
		"prefix":"CWtextscript",
		"body":[
			"\\CWtextscript{${1:text}}"
		]
	},


	"countword[]{}":{
		"prefix":"countword",
		"body":[
			"\\countword[${2:subst%text}]{${1:word%text}}"
		]
	}
}
