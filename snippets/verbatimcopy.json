{


	"setOutputDir{}":{
		"prefix":"setOutputDir",
		"body":[
			"\\setOutputDir{${1:directory%definition}}"
		]
	},


	"VerbatimCopy{}{}":{
		"prefix":"VerbatimCopy",
		"body":[
			"\\VerbatimCopy{${1:input file%file}}{${2:output file%file}}"
		]
	},


	"VCverbaction{}{}":{
		"prefix":"VCverbaction",
		"body":[
			"\\VCverbaction{${1:arg1}}{${2:arg2}}"
		]
	}
}
