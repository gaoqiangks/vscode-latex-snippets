{


	"LinkNodes{}":{
		"prefix":"LinkNodes",
		"body":[
			"\\LinkNodes{${1:expression}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"LinkNodes[]{}":{
		"prefix":"LinkNodes",
		"body":[
			"\\LinkNodes[${2:keyvals}]{${1:expression}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"AddNode":{
		"prefix":"AddNode",
		"body":[
			"\\AddNode"
		],
		"luasnip": {"priority": 3000} 
	},


	"AddNode[]":{
		"prefix":"AddNode",
		"body":[
			"\\AddNode[${1:number}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"NodesList":{
		"prefix":"NodesList",
		"body":[
			"\\begin{NodesList}",
				"\t$1",
			"\\end{NodesList}"
		],
		"luasnip": {"priority": 3000} 
	},


	"NodesList[]":{
		"prefix":"NodesList",
		"body":[
			"\\begin{NodesList}",
				"\t[${1:keyvals}]",
			"\\end{NodesList}"
		],
		"luasnip": {"priority": 1000} 
	}
}
