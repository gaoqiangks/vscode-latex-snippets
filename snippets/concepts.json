{


	"NewConcept{}{}":{
		"prefix":"NewConcept",
		"body":[
			"\\NewConcept{${1:concept key}}{${2:options%keyvals}}"
		]
	},


	"ConceptOption{}{}":{
		"prefix":"ConceptOption",
		"body":[
			"\\ConceptOption{${1:concept key}}{${2:option%keyvals}}"
		]
	},


	"ConceptName{}":{
		"prefix":"ConceptName",
		"body":[
			"\\ConceptName{${1:concept key}}"
		]
	},


	"ConceptName^{}":{
		"prefix":"ConceptName^",
		"body":[
			"\\ConceptName^{${1:concept key}}"
		]
	},


	"ConceptName^*{}":{
		"prefix":"ConceptName^",
		"body":[
			"\\ConceptName^*{${1:concept key}}"
		]
	},


	"ConceptSymbol{}":{
		"prefix":"ConceptSymbol",
		"body":[
			"\\ConceptSymbol{${1:concept key}}"
		]
	},


	"ConceptSymbols{}{}":{
		"prefix":"ConceptSymbols",
		"body":[
			"\\ConceptSymbols{${1:concept key}}{${2:symbol indices}}"
		]
	},


	"ConceptNameAndSymbols{}{}":{
		"prefix":"ConceptNameAndSymbols",
		"body":[
			"\\ConceptNameAndSymbols{${1:concept key}}{${2:symbol indices}}"
		]
	},


	"ConceptName*{}":{
		"prefix":"*ConceptName",
		"body":[
			"\\ConceptName*{${1:concept key}}"
		]
	},


	"ConceptSymbol{}[]":{
		"prefix":"ConceptSymbol",
		"body":[
			"\\ConceptSymbol{${1:concept key}}[${2:symbol index}]"
		]
	},


	"ConceptSymbols{}[]{}":{
		"prefix":"ConceptSymbols",
		"body":[
			"\\ConceptSymbols{${1:concept key}}[${3:separator}]{${2:symbol indices}}"
		]
	},


	"ConceptSymbols{}[][]{}":{
		"prefix":"ConceptSymbols",
		"body":[
			"\\ConceptSymbols{${1:concept key}}[${3:separator}][${4:last separator}]{${2:symbol indices}}"
		]
	},


	"ConceptNameAndSymbols{}[]{}":{
		"prefix":"ConceptNameAndSymbols",
		"body":[
			"\\ConceptNameAndSymbols{${1:concept key}}[${3:separator}]{${2:symbol indices}}"
		]
	},


	"ConceptNameAndSymbols{}[][]{}":{
		"prefix":"ConceptNameAndSymbols",
		"body":[
			"\\ConceptNameAndSymbols{${1:concept key}}[${3:separator}][${4:last separator}]{${2:symbol indices}}"
		]
	}
}
