{


	"pdsetup":{
		"prefix":"pdsetup",
		"body":[
			"\\pdsetup{${1:options%keyvals}}"
		]
	},


	"maketitle":{
		"prefix":"maketitle",
		"body":[
			"\\maketitle[${1:options%keyvals}]"
		]
	},


	"pause":{
		"prefix":"pause",
		"body":[
