{


	"simplenode{}{}":{
		"prefix":"simplenode",
		"body":[
			"\\simplenode{${1:name}}{${2:text}}"
		]
	},


	"examplenode{}{}":{
		"prefix":"examplenode",
		"body":[
			"\\examplenode{${1:name}}{${2:text}}"
		]
	},


	"alertnode{}{}":{
		"prefix":"alertnode",
		"body":[
			"\\alertnode{${1:name}}{${2:text}}"
		]
	},


	"warnnode{}{}":{
		"prefix":"warnnode",
		"body":[
			"\\warnnode{${1:name}}{${2:text}}"
		]
	},


	"link{}{}":{
		"prefix":"link",
		"body":[
			"\\link{${1:name1}}{${2:name2}}"
		]
	},


	"gettikzxy{}{}{}":{
		"prefix":"gettikzxy",
		"body":[
			"\\gettikzxy{${1:(${4:x},${5:y})}}{${2:xcmd%cmd}}{${3:ycmd%cmd}}"
		]
	},


	"mynode{}{}":{
		"prefix":"mynode",
		"body":[
			"\\mynode{${1:draw color}}{${2:fill color}}"
		]
	},


	"myline{}{}":{
		"prefix":"myline",
		"body":[
			"\\myline{${1:anchor1}}{${2:anchor2}}"
		]
	}
}
