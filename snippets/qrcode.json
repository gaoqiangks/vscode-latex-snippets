{


	"qrcode{}":{
		"prefix":"qrcode",
		"body":[
			"\\qrcode{${1:URL}}"
		]
	},


	"qrset{}":{
		"prefix":"qrset",
		"body":[
			"\\qrset{${1:options%keyvals}}"
		]
	},


	"qrcode*{}":{
		"prefix":"*qrcode",
		"body":[
			"\\qrcode*{${1:URL}}"
		]
	},


	"qrcode*[]{}":{
		"prefix":"*qrcode",
		"body":[
			"\\qrcode*[${2:options%keyvals}]{${1:URL}}"
		]
	},


	"qrcode[]{}":{
		"prefix":"qrcode",
		"body":[
			"\\qrcode[${2:options%keyvals}]{${1:URL}}"
		]
	}
}
