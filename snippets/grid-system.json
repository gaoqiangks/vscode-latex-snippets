{


	"Row":{
		"prefix":"Row",
		"body":[
			"\\begin{Row}",
				"\t$1",
			"\\end{Row}"
		],
		"luasnip": {"priority": 3000} 
	},


	"Row[]":{
		"prefix":"Row",
		"body":[
			"\\begin{Row}",
				"\t[${1:options%keyvals}]",
			"\\end{Row}"
		],
		"luasnip": {"priority": 1000} 
	},


	"Cell":{
		"prefix":"Cell",
		"body":[
			"\\begin{Cell}",
				"\t$1",
			"\\end{Cell}"
		],
		"luasnip": {"priority": 3000} 
	},


	"row{}{}":{
		"prefix":"row",
		"body":[
			"\\begin{row}",
				"\t{${1:num of cols}}{${2:num of cells}}",
			"\\end{row}"
		],
		"luasnip": {"priority": 3000} 
	},


	"row[]{}{}":{
		"prefix":"row",
		"body":[
			"\\begin{row}",
				"\t[${3:options%keyvals}]{${1:num of cols}}{${2:num of cells}}",
			"\\end{row}"
		],
		"luasnip": {"priority": 1000} 
	},


	"cell{}":{
		"prefix":"cell",
		"body":[
			"\\begin{cell}",
				"\t{${1:num of cols}}",
			"\\end{cell}"
		],
		"luasnip": {"priority": 3000} 
	}
}
