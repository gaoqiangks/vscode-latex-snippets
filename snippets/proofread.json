{


	"skp":{
		"prefix":"skp",
		"body":[
			"\\skp"
		]
	},


	"del{}":{
		"prefix":"del",
		"body":[
			"\\del{${1:phrase%text}}"
		]
	},


	"yel{}":{
		"prefix":"yel",
		"body":[
			"\\yel{${1:phrase%text}}"
		]
	},


	"add{}":{
		"prefix":"add",
		"body":[
			"\\add{${1:phrase%text}}"
		]
	},


	"rep{}{}":{
		"prefix":"rep",
		"body":[
			"\\rep{${1:phrase%text}}{${2:replacement}}"
		]
	},


	"com{}":{
		"prefix":"com",
		"body":[
			"\\com{${1:comment%text}}"
		]
	},


	"hilite{}":{
		"prefix":"hilite",
		"body":[
			"\\hilite{${1:phrase%text}}"
		]
	},


	"skp[]":{
		"prefix":"skp",
		"body":[
			"\\skp[${1:number}]"
		]
	},


	"yel[]{}":{
		"prefix":"yel",
		"body":[
			"\\yel[${2:comment%text}]{${1:phrase%text}}"
		]
	},


	"hilite[]{}":{
		"prefix":"hilite",
		"body":[
			"\\hilite[${2:options%keyvals}]{${1:phrase%text}}"
		]
	}
}
