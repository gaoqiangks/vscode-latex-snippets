{


	"thesubequation":{
		"prefix":"thesubequation",
		"body":[
			"\\thesubequation"
		]
	},


	"slabel":{
		"prefix":"slabel",
		"body":[
			"\\slabel{${1:label}}"
		]
	},


	"subeqnarray":{
		"prefix":"subeqnarray",
		"body":[
			"\\begin{subeqnarray}",
			"\t$1",
			"\\end{subeqnarray}"
		]
	},


	"subeqnarray*":{
		"prefix":"subeqnarray*",
		"body":[
			"\\begin{subeqnarray*}",
			"\t$1",
			"\\end{subeqnarray*}"
		]
	}
}
