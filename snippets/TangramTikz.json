{


	"TangramTikz{}":{
		"prefix":"TangramTikz",
		"body":[
			"\\TangramTikz{${1:tangram name}}"
		]
	},


	"TangramTikz<TikZ options>{}":{
		"prefix":"TangramTikz<TikZ options>",
		"body":[
			"\\TangramTikz<TikZ options>{${1:tangram name}}"
		]
	},


	"TangramTikz[]{}":{
		"prefix":"TangramTikz",
		"body":[
			"\\TangramTikz[${2:options%keyvals}]{${1:tangram name}}"
		]
	},


	"TangramTikz[]<>{}":{
		"prefix":"TangramTikz",
		"body":[
			"\\TangramTikz[${2:options%keyvals}]<${3:TikZ options}>{${1:tangram name}}"
		]
	},


	"PieceTangram[](){}":{
		"prefix":"PieceTangram",
		"body":[
			"\\PieceTangram[${2:options%keyvals}](${3:x},${4:y}){${1:piece name}}"
		]
	},


	"PieceTangram[]<>(){}":{
		"prefix":"PieceTangram",
		"body":[
			"\\PieceTangram[${2:options%keyvals}]<${3:TikZ options}>(${4:x},${5:y}){${1:piece name}}"
		]
	},


	"EnvTangramTikz":{
		"prefix":"EnvTangramTikz",
		"body":[
			"\\begin{EnvTangramTikz}",
				"\t$1",
			"\\end{EnvTangramTikz}"
		]
	},


	"EnvTangramTikz":{
		"prefix":"EnvTangramTikz",
		"body":[
			"\\begin{EnvTangramTikz}",
				"\t$1",
			"\\end{EnvTangramTikz}"
		]
	},


	"EnvTangramTikz":{
		"prefix":"EnvTangramTikz",
		"body":[
			"\\begin{EnvTangramTikz}",
				"\t$1",
			"\\end{EnvTangramTikz}"
		]
	}
}
