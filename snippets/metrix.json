{


	"metricsymbols{}":{
		"prefix":"metricsymbols",
		"body":[
			"\\metricsymbols{${1:symbols}}"
		]
	},


	"metrics{}{}":{
		"prefix":"metrics",
		"body":[
			"\\metrics{${1:symbols}}{${2:syllables}}"
		]
	},


	"acct{}":{
		"prefix":"acct",
		"body":[
			"\\acct{${1:vowel}}"
		]
	},


	"brv{}":{
		"prefix":"brv",
		"body":[
			"\\brv{${1:vowel}}"
		]
	},


	"lng{}":{
		"prefix":"lng",
		"body":[
			"\\lng{${1:vowel}}"
		]
	},


	"bow{}":{
		"prefix":"bow",
		"body":[
			"\\bow{${1:syllables}}"
		]
	},


	"verseref{}":{
		"prefix":"verseref",
		"body":[
			"\\verseref{${1:reference}}"
		]
	},


	"setmetrixvar{}{}":{
		"prefix":"setmetrixvar",
		"body":[
			"\\setmetrixvar{${1:variable%keyvals}}{${2:value}}"
		]
	},


	"usemetrixvar{}":{
		"prefix":"usemetrixvar",
		"body":[
			"\\usemetrixvar{${1:variable%keyvals}}"
		]
	},


	"metricsymbols*{}":{
		"prefix":"*metricsymbols",
		"body":[
			"\\metricsymbols*{${1:symbols}}"
		]
	},


	"metricsymbols*[]{}":{
		"prefix":"*metricsymbols",
		"body":[
			"\\metricsymbols*[${2:number=higlighting}]{${1:symbols}}"
		]
	},


	"metricsymbols[]{}":{
		"prefix":"metricsymbols",
		"body":[
			"\\metricsymbols[${2:number=higlighting}]{${1:symbols}}"
		]
	},


	"metrics[]{}{}":{
		"prefix":"metrics",
		"body":[
			"\\metrics[${3:number=higlighting}]{${1:symbols}}{${2:syllables}}"
		]
	},


	"symbolline":{
		"prefix":"symbolline",
		"body":[
			"\\begin{symbolline}",
				"\t$1",
			"\\end{symbolline}"
		]
	},


	"metricverses":{
		"prefix":"metricverses",
		"body":[
			"\\begin{metricverses}",
				"\t$1",
			"\\end{metricverses}"
		]
	},


	"metricverses":{
		"prefix":"metricverses",
		"body":[
			"\\begin{metricverses}",
				"\t$1",
			"\\end{metricverses}"
		]
	}
}
