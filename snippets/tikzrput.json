			"\\rput{${1:angle}}(${3:x},${4:y}){${2:object%text}}"
		]
	},


	"tikzrputPtVirCode":{
		"prefix":"tikzrputPtVirCode",
		"body":[
			"\\tikzrputPtVirCode"
		]
	},


	"tikzrputAtCode":{
		"prefix":"tikzrputAtCode",
		"body":[
			"\\tikzrputAtCode"
		]
	},


	"tikzrputTwoPtCode":{
		"prefix":"tikzrputTwoPtCode",
		"body":[
			"\\tikzrputTwoPtCode"
		]
	},


	"rput[](){}":{
		"prefix":"rput",
		"body":[
			"\\rput[${2:refpoint}](${3:x},${4:y}){${1:object%text}}"
		]
	},


	"rput[]{}(){}":{
		"prefix":"rput",
