{


	"verbatiminput":{
		"prefix":"verbatiminput",
		"body":[
			"\\verbatiminput{${1:file}}"
		]
	},


	"verbatiminput*":{
		"prefix":"verbatiminput*",
		"body":[
			"\\verbatiminput*{${1:file}}"
		]
	},


	"comment":{
		"prefix":"comment",
		"body":[
			"\\begin{comment}",
			"\t$1",
			"\\end{comment}"
		]
	}
}
