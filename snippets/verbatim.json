{


	"verbatiminput{}":{
		"prefix":"verbatiminput",
		"body":[
			"\\verbatiminput{${1:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"verbatiminput*{}":{
		"prefix":"*verbatiminput",
		"body":[
			"\\verbatiminput*{${1:file}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"comment":{
		"prefix":"comment",
		"body":[
			"\\begin{comment}",
				"\t$1",
			"\\end{comment}"
		],
		"luasnip": {"priority": 3000} 
	}
}
