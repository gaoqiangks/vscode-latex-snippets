{


	"includecomment{}":{
		"prefix":"includecomment",
		"body":[
			"\\includecomment{${1:envname}}"
		]
	},


	"excludecomment{}":{
		"prefix":"excludecomment",
		"body":[
			"\\excludecomment{${1:envname}}"
		]
	},


	"CommentCutFile":{
		"prefix":"CommentCutFile",
		"body":[
			"\\CommentCutFile"
		]
	},


	"ProcessCutFile":{
		"prefix":"ProcessCutFile",
		"body":[
			"\\ProcessCutFile"
		]
	},


	"generalcomment{}{}{}":{
		"prefix":"generalcomment",
		"body":[
			"\\generalcomment{${1:envname}}{${2:begdef}}{${3:enddef}}"
		]
	},


	"specialcomment{}{}{}":{
		"prefix":"specialcomment",
		"body":[
			"\\specialcomment{${1:envname}}{${2:begdef}}{${3:enddef}}"
		]
	},


	"Thiscomment{}":{
		"prefix":"Thiscomment",
		"body":[
			"\\Thiscomment{${1:line stuff}}"
		]
	},


	"WriteCommentLine{}":{
		"prefix":"WriteCommentLine",
		"body":[
			"\\WriteCommentLine{${1:line stuff}}"
		]
	},


	"ThisComment{}":{
		"prefix":"ThisComment",
		"body":[
			"\\ThisComment{${1:line stuff}}"
		]
	},


	"processcomment{}{}{}{}":{
		"prefix":"processcomment",
		"body":[
			"\\processcomment{${1:envname}}{${2:def}}{${3:begdef}}{${4:enddef}}"
		]
	},


	"comment":{
		"prefix":"comment",
		"body":[
			"\\begin{comment}",
				"\t$1",
			"\\end{comment}"
		]
	}
}
