{


	"crop":{
		"prefix":"crop",
		"body":[
			"\\crop"
		]
	},


	"cropdef{}{}{}{}{}":{
		"prefix":"cropdef",
		"body":[
			"\\cropdef{${1:ul-macro}}{${2:ur-macro}}{${3:ll-macro}}{${4:lr-macro}}{${5:mode name}}"
		]
	},


	"stockwidth":{
		"prefix":"stockwidth",
		"body":[
			"\\stockwidth"
		]
	},


	"stockheight":{
		"prefix":"stockheight",
		"body":[
			"\\stockheight"
		]
	},


	"crop[]":{
		"prefix":"crop",
		"body":[
			"\\crop[${1:options%keyvals}]"
		]
	},


	"cropdef[]{}{}{}{}{}":{
		"prefix":"cropdef",
		"body":[
			"\\cropdef[${6:page-info macro}]{${1:ul-macro}}{${2:ur-macro}}{${3:ll-macro}}{${4:lr-macro}}{${5:mode name}}"
		]
	}
}
