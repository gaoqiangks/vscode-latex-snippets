{


	"VautoSubF{}{}{}{}{}":{
		"prefix":"VautoSubF",
		"body":[
			"\\VautoSubF{${1:startline}}{${2:endline}}{${3:file}}{${4:caption}}{${5:label}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"VautoSubF[]{}{}{}{}{}":{
		"prefix":"VautoSubF",
		"body":[
			"\\VautoSubF[${6:lines-numbered}]{${1:startline}}{${2:endline}}{${3:file}}{${4:caption}}{${5:label}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"VautoSfFont{}":{
		"prefix":"VautoSfFont",
		"body":[
			"\\VautoSfFont{${1:font commands}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"VautoSfFont[]{}":{
		"prefix":"VautoSfFont",
		"body":[
			"\\VautoSfFont[${2:linenum-font-commands}]{${1:font-commands}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"VautoPl{}":{
		"prefix":"VautoPl",
		"body":[
			"\\VautoPl{${1:placement}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"VautoLines{}":{
