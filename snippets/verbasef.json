{


	"VautoSubF":{
		"prefix":"VautoSubF",
		"body":[
			"\\VautoSubF{${1:startline}}{${2:endline}}{${3:file}}{${4:caption}}{${5:label}}"
		]
	},


	"VautoSubF":{
		"prefix":"VautoSubF",
		"body":[
			"\\VautoSubF[${6:lines-numbered}]{${1:startline}}{${2:endline}}{${3:file}}{${4:caption}}{${5:label}}"
		]
	},


	"VautoSfFont":{
		"prefix":"VautoSfFont",
		"body":[
			"\\VautoSfFont{${1:font commands}}"
		]
	},


	"VautoSfFont":{
		"prefix":"VautoSfFont",
		"body":[
			"\\VautoSfFont[${2:linenum-font-commands}]{${1:font-commands}}"
		]
	},


	"VautoPl":{
		"prefix":"VautoPl",
		"body":[
			"\\VautoPl{${1:placement}}"
		]
	},


	"VautoLines":{
		"prefix":"VautoLines",
		"body":[
			"\\VautoLines{${1:lines-per-page}}"
		]
	},


	"VautoLines":{
		"prefix":"VautoLines",
		"body":[
			"\\VautoLines[${2:first-page-lines}]{${1:lines-per-page}}"
		]
	},


	"VautoSubFF":{
		"prefix":"VautoSubFF",
		"body":[
			"\\VautoSubFF{${1:font commands}}"
		]
	},


	"VautoSubFF":{
		"prefix":"VautoSubFF",
		"body":[
			"\\VautoSubFF[${2:linenum-font-commands}]{${1:font-commands}}"
		]
	},


	"ifsubstr":{
		"prefix":"ifsubstr",
		"body":[
			"\\ifsubstr{${1:string}}{${2:substring}}{${3:true}}{${4:false}}"
		]
	}
}
