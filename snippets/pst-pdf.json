{


	"PDFcontainer":{
		"prefix":"PDFcontainer",
		"body":[
			"\\PDFcontainer"
		]
	},


	"savepicture":{
		"prefix":"savepicture",
		"body":[
			"\\savepicture{${1:name}}"
		]
	},


	"usepicture":{
		"prefix":"usepicture",
		"body":[
			"\\usepicture{${1:name}}"
		]
	},


	"usepicture":{
		"prefix":"usepicture",
		"body":[
			"\\usepicture[${2:options%keyvals}]{${1:name}}"
		]
	},


	"thepspicture":{
		"prefix":"thepspicture",
		"body":[
			"\\thepspicture"
		]
	},


	"postscript":{
		"prefix":"postscript",
		"body":[
			"\\begin{postscript}",
			"\t$1",
			"\\end{postscript}"
		]
	}
}
