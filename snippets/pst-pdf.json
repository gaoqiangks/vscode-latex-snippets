{


	"PDFcontainer":{
		"prefix":"PDFcontainer",
		"body":[
			"\\PDFcontainer"
		],
		"luasnip": {"priority": 3000} 
	},


	"savepicture{}":{
		"prefix":"savepicture",
		"body":[
			"\\savepicture{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"usepicture{}":{
		"prefix":"usepicture",
		"body":[
			"\\usepicture{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"usepicture[]{}":{
		"prefix":"usepicture",
		"body":[
			"\\usepicture[${2:options%keyvals}]{${1:name}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"thepspicture":{
		"prefix":"thepspicture",
		"body":[
			"\\thepspicture"
		],
		"luasnip": {"priority": 3000} 
	},


	"postscript":{
		"prefix":"postscript",
		"body":[
			"\\begin{postscript}",
				"\t$1",
			"\\end{postscript}"
		],
		"luasnip": {"priority": 3000} 
	},


	"postscript[]":{
		"prefix":"postscript",
		"body":[
			"\\begin{postscript}",
				"\t[${1:options%keyvals}]",
			"\\end{postscript}"
		],
		"luasnip": {"priority": 1000} 
	}
}
