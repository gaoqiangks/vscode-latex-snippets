{


	"embedIcon{}":{
		"prefix":"embedIcon",
		"body":[
			"\\embedIcon{${1:imagefile}}"
		]
	},


	"csOf{}":{
		"prefix":"csOf",
		"body":[
			"\\csOf{${1:name}}"
		]
	},


	"heightOf{}":{
		"prefix":"heightOf",
		"body":[
			"\\heightOf{${1:name}}"
		]
	},


	"widthOf{}":{
		"prefix":"widthOf",
		"body":[
			"\\widthOf{${1:name}}"
		]
	},


	"sortitem{}":{
		"prefix":"sortitem",
		"body":[
			"\\sortitem{${1:arg}}"
		]
	},


	"embedIcon[]{}":{
		"prefix":"embedIcon",
		"body":[
			"\\embedIcon[${2:options%keyvals}]{${1:imagefile}}"
		]
	},


	"sortitem[]{}":{
		"prefix":"sortitem",
		"body":[
			"\\sortitem[${2:opt}]{${1:arg}}"
		]
	},


	"embedding":{
		"prefix":"embedding",
		"body":[
			"\\begin{embedding}",
				"\t$1",
			"\\end{embedding}"
		]
	},


	"sortedlist":{
		"prefix":"sortedlist",
		"body":[
			"\\begin{sortedlist}",
				"\t$1",
			"\\end{sortedlist}"
		]
	}
}
