{


	"ConteqExplStyle{}":{
		"prefix":"ConteqExplStyle",
		"body":[
			"\\ConteqExplStyle{${1:explanation}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"ConteqSetDefaultLayout{}":{
		"prefix":"ConteqSetDefaultLayout",
		"body":[
			"\\ConteqSetDefaultLayout{${1:layout%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"ConteqDefineLayout{}{}{}{}{}{}{}":{
		"prefix":"ConteqDefineLayout",
		"body":[
			"\\ConteqDefineLayout{${1:name}}{${2:before}}{${3:left}}{${4:right1}}{${5:right2}}{${6:sep}}{${7:after}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"conteq":{
		"prefix":"conteq",
		"body":[
			"\\begin{conteq}",
				"\t$1",
			"\\end{conteq}"
		],
		"luasnip": {"priority": 3000} 
	},


	"conteq[]":{
		"prefix":"conteq",
		"body":[
			"\\begin{conteq}",
				"\t[${1:layout%keyvals}]",
			"\\end{conteq}"
		],
		"luasnip": {"priority": 1000} 
	}
}
