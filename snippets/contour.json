{


	"contour":{
		"prefix":"contour",
		"body":[
			"\\contour{${1:color}}{${2:text}}"
		]
	},


	"contour*":{
		"prefix":"contour*",
		"body":[
			"\\contour*{${1:color}}{${2:text}}"
		]
	},


	"contour":{
		"prefix":"contour",
		"body":[
			"\\contour[${3:number}]{${1:color}}{${2:text}}"
		]
	},


	"contourlength":{
		"prefix":"contourlength",
		"body":[
			"\\contourlength{${1:length}}"
		]
	},


	"contournumber":{
		"prefix":"contournumber",
		"body":[
			"\\contournumber{${1:number}}"
		]
	}
}
