{


	"newverbatim{}{}{}{}{}":{
		"prefix":"newverbatim",
		"body":[
			"\\newverbatim{${1:envname}}{${2:beg-def-outer}}{${3:beg-def-inner}}{${4:end-def-inner}}{${5:end-def-outer}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"newverbatim{}[]{}{}{}{}":{
		"prefix":"newverbatim",
		"body":[
			"\\newverbatim{${1:envname}}[${6:args}]{${2:beg-def-outer}}{${3:beg-def-inner}}{${4:end-def-inner}}{${5:end-def-outer}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"newverbatim{}[][]{}{}{}{}":{
		"prefix":"newverbatim",
		"body":[
			"\\newverbatim{${1:envname}}[${6:args}][${7:default}]{${2:beg-def-outer}}{${3:beg-def-inner}}{${4:end-def-inner}}{${5:end-def-outer}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"renewverbatim{}{}{}{}{}":{
		"prefix":"renewverbatim",
		"body":[
			"\\renewverbatim{${1:envname}}{${2:beg-def-outer}}{${3:beg-def-inner}}{${4:end-def-inner}}{${5:end-def-outer}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"renewverbatim{}[]{}{}{}{}":{
		"prefix":"renewverbatim",
		"body":[
			"\\renewverbatim{${1:envname}}[${6:args}]{${2:beg-def-outer}}{${3:beg-def-inner}}{${4:end-def-inner}}{${5:end-def-outer}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"renewverbatim{}[][]{}{}{}{}":{
		"prefix":"renewverbatim",
		"body":[
			"\\renewverbatim{${1:envname}}[${6:args}][${7:default}]{${2:beg-def-outer}}{${3:beg-def-inner}}{${4:end-def-inner}}{${5:end-def-outer}}"
		],
		"luasnip": {"priority": 1000} 
	}
}
