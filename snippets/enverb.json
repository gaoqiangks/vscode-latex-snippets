{


	"enverb{}":{
		"prefix":"enverb",
		"body":[
			"\\enverb{${1:keyvals}}"
		]
	},


	"enverbBody":{
		"prefix":"enverbBody",
		"body":[
			"\\enverbBody"
		]
	},


	"enverbExecute":{
		"prefix":"enverbExecute",
		"body":[
			"\\enverbExecute"
		]
	},


	"enverbListing{}{}":{
		"prefix":"enverbListing",
		"body":[
			"\\enverbListing{${1:env}}{${2:args}}"
		]
	},


	"enverbsetup{}":{
		"prefix":"enverbsetup",
		"body":[
			"\\enverbsetup{${1:keyvals}}"
		]
	},


	"enverb*{}":{
		"prefix":"*enverb",
		"body":[
			"\\enverb*{${1:keyvals}}"
		]
	},


	"enverb{}[]":{
		"prefix":"enverb",
		"body":[
			"\\enverb{${1:keyvals}}[${2:keyvals%plain}]"
		]
	}
}
