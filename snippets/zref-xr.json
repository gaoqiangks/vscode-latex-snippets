{


	"zexternaldocument{}":{
		"prefix":"zexternaldocument",
		"body":[
			"\\zexternaldocument{${1:file}}"
		]
	},


	"zxrsetup{}":{
		"prefix":"zxrsetup",
		"body":[
			"\\zxrsetup{${1:options%keyvals}}"
		]
	},


	"zexternaldocument*{}":{
		"prefix":"*zexternaldocument",
		"body":[
			"\\zexternaldocument*{${1:file}}"
		]
	},


	"zexternaldocument*[]{}[]":{
		"prefix":"*zexternaldocument",
		"body":[
			"\\zexternaldocument*[${2:prefix}]{${1:file}}[${3:url}]"
		]
	},


	"zexternaldocument[]{}[]":{
		"prefix":"zexternaldocument",
		"body":[
			"\\zexternaldocument[${2:prefix}]{${1:file}}[${3:url}]"
		]
	}
}
