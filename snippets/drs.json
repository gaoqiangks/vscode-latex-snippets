{


	"drs{}{}":{
		"prefix":"drs",
		"body":[
			"\\drs{${1:variables}}{${2:conditions}}"
		]
	},


	"ifdrs{}{}{}{}":{
		"prefix":"ifdrs",
		"body":[
			"\\ifdrs{${1:lvar}}{${2:lcond}}{${3:rvar}}{${4:rcond}}"
		]
	},


	"condrs{}{}{}{}{}":{
		"prefix":"condrs",
		"body":[
			"\\condrs{${1:lvar}}{${2:lcond}}{${3:connective}}{${4:rvar}}{${5:rcond}}"
		]
	},


	"qdrs{}{}{}{}{}{}":{
		"prefix":"qdrs",
		"body":[
			"\\qdrs{${1:lvar}}{${2:lcond}}{${3:quantifier}}{${4:qvar}}{${5:rvar}}{${6:rcond}}"
		]
	},


	"negdrs{}{}":{
		"prefix":"negdrs",
		"body":[
			"\\negdrs{${1:variables}}{${2:conditions}}"
		]
	},


	"drsdiamond{}{}":{
		"prefix":"drsdiamond",
		"body":[
			"\\drsdiamond{${1:quantifier}}{${2:variable}}"
		]
	},


	"sdrs{}{}{}":{
		"prefix":"sdrs",
		"body":[
			"\\sdrs{${1:sentence%text}}{${2:variables}}{${3:conditions}}"
		]
	},


	"alifdrs{}{}{}{}":{
		"prefix":"alifdrs",
		"body":[
			"\\alifdrs{${1:lvar}}{${2:lcond}}{${3:rvar}}{${4:rcond}}"
