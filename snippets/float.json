{


	"newfloat{}{}{}":{
		"prefix":"newfloat",
		"body":[
			"\\newfloat{${1:float type%envname}}{${2:placement}}{${3:ext}}"
		]
	},


	"floatstyle{}":{
		"prefix":"floatstyle",
		"body":[
			"\\floatstyle{${1:default style%keyvals}}"
		]
	},


	"floatname{}{}":{
		"prefix":"floatname",
		"body":[
			"\\floatname{${1:float type}}{${2:name}}"
		]
	},


	"floatplacement{}{}":{
		"prefix":"floatplacement",
		"body":[
			"\\floatplacement{${1:float type}}{${2:default placement}}"
		]
	},


	"restylefloat{}":{
		"prefix":"restylefloat",
		"body":[
			"\\restylefloat{${1:float type}}"
		]
	},


	"listof{}{}":{
		"prefix":"listof",
		"body":[
			"\\listof{${1:float type}}{${2:title}}"
		]
	},


	"newfloat{}{}{}[]":{
		"prefix":"newfloat",
		"body":[
			"\\newfloat{${1:float type%envname}}{${2:placement}}{${3:ext}}[${4:within}]"
		]
	}
}
