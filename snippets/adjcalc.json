		"body":[
			"\\adjsetcounter{${1:counter}}{${2:integer}}"
		]
	},


	"adjaddtocounter":{
		"prefix":"adjaddtocounter",
		"body":[
			"\\adjaddtocounter{${1:counter}}{${2:integer}}"
		]
	},


	"adjsetlengthdefault":{
		"prefix":"adjsetlengthdefault",
		"body":[
			"\\adjsetlengthdefault{${1:length macro}}{${2:length}}"
		]
