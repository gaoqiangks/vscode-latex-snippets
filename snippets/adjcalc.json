		"body":[
			"\\adjcalcset{${1:options%keyvals}}"
		]
	},


	"adjsetlength":{
		"prefix":"adjsetlength",
		"body":[
			"\\adjsetlength{${1:length macro}}{${2:length}}"
		]
	},


	"adjaddtolength":{
		"prefix":"adjaddtolength",
