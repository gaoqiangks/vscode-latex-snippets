{


	"hangpara":{
		"prefix":"hangpara",
		"body":[
			"\\hangpara{${1:indent%l}}{${2:afternum}}"
		]
	},


	"nhpt":{
		"prefix":"nhpt",
		"body":[
			"\\nhpt"
		]
	},


	"nhlq":{
		"prefix":"nhlq",
		"body":[
			"\\nhlq"
		]
	},


	"nhrq":{
		"prefix":"nhrq",
		"body":[
			"\\nhrq"
		]
	},


	"activatepunct":{
		"prefix":"activatepunct",
		"body":[
			"\\activatepunct"
		]
	},


	"hangparas":{
		"prefix":"hangparas",
		"body":[
			"\\begin{hangparas}",
			"\t$1",
			"\\end{hangparas}"
		]
	},


	"hangpunct":{
		"prefix":"hangpunct",
		"body":[
			"\\begin{hangpunct}",
			"\t$1",
			"\\end{hangpunct}"
		]
	}
}
