{


	"nodefaultcolumn":{
		"prefix":"nodefaultcolumn",
		"body":[
			"\\nodefaultcolumn"
		]
	},


	"defaultcolumn{}":{
		"prefix":"defaultcolumn",
		"body":[
			"\\defaultcolumn{${1:colspec}}"
		]
	},


	"column{}{}":{
		"prefix":"column",
		"body":[
			"\\column{${1:column id}}{${2:spec}}"
		]
	},


	">":{
		"prefix":">",
		"body":[
			"\\>"
		]
	},


	"<":{
		"prefix":"<",
		"body":[
			"\\<"
		]
	},


	"fromto{}{}{}":{
		"prefix":"fromto",
		"body":[
			"\\fromto{${1:from id}}{${2:to id}}{${3:text}}"
		]
	},


	"savecolumns":{
		"prefix":"savecolumns",
		"body":[
			"\\savecolumns"
		]
	},


	"restorecolumns":{
		"prefix":"restorecolumns",
		"body":[
			"\\restorecolumns"
		]
	},


	"beginpolytable":{
		"prefix":"beginpolytable",
		"body":[
			"\\beginpolytable"
		]
	},


	"disktables":{
		"prefix":"disktables",
		"body":[
			"\\disktables"
		]
	},


	"maxcolumn":{
		"prefix":"maxcolumn",
		"body":[
			"\\maxcolumn"
		]
	},


	"memorytables":{
		"prefix":"memorytables",
		"body":[
			"\\memorytables"
		]
	},


	"nextline":{
		"prefix":"nextline",
		"body":[
			"\\nextline"
		]
	},


	"column[]{}{}":{
		"prefix":"column",
		"body":[
			"\\column[${3:dimen%l}]{${1:column id}}{${2:spec}}"
		]
	},


	">[]":{
		"prefix":">",
		"body":[
			"\\>[${1:from id}]"
		]
	},


	">[][]":{
		"prefix":">",
		"body":[
			"\\>[${1:from id}][${2:spec}]"
		]
	},


	"<[]":{
		"prefix":"<",
		"body":[
			"\\<[${1:from id}]"
		]
	},


	"savecolumns[]":{
		"prefix":"savecolumns",
		"body":[
			"\\savecolumns[${1:set id}]"
		]
	},


	"restorecolumns[]":{
		"prefix":"restorecolumns",
		"body":[
			"\\restorecolumns[${1:set id}]"
		]
	},


	"nextline[]":{
		"prefix":"nextline",
		"body":[
			"\\nextline[${1:dimen%l}]"
		]
	},


	"pboxed":{
		"prefix":"pboxed",
		"body":[
			"\\begin{pboxed}",
				"\t$1",
			"\\end{pboxed}"
		]
	},


	"ptboxed":{
		"prefix":"ptboxed",
		"body":[
			"\\begin{ptboxed}",
				"\t$1",
			"\\end{ptboxed}"
		]
	},


	"pmboxed":{
		"prefix":"pmboxed",
		"body":[
			"\\begin{pmboxed}",
				"\t$1",
			"\\end{pmboxed}"
		]
	},


	"ptabular":{
		"prefix":"ptabular",
		"body":[
			"\\begin{ptabular}",
				"\t$1",
			"\\end{ptabular}"
		]
	},


	"parray":{
		"prefix":"parray",
		"body":[
			"\\begin{parray}",
				"\t$1",
			"\\end{parray}"
		]
	}
}
