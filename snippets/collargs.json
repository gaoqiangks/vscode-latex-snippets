{


	"CollectArguments{}{}":{
		"prefix":"CollectArguments",
		"body":[
			"\\CollectArguments{${1:argspec}}{${2:next-code}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"CollectArguments[]{}{}":{
		"prefix":"CollectArguments",
		"body":[
			"\\CollectArguments[${3:options%keyvals}]{${1:argspec}}{${2:next-code}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"CollectArgumentsRaw{}{}{}":{
		"prefix":"CollectArgumentsRaw",
		"body":[
			"\\CollectArgumentsRaw{${1:option-setting code}}{${2:argspec}}{${3:next-code}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
