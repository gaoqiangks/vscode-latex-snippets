{


	"TOCclone{}{}":{
		"prefix":"TOCclone",
		"body":[
			"\\TOCclone{${1:source ext}}{${2:destination ext}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"TOCclone[]{}{}":{
		"prefix":"TOCclone",
		"body":[
			"\\TOCclone[${3:list heading}]{${1:source ext}}{${2:destination ext}}"
		],
		"luasnip": {"priority": 1000} 
	}
}
