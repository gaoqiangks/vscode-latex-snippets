{


	"FigPut":{
		"prefix":"FigPut",
		"body":[
			"\\FigPut{${1:fig_name,fig_ht}}"
		]
	},


	"FigPut":{
		"prefix":"FigPut",
		"body":[
			"\\FigPut{${1:fig_name,fig_ht}}[${2:options}]"
		]
	},


	"SetInnerMargin":{
		"prefix":"SetInnerMargin",
		"body":[
			"\\SetInnerMargin{${1:length}}"
		]
	},


	"SetOuterMargin":{
		"prefix":"SetOuterMargin",
		"body":[
			"\\SetOuterMargin{${1:length}}"
		]
	},


	"NeverSkip":{
		"prefix":"NeverSkip",
		"body":[
			"\\NeverSkip"
		]
	},


	"AllowSkip":{
		"prefix":"AllowSkip",
		"body":[
			"\\AllowSkip"
		]
	},


	"LoadFigureCode":{
		"prefix":"LoadFigureCode",
		"body":[
			"\\LoadFigureCode{${1:file}}"
		]
	},


	"figput":{
		"prefix":"figput",
		"body":[
			"\\begin{figput}",
			"\t$1",
			"\\end{figput}"
		]
	}
}
