{


	"grrubystyle":{
		"prefix":"grrubystyle",
		"body":[
			"\\grrubystyle{${1:options}}"
		]
	},


	"grrubycolor":{
		"prefix":"grrubycolor",
		"body":[
			"\\grrubycolor{${1:color}}"
		]
	},


	"grexpos":{
		"prefix":"grexpos",
		"body":[
			"\\grexpos"
		]
	},


	"grexpos":{
		"prefix":"grexpos",
		"body":[
			"\\grexpos[${1:options}]"
		]
	},


	"grrubyUserDefined":{
		"prefix":"grrubyUserDefined",
		"body":[
			"\\grrubyUserDefined{${1:hangul}}{${2:hanja}}"
		]
	},


	"grrubypars":{
		"prefix":"grrubypars",
		"body":[
			"\\begin{grrubypars}",
			"\t$1",
			"\\end{grrubypars}"
		]
	},


	"grruby":{
		"prefix":"grruby",
		"body":[
			"\\begin{grruby}",
			"\t$1",
			"\\end{grruby}"
		]
	}
}
