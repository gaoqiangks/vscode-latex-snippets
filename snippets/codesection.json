{


	"DefineCodeSection{}":{
		"prefix":"DefineCodeSection",
		"body":[
			"\\DefineCodeSection{${1:name}}"
		]
	},


	"SetCodeSection{}{}":{
		"prefix":"SetCodeSection",
		"body":[
			"\\SetCodeSection{${1:name}}{${2:true|false%keyvals}}"
		]
	},


	"BeginCodeSection{}":{
		"prefix":"BeginCodeSection",
		"body":[
			"\\BeginCodeSection{${1:name}}"
		]
	},


	"EndCodeSection{}":{
		"prefix":"EndCodeSection",
		"body":[
			"\\EndCodeSection{${1:name}}"
		]
	},


	"DefineCodeSection[]{}":{
		"prefix":"DefineCodeSection",
		"body":[
			"\\DefineCodeSection[${2:true|false%keyvals}]{${1:name}}"
		]
	}
}
