{


	"luadirect":{
		"prefix":"luadirect",
		"body":[
			"\\luadirect{${1:lua code%definition}}"
		]
	},


	"luaexec":{
		"prefix":"luaexec",
		"body":[
			"\\luaexec{${1:lua code%definition}}"
		]
	},


	"luastring":{
		"prefix":"luastring",
		"body":[
			"\\luastring{${1:string}}"
		]
	},


	"luastringN":{
		"prefix":"luastringN",
		"body":[
			"\\luastringN{${1:string}}"
		]
	},


	"luastringO":{
		"prefix":"luastringO",
		"body":[
			"\\luastringO{${1:string}}"
		]
	},


	"LuaCodeDebugOn":{
		"prefix":"LuaCodeDebugOn",
		"body":[
			"\\LuaCodeDebugOn"
		]
	},


	"LuaCodeDebugOff":{
		"prefix":"LuaCodeDebugOff",
		"body":[
			"\\LuaCodeDebugOff"
		]
	},


	"luacode*":{
		"prefix":"luacode*",
		"body":[
			"\\begin{luacode*}",
			"\t$1",
			"\\end{luacode*}"
		]
	},


	"luacode":{
		"prefix":"luacode",
		"body":[
			"\\begin{luacode}",
			"\t$1",
			"\\end{luacode}"
		]
	}
}
