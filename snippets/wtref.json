{


	"newref{}":{
		"prefix":"newref",
		"body":[
			"\\newref{${1:ref types}}"
		]
	},


	"setrefstyle{}{}":{
		"prefix":"setrefstyle",
		"body":[
			"\\setrefstyle{${1:ref types}}{${2:options%keyvals}}"
		]
	},


	"newref[]{}":{
		"prefix":"newref",
		"body":[
			"\\newref[${2:options%keyvals}]{${1:ref types}}"
		]
	}
}
