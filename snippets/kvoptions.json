{


	"ProcessKeyvalOptions{}":{
		"prefix":"ProcessKeyvalOptions",
		"body":[
			"\\ProcessKeyvalOptions{${1:family}}"
		]
	},


	"ProcessLocalKeyvalOptions{}":{
		"prefix":"ProcessLocalKeyvalOptions",
		"body":[
			"\\ProcessLocalKeyvalOptions{${1:family}}"
		]
	},


	"SetupKeyvalOptions{}":{
		"prefix":"SetupKeyvalOptions",
		"body":[
			"\\SetupKeyvalOptions{${1:options%keyvals}}"
		]
	},


	"DeclareStringOption{}":{
		"prefix":"DeclareStringOption",
		"body":[
			"\\DeclareStringOption{${1:key%plain}}"
		]
	},


	"DeclareBoolOption{}":{
		"prefix":"DeclareBoolOption",
		"body":[
			"\\DeclareBoolOption{${1:key%plain}}"
		]
	},


	"DeclareComplementaryOption{}{}":{
		"prefix":"DeclareComplementaryOption",
		"body":[
			"\\DeclareComplementaryOption{${1:key%plain}}{${2:parent}}"
		]
	},


	"DeclareVoidOption{}{}":{
		"prefix":"DeclareVoidOption",
		"body":[
			"\\DeclareVoidOption{${1:key%plain}}{${2:code}}"
		]
	},


	"DeclareDefaultOption{}":{
		"prefix":"DeclareDefaultOption",
		"body":[
			"\\DeclareDefaultOption{${1:code}}"
		]
	},


	"DeclareLocalOption{}":{
		"prefix":"DeclareLocalOption",
		"body":[
			"\\DeclareLocalOption{${1:option}}"
		]
	},


	"DeclareLocalOptions{}":{
		"prefix":"DeclareLocalOptions",
		"body":[
			"\\DeclareLocalOptions{${1:options}}"
		]
	},


	"DisableKeyvalOption{}{}":{
		"prefix":"DisableKeyvalOption",
		"body":[
			"\\DisableKeyvalOption{${1:family}}{${2:key%plain}}"
		]
	},


	"AddToKeyvalOption{}{}{}":{
		"prefix":"AddToKeyvalOption",
		"body":[
			"\\AddToKeyvalOption{${1:family}}{${2:key%plain}}{${3:code}}"
		]
	},


	"ProcessKeyvalOptions*":{
		"prefix":"*ProcessKeyvalOptions",
		"body":[
			"\\ProcessKeyvalOptions*"
		]
	},


	"ProcessLocalKeyvalOptions*":{
		"prefix":"*ProcessLocalKeyvalOptions",
		"body":[
			"\\ProcessLocalKeyvalOptions*"
		]
	},


	"AddToKeyvalOption*{}{}":{
		"prefix":"*AddToKeyvalOption",
		"body":[
			"\\AddToKeyvalOption*{${1:key%plain}}{${2:code}}"
		]
	},


	"DeclareStringOption[]{}[]":{
		"prefix":"DeclareStringOption",
		"body":[
			"\\DeclareStringOption[${2:init}]{${1:key%plain}}[${3:default}]"
		]
	},


	"DeclareBoolOption[]{}":{
		"prefix":"DeclareBoolOption",
		"body":[
			"\\DeclareBoolOption[${2:init}]{${1:key%plain}}"
		]
	},


	"DisableKeyvalOption[]{}{}":{
		"prefix":"DisableKeyvalOption",
		"body":[
			"\\DisableKeyvalOption[${3:options%keyvals}]{${1:family}}{${2:key%plain}}"
		]
	}
}
