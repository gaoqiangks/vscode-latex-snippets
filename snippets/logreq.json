{


	"binary{}":{
		"prefix":"binary",
		"body":[
			"\\binary{${1:package}}"
		]
	},


	"cmdline{}":{
		"prefix":"cmdline",
		"body":[
			"\\cmdline{${1:specs}}"
		]
	},


	"DeclareLogreqAttribute{}{}{}":{
		"prefix":"DeclareLogreqAttribute",
		"body":[
			"\\DeclareLogreqAttribute{${1:element}}{${2:attribute}}{${3:dtd specs}}"
		]
	},


	"DeclareLogreqContainer{}{}":{
		"prefix":"DeclareLogreqContainer",
		"body":[
			"\\DeclareLogreqContainer{${1:element}}{${2:dtd specs}}"
		]
	},


	"DeclareLogreqElement{}{}":{
		"prefix":"DeclareLogreqElement",
		"body":[
			"\\DeclareLogreqElement{${1:element}}{${2:dtd specs}}"
		]
	},


	"file{}":{
		"prefix":"file",
		"body":[
			"\\file{${1:file}}"
		]
	},


	"generic{}":{
		"prefix":"generic",
		"body":[
			"\\generic{${1:package}}"
		]
	},


	"infile{}":{
		"prefix":"infile",
		"body":[
			"\\infile{${1:file}}"
		]
	},


	"logrequest{}":{
		"prefix":"logrequest",
		"body":[
			"\\logrequest{${1:request}}"
		]
	},


	"ltxrequest{}{}{}":{
		"prefix":"ltxrequest",
		"body":[
			"\\ltxrequest{${1:package}}{${2:active}}{${3:request}}"
		]
	},


	"option{}":{
		"prefix":"option",
		"body":[
			"\\option{${1:option}}"
		]
	},


	"outfile{}":{
		"prefix":"outfile",
		"body":[
			"\\outfile{${1:file}}"
		]
	},


	"DeclareLogreqAttribute{}[]{}{}":{
		"prefix":"DeclareLogreqAttribute",
		"body":[
			"\\DeclareLogreqAttribute{${1:element}}[${4:default}]{${2:attribute}}{${3:dtd specs}}"
		]
	},


	"external[]{}":{
		"prefix":"external",
		"body":[
			"\\external[${2:keyvals}]{${1:specs}}"
		]
	},


	"internal[]{}":{
		"prefix":"internal",
		"body":[
			"\\internal[${2:keyvals}]{${1:specs}}"
		]
	},


	"logrequest[]{}":{
		"prefix":"logrequest",
		"body":[
			"\\logrequest[${2:options%keyvals}]{${1:request}}"
		]
	},


	"provides[]{}":{
		"prefix":"provides",
		"body":[
			"\\provides[${2:keyvals}]{${1:file specs}}"
		]
	},


	"requests[]{}":{
		"prefix":"requests",
		"body":[
			"\\requests[${2:keyvals}]{${1:specs}}"
		]
	},


	"requires[]{}":{
		"prefix":"requires",
		"body":[
			"\\requires[${2:keyvals}]{${1:file specs}}"
		]
	}
}
