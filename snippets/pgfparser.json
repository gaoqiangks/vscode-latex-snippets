{


	"pgfparserparse{}":{
		"prefix":"pgfparserparse",
		"body":[
			"\\pgfparserparse{${1:parser name}}"
		]
	},


	"pgfparserdef{}{}{}{}":{
		"prefix":"pgfparserdef",
		"body":[
			"\\pgfparserdef{${1:parser name}}{${2:state}}{${3:symbol meaning}}{${4:action}}"
		]
	},


	"pgfparserlet{}{}{}{}":{
		"prefix":"pgfparserlet",
		"body":[
			"\\pgfparserlet{${1:parser1}}{${2:state1}}{${3:meaning1}}{${4:meaning2}}"
		]
	},


	"pgfparserdefunknown{}{}{}":{
		"prefix":"pgfparserdefunknown",
		"body":[
			"\\pgfparserdefunknown{${1:parser name}}{${2:state}}{${3:action}}"
		]
	},


	"pgfparserdeffinal{}{}":{
		"prefix":"pgfparserdeffinal",
		"body":[
			"\\pgfparserdeffinal{${1:parser name}}{${2:action}}"
		]
	},


	"pgfparserswitch{}":{
		"prefix":"pgfparserswitch",
		"body":[
			"\\pgfparserswitch{${1:state}}"
		]
	},


	"pgfparserifmark{}{}{}":{
		"prefix":"pgfparserifmark",
		"body":[
			"\\pgfparserifmark{${1:arg}}{${2:true}}{${3:false}}"
		]
	},


	"pgfparserreinsert":{
		"prefix":"pgfparserreinsert",
		"body":[
			"\\pgfparserreinsert"
		]
	},


	"pgfparserstate":{
		"prefix":"pgfparserstate",
		"body":[
			"\\pgfparserstate"
		]
	},


	"pgfparsertoken":{
		"prefix":"pgfparsertoken",
		"body":[
			"\\pgfparsertoken"
		]
	},


	"pgfparserletter":{
		"prefix":"pgfparserletter",
		"body":[
			"\\pgfparserletter"
		]
	},


	"pgfparserset{}":{
		"prefix":"pgfparserset",
		"body":[
			"\\pgfparserset{${1:options%keyvals}}"
		]
	},


	"pgfparserdef{}{}{}[]{}":{
		"prefix":"pgfparserdef",
		"body":[
			"\\pgfparserdef{${1:parser name}}{${2:state}}{${3:symbol meaning}}[${5:args}]{${4:action}}"
		]
	},


	"pgfparserlet{}{}{}[][]{}":{
		"prefix":"pgfparserlet",
		"body":[
			"\\pgfparserlet{${1:parser1}}{${2:state1}}{${3:meaning1}}[${5:opt1}][${6:opt2}]{${4:meaning2}}"
		]
	}
}
