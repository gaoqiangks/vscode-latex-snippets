{


	"ifdraft":{
		"prefix":"ifdraft",
		"body":[
			"\\ifdraft{${1:draft case}}{${2:final case}}"
		]
	},


	"ifoptiondraft":{
		"prefix":"ifoptiondraft",
		"body":[
			"\\ifoptiondraft{${1:option given}}{${2:option not given}}"
		]
	},


	"ifoptionfinal":{
		"prefix":"ifoptionfinal",
		"body":[
			"\\ifoptionfinal{${1:option given}}{${2:option not given}}"
		]
	}
}
