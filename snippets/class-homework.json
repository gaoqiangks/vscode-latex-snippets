{


	"pretitle{}":{
		"prefix":"pretitle",
		"body":[
			"\\pretitle{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"ManualNumbering{}{}":{
		"prefix":"ManualNumbering",
		"body":[
			"\\ManualNumbering{${1:envname}}{${2:number}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"answer":{
		"prefix":"answer",
		"body":[
			"\\begin{answer}",
				"\t$1",
			"\\end{answer}"
		],
		"luasnip": {"priority": 3000} 
	},


	"answer[]":{
		"prefix":"answer",
		"body":[
			"\\begin{answer}",
				"\t[${1:heading%text}]",
			"\\end{answer}"
		],
		"luasnip": {"priority": 1000} 
	},


	"solution":{
		"prefix":"solution",
		"body":[
			"\\begin{solution}",
				"\t$1",
			"\\end{solution}"
		],
		"luasnip": {"priority": 3000} 
	},


	"solution[]":{
		"prefix":"solution",
		"body":[
			"\\begin{solution}",
				"\t[${1:heading%text}]",
			"\\end{solution}"
		],
		"luasnip": {"priority": 1000} 
	}
}
