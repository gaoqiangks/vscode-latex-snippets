{


	"setluaoption{}{}{}":{
		"prefix":"setluaoption",
		"body":[
			"\\setluaoption{${1:family}}{${2:option%keyvals}}{${3:value}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"useluaoption{}{}":{
		"prefix":"useluaoption",
		"body":[
			"\\useluaoption{${1:family}}{${2:option%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
