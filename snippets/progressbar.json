{


	"progressbar{}":{
		"prefix":"progressbar",
		"body":[
			"\\progressbar{${1:number}}"
		]
	},


	"progressbarchange{}":{
		"prefix":"progressbarchange",
		"body":[
			"\\progressbarchange{${1:options%keyvals}}"
		]
	},


	"progressbar[]{}":{
		"prefix":"progressbar",
		"body":[
			"\\progressbar[${2:options%keyvals}]{${1:number}}"
		]
	}
}
