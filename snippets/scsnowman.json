{


	"scsnowman":{
		"prefix":"scsnowman",
		"body":[
			"\\scsnowman"
		]
	},


	"scsnowmandefault{}":{
		"prefix":"scsnowmandefault",
		"body":[
			"\\scsnowmandefault{${1:keyvals}}"
		]
	},


	"scsnowmannumeral{}":{
		"prefix":"scsnowmannumeral",
		"body":[
			"\\scsnowmannumeral{${1:number}}"
		]
	},


	"usescsnowmanlibrary{}":{
		"prefix":"usescsnowmanlibrary",
		"body":[
			"\\usescsnowmanlibrary{${1:library%file}}"
		]
	},


	"makeitemsnowman":{
		"prefix":"makeitemsnowman",
		"body":[
			"\\makeitemsnowman"
		]
	},


	"makeitemother":{
		"prefix":"makeitemother",
		"body":[
			"\\makeitemother"
		]
	},


	"makeqedsnowman":{
		"prefix":"makeqedsnowman",
		"body":[
			"\\makeqedsnowman"
		]
	},


	"makeqedother":{
		"prefix":"makeqedother",
		"body":[
			"\\makeqedother"
		]
	},


	"scsnowman[]":{
		"prefix":"scsnowman",
		"body":[
			"\\scsnowman[${1:options%keyvals}]"
		]
	},


	"scsnowmannumeral[]{}":{
		"prefix":"scsnowmannumeral",
		"body":[
			"\\scsnowmannumeral[${2:options%keyvals}]{${1:number}}"
		]
	}
}
