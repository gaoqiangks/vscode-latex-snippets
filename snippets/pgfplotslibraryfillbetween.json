{


	"tikzsegmentindex":{
		"prefix":"tikzsegmentindex",
		"body":[
			"\\tikzsegmentindex"
		]
	},


	"tikzfillbetween":{
		"prefix":"tikzfillbetween",
		"body":[
			"\\tikzfillbetween[${2:options%keyvals}]{${1:draw style}}"
		]
	},


	"tikzgetnamedpath":{
		"prefix":"tikzgetnamedpath",
		"body":[
			"\\tikzgetnamedpath{${1:path name}}"
		]
	},


	"tikznamecurrentpath":{
		"prefix":"tikznamecurrentpath",
		"body":[
			"\\tikznamecurrentpath{${1:name}}"
		]
	},


	"tikzpathintersectionsegments":{
		"prefix":"tikzpathintersectionsegments",
		"body":[
			"\\tikzpathintersectionsegments[${1:options%keyvals}]"
		]
	}
}
