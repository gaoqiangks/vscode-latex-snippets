{


	"addvskip{}":{
		"prefix":"addvskip",
		"body":[
			"\\addvskip{${1:length}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"lbox[]{}":{
		"prefix":"lbox",
		"body":[
			"\\lbox[${2:options}]{${1:content%text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"lbox{}":{
		"prefix":"lbox",
		"body":[
			"\\lbox{${1:content%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"nofirstindent":{
		"prefix":"nofirstindent",
		"body":[
			"\\nofirstindent"
		],
		"luasnip": {"priority": 3000} 
	},


	"nofirstparskip":{
		"prefix":"nofirstparskip",
		"body":[
			"\\nofirstparskip"
		],
		"luasnip": {"priority": 3000} 
	},


	"unvcolorbox{}{}":{
		"prefix":"unvcolorbox",
		"body":[
			"\\unvcolorbox{${1:color}}{${2:vboxname}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"vcolorbox{}{}":{
		"prefix":"vcolorbox",
		"body":[
			"\\vcolorbox{${1:color}}{${2:vboxname}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"longbox":{
		"prefix":"longbox",
		"body":[
			"\\begin{longbox}",
				"\t$1",
			"\\end{longbox}"
		],
		"luasnip": {"priority": 3000} 
	},


	"longbox[]":{
		"prefix":"longbox",
		"body":[
			"\\begin{longbox}",
				"\t[${1:options}]",
			"\\end{longbox}"
		],
		"luasnip": {"priority": 1000} 
	},


	"lvbox{}{}":{
		"prefix":"lvbox",
		"body":[
			"\\begin{lvbox}",
				"\t{${1:boxname}}{${2:width}}",
			"\\end{lvbox}"
		],
		"luasnip": {"priority": 3000} 
	}
}
