{


	"addvskip{}":{
		"prefix":"addvskip",
		"body":[
			"\\addvskip{${1:length}}"
		]
	},


	"lbox{}":{
		"prefix":"lbox",
		"body":[
			"\\lbox{${1:content%text}}"
		]
	},


	"nofirstindent":{
		"prefix":"nofirstindent",
		"body":[
			"\\nofirstindent"
		]
	},


	"nofirstparskip":{
		"prefix":"nofirstparskip",
		"body":[
			"\\nofirstparskip"
		]
	},


	"unvcolorbox{}{}":{
		"prefix":"unvcolorbox",
		"body":[
			"\\unvcolorbox{${1:color}}{${2:vboxname}}"
		]
	},


	"vcolorbox{}{}":{
		"prefix":"vcolorbox",
		"body":[
			"\\vcolorbox{${1:color}}{${2:vboxname}}"
		]
	},


	"lbox[]{}":{
		"prefix":"lbox",
		"body":[
			"\\lbox[${2:options}]{${1:content%text}}"
		]
	},


	"longbox":{
		"prefix":"longbox",
		"body":[
			"\\begin{longbox}",
				"\t$1",
			"\\end{longbox}"
		]
	},


	"lvbox":{
		"prefix":"lvbox",
		"body":[
			"\\begin{lvbox}",
				"\t$1",
			"\\end{lvbox}"
		]
	},


	"longbox":{
		"prefix":"longbox",
		"body":[
			"\\begin{longbox}",
				"\t$1",
			"\\end{longbox}"
		]
	}
}
