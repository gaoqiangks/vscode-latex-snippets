{


	"sfrac{}{}":{
		"prefix":"sfrac",
		"body":[
			"\\sfrac{${1:num}}{${2:denom}}"
		]
	},


	"sfrac{}[]{}":{
		"prefix":"sfrac",
		"body":[
			"\\sfrac{${1:num}}[${3:sep}]{${2:denom}}"
		]
	},


	"sfrac[]{}{}":{
		"prefix":"sfrac",
		"body":[
			"\\sfrac[${3:instance}]{${1:num}}{${2:denom}}"
		]
	},


	"sfrac[]{}[]{}":{
		"prefix":"sfrac",
		"body":[
			"\\sfrac[${3:instance}]{${1:num}}[${4:sep}]{${2:denom}}"
		]
	}
}
