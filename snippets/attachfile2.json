{


	"attachfile{}":{
		"prefix":"attachfile",
		"body":[
			"\\attachfile{${1:file}}"
		]
	},


	"noattachfile":{
		"prefix":"noattachfile",
		"body":[
			"\\noattachfile"
		]
	},


	"notextattachfile{}":{
		"prefix":"notextattachfile",
		"body":[
			"\\notextattachfile{${1:text}}"
		]
	},


	"textattachfile{}{}":{
		"prefix":"textattachfile",
		"body":[
			"\\textattachfile{${1:file}}{${2:text}}"
		]
	},


	"attachfilesetup{}":{
		"prefix":"attachfilesetup",
		"body":[
			"\\attachfilesetup{${1:options%keyvals}}"
		]
	},


	"attachfile[]{}":{
		"prefix":"attachfile",
		"body":[
			"\\attachfile[${2:options%keyvals}]{${1:file}}"
		]
	},


	"noattachfile[]":{
		"prefix":"noattachfile",
		"body":[
			"\\noattachfile[${1:options%keyvals}]"
		]
	},


	"notextattachfile[]{}":{
		"prefix":"notextattachfile",
		"body":[
			"\\notextattachfile[${2:options%keyvals}]{${1:text}}"
		]
	},


	"textattachfile[]{}{}":{
		"prefix":"textattachfile",
		"body":[
			"\\textattachfile[${3:options%keyvals}]{${1:file}}{${2:text}}"
		]
	}
}
