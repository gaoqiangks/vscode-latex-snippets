{


	"pxlThisTeX":{
		"prefix":"pxlThisTeX",
		"body":[
			"\\pxlThisTeX"
		]
	},


	"pxlThisTeX":{
		"prefix":"pxlThisTeX",
		"body":[
			"\\pxlThisTeX[${1:engine%keyvals}]"
		]
	},


	"pxlThisLaTeX":{
		"prefix":"pxlThisLaTeX",
		"body":[
			"\\pxlThisLaTeX"
		]
	},


	"pxlThisPLaTeX":{
		"prefix":"pxlThisPLaTeX",
		"body":[
			"\\pxlThisPLaTeX"
		]
	},


	"IfpxlTeX":{
		"prefix":"IfpxlTeX",
		"body":[
			"\\IfpxlTeX{${1:engine%keyvals}}{${2:true}}{${3:false}}"
		]
	},


	"IfpxlTeX*":{
		"prefix":"IfpxlTeX*",
		"body":[
			"\\IfpxlTeX*{${1:engine%keyvals}}{${2:code}}"
		]
	},


	"IfpxlTeX*":{
		"prefix":"IfpxlTeX*",
		"body":[
			"\\IfpxlTeX*[${3:T/F}]{${1:engine%keyvals}}{${2:code}}"
		]
	},


	"IfpxlTeXpxl":{
		"prefix":"IfpxlTeXpxl",
		"body":[
			"\\IfpxlTeXpxl{${1:pdftex cmd}}{${2:xetex cmd}}{${3:luatex cmd}}"
		]
	},


	"IfpxlTeXpxl*":{
		"prefix":"IfpxlTeXpxl*",
		"body":[
			"\\IfpxlTeXpxl*{${1:engine1%keyvals}}{${2:engine2%keyvals}}{${3:engine1 cmd}}{${4:engine2 cmd}}"
		]
	},


	"pxlRequireTeX":{
		"prefix":"pxlRequireTeX",
		"body":[
			"\\pxlRequireTeX{${1:engine%keyvals}}"
		]
	}
}
