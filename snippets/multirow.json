		"body":[
			"\\multirowdebugtrue"
		],
		"luasnip": {"priority": 3000} 
	},


	"multirowdebugfalse":{
		"prefix":"multirowdebugfalse",
		"body":[
			"\\multirowdebugfalse"
		],
		"luasnip": {"priority": 3000} 
	},


	"bigstrutjot":{
		"prefix":"bigstrutjot",
		"body":[
			"\\bigstrutjot"
		],
		"luasnip": {"priority": 3000} 
	},


	"STneed{}":{
