{


	"multirow{}{}{}":{
		"prefix":"multirow",
		"body":[
			"\\multirow{${1:number of rows}}{${2:width or *}}{${3:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"multirow[]{}{}{}":{
		"prefix":"multirow",
		"body":[
			"\\multirow[${4:vpos%keyvals}]{${1:number of rows}}{${2:width or *}}{${3:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"multirow{}[]{}{}":{
		"prefix":"multirow",
		"body":[
			"\\multirow{${1:number of rows}}[${4:bigstruts}]{${2:width or *}}{${3:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"multirow{}{}[]{}":{
		"prefix":"multirow",
		"body":[
			"\\multirow{${1:number of rows}}{${2:width or *}}[${4:vmove%l}]{${3:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"multirow[]{}[]{}[]{}":{
		"prefix":"multirow",
		"body":[
			"\\multirow[${4:vpos%keyvals}]{${1:number of rows}}[${5:bigstruts}]{${2:width or *}}[${6:vmove%l}]{${3:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"multirowsetup":{
		"prefix":"multirowsetup",
		"body":[
			"\\multirowsetup"
		],
		"luasnip": {"priority": 3000} 
	},


	"multirowdebugtrue":{
