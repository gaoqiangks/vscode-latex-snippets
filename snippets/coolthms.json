{


	"definetheorem{}{}":{
		"prefix":"definetheorem",
		"body":[
			"\\definetheorem{${1:envname}}{${2:thm name}}"
		]
	},


	"Label{}":{
		"prefix":"Label",
		"body":[
			"\\Label{${1:key}}"
		]
	},


	"proofname":{
		"prefix":"proofname",
		"body":[
			"\\proofname"
		]
	},


	"theoremsymbol{}":{
		"prefix":"theoremsymbol",
		"body":[
			"\\theoremsymbol{${1:symbol}}"
		]
	},


	"definetheorem[]{}{}":{
		"prefix":"definetheorem",
		"body":[
			"\\definetheorem[${3:numbered like}]{${1:envname}}{${2:thm name}}"
		]
	},


	"definetheorem{}{}[]":{
		"prefix":"definetheorem",
		"body":[
			"\\definetheorem{${1:envname}}{${2:thm name}}[${3:counter within}]"
		]
	},


	"definetheorem[]{}{}[]":{
		"prefix":"definetheorem",
		"body":[
			"\\definetheorem[${3:numbered like}]{${1:envname}}{${2:thm name}}[${4:counter within}]"
		]
	},


	"definetheorem{}[]{}":{
		"prefix":"definetheorem",
		"body":[
			"\\definetheorem{${1:envname}}[${3:thm plural name}]{${2:thm name}}"
		]
	},


	"theoremmarkup[][][][][][]":{
		"prefix":"theoremmarkup",
		"body":[
			"\\theoremmarkup[${1:header font}][${2:body font}][${3:symbol}][${4:indent}][${5:separator}][${6:numbering}]"
		]
	},


	"proof":{
		"prefix":"proof",
		"body":[
			"\\begin{proof}",
				"\t$1",
			"\\end{proof}"
		]
	}
}
