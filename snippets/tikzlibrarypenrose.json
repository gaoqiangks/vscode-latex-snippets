{


	"BakePenroseTile":{
		"prefix":"BakePenroseTile",
		"body":[
			"\\BakePenroseTile{${1:name%keyvals}}"
		]
	},


	"PenroseDecomposition":{
		"prefix":"PenroseDecomposition",
		"body":[
			"\\PenroseDecomposition{${1:type%keyvals}}{${2:level}}{${3:seed}}"
		]
	},


	"PenroseDecomposition":{
		"prefix":"PenroseDecomposition",
		"body":[
			"\\PenroseDecomposition[${4:TikZ keys}]{${1:type%keyvals}}{${2:level}}{${3:seed}}"
		]
	},


	"DefineTile":{
		"prefix":"DefineTile",
		"body":[
			"\\DefineTile{${1:name%specialDef}}{${2:sides}}{${3:coordinates}}"
		]
	}
}
