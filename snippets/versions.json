{


	"markversion{}":{
		"prefix":"markversion",
		"body":[
			"\\markversion{${1:version%envname}}"
		]
	},


	"includeversion{}":{
		"prefix":"includeversion",
		"body":[
			"\\includeversion{${1:version%envname}}"
		]
	},


	"excludeversion{}":{
		"prefix":"excludeversion",
		"body":[
			"\\excludeversion{${1:version%envname}}"
		]
	},


	"processifversion{}{}":{
		"prefix":"processifversion",
		"body":[
			"\\processifversion{${1:version}}{${2:code}}"
		]
	},


	"includeversionnogroup{}":{
		"prefix":"includeversionnogroup",
		"body":[
			"\\includeversionnogroup{${1:version%envname}}"
		]
	},


	"versionmessage{}{}":{
		"prefix":"versionmessage",
		"body":[
			"\\versionmessage{${1:version}}{${2:type%keyvals}}"
		]
	},


	"comment":{
		"prefix":"comment",
		"body":[
			"\\begin{comment}",
				"\t$1",
			"\\end{comment}"
		]
	}
}
