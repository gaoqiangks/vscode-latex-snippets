{


	"Begin":{
		"prefix":"Begin",
		"body":[
			"\\Begin"
		]
	},


	"End":{
		"prefix":"End",
		"body":[
			"\\End"
		]
	},


	"For{}{}":{
		"prefix":"For",
		"body":[
			"\\For{${1:assign%formula}}{${2:expr%formula}}"
		]
	},


	"While{}":{
		"prefix":"While",
		"body":[
			"\\While{${1:expr%formula}}"
		]
	},


	"Repeat":{
		"prefix":"Repeat",
		"body":[
			"\\Repeat"
		]
	},


	"Until{}":{
		"prefix":"Until",
		"body":[
			"\\Until{${1:expr%formula}}"
		]
	},


	"If{}":{
		"prefix":"If",
		"body":[
			"\\If{${1:expr%formula}}"
		]
	},


	"Else":{
		"prefix":"Else",
		"body":[
			"\\Else"
		]
	},


	"Procedure":{
		"prefix":"Procedure",
		"body":[
			"\\Procedure"
		]
	},


	"Function":{
		"prefix":"Function",
		"body":[
			"\\Function"
		]
	},


	"textkeyword{}":{
		"prefix":"textkeyword",
		"body":[
			"\\textkeyword{${1:text}}"
		]
	}
}
