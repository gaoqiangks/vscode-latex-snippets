		]
	},


	"AutoPuncItems":{
		"prefix":"AutoPuncItems",
		"body":[
			"\\begin{AutoPuncItems}",
				"\t$1",
			"\\end{AutoPuncItems}"
		]
	},


	"AutoPuncItemsO":{
