{


	"APomit":{
		"prefix":"APomit",
		"body":[
			"\\APomit"
		]
	},


	"APpass":{
		"prefix":"APpass",
		"body":[
			"\\APpass"
		]
	},


	"setAPeach{}{}{}":{
		"prefix":"setAPeach",
		"body":[
			"\\setAPeach{${1:default punct}}{${2:second last punct}}{${3:last punct}}"
		]
	},


	"setAPall{}":{
		"prefix":"setAPall",
		"body":[
			"\\setAPall{${1:punct}}"
		]
	},


	"setAPdef{}":{
		"prefix":"setAPdef",
		"body":[
			"\\setAPdef{${1:punct}}"
		]
	},


	"setAPseclast{}":{
		"prefix":"setAPseclast",
		"body":[
			"\\setAPseclast{${1:punct}}"
		]
	},


	"setAPlast{}":{
		"prefix":"setAPlast",
		"body":[
			"\\setAPlast{${1:punct}}"
		]
	},


	"enableAPautopassnest":{
		"prefix":"enableAPautopassnest",
		"body":[
			"\\enableAPautopassnest"
		]
	},


	"disableAPautopassnest":{
		"prefix":"disableAPautopassnest",
		"body":[
			"\\disableAPautopassnest"
		]
	},


	"enableAPprotectnest":{
