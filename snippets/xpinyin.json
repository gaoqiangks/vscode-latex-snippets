{


	"xpinyin{}{}":{
		"prefix":"xpinyin",
		"body":[
			"\\xpinyin{${1:character}}{${2:pinyin}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"xpinyin[]{}{}":{
		"prefix":"xpinyin",
		"body":[
			"\\xpinyin[${3:options%keyvals}]{${1:character}}{${2:pinyin}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"xpinyin*{}":{
		"prefix":"*xpinyin",
		"body":[
			"\\xpinyin*{${1:text}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"xpinyin*[]{}":{
		"prefix":"*xpinyin",
		"body":[
			"\\xpinyin*[${2:options%keyvals}]{${1:text}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"pinyin{}":{
		"prefix":"pinyin",
		"body":[
			"\\pinyin{${1:pinyin}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"pinyin[]{}":{
		"prefix":"pinyin",
		"body":[
			"\\pinyin[${2:options%keyvals}]{${1:pinyin}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"setpinyin{}{}":{
		"prefix":"setpinyin",
		"body":[
			"\\setpinyin{${1:characters}}{${2:pinyin}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"xpinyinsetup{}":{
		"prefix":"xpinyinsetup",
		"body":[
			"\\xpinyinsetup{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"disablepinyin":{
		"prefix":"disablepinyin",
		"body":[
			"\\disablepinyin"
		],
		"luasnip": {"priority": 3000} 
	},


	"enablepinyin":{
		"prefix":"enablepinyin",
		"body":[
			"\\enablepinyin"
		],
		"luasnip": {"priority": 3000} 
	},


	"pinyinscope":{
		"prefix":"pinyinscope",
		"body":[
			"\\begin{pinyinscope}",
				"\t$1",
			"\\end{pinyinscope}"
		],
		"luasnip": {"priority": 3000} 
	},


	"pinyinscope[]":{
		"prefix":"pinyinscope",
		"body":[
			"\\begin{pinyinscope}",
				"\t[${1:options%keyvals}]",
			"\\end{pinyinscope}"
		],
		"luasnip": {"priority": 1000} 
	}
}
