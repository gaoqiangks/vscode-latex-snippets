{


	"xpinyin":{
		"prefix":"xpinyin",
		"body":[
			"\\xpinyin{${1:character}}{${2:pinyin}}"
		]
	},


	"xpinyin":{
		"prefix":"xpinyin",
		"body":[
			"\\xpinyin[${3:options%keyvals}]{${1:character}}{${2:pinyin}}"
		]
	},


	"xpinyin*":{
		"prefix":"xpinyin*",
		"body":[
			"\\xpinyin*{${1:text}}"
		]
	},


	"xpinyin*":{
		"prefix":"xpinyin*",
		"body":[
			"\\xpinyin*[${2:options%keyvals}]{${1:text}}"
		]
	},


	"pinyin":{
		"prefix":"pinyin",
		"body":[
			"\\pinyin{${1:pinyin}}"
		]
	},


	"pinyin":{
		"prefix":"pinyin",
		"body":[
			"\\pinyin[${2:options%keyvals}]{${1:pinyin}}"
		]
	},


	"setpinyin":{
		"prefix":"setpinyin",
		"body":[
			"\\setpinyin{${1:characters}}{${2:pinyin}}"
		]
	},


	"xpinyinsetup":{
		"prefix":"xpinyinsetup",
		"body":[
			"\\xpinyinsetup{${1:options%keyvals}}"
		]
	},


	"disablepinyin":{
		"prefix":"disablepinyin",
		"body":[
			"\\disablepinyin"
		]
	},


	"enablepinyin":{
		"prefix":"enablepinyin",
		"body":[
			"\\enablepinyin"
		]
	},


	"pinyinscope":{
		"prefix":"pinyinscope",
		"body":[
			"\\begin{pinyinscope}",
			"\t$1",
			"\\end{pinyinscope}"
		]
	}
}
