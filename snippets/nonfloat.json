{


	"tabcaption{}":{
		"prefix":"tabcaption",
		"body":[
			"\\tabcaption{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tabcaption[]{}":{
		"prefix":"tabcaption",
		"body":[
			"\\tabcaption[${2:short text%text}]{${1:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"figcaption{}":{
		"prefix":"figcaption",
		"body":[
			"\\figcaption{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"figcaption[]{}":{
		"prefix":"figcaption",
		"body":[
			"\\figcaption[${2:short text%text}]{${1:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"topcaption{}":{
