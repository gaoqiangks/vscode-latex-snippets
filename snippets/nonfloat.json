	"narrow{}{}":{
		"prefix":"narrow",
		"body":[
			"\\begin{narrow}",
				"\t{${1:left margin%l}}{${2:right margin%l}}",
			"\\end{narrow}"
		],
		"luasnip": {"priority": 3000} 
	}
}
