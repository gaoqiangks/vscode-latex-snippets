{


	"tabcaption":{
		"prefix":"tabcaption",
		"body":[
			"\\tabcaption{${1:text}}"
		]
	},


	"tabcaption":{
		"prefix":"tabcaption",
		"body":[
			"\\tabcaption[${2:short text%text}]{${1:text}}"
		]
	},


	"figcaption":{
		"prefix":"figcaption",
		"body":[
			"\\figcaption{${1:text}}"
		]
	},


	"figcaption":{
		"prefix":"figcaption",
		"body":[
			"\\figcaption[${2:short text%text}]{${1:text}}"
		]
	},


	"topcaption":{
		"prefix":"topcaption",
		"body":[
			"\\topcaption{${1:text}}"
		]
	},


	"topcaption":{
		"prefix":"topcaption",
		"body":[
			"\\topcaption[${2:short text%text}]{${1:text}}"
		]
	},


	"narrow":{
		"prefix":"narrow",
		"body":[
			"\\begin{narrow}",
			"\t$1",
			"\\end{narrow}"
		]
	}
}
