{


	"note{}{}":{
		"prefix":"note",
		"body":[
			"\\note{${1:height}}{${2:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"note[]{}{}":{
		"prefix":"note",
		"body":[
			"\\note[${3:alignment%keyvals}]{${1:height}}{${2:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


