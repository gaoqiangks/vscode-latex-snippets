{


	"regexpatchcmd{}{}{}{}{}":{
		"prefix":"regexpatchcmd",
		"body":[
			"\\regexpatchcmd{${1:command}}{${2:search}}{${3:replace once}}{${4:success}}{${5:failure}}"
		]
	},


	"regexpatchbibmacro{}{}{}{}{}":{
		"prefix":"regexpatchbibmacro",
		"body":[
			"\\regexpatchbibmacro{${1:name}}{${2:search}}{${3:replace once}}{${4:success}}{${5:failure}}"
		]
	},


	"xpatchoptarg{}{}":{
		"prefix":"xpatchoptarg",
