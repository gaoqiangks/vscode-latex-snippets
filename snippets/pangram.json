{


	"pangram":{
		"prefix":"pangram",
		"body":[
			"\\pangram"
		]
	},


	"PangramSetup{}":{
		"prefix":"PangramSetup",
		"body":[
			"\\PangramSetup{${1:options%keyvals}}"
		]
	},


	"NewPangramClass{}{}":{
		"prefix":"NewPangramClass",
		"body":[
			"\\NewPangramClass{${1:text class}}{${2:text}}"
		]
	},


	"pangram[]":{
		"prefix":"pangram",
		"body":[
			"\\pangram[${1:options%keyvals}]"
		]
	}
}
