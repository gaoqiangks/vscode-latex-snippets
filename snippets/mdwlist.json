{


	"desclabelstyle{}":{
		"prefix":"desclabelstyle",
		"body":[
			"\\desclabelstyle{${1:style}}"
		]
	},


	"desclabelwidth{}":{
		"prefix":"desclabelwidth",
		"body":[
			"\\desclabelwidth{${1:length}}"
		]
	},


	"multilinelabel":{
		"prefix":"multilinelabel",
		"body":[
			"\\multilinelabel"
		]
	},


	"nextlinelabel":{
		"prefix":"nextlinelabel",
		"body":[
			"\\nextlinelabel"
		]
	},


	"pushlabel":{
		"prefix":"pushlabel",
		"body":[
			"\\pushlabel"
		]
	},


	"resume{}":{
		"prefix":"resume",
		"body":[
			"\\resume{${1:envname}}"
		]
	},


	"suspend{}":{
		"prefix":"suspend",
		"body":[
			"\\suspend{${1:envname}}"
		]
	},


	"resume[]{}":{
		"prefix":"resume",
		"body":[
			"\\resume[${2:magicname}]{${1:envname}}"
		]
	},


	"suspend[]{}":{
		"prefix":"suspend",
		"body":[
			"\\suspend[${2:magicname}]{${1:envname}}"
		]
	},


	"description*":{
		"prefix":"*description",
		"body":[
			"\\begin{description*}",
				"\t$1",
			"\\end{description*}"
		]
	},


	"enumerate*":{
		"prefix":"*enumerate",
		"body":[
			"\\begin{enumerate*}",
				"\t$1",
			"\\end{enumerate*}"
		]
	},


	"itemize*":{
		"prefix":"*itemize",
		"body":[
			"\\begin{itemize*}",
				"\t$1",
			"\\end{itemize*}"
		]
	}
}
