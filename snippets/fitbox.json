{


	"fitbox{}":{
		"prefix":"fitbox",
		"body":[
			"\\fitbox{${1:content%text}}"
		]
	},


	"fitboxset{}":{
		"prefix":"fitboxset",
		"body":[
			"\\fitboxset{${1:options%keyvals}}"
		]
	},


	"fitboxnatwidth":{
		"prefix":"fitboxnatwidth",
		"body":[
			"\\fitboxnatwidth"
		]
	},


	"fitboxnatheight":{
		"prefix":"fitboxnatheight",
		"body":[
			"\\fitboxnatheight"
		]
	},


	"SetFitboxLayout{}{}":{
		"prefix":"SetFitboxLayout",
		"body":[
			"\\SetFitboxLayout{${1:rows}}{${2:columns}}"
		]
	},


	"fitbox*{}":{
		"prefix":"*fitbox",
		"body":[
			"\\fitbox*{${1:content%text}}"
		]
	},


	"fitbox*[]{}":{
		"prefix":"*fitbox",
		"body":[
			"\\fitbox*[${2:options%keyvals}]{${1:content%text}}"
		]
	},


	"fitbox[]{}":{
		"prefix":"fitbox",
		"body":[
			"\\fitbox[${2:options%keyvals}]{${1:content%text}}"
		]
	},


	"SetFitboxLayout[]{}{}":{
		"prefix":"SetFitboxLayout",
		"body":[
			"\\SetFitboxLayout[${3:options%keyvals}]{${1:rows}}{${2:columns}}"
		]
	}
}
