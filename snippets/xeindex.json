{


	"IndexList{}{}":{
		"prefix":"IndexList",
		"body":[
			"\\IndexList{${1:name}}{${2:list of entries}}"
		]
	},


	"StopIndexList{}":{
		"prefix":"StopIndexList",
		"body":[
			"\\StopIndexList{${1:lists}}"
		]
	},


	"StopIndex":{
		"prefix":"StopIndex",
		"body":[
			"\\StopIndex"
		]
	},


	"NoIndex{}":{
		"prefix":"NoIndex",
		"body":[
			"\\NoIndex{${1:text}}"
		]
	},


	"IndexList*{}{}":{
		"prefix":"*IndexList",
		"body":[
			"\\IndexList*{${1:name}}{${2:list of entries}}"
		]
	}
}
