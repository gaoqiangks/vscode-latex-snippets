{


	"DefineTile":{
		"prefix":"DefineTile",
		"body":[
			"\\DefineTile{${1:name%specialDef}}{${2:sides}}{${3:vertices}}"
		]
	},


	"BakeTile":{
		"prefix":"BakeTile",
		"body":[
			"\\BakeTile{${1:name%keyvals}}"
		]
	},


	"TilingDecomposition":{
		"prefix":"TilingDecomposition",
		"body":[
			"\\TilingDecomposition{${1:type%keyvals}}{${2:level}}{${3:seed}}"
		]
	},


	"TilingDecomposition":{
		"prefix":"TilingDecomposition",
		"body":[
			"\\TilingDecomposition[${4:TikZ keys}]{${1:type%keyvals}}{${2:level}}{${3:seed}}"
		]
	}
}
