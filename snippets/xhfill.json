{


	"xhrulefill":{
		"prefix":"xhrulefill",
		"body":[
			"\\xhrulefill{${1:color}}{${2:ruleheight%l}}"
		]
	},


	"xrfill":{
		"prefix":"xrfill",
		"body":[
			"\\xrfill{${1:ruleheight%l}}"
		]
	},


	"xrfill":{
		"prefix":"xrfill",
		"body":[
			"\\xrfill{${1:ruleheight%l}}[${2:color}]"
		]
	},


	"xrfill":{
		"prefix":"xrfill",
		"body":[
			"\\xrfill[${2:raise%l}]{${1:ruleheight%l}}[${3:color}]"
		]
	},


	"xhrectanglefill":{
		"prefix":"xhrectanglefill",
		"body":[
			"\\xhrectanglefill{${1:height}}{${2:linewidth%l}}"
		]
	},


	"xdotfill":{
		"prefix":"xdotfill",
		"body":[
			"\\xdotfill{${1:dotheight%l}}"
		]
	},


	"xdotfill":{
		"prefix":"xdotfill",
		"body":[
			"\\xdotfill{${1:dotheight%l}}[${2:color}]"
		]
	}
}
