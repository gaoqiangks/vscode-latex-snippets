{


	"nto":{
		"prefix":"nto",
		"body":[
			"\\nto{${1:num1}}{${2:num2}}"
		]
	},


	"ito":{
		"prefix":"ito",
		"body":[
			"\\ito{${1:num}}"
		]
	},


	"oto":{
		"prefix":"oto",
		"body":[
			"\\oto{${1:num}}"
		]
	},


	"setOTMstyle":{
		"prefix":"setOTMstyle",
		"body":[
			"\\setOTMstyle{${1:style}}"
		]
	},


	"setOTMstyle":{
		"prefix":"setOTMstyle",
		"body":[
			"\\setOTMstyle[${2:params%keyvals}]{${1:style}}"
		]
	},


	"newOTMstyle":{
		"prefix":"newOTMstyle",
		"body":[
			"\\newOTMstyle{${1:name}}{${2:definition}}"
		]
	},


	"newOTMstyle":{
		"prefix":"newOTMstyle",
		"body":[
			"\\newOTMstyle[${3:params}]{${1:name}}{${2:definition}}"
		]
	},


	"getOTMparameter":{
		"prefix":"getOTMparameter",
		"body":[
			"\\getOTMparameter{${1:param}}"
		]
	},


	"renewOTMstyle":{
		"prefix":"renewOTMstyle",
		"body":[
			"\\renewOTMstyle{${1:name}}{${2:definition}}"
		]
	},


	"newOTMstyle":{
		"prefix":"newOTMstyle",
		"body":[
			"\\newOTMstyle[${3:params}]{${1:name}}{${2:definition}}"
		]
	},


	"newOTMparameter":{
		"prefix":"newOTMparameter",
		"body":[
			"\\newOTMparameter{${1:style}}{${2:param}}{${3:value}}"
		]
	}
}
