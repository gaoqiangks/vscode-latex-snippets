{


	"useclass{}":{
		"prefix":"useclass",
		"body":[
			"\\useclass{${1:class}}"
		]
	},


	"useclass*{}":{
		"prefix":"*useclass",
		"body":[
			"\\useclass*{${1:class}}"
		]
	},


	"useclass*[]{}":{
		"prefix":"*useclass",
		"body":[
			"\\useclass*[${2:options%keyvals}]{${1:class}}"
		]
	},


	"useclass[]{}":{
		"prefix":"useclass",
		"body":[
			"\\useclass[${2:options%keyvals}]{${1:class}}"
		]
	}
}
