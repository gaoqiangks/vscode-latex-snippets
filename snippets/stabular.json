{


	"stabular{}":{
		"prefix":"stabular",
		"body":[
			"\\begin{stabular}",
				"\t{${1:cols}}",
			"\\end{stabular}"
		],
		"luasnip": {"priority": 3000} 
	},


	"stabular[]{}":{
		"prefix":"stabular",
		"body":[
			"\\begin{stabular}",
				"\t[${2:pos}]{${1:cols}}",
			"\\end{stabular}"
		],
		"luasnip": {"priority": 1000} 
	},


	"stabular*{}[]{}":{
		"prefix":"*stabular",
		"body":[
			"\\begin{stabular*}",
				"\t{${1:width}}[${3:pos}]{${2:cols}}",
			"\\end{stabular*}"
		],
		"luasnip": {"priority": 2000} 
	},


	"stabular*{}{}":{
		"prefix":"*stabular",
		"body":[
			"\\begin{stabular*}",
				"\t{${1:width}}{${2:cols}}",
			"\\end{stabular*}"
		],
		"luasnip": {"priority": 2000} 
	}
}
