{


	"nextgroupplot":{
		"prefix":"nextgroupplot",
		"body":[
			"\\nextgroupplot"
		]
	},


	"nextgroupplot[]":{
		"prefix":"nextgroupplot",
		"body":[
			"\\nextgroupplot[${1:options%keyvals}]"
		]
	},


	"groupplot":{
		"prefix":"groupplot",
		"body":[
			"\\begin{groupplot}",
				"\t$1",
			"\\end{groupplot}"
		]
	},


	"groupplot":{
		"prefix":"groupplot",
		"body":[
			"\\begin{groupplot}",
				"\t$1",
			"\\end{groupplot}"
		]
	}
}
