{


	"controlbutton{}{}{}":{
		"prefix":"controlbutton",
		"body":[
			"\\controlbutton{${1:caption%text}}{${2:target frame}}{${3:options%keyvals}}"
		]
	},


	"controlbutton[]{}{}{}":{
		"prefix":"controlbutton",
		"body":[
			"\\controlbutton[${4:sequence file,start page}]{${1:caption%text}}{${2:target frame}}{${3:options%keyvals}}"
		]
	},


	"controlbutton[]{}{}{}":{
		"prefix":"controlbutton",
		"body":[
			"\\controlbutton[${4:sequence file,start page,end page}]{${1:caption%text}}{${2:target frame}}{${3:options%keyvals}}"
		]
	},


	"animation":{
		"prefix":"animation",
		"body":[
			"\\begin{animation}",
				"\t$1",
			"\\end{animation}"
		]
	},


	"aframe":{
		"prefix":"aframe",
		"body":[
			"\\begin{aframe}",
				"\t$1",
			"\\end{aframe}"
		]
	},


	"aframe":{
		"prefix":"aframe",
		"body":[
			"\\begin{aframe}",
				"\t$1",
			"\\end{aframe}"
		]
	}
}
