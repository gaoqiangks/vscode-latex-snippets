{


	"controlbutton{}{}{}":{
		"prefix":"controlbutton",
		"body":[
			"\\controlbutton{${1:caption%text}}{${2:target frame}}{${3:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"controlbutton[]{}{}{}":{
		"prefix":"controlbutton",
		"body":[
			"\\controlbutton[${4:sequence file,start page}]{${1:caption%text}}{${2:target frame}}{${3:options%keyvals}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"controlbutton[]{}{}{}":{
		"prefix":"controlbutton",
		"body":[
			"\\controlbutton[${4:sequence file,start page,end page}]{${1:caption%text}}{${2:target frame}}{${3:options%keyvals}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"animation{}{}":{
		"prefix":"animation",
		"body":[
			"\\begin{animation}",
				"\t{${1:width}}{${2:height}}",
			"\\end{animation}"
		],
		"luasnip": {"priority": 3000} 
	},


	"aframe{}{}":{
		"prefix":"aframe",
		"body":[
			"\\begin{aframe}",
				"\t{${1:frame name}}{${2:imagefile}}",
			"\\end{aframe}"
		],
		"luasnip": {"priority": 3000} 
	},


	"aframe{}{}":{
		"prefix":"aframe",
		"body":[
			"\\begin{aframe}",
				"\t{${1:frame name}}{${2:imagefile,filepage}}",
			"\\end{aframe}"
		],
		"luasnip": {"priority": 3000} 
	}
}
