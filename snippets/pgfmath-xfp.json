{


	"pgfmxfpdeclarefunction{}{}{}":{
		"prefix":"pgfmxfpdeclarefunction",
		"body":[
			"\\pgfmxfpdeclarefunction{${1:name}}{${2:args}}{${3:fp expression}}"
		]
	},


	"pgfmxfpdeclarefunction{}{}[]{}":{
		"prefix":"pgfmxfpdeclarefunction",
		"body":[
			"\\pgfmxfpdeclarefunction{${1:name}}{${2:args}}[${4:process-args}]{${3:fp expression}}"
		]
	}
}
