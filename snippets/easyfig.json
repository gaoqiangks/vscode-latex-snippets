{


	"Figure{}":{
		"prefix":"Figure",
		"body":[
			"\\Figure{${1:imagefile}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"Figure[]{}":{
		"prefix":"Figure",
		"body":[
			"\\Figure[${2:options%keyvals}]{${1:imagefile}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"Figure*{}":{
		"prefix":"*Figure",
		"body":[
			"\\Figure*{${1:imagefile}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"Figure*[]{}":{
		"prefix":"*Figure",
		"body":[
			"\\Figure*[${2:options%keyvals}]{${1:imagefile}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"easyfigdefault{}":{
		"prefix":"easyfigdefault",
		"body":[
			"\\easyfigdefault{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"easyfigdefault*{}":{
		"prefix":"*easyfigdefault",
		"body":[
			"\\easyfigdefault*{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 2000} 
	}
}
