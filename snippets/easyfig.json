{


	"Figure":{
		"prefix":"Figure",
		"body":[
			"\\Figure{${1:imagefile}}"
		]
	},


	"Figure":{
		"prefix":"Figure",
		"body":[
			"\\Figure[${2:options%keyvals}]{${1:imagefile}}"
		]
	},


	"Figure*":{
		"prefix":"Figure*",
		"body":[
			"\\Figure*{${1:imagefile}}"
		]
	},


	"Figure*":{
		"prefix":"Figure*",
		"body":[
			"\\Figure*[${2:options%keyvals}]{${1:imagefile}}"
		]
	},


	"easyfigdefault":{
		"prefix":"easyfigdefault",
		"body":[
			"\\easyfigdefault{${1:options%keyvals}}"
		]
	},


	"easyfigdefault*":{
		"prefix":"easyfigdefault*",
		"body":[
			"\\easyfigdefault*{${1:options%keyvals}}"
		]
	}
}
