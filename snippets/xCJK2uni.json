{


	"useCJKencmap{}":{
		"prefix":"useCJKencmap",
		"body":[
			"\\useCJKencmap{${1:encoding}}"
		]
	},


	"CJKchartouni{}":{
		"prefix":"CJKchartouni",
		"body":[
			"\\CJKchartouni{${1:character}}"
		]
	},


	"CJKsfdtouni{}{}":{
		"prefix":"CJKsfdtouni",
		"body":[
			"\\CJKsfdtouni{${1:plane}}{${2:slot}}"
		]
	}
}
