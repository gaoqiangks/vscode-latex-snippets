{


	"longest{}":{
		"prefix":"longest",
		"body":[
			"\\longest{${1:song line}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"numbercouplets":{
		"prefix":"numbercouplets",
		"body":[
			"\\numbercouplets"
		],
		"luasnip": {"priority": 3000} 
	},


	"inputsong{}{}":{
		"prefix":"inputsong",
		"body":[
			"\\inputsong{${1:file}}{${2:stanzas per slide}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"inputsong{}{}[]":{
		"prefix":"inputsong",
		"body":[
			"\\inputsong{${1:file}}{${2:stanzas per slide}}[${3:couplet list}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"inputsong*{}{}":{
		"prefix":"*inputsong",
		"body":[
			"\\inputsong*{${1:file}}{${2:stanzas per slide}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"inputsong*{}{}[]":{
		"prefix":"*inputsong",
		"body":[
			"\\inputsong*{${1:file}}{${2:stanzas per slide}}[${3:couplet list}]"
		],
		"luasnip": {"priority": 2000} 
	},


	"song{}":{
		"prefix":"song",
		"body":[
			"\\begin{song}",
				"\t{${1:stanzas per slide}}",
			"\\end{song}"
		],
		"luasnip": {"priority": 3000} 
	},


	"song{}[]":{
		"prefix":"song",
		"body":[
			"\\begin{song}",
				"\t{${1:stanzas per slide}}[${2:couplet list}]",
			"\\end{song}"
		],
		"luasnip": {"priority": 1000} 
	},


	"intro":{
		"prefix":"intro",
		"body":[
			"\\begin{intro}",
				"\t$1",
			"\\end{intro}"
		],
		"luasnip": {"priority": 3000} 
	},


	"refrain":{
		"prefix":"refrain",
		"body":[
			"\\begin{refrain}",
				"\t$1",
			"\\end{refrain}"
		],
		"luasnip": {"priority": 3000} 
	},


	"couplet":{
		"prefix":"couplet",
		"body":[
			"\\begin{couplet}",
				"\t$1",
			"\\end{couplet}"
		],
		"luasnip": {"priority": 3000} 
	},


	"final":{
		"prefix":"final",
		"body":[
			"\\begin{final}",
				"\t$1",
			"\\end{final}"
		],
		"luasnip": {"priority": 3000} 
	}
}
