{


	"declaretheoremstyle{}":{
		"prefix":"declaretheoremstyle",
		"body":[
			"\\declaretheoremstyle{${1:style name}}"
		]
	},


	"declaretheorem{}":{
		"prefix":"declaretheorem",
		"body":[
			"\\declaretheorem{${1:envname}}"
		]
	},


	"thmcontinues{}":{
		"prefix":"thmcontinues",
		"body":[
			"\\thmcontinues{${1:key}}"
		]
	},


	"declaretheoremstyle[]{}":{
		"prefix":"declaretheoremstyle",
		"body":[
			"\\declaretheoremstyle[${2:options%keyvals}]{${1:style name}}"
		]
	},


	"declaretheorem[]{}":{
		"prefix":"declaretheorem",
		"body":[
			"\\declaretheorem[${2:options%keyvals}]{${1:envname}}"
		]
	},


	"declaretheorem[]{}[]":{
		"prefix":"declaretheorem",
		"body":[
			"\\declaretheorem[${2:options%keyvals}]{${1:envname}}[${3:options}]"
		]
	},


	"declaretheorem{}[]":{
		"prefix":"declaretheorem",
		"body":[
			"\\declaretheorem{${1:envname}}[${2:options}]"
		]
	}
}
