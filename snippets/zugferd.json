{


	"SetZUGFeRDData":{
		"prefix":"SetZUGFeRDData",
		"body":[
			"\\SetZUGFeRDData{${1:keyvals}}"
		]
	},


	"SetZUGFeRDData*":{
		"prefix":"SetZUGFeRDData*",
		"body":[
			"\\SetZUGFeRDData*{${1:keyvals}}"
		]
	},


	"InsertZUGFeRDData":{
		"prefix":"InsertZUGFeRDData",
		"body":[
			"\\InsertZUGFeRDData{${1:data-selection}}"
		]
	},


	"InsertZUGFeRDData":{
		"prefix":"InsertZUGFeRDData",
		"body":[
			"\\InsertZUGFeRDData[${2:special mode option}]{${1:data-selection}}"
		]
	},


	"startWritingZUGFeRDxml":{
		"prefix":"startWritingZUGFeRDxml",
		"body":[
			"\\startWritingZUGFeRDxml"
		]
	},


	"stopWritingZUGFeRDxml":{
		"prefix":"stopWritingZUGFeRDxml",
		"body":[
			"\\stopWritingZUGFeRDxml"
		]
	},


	"setupZUGFeRDStrings":{
		"prefix":"setupZUGFeRDStrings",
		"body":[
			"\\setupZUGFeRDStrings{${1:property}}{${2:settings}}"
		]
	},


	"ZUGFeRD":{
		"prefix":"ZUGFeRD",
		"body":[
			"\\begin{ZUGFeRD}",
			"\t$1",
			"\\end{ZUGFeRD}"
		]
	}
}
