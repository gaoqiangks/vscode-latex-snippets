{


	"grabbox":{
		"prefix":"grabbox",
		"body":[
			"\\grabbox{${1:box register}}{${2:box type}}{${3:afterwards}}"
		]
	},


	"grabbox":{
		"prefix":"grabbox",
		"body":[
			"\\grabbox[${4:pre pre}]{${1:box register}}[${5:pre}]{${2:box type}}[${6:post}]{${3:afterwards}}"
		]
	},


	"grabbox*":{
		"prefix":"grabbox*",
		"body":[
			"\\grabbox*{${1:box register}}{${2:box type}}{${3:afterwards}}"
		]
	},


	"grabbox*":{
		"prefix":"grabbox*",
		"body":[
			"\\grabbox*[${4:pre pre}]{${1:box register}}[${5:pre}]{${2:box type}}[${6:post}]{${3:afterwards}}"
		]
	}
}
