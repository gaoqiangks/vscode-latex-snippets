{


	"definecollection{}":{
		"prefix":"definecollection",
		"body":[
			"\\definecollection{${1:name}}"
		]
	},


	"includecollection{}":{
		"prefix":"includecollection",
		"body":[
			"\\includecollection{${1:name}}"
		]
	},


	"collect*":{
		"prefix":"collect",
		"body":[
			"\\begin{collect*}",
				"\t$1",
			"\\end{collect*}"
		]
	},


	"collect":{
		"prefix":"collect",
		"body":[
			"\\begin{collect}",
				"\t$1",
			"\\end{collect}"
		]
	},


	"collectinmacro":{
		"prefix":"collectinmacro",
		"body":[
			"\\begin{collectinmacro}",
				"\t$1",
			"\\end{collectinmacro}"
		]
	}
}
