{


	"commaform{}":{
		"prefix":"commaform",
		"body":[
			"\\commaform{${1:counter}}"
		]
	},


	"commaformtoken":{
		"prefix":"commaformtoken",
		"body":[
			"\\commaformtoken"
		]
	},


	"addcomma{}{}{}{}":{
		"prefix":"addcomma",
		"body":[
			"\\addcomma{${1:digit1}}{${2:digit2}}{${3:digit3}}{${4:digit4 or \\relax}}"
		]
	}
}
