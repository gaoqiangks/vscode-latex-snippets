{


	"toprule":{
		"prefix":"toprule",
		"body":[
			"\\toprule"
		]
	},


	"midrule":{
		"prefix":"midrule",
		"body":[
			"\\midrule"
		]
	},


	"cmidrule{}":{
		"prefix":"cmidrule",
		"body":[
			"\\cmidrule{${1:i-j}}"
		]
	},


	"bottomrule":{
		"prefix":"bottomrule",
		"body":[
			"\\bottomrule"
		]
	},


	"cmidrulemore{}":{
		"prefix":"cmidrulemore",
		"body":[
			"\\cmidrulemore{${1:i-j}}"
		]
	},


	"morecmidrules":{
		"prefix":"morecmidrules",
		"body":[
			"\\morecmidrules"
		]
	},


	"specialrule{}{}{}":{
		"prefix":"specialrule",
		"body":[
			"\\specialrule{${1:options%keyvals}}{${2:belowsep}}{${3:abovesep}}"
		]
	},


	"addrowspace":{
		"prefix":"addrowspace",
		"body":[
			"\\addrowspace"
		]
	},


	"addlinespace":{
		"prefix":"addlinespace",
		"body":[
			"\\addlinespace"
		]
	},


	"toprule[]":{
		"prefix":"toprule",
		"body":[
			"\\toprule[${1:options%keyvals}]"
		]
	},


	"midrule[]":{
		"prefix":"midrule",
		"body":[
			"\\midrule[${1:options%keyvals}]"
		]
	},


	"cmidrule[]{}":{
		"prefix":"cmidrule",
		"body":[
			"\\cmidrule[${2:options%keyvals}]{${1:i-j}}"
		]
	},


	"bottomrule[]":{
		"prefix":"bottomrule",
		"body":[
			"\\bottomrule[${1:options%keyvals}]"
		]
	},


	"cmidrulemore[]{}":{
		"prefix":"cmidrulemore",
		"body":[
			"\\cmidrulemore[${2:options%keyvals}]{${1:i-j}}"
		]
	},


	"addrowspace[]":{
		"prefix":"addrowspace",
		"body":[
			"\\addrowspace[${1:space%l}]"
		]
	},


	"addlinespace[]":{
		"prefix":"addlinespace",
		"body":[
			"\\addlinespace[${1:space%l}]"
		]
	},


	"booktabs":{
		"prefix":"booktabs",
		"body":[
			"\\begin{booktabs}",
				"\t$1",
			"\\end{booktabs}"
		]
	},


	"longtabs":{
		"prefix":"longtabs",
		"body":[
			"\\begin{longtabs}",
				"\t$1",
			"\\end{longtabs}"
		]
	},


	"talltabs":{
		"prefix":"talltabs",
		"body":[
			"\\begin{talltabs}",
				"\t$1",
			"\\end{talltabs}"
		]
	}
}
