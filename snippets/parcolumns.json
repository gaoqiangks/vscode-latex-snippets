{


	"colchunk{}":{
		"prefix":"colchunk",
		"body":[
			"\\colchunk{${1:chunk%text}}"
		]
	},


	"colplacechunks":{
		"prefix":"colplacechunks",
		"body":[
			"\\colplacechunks"
		]
	},


	"colchunk[]{}":{
		"prefix":"colchunk",
		"body":[
			"\\colchunk[${2:column}]{${1:chunk%text}}"
		]
	},


	"parcolumns":{
		"prefix":"parcolumns",
		"body":[
			"\\begin{parcolumns}",
				"\t$1",
			"\\end{parcolumns}"
		]
	},


	"parcolumns":{
		"prefix":"parcolumns",
		"body":[
			"\\begin{parcolumns}",
				"\t$1",
			"\\end{parcolumns}"
		]
	}
}
