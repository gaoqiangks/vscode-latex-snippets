{


	"Arg{}":{
		"prefix":"Arg",
		"body":[
			"\\Arg{${1:text%plain}}"
		]
	},


	"marg{}":{
		"prefix":"marg",
		"body":[
			"\\marg{${1:text%plain}}"
		]
	},


	"oarg{}":{
		"prefix":"oarg",
		"body":[
			"\\oarg{${1:text%plain}}"
		]
	},


	"parg{}":{
		"prefix":"parg",
		"body":[
			"\\parg{${1:text%plain}}"
		]
	},


	"addurl{}{}":{
		"prefix":"addurl",
		"body":[
			"\\addurl{${1:URL}}{${2:see below}}"
		]
	},


	"addCTAN{}{}":{
		"prefix":"addCTAN",
		"body":[
			"\\addCTAN{${1:URL}}{${2:see below}}"
		]
	},


	"CTAN{}":{
		"prefix":"CTAN",
		"body":[
			"\\CTAN{${1:path%URL}}"
		]
	},


	"dbx":{
		"prefix":"dbx",
		"body":[
			"\\dbx"
		]
	},


	"lastlinedim":{
		"prefix":"lastlinedim",
		"body":[
			"\\lastlinedim"
		]
	},


	"getlastlinesize":{
		"prefix":"getlastlinesize",
		"body":[
			"\\getlastlinesize"
		]
	},


	"mypath":{
		"prefix":"mypath",
		"body":[
			"\\mypath"
		]
	},


	"href{}{}":{
		"prefix":"href",
		"body":[
			"\\href{${1:URL}}{${2:text}}"
		]
	},


	"Arg*{}":{
		"prefix":"*Arg",
		"body":[
			"\\Arg*{${1:text%plain}}"
		]
	},


	"marg*{}":{
		"prefix":"*marg",
		"body":[
			"\\marg*{${1:text%plain}}"
		]
	},


	"oarg*{}":{
		"prefix":"*oarg",
		"body":[
			"\\oarg*{${1:text%plain}}"
		]
	},


	"parg*{}":{
		"prefix":"*parg",
		"body":[
			"\\parg*{${1:text%plain}}"
		]
	},


	"addurl[]{}{}":{
		"prefix":"addurl",
		"body":[
			"\\addurl[${3:pre text}]{${1:URL}}{${2:see below}}"
		]
	},


	"addCTAN[]{}{}":{
		"prefix":"addCTAN",
		"body":[
			"\\addCTAN[${3:pre text}]{${1:URL}}{${2:see below}}"
		]
	},


	"dbx[]":{
		"prefix":"dbx",
		"body":[
			"\\dbx[${1:width}]"
		]
	},


	"syntax":{
		"prefix":"syntax",
		"body":[
			"\\begin{syntax}",
				"\t$1",
			"\\end{syntax}"
		]
	},


	"syntax*":{
		"prefix":"*syntax",
		"body":[
			"\\begin{syntax*}",
				"\t$1",
			"\\end{syntax*}"
		]
	}
}
