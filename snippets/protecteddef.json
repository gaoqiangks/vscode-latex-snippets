{


	"ProtectedDef{}{}":{
		"prefix":"ProtectedDef",
		"body":[
			"\\ProtectedDef{${1:cmd}}{${2:definition}}"
		]
	},


	"ProtectedDef*{}{}":{
		"prefix":"*ProtectedDef",
		"body":[
			"\\ProtectedDef*{${1:cmd}}{${2:definition}}"
		]
	},


	"ProtectedDef*{}[]{}":{
		"prefix":"*ProtectedDef",
		"body":[
			"\\ProtectedDef*{${1:cmd}}[${3:args}]{${2:definition}}"
		]
	},


	"ProtectedDef{}[]{}":{
		"prefix":"ProtectedDef",
		"body":[
			"\\ProtectedDef{${1:cmd}}[${3:args}]{${2:definition}}"
		]
	}
}
