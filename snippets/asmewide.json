{


	"savparskip":{
		"prefix":"savparskip",
		"body":[
			"\\savparskip"
		],
		"luasnip": {"priority": 3000} 
	},


	"widetext":{
		"prefix":"widetext",
		"body":[
			"\\begin{widetext}",
				"\t$1",
			"\\end{widetext}"
		],
		"luasnip": {"priority": 3000} 
	},


	"widetext[]":{
		"prefix":"widetext",
		"body":[
			"\\begin{widetext}",
				"\t[${1:stripsep}]",
			"\\end{widetext}"
		],
		"luasnip": {"priority": 1000} 
	},


	"widetext[][]":{
		"prefix":"widetext",
		"body":[
			"\\begin{widetext}",
				"\t[${1:stripsep}][${2:placement}]",
			"\\end{widetext}"
		],
		"luasnip": {"priority": 1000} 
	}
}
