{


	"termlabel":{
		"prefix":"termlabel",
		"body":[
			"\\termlabel"
		]
	},


	"termlabelfont":{
		"prefix":"termlabelfont",
		"body":[
			"\\termlabelfont"
		]
	},


	"termindent":{
		"prefix":"termindent",
		"body":[
			"\\termindent"
		]
	},


	"termlist":{
		"prefix":"termlist",
		"body":[
			"\\begin{termlist}",
				"\t$1",
			"\\end{termlist}"
		]
	},


	"term":{
		"prefix":"term",
		"body":[
			"\\begin{term}",
				"\t$1",
			"\\end{term}"
		]
	},


	"termlist*":{
		"prefix":"*termlist",
		"body":[
			"\\begin{termlist*}",
				"\t$1",
			"\\end{termlist*}"
		]
	},


	"term*":{
		"prefix":"*term",
		"body":[
			"\\begin{term*}",
				"\t$1",
			"\\end{term*}"
		]
	}
}
