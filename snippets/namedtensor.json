		"body":[
			"\\ncat{${1:axis name}}"
		]
	},


	"nbin{}{}":{
		"prefix":"nbin",
		"body":[
			"\\nbin{${1:axis name}}{${2:symbol}}"
		]
	},


	"nsum{}":{
		"prefix":"nsum",
		"body":[
			"\\nsum{${1:axis name}}"
		]
	},


	"nfun{}{}":{
		"prefix":"nfun",
		"body":[
			"\\nfun{${1:axis name}}{${2:function}}"
