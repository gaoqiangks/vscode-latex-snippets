{


	"gmpoptions{}":{
		"prefix":"gmpoptions",
		"body":[
			"\\gmpoptions{${1:options%keyvals}}"
		]
	},


	"usempxclass{}":{
		"prefix":"usempxclass",
		"body":[
			"\\usempxclass{${1:class}}"
		]
	},


	"usempxpackage{}":{
		"prefix":"usempxpackage",
		"body":[
			"\\usempxpackage{${1:package}}"
		]
	},


	"resetmpxpackages":{
		"prefix":"resetmpxpackages",
		"body":[
			"\\resetmpxpackages"
		]
	},


	"mpxcommands{}":{
		"prefix":"mpxcommands",
		"body":[
			"\\mpxcommands{${1:commands}}"
		]
	},


	"resetmpxcommands":{
		"prefix":"resetmpxcommands",
		"body":[
			"\\resetmpxcommands"
		]
	},


	"mpdim{}":{
		"prefix":"mpdim",
		"body":[
			"\\mpdim{${1:dimen param}}"
		]
	},


	"usempost{}":{
		"prefix":"usempost",
		"body":[
			"\\usempost{${1:string}}"
		]
	},


	"btex":{
		"prefix":"btex",
		"body":[
			"\\btex"
		]
	},


	"verbatimtex":{
		"prefix":"verbatimtex",
		"body":[
			"\\verbatimtex"
		]
	},


	"usempxclass[]{}":{
		"prefix":"usempxclass",
		"body":[
			"\\usempxclass[${2:options}]{${1:class}}"
		]
	},


	"usempxpackage[]{}":{
		"prefix":"usempxpackage",
		"body":[
			"\\usempxpackage[${2:options}]{${1:package}}"
		]
	},


	"usempost[]{}":{
		"prefix":"usempost",
		"body":[
			"\\usempost[${2:options%keyvals}]{${1:string}}"
		]
	},


	"mpost":{
		"prefix":"mpost",
		"body":[
			"\\begin{mpost}",
				"\t$1",
			"\\end{mpost}"
		]
	},


	"mpost*":{
		"prefix":"*mpost",
		"body":[
			"\\begin{mpost*}",
				"\t$1",
			"\\end{mpost*}"
		]
	},


	"mpost":{
		"prefix":"mpost",
		"body":[
			"\\begin{mpost}",
				"\t$1",
			"\\end{mpost}"
		]
	},


	"mpost*":{
		"prefix":"mpost",
		"body":[
			"\\begin{mpost*}",
				"\t$1",
			"\\end{mpost*}"
		]
	}
}
