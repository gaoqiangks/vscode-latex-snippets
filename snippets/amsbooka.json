{


	"barefootnote{}":{
		"prefix":"barefootnote",
		"body":[
			"\\barefootnote{${1:text}}"
		]
	},


	"partauthor":{
		"prefix":"partauthor",
		"body":[
			"\\partauthor"
		]
	},


	"inchapterbibliography":{
		"prefix":"inchapterbibliography",
		"body":[
			"\\begin{inchapterbibliography}",
				"\t$1",
			"\\end{inchapterbibliography}"
		]
	}
}
