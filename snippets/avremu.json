{


	"useavremulibrary{}":{
		"prefix":"useavremulibrary",
		"body":[
			"\\useavremulibrary{${1:list of libraries}}"
		]
	},


	"avrloadc{}":{
		"prefix":"avrloadc",
		"body":[
			"\\avrloadc{${1:file}}"
		]
	},


	"avrcompile{}":{
		"prefix":"avrcompile",
		"body":[
			"\\avrcompile{${1:file}}"
		]
	},


	"avrloadihex{}":{
		"prefix":"avrloadihex",
		"body":[
			"\\avrloadihex{${1:file}}"
		]
	},


	"ifavrbreak":{
		"prefix":"ifavrbreak",
		"body":[
			"\\ifavrbreak"
		]
	},


	"avrbreaktrue":{
		"prefix":"avrbreaktrue",
		"body":[
			"\\avrbreaktrue"
		]
	},


	"avrbreakfalse":{
		"prefix":"avrbreakfalse",
		"body":[
			"\\avrbreakfalse"
		]
	},


	"avrstep":{
		"prefix":"avrstep",
		"body":[
			"\\avrstep"
		]
	},


	"avrrun":{
		"prefix":"avrrun",
		"body":[
			"\\avrrun"
		]
	},


	"avrsinglestep":{
		"prefix":"avrsinglestep",
		"body":[
			"\\avrsinglestep"
		]
	},


	"avrinstrcount":{
		"prefix":"avrinstrcount",
		"body":[
			"\\avrinstrcount"
		]
	},


	"avrUDR":{
		"prefix":"avrUDR",
		"body":[
			"\\avrUDR"
		]
	},


	"avrUDRclear":{
		"prefix":"avrUDRclear",
		"body":[
			"\\avrUDRclear"
		]
	},


	"avrdrawiter{}":{
		"prefix":"avrdrawiter",
		"body":[
			"\\avrdrawiter{${1:arg}}"
		]
	},


	"avrdrawSize{}{}":{
		"prefix":"avrdrawSize",
		"body":[
			"\\avrdrawSize{${1:arg1}}{${2:arg2}}"
		]
	},


	"avrdrawppm{}":{
		"prefix":"avrdrawppm",
		"body":[
			"\\avrdrawppm{${1:file}}"
		]
	},


	"avrloadc[]{}":{
		"prefix":"avrloadc",
		"body":[
			"\\avrloadc[${2:compiler options}]{${1:file}}"
		]
	},


	"avrcompile[]{}":{
		"prefix":"avrcompile",
		"body":[
			"\\avrcompile[${2:compiler options}]{${1:file}}"
		]
	},


	"avrstep[]":{
		"prefix":"avrstep",
		"body":[
			"\\avrstep[${1:number}]"
		]
	}
}
