{


	"animategraphics":{
		"prefix":"animategraphics",
		"body":[
			"\\animategraphics{${1:frame rate}}{${2:file basename}}{${3:first}}{${4:last}}"
		]
	},


	"animategraphics":{
		"prefix":"animategraphics",
		"body":[
			"\\animategraphics[${5:options%keyvals}]{${1:frame rate}}{${2:file basename}}{${3:first}}{${4:last}}"
		]
	},


	"newframe":{
		"prefix":"newframe",
		"body":[
			"\\newframe"
		]
	},


	"newframe":{
		"prefix":"newframe",
		"body":[
			"\\newframe[${1:frame rate}]"
		]
	},


	"newframe*":{
		"prefix":"newframe*",
		"body":[
			"\\newframe*"
		]
	},


	"newframe*":{
		"prefix":"newframe*",
		"body":[
			"\\newframe*[${1:frame rate}]"
		]
	},


	"multiframe":{
		"prefix":"multiframe",
		"body":[
			"\\multiframe{${1:number of frames}}{${2:variables}}{${3:content%definition}}"
		]
	},


	"multiframebreak":{
		"prefix":"multiframebreak",
		"body":[
			"\\multiframebreak"
		]
	},


	"animateinline":{
		"prefix":"animateinline",
		"body":[
			"\\begin{animateinline}",
			"\t$1",
			"\\end{animateinline}"
		]
	}
