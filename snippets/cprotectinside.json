{


	"cprotectinside{}{}":{
		"prefix":"cprotectinside",
		"body":[
			"\\cprotectinside{${1:delimiter}}{${2:content%text}}"
		]
	},


	"cprotectinsideAppend":{
		"prefix":"cprotectinsideAppend",
		"body":[
			"\\cprotectinsideAppend"
		]
	},


	"cprotectinsideReexec{}":{
		"prefix":"cprotectinsideReexec",
		"body":[
			"\\cprotectinsideReexec{${1:arg}}"
		]
	}
}
