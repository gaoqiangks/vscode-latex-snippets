{


	"zcheck{}{}":{
		"prefix":"zcheck",
		"body":[
			"\\zcheck{${1:labellist}}{${2:text}}"
		]
	},


	"zctarget{}{}":{
		"prefix":"zctarget",
		"body":[
			"\\zctarget{${1:label}}{${2:text}}"
		]
	},


	"zrefchecksetup{}":{
		"prefix":"zrefchecksetup",
		"body":[
			"\\zrefchecksetup{${1:options%keyvals}}"
		]
	},


	"zcheck[]{}{}":{
		"prefix":"zcheck",
		"body":[
			"\\zcheck[${3:checks%keyvals}]{${1:labellist}}{${2:text}}"
		]
	},


	"zcregion":{
		"prefix":"zcregion",
		"body":[
			"\\begin{zcregion}",
				"\t$1",
			"\\end{zcregion}"
		]
	}
}
