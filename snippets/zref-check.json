{


	"zcheck{}{}":{
		"prefix":"zcheck",
		"body":[
			"\\zcheck{${1:labellist}}{${2:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"zcheck[]{}{}":{
		"prefix":"zcheck",
		"body":[
			"\\zcheck[${3:checks%keyvals}]{${1:labellist}}{${2:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"zctarget{}{}":{
		"prefix":"zctarget",
		"body":[
			"\\zctarget{${1:label}}{${2:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"zrefchecksetup{}":{
		"prefix":"zrefchecksetup",
		"body":[
			"\\zrefchecksetup{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"zcregion{}":{
		"prefix":"zcregion",
		"body":[
			"\\begin{zcregion}",
				"\t{${1:label}}",
			"\\end{zcregion}"
		],
		"luasnip": {"priority": 3000} 
	}
}
