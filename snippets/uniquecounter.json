{


	"UniqueCounterNew":{
		"prefix":"UniqueCounterNew",
		"body":[
			"\\UniqueCounterNew{${1:name}}"
		]
	},


	"UniqueCounterCall":{
		"prefix":"UniqueCounterCall",
		"body":[
			"\\UniqueCounterCall{${1:counter}}{${2:code}}"
		]
	},


	"UniqueCounterIncrement":{
		"prefix":"UniqueCounterIncrement",
		"body":[
			"\\UniqueCounterIncrement{${1:counter}}"
		]
	},


	"UniqueCounterGet":{
		"prefix":"UniqueCounterGet",
		"body":[
			"\\UniqueCounterGet{${1:counter}}"
		]
	}
}
