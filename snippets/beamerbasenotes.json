{


	"note{}":{
		"prefix":"note",
		"body":[
			"\\note{${1:note text%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"note<overlay specification>{}":{
		"prefix":"note<overlay specification>",
		"body":[
			"\\note<overlay specification>{${1:note text%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"note[]{}":{
		"prefix":"note",
		"body":[
			"\\note[${2:options%keyvals}]{${1:note text%text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"note[]<>{}":{
		"prefix":"note",
		"body":[
			"\\note[${2:options%keyvals}]<${3:overlay specification}>{${1:note text%text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"insertnote":{
		"prefix":"insertnote",
		"body":[
			"\\insertnote"
		],
		"luasnip": {"priority": 3000} 
	},


	"AtBeginNote{}":{
		"prefix":"AtBeginNote",
		"body":[
			"\\AtBeginNote{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"AtEndNote{}":{
		"prefix":"AtEndNote",
		"body":[
			"\\AtEndNote{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"insertslideintonotes{}":{
		"prefix":"insertslideintonotes",
		"body":[
			"\\insertslideintonotes{${1:magnification}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
