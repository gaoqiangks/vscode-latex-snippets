{


	"appendmask":{
		"prefix":"appendmask",
		"body":[
			"\\appendmask[${2:prefix}]{${1:key%plain}}[${3:values}]"
		]
	},


	"ifinmask":{
		"prefix":"ifinmask",
		"body":[
			"\\ifinmask[${4:prefix}]{${1:key%plain}}[${5:values}]{${2:true}}{${3:false}}"
		]
	},


	"clearmask":{
		"prefix":"clearmask",
		"body":[
			"\\clearmask"
		]
	}
}
