			"\\ifinmask[${4:prefix}]{${1:key%plain}}[${5:values}]{${2:true}}{${3:false}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"clearmask":{
		"prefix":"clearmask",
		"body":[
			"\\clearmask"
		],
		"luasnip": {"priority": 3000} 
	}
}
