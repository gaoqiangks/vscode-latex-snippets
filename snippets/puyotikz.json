	},


	"puyoboard[]{}{}":{
		"prefix":"puyoboard",
		"body":[
			"\\puyoboard[${3:options%keyvals}]{${1:board string}}{${2:next puyos string}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"puyomarker{}":{
		"prefix":"puyomarker",
		"body":[
			"\\puyomarker{${1:marker string}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"puyogrid{}":{
		"prefix":"puyogrid",
		"body":[
			"\\puyogrid{${1:board string}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"puyogrid[]{}":{
		"prefix":"puyogrid",
		"body":[
			"\\puyogrid[${2:options%keyvals}]{${1:board string}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"puyocolor{}{}":{
		"prefix":"puyocolor",
		"body":[
			"\\puyocolor{${1:name}}{${2:r,g,b}}"
		],
		"luasnip": {"priority": 3000} 
