{


	"nextRow":{
		"prefix":"nextRow",
		"body":[
			"\\nextRow"
		]
	},


	"lb":{
		"prefix":"lb",
		"body":[
			"\\lb"
		]
	},


	"setMultiColRow":{
		"prefix":"setMultiColRow",
		"body":[
			"\\setMultiColRow{${1:col num}}{${2:row num}}{${3:max width%l}}{${4:min width%l}}{${5:hmode}}{${6:vmode}}{${7:left sep%l}}{${8:right sep%l}}{${9:text}}"
		]
	},


	"setMultiColumn":{
		"prefix":"setMultiColumn",
		"body":[
			"\\setMultiColumn{${1:col num}}{${2:max width%l}}{${3:min width%l}}{${4:hmode}}{${5:vmode}}{${6:left sep%l}}{${7:right sep%l}}{${8:text}}"
		]
	},


	"setMultiRow":{
		"prefix":"setMultiRow",
		"body":[
			"\\setMultiRow{${1:col num}}{${2:text}}"
		]
	},


	"setMultiRow":{
		"prefix":"setMultiRow",
		"body":[
			"\\setMultiRow{${1:col num}}[${3:min height%l}]{${2:text}}"
		]
	},


	"tableFormatedCell":{
		"prefix":"tableFormatedCell",
		"body":[
			"\\tableFormatedCell{${1:max width%l}}{${2:hmode}}{${3:vmode}}"
		]
	},


	"tableFormatedCell":{
		"prefix":"tableFormatedCell",
		"body":[
			"\\tableFormatedCell{${1:max width%l}}[${4:min width%l}]{${2:hmode}}{${3:vmode}}"
		]
	},


	"forceRowHeight":{
		"prefix":"forceRowHeight",
		"body":[
			"\\forceRowHeight{${1:height}}"
		]
	}
}
