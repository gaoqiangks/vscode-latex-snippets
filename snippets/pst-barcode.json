{


	"psbarcode":{
		"prefix":"psbarcode",
		"body":[
			"\\psbarcode{${1:text or filename%file}}{${2:PS options}}{${3:barcode type}}"
		]
	},


	"psbarcode":{
		"prefix":"psbarcode",
		"body":[
			"\\psbarcode[${4:options%keyvals}]{${1:text or filename%file}}{${2:PS options}}{${3:barcode type}}"
		]
	},


	"QR":{
		"prefix":"QR",
		"body":[
			"\\QR{${1:URL}}"
		]
	},


	"QR<label>":{
		"prefix":"QR<label>",
		"body":[
			"\\QR<label>{${1:URL}}"
		]
	},


	"QR":{
		"prefix":"QR",
		"body":[
			"\\QR[${2:scale}]{${1:URL}}"
		]
	},


	"QR":{
		"prefix":"QR",
		"body":[
			"\\QR[${2:scale}]<${3:label}>{${1:URL}}"
		]
	}
}
