{


	"DefineCurrency{}{}":{
		"prefix":"DefineCurrency",
		"body":[
			"\\DefineCurrency{${1:ISO}}{${2:maps%keyvals}}"
		]
	},


	"CurrencySetup{}":{
		"prefix":"CurrencySetup",
		"body":[
			"\\CurrencySetup{${1:maps%keyvals}}"
		]
	},


	"CurrencySetupAppend{}":{
		"prefix":"CurrencySetupAppend",
		"body":[
			"\\CurrencySetupAppend{${1:maps%keyvals}}"
		]
	},


	"currencyunit":{
		"prefix":"currencyunit",
		"body":[
			"\\currencyunit"
		]
	},


	"displayCurrency{}{}":{
		"prefix":"displayCurrency",
		"body":[
			"\\displayCurrency{${1:ISO}}{${2:value}}"
		]
	},


	"displayCurrencySymbol{}":{
		"prefix":"displayCurrencySymbol",
		"body":[
			"\\displayCurrencySymbol{${1:ISO}}"
		]
	},


	"SetPrecision":{
		"prefix":"SetPrecision",
		"body":[
			"\\SetPrecision"
		]
	},


	"displayCurrency{}[]{}":{
		"prefix":"displayCurrency",
		"body":[
			"\\displayCurrency{${1:ISO}}[${3:options}]{${2:value}}"
		]
	},


	"displayCurrencySymbol{}[]":{
		"prefix":"displayCurrencySymbol",
		"body":[
			"\\displayCurrencySymbol{${1:ISO}}[${2:options}]"
		]
	}
}
