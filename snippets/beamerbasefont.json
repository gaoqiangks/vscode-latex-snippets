{


	"setbeamerfont":{
		"prefix":"setbeamerfont",
		"body":[
			"\\setbeamerfont{${1:beamer-font name}}{${2:attributes%keyvals}}"
		]
	},


	"setbeamerfont*":{
		"prefix":"setbeamerfont*",
		"body":[
			"\\setbeamerfont*{${1:beamer-font name}}{${2:attributes%keyvals}}"
		]
	},


	"usebeamerfont":{
		"prefix":"usebeamerfont",
		"body":[
			"\\usebeamerfont{${1:beamer-font name}}"
		]
	},


	"usebeamerfont*":{
		"prefix":"usebeamerfont*",
		"body":[
			"\\usebeamerfont*{${1:beamer-font name}}"
		]
	},


	"Tiny":{
		"prefix":"Tiny",
		"body":[
			"\\Tiny"
		]
	},


	"TINY":{
		"prefix":"TINY",
		"body":[
			"\\TINY"
		]
	}
}
