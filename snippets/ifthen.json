	"equal":{
		"prefix":"equal",
		"body":[
			"\\equal{${1:string}}{${2:string}}"
		]
	},


	"AND":{
		"prefix":"AND",
		"body":[
			"\\AND"
		]
	},


	"OR":{
		"prefix":"OR",
