{


	"caption*":{
		"prefix":"caption*",
		"body":[
			"\\caption*{${1:heading%text}}"
		]
	},


	"captionof":{
		"prefix":"captionof",
		"body":[
			"\\captionof{${1:float type}}[${3:list entry}]{${2:heading%text}}"
		]
	},


	"captionof":{
		"prefix":"captionof",
		"body":[
			"\\captionof{${1:float type}}{${2:heading%text}}"
		]
	},


	"captionof*":{
		"prefix":"captionof*",
		"body":[
			"\\captionof*{${1:float type}}{${2:heading%text}}"
		]
	},


	"setcaptiontype":{
		"prefix":"setcaptiontype",
		"body":[
			"\\setcaptiontype{${1:float type}}"
		]
	},


	"setcaptiontype*":{
		"prefix":"setcaptiontype*",
		"body":[
			"\\setcaptiontype*{${1:float type}}"
		]
	}
}
