{


	"caption*{}":{
		"prefix":"*caption",
		"body":[
			"\\caption*{${1:heading%text}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"captionof{}[]{}":{
		"prefix":"captionof",
		"body":[
			"\\captionof{${1:float type}}[${3:list entry}]{${2:heading%text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"captionof{}{}":{
		"prefix":"captionof",
		"body":[
			"\\captionof{${1:float type}}{${2:heading%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"captionof*{}{}":{
		"prefix":"*captionof",
		"body":[
			"\\captionof*{${1:float type}}{${2:heading%text}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"setcaptiontype{}":{
		"prefix":"setcaptiontype",
		"body":[
			"\\setcaptiontype{${1:float type}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"setcaptiontype*{}":{
		"prefix":"*setcaptiontype",
		"body":[
			"\\setcaptiontype*{${1:float type}}"
		],
		"luasnip": {"priority": 2000} 
	}
}
