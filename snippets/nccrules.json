{


	"dashrule":{
		"prefix":"dashrule",
		"body":[
			"\\dashrule{${1:h-pattern}}{${2:v-pattern}}"
		]
	},


	"dashrule":{
		"prefix":"dashrule",
		"body":[
			"\\dashrule[${3:raise%l}]{${1:h-pattern}}{${2:v-pattern}}"
		]
	},


	"dashrulefill":{
		"prefix":"dashrulefill",
		"body":[
			"\\dashrulefill{${1:h-pattern}}{${2:v-pattern}}"
		]
	},


	"dashrulefill":{
		"prefix":"dashrulefill",
		"body":[
			"\\dashrulefill[${3:raise%l}]{${1:h-pattern}}{${2:v-pattern}}"
		]
	},


	"newfootnoterule":{
		"prefix":"newfootnoterule",
		"body":[
			"\\newfootnoterule{${1:prefix}}{${2:rule code}}"
		]
	},


	"newfootnoterule":{
		"prefix":"newfootnoterule",
		"body":[
			"\\newfootnoterule{${1:prefix}}[${3:width}]{${2:rule code}}"
		]
	},


	"newfootnotedashrule":{
		"prefix":"newfootnotedashrule",
		"body":[
			"\\newfootnotedashrule{${1:prefix}}{${2:h-pattern}}{${3:v-pattern}}"
		]
	},


	"newfootnotedashrule":{
		"prefix":"newfootnotedashrule",
		"body":[
			"\\newfootnotedashrule{${1:prefix}}[${4:width}]{${2:h-pattern}}{${3:v-pattern}}"
		]
	}
}
