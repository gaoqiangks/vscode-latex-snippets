{


	"coverimage{}":{
		"prefix":"coverimage",
		"body":[
			"\\coverimage{${1:imagefile}}"
		]
	},


	"epubpage":{
		"prefix":"epubpage",
		"body":[
			"\\epubpage"
		]
	},


	"DeclareLanguageEbook{}{}":{
		"prefix":"DeclareLanguageEbook",
		"body":[
			"\\DeclareLanguageEbook{${1:lang name}}{${2:lang code}}"
		]
	},


	"GetLanguage":{
		"prefix":"GetLanguage",
		"body":[
			"\\GetLanguage"
		]
	},


	"coverimage[]{}":{
		"prefix":"coverimage",
		"body":[
			"\\coverimage[${2:options%keyvals}]{${1:imagefile}}"
		]
	}
}
