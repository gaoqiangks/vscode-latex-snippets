{


	"boolexpr":{
		"prefix":"boolexpr",
		"body":[
			"\\boolexpr{${1:expression}}"
		]
	},


	"AND":{
		"prefix":"AND",
		"body":[
			"\\AND"
		]
	},


	"OR":{
		"prefix":"OR",
		"body":[
			"\\OR"
		]
	},


	"ifswitch":{
		"prefix":"ifswitch",
		"body":[
			"\\ifswitch"
		]
	},


	"ifboolexpr":{
		"prefix":"ifboolexpr",
		"body":[
			"\\ifboolexpr{${1:expression}}{${2:true}}{${3:false}}"
		]
	},


	"switch":{
		"prefix":"switch",
		"body":[
			"\\switch"
		]
	},


	"case":{
		"prefix":"case",
		"body":[
			"\\case{${1:expression}}"
		]
	},


	"otherwise":{
		"prefix":"otherwise",
		"body":[
			"\\otherwise"
		]
	},


	"endswitch":{
		"prefix":"endswitch",
		"body":[
			"\\endswitch"
		]
	}
}
