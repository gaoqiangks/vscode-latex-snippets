{


	"listfiles[]":{
		"prefix":"listfiles",
		"body":[
			"\\listfiles[${1:number}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"MaxLengthEmptyList{}":{
		"prefix":"MaxLengthEmptyList",
		"body":[
			"\\MaxLengthEmptyList{${1:⟨digit⟩⟨digit⟩}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"MaxLengthEmptyList{}[]":{
		"prefix":"MaxLengthEmptyList",
		"body":[
			"\\MaxLengthEmptyList{${1:⟨digit⟩⟨digit⟩}}[${2:read-again-files}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"SetLongNameFileListChars{}":{
		"prefix":"SetLongNameFileListChars",
		"body":[
			"\\SetLongNameFileListChars{${1:number}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
