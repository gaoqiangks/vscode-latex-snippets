{


	"polyset{}":{
		"prefix":"polyset",
		"body":[
			"\\polyset{${1:options%keyvals}}"
		]
	},


	"polylongdiv{}{}":{
		"prefix":"polylongdiv",
		"body":[
			"\\polylongdiv{${1:polynom1%formula}}{${2:polynom2%formula}}"
		]
	},


	"polyhornerscheme{}":{
		"prefix":"polyhornerscheme",
