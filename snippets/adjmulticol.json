	"adjmulticols{}{}{}":{
		"prefix":"adjmulticols",
		"body":[
			"\\begin{adjmulticols}",
				"\t{${1:number}}{${2:inner margin%l}}{${3:outer margin%l}}",
			"\\end{adjmulticols}"
		],
		"luasnip": {"priority": 3000} 
	},


	"adjmulticols*{}{}{}":{
		"prefix":"*adjmulticols",
