{


	"psblurbox{}":{
		"prefix":"psblurbox",
		"body":[
			"\\psblurbox{${1:stuff}}"
		]
	},


	"ifpsblur":{
		"prefix":"ifpsblur",
		"body":[
			"\\ifpsblur"
		]
	},


	"psblurtrue":{
		"prefix":"psblurtrue",
		"body":[
			"\\psblurtrue"
		]
	},


	"psblurfalse":{
		"prefix":"psblurfalse",
		"body":[
			"\\psblurfalse"
		]
	},


	"psblurbox[]{}":{
		"prefix":"psblurbox",
		"body":[
			"\\psblurbox[${2:options%keyvals}]{${1:stuff}}"
		]
	}
}
