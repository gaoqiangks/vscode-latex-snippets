			"\\NameTheorem{${1:envname}}{${2:keyvals}}"
		]
	},


	"CreateTheorem{}{}":{
		"prefix":"CreateTheorem",
		"body":[
			"\\CreateTheorem{${1:envname}}{${2:keyvals}}"
		]
	},


