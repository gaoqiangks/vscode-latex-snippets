{


	"edtable{}{}":{
		"prefix":"edtable",
		"body":[
			"\\begin{edtable}",
				"\t{${1:table envname}}{${2:preamble}}",
			"\\end{edtable}"
		],
		"luasnip": {"priority": 3000} 
	},


	"edtable[]{}{}":{
		"prefix":"edtable",
		"body":[
			"\\begin{edtable}",
				"\t[$ or $$%plain]{${1:array envname}}{${2:preamble}}",
			"\\end{edtable}"
		],
		"luasnip": {"priority": 1000} 
	}
}
