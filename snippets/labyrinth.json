{


	"h":{
		"prefix":"h",
		"body":[
			"\\h"
		]
	},


	"labyrinthset{}":{
		"prefix":"labyrinthset",
		"body":[
			"\\labyrinthset{${1:options%keyvals}}"
		]
	},


	"putsymbol(){}":{
		"prefix":"putsymbol",
		"body":[
			"\\putsymbol(${2:x},${3:y}){${1:symbol}}"
		]
	},


	"plus":{
		"prefix":"plus",
		"body":[
			"\\plus"
		]
	},


	"minus":{
		"prefix":"minus",
		"body":[
			"\\minus"
		]
	},


	"ast":{
		"prefix":"ast",
		"body":[
			"\\ast"
		]
	},


	"labyrinthsolution(){}":{
		"prefix":"labyrinthsolution",
		"body":[
			"\\labyrinthsolution(${2:x},${3:y}){${1:solution code}}"
		]
	},


	"solutionset{}":{
		"prefix":"solutionset",
		"body":[
			"\\solutionset{${1:options%keyvals}}"
		]
	},


	"autosolution()(){}":{
		"prefix":"autosolution",
		"body":[
			"\\autosolution(${2:x1},${3:y1})(${4:x2},${5:y2}){${1:first direction}}"
		]
	},


	"solutionpath":{
		"prefix":"solutionpath",
		"body":[
			"\\solutionpath"
		]
	},


	"truncdiv{}{}":{
		"prefix":"truncdiv",
		"body":[
			"\\truncdiv{${1:num1}}{${2:num2}}"
		]
	},


	"labyrinthsolution[](){}":{
		"prefix":"labyrinthsolution",
		"body":[
			"\\labyrinthsolution[${2:options%keyvals}](${3:x},${4:y}){${1:solution code}}"
		]
	},


	"autosolution[]()(){}":{
		"prefix":"autosolution",
		"body":[
			"\\autosolution[${2:options%keyvals}](${3:x1},${4:y1})(${5:x2},${6:y2}){${1:first direction}}"
		]
	},


	"labyrinth":{
		"prefix":"labyrinth",
		"body":[
			"\\begin{labyrinth}",
				"\t$1",
			"\\end{labyrinth}"
		]
	},


	"labyrinth":{
		"prefix":"labyrinth",
		"body":[
			"\\begin{labyrinth}",
				"\t$1",
			"\\end{labyrinth}"
		]
	}
}
