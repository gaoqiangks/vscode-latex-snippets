{


	"tikzmark":{
		"prefix":"tikzmark",
		"body":[
			"\\tikzmark{${1:name}}"
		]
	},


	"tikzmark":{
		"prefix":"tikzmark",
		"body":[
			"\\tikzmark[${2:drawing command}]{${1:name}}"
		]
	},


	"pgfmark":{
		"prefix":"pgfmark",
		"body":[
			"\\pgfmark{${1:name}}"
		]
	},


	"iftikzmark":{
		"prefix":"iftikzmark",
		"body":[
			"\\iftikzmark{${1:name}}{${2:true}}{${3:false}}"
		]
	},


	"iftikzmarkexists":{
		"prefix":"iftikzmarkexists",
		"body":[
			"\\iftikzmarkexists{${1:name}}"
		]
	},


	"iftikzmarkoncurrentpage":{
		"prefix":"iftikzmarkoncurrentpage",
		"body":[
			"\\iftikzmarkoncurrentpage{${1:name}}"
		]
	},


	"iftikzmarkonpage":{
		"prefix":"iftikzmarkonpage",
		"body":[
			"\\iftikzmarkonpage{${1:name}}{${2:page}}"
		]
	},


	"tikzmarknode":{
		"prefix":"tikzmarknode",
		"body":[
			"\\tikzmarknode{${1:name}}{${2:contents}}"
		]
	},


	"tikzmarknode":{
		"prefix":"tikzmarknode",
		"body":[
			"\\tikzmarknode[${3:options%keyvals}]{${1:name}}{${2:contents}}"
		]
	},


	"subnode":{
		"prefix":"subnode",
		"body":[
			"\\subnode{${1:name}}{${2:content}}"
		]
	},


	"subnode":{
		"prefix":"subnode",
		"body":[
			"\\subnode[${3:options%keyvals}]{${1:name}}{${2:content}}"
		]
	},


	"SaveNode":{
		"prefix":"SaveNode",
		"body":[
			"\\SaveNode{${1:name}}"
		]
	},


	"SaveNode":{
		"prefix":"SaveNode",
		"body":[
			"\\SaveNode[${2:group name}]{${1:name}}"
		]
	},


	"usetikzmarklibrary":{
		"prefix":"usetikzmarklibrary",
		"body":[
			"\\usetikzmarklibrary{${1:library%keyvals}}"
		]
	},


	"StartHighlighting":{
		"prefix":"StartHighlighting",
		"body":[
			"\\StartHighlighting"
		]
	},


	"StartHighlighting":{
		"prefix":"StartHighlighting",
		"body":[
			"\\StartHighlighting[${1:options%keyvals}]"
		]
	},


	"StopHighlighting":{
		"prefix":"StopHighlighting",
		"body":[
			"\\StopHighlighting"
		]
	},


	"Highlight":{
		"prefix":"Highlight",
		"body":[
			"\\Highlight{${1:text}}"
		]
	},


	"Highlight":{
		"prefix":"Highlight",
		"body":[
			"\\Highlight[${2:options%keyvals}]{${1:text}}"
		]
	},


	"tikzmarkmath":{
		"prefix":"tikzmarkmath",
		"body":[
			"\\begin{tikzmarkmath}",
			"\t$1",
			"\\end{tikzmarkmath}"
		]
	}
}
