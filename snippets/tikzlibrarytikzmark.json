		],
		"luasnip": {"priority": 3000} 
	},


	"StartHighlighting":{
		"prefix":"StartHighlighting",
		"body":[
			"\\StartHighlighting"
		],
		"luasnip": {"priority": 3000} 
	},


	"StartHighlighting[]":{
		"prefix":"StartHighlighting",
		"body":[
			"\\StartHighlighting[${1:options%keyvals}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"StopHighlighting":{
		"prefix":"StopHighlighting",
		"body":[
			"\\StopHighlighting"
		],
		"luasnip": {"priority": 3000} 
	},


	"Highlight{}":{
		"prefix":"Highlight",
		"body":[
			"\\Highlight{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"Highlight[]{}":{
		"prefix":"Highlight",
		"body":[
			"\\Highlight[${2:options%keyvals}]{${1:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"tikzmarkmath":{
		"prefix":"tikzmarkmath",
		"body":[
			"\\begin{tikzmarkmath}",
				"\t$1",
			"\\end{tikzmarkmath}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tikzmarkmath[]":{
		"prefix":"tikzmarkmath",
		"body":[
			"\\begin{tikzmarkmath}",
				"\t[${1:prefix}]",
			"\\end{tikzmarkmath}"
		],
		"luasnip": {"priority": 1000} 
	}
}
