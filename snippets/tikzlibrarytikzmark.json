{


	"tikzmark{}":{
		"prefix":"tikzmark",
		"body":[
			"\\tikzmark{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tikzmark[]{}":{
		"prefix":"tikzmark",
		"body":[
			"\\tikzmark[${2:drawing command}]{${1:name}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"pgfmark{}":{
		"prefix":"pgfmark",
		"body":[
			"\\pgfmark{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"iftikzmark{}{}{}":{
		"prefix":"iftikzmark",
		"body":[
			"\\iftikzmark{${1:name}}{${2:true}}{${3:false}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"iftikzmarkexists{}":{
		"prefix":"iftikzmarkexists",
		"body":[
			"\\iftikzmarkexists{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	},


