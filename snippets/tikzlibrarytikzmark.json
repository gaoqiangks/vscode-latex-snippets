	"iftikzmarkonpage{}{}":{
		"prefix":"iftikzmarkonpage",
		"body":[
			"\\iftikzmarkonpage{${1:name}}{${2:page}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tikzmarknode{}{}":{
		"prefix":"tikzmarknode",
		"body":[
			"\\tikzmarknode{${1:name}}{${2:contents}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tikzmarknode[]{}{}":{
		"prefix":"tikzmarknode",
		"body":[
			"\\tikzmarknode[${3:options%keyvals}]{${1:name}}{${2:contents}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"subnode{}{}":{
		"prefix":"subnode",
		"body":[
			"\\subnode{${1:name}}{${2:content}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"subnode[]{}{}":{
		"prefix":"subnode",
		"body":[
			"\\subnode[${3:options%keyvals}]{${1:name}}{${2:content}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"SaveNode{}":{
		"prefix":"SaveNode",
		"body":[
			"\\SaveNode{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"SaveNode[]{}":{
		"prefix":"SaveNode",
		"body":[
			"\\SaveNode[${2:group name}]{${1:name}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"usetikzmarklibrary{}":{
		"prefix":"usetikzmarklibrary",
		"body":[
