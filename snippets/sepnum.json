{


	"sepnum{}{}{}{}":{
		"prefix":"sepnum",
		"body":[
			"\\sepnum{${1:symbol}}{${2:whole-part-sep}}{${3:frac-part-sep}}{${4:number}}"
		]
	},


	"sepnumform{}{}{}{}":{
		"prefix":"sepnumform",
		"body":[
			"\\sepnumform{${1:symbol}}{${2:whole-part-sep}}{${3:frac-part-sep}}{${4:counter}}"
		]
	},


	"printnum{}":{
		"prefix":"printnum",
		"body":[
			"\\printnum{${1:number}}"
		]
	},


	"printnumKomma":{
		"prefix":"printnumKomma",
		"body":[
			"\\printnumKomma"
		]
	},


	"printnumTrenner":{
		"prefix":"printnumTrenner",
		"body":[
			"\\printnumTrenner"
		]
	}
}
