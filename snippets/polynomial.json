{


	"polynomial{}":{
		"prefix":"polynomial",
		"body":[
			"\\polynomial{${1:coeff1,coeff2,...}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"polynomial[]{}":{
		"prefix":"polynomial",
		"body":[
			"\\polynomial[${2:options%keyvals}]{${1:coeff1,coeff2,...}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"polynomialfrac{}{}":{
		"prefix":"polynomialfrac",
		"body":[
			"\\polynomialfrac{${1:numerator coeffs}}{${2:denominator coeffs}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"polynomialfrac[]{}{}":{
		"prefix":"polynomialfrac",
		"body":[
			"\\polynomialfrac[${3:options%keyvals}]{${1:numerator coeffs}}{${2:denominator coeffs}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"polynomialstyle{}":{
		"prefix":"polynomialstyle",
		"body":[
			"\\polynomialstyle{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
