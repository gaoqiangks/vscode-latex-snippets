{


	"hasse{}{}":{
		"prefix":"hasse",
		"body":[
			"\\hasse{${1:letter}}{${2:rank}}"
		]
	},


	"hasseDiagrams{}":{
		"prefix":"hasseDiagrams",
		"body":[
			"\\hasseDiagrams{${1:type1;type2;...}}"
		]
	},


	"rootSystemHeight{}":{
		"prefix":"rootSystemHeight",
		"body":[
			"\\rootSystemHeight{${1:count register}}"
		]
	},


	"rootSystemWidthAtGrade{}{}":{
		"prefix":"rootSystemWidthAtGrade",
		"body":[
			"\\rootSystemWidthAtGrade{${1:grade}}{${2:count register}}"
		]
	},


	"rootSum{}{}{}":{
		"prefix":"rootSum",
		"body":[
			"\\rootSum{${1:first-pos}}{${2:second-pos}}{${3:cmd}}"
		]
	},


	"drawRootAsDynkinSum{}{}":{
		"prefix":"drawRootAsDynkinSum",
		"body":[
			"\\drawRootAsDynkinSum{${1:arg1}}{${2:arg2}}"
		]
	},


	"edgeQuote{}":{
		"prefix":"edgeQuote",
		"body":[
			"\\edgeQuote{${1:arg}}"
		]
	},


	"forAllPositiveRoots{}":{
		"prefix":"forAllPositiveRoots",
		"body":[
			"\\forAllPositiveRoots{${1:command}}"
		]
	},


	"hasse[]{}{}":{
		"prefix":"hasse",
		"body":[
			"\\hasse[${3:options}]{${1:letter}}{${2:rank}}"
		]
	},


	"rootSystemHeight[][]{}":{
		"prefix":"rootSystemHeight",
		"body":[
			"\\rootSystemHeight[${2:letter}][${3:rank}]{${1:count register}}"
		]
	},


	"rootSystemWidthAtGrade[]{}{}":{
		"prefix":"rootSystemWidthAtGrade",
		"body":[
			"\\rootSystemWidthAtGrade[${3:letter}]{${1:grade}}{${2:count register}}"
		]
	},


	"rootSystemWidthAtGrade[][]{}{}":{
		"prefix":"rootSystemWidthAtGrade",
		"body":[
			"\\rootSystemWidthAtGrade[${3:letter}][${4:rank}]{${1:grade}}{${2:count register}}"
		]
	},


	"rootSum[]{}{}{}":{
		"prefix":"rootSum",
		"body":[
			"\\rootSum[${4:letter}]{${1:first-pos}}{${2:second-pos}}{${3:cmd}}"
		]
	},


	"rootSum[][]{}{}{}":{
		"prefix":"rootSum",
		"body":[
			"\\rootSum[${4:letter}][${5:rank}]{${1:first-pos}}{${2:second-pos}}{${3:cmd}}"
		]
	}
}
