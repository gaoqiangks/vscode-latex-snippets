{


	"readgraph{}":{
		"prefix":"readgraph",
		"body":[
			"\\readgraph{${1:graph specs}}"
		]
	},


	"dijkstra{}{}":{
		"prefix":"dijkstra",
		"body":[
			"\\dijkstra{${1:vertex1}}{${2:vertex2}}"
		]
	},


	"setdijk{}":{
		"prefix":"setdijk",
		"body":[
			"\\setdijk{${1:options%keyvals}}"
		]
	},


	"setdijkdefault{}":{
		"prefix":"setdijkdefault",
		"body":[
			"\\setdijkdefault{${1:options%keyvals}}"
		]
	},


	"dijkdist":{
		"prefix":"dijkdist",
		"body":[
			"\\dijkdist"
		]
	},


	"dijkpath":{
		"prefix":"dijkpath",
		"body":[
			"\\dijkpath"
		]
	},


	"initdijk":{
		"prefix":"initdijk",
		"body":[
			"\\initdijk"
		]
	},


	"formatnodewithprev{}{}":{
		"prefix":"formatnodewithprev",
		"body":[
			"\\formatnodewithprev{${1:arg1}}{${2:arg2}}"
		]
	},


	"highlightfirstnode{}":{
		"prefix":"highlightfirstnode",
		"body":[
			"\\highlightfirstnode{${1:arg}}"
		]
	},


	"highlightnode{}{}":{
		"prefix":"highlightnode",
		"body":[
			"\\highlightnode{${1:arg1}}{${2:arg2}}"
		]
	},


	"readgraph*{}":{
		"prefix":"*readgraph",
		"body":[
			"\\readgraph*{${1:graph specs}}"
		]
	},


	"dijkstra[]{}{}":{
		"prefix":"dijkstra",
		"body":[
			"\\dijkstra[${3:options%keyvals}]{${1:vertex1}}{${2:vertex2}}"
		]
	}
}
