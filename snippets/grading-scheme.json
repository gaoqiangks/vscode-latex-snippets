{


	"entry":{
		"prefix":"entry",
		"body":[
			"\\entry{${1:description%text}}{${2:points}}"
		]
	},


	"block":{
		"prefix":"block",
		"body":[
			"\\begin{block}",
			"\t$1",
			"\\end{block}"
		]
	},


	"gradingscheme":{
		"prefix":"gradingscheme",
		"body":[
			"\\begin{gradingscheme}",
			"\t$1",
			"\\end{gradingscheme}"
		]
	}
}
