{


	"entry{}{}":{
		"prefix":"entry",
		"body":[
			"\\entry{${1:description%text}}{${2:points}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"gradingscheme{}":{
		"prefix":"gradingscheme",
		"body":[
			"\\begin{gradingscheme}",
				"\t{${1:operator}}",
			"\\end{gradingscheme}"
		],
		"luasnip": {"priority": 3000} 
	},


	"gradingscheme[]{}":{
		"prefix":"gradingscheme",
		"body":[
			"\\begin{gradingscheme}",
				"\t[${2:description%text}]{${1:operator}}",
			"\\end{gradingscheme}"
		],
		"luasnip": {"priority": 1000} 
	},


	"block{}":{
		"prefix":"block",
		"body":[
			"\\begin{block}",
				"\t{${1:operator}}",
			"\\end{block}"
		],
		"luasnip": {"priority": 3000} 
	},


	"block[]{}":{
		"prefix":"block",
		"body":[
			"\\begin{block}",
				"\t[${2:description%text}]{${1:operator}}",
			"\\end{block}"
		],
		"luasnip": {"priority": 1000} 
	}
}
