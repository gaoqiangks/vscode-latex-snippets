{


	"NewMultilangCmd{}{}":{
		"prefix":"NewMultilangCmd",
		"body":[
			"\\NewMultilangCmd{${1:command}}{${2:options%keyvals}}"
		]
	},


	"NewMultilangEnv{}{}":{
		"prefix":"NewMultilangEnv",
		"body":[
			"\\NewMultilangEnv{${1:envname}}{${2:options%keyvals}}"
		]
	},


	"NewMultilangType{}{}":{
		"prefix":"NewMultilangType",
		"body":[
			"\\NewMultilangType{${1:type name}}{${2:format%definition}}"
		]
	},


	"NewMultilangType[]{}{}":{
		"prefix":"NewMultilangType",
		"body":[
			"\\NewMultilangType[${3:arg count}]{${1:type name}}{${2:format%definition}}"
		]
	}
}
