{


	"chordbox{}{}":{
		"prefix":"chordbox",
		"body":[
			"\\chordbox{${1:chord-name%formula}}{${2:fret-positions}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"chordbox[]{}{}":{
		"prefix":"chordbox",
		"body":[
			"\\chordbox[${3:base-fret}]{${1:chord-name%formula}}{${2:fret-positions}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"bchordbox{}{}{}":{
		"prefix":"bchordbox",
		"body":[
			"\\bchordbox{${1:chord-name%formula}}{${2:fret-positions}}{${3:barre-frets}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"bchordbox[]{}{}{}":{
		"prefix":"bchordbox",
		"body":[
			"\\bchordbox[${4:base-fret}]{${1:chord-name%formula}}{${2:fret-positions}}{${3:barre-frets}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"chordboxenv{}{}{}":{
		"prefix":"chordboxenv",
		"body":[
			"\\begin{chordboxenv}",
				"\t{${1:basefret}}{${2:chord-name%formula}}{${3:fret-positions}}",
			"\\end{chordboxenv}"
		],
		"luasnip": {"priority": 3000} 
	}
}
