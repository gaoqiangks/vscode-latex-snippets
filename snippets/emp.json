{


	"empuse{}":{
		"prefix":"empuse",
		"body":[
			"\\empuse{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"empTeX{}":{
		"prefix":"empTeX",
		"body":[
			"\\empTeX{${1:LaTeX prelude%definition}}"
		],
		"luasnip": {"priority": 3000} 
	},


