	"empfile":{
		"prefix":"empfile",
		"body":[
			"\\begin{empfile}",
				"\t$1",
			"\\end{empfile}"
		],
		"luasnip": {"priority": 3000} 
	},


	"empfile[]":{
		"prefix":"empfile",
		"body":[
			"\\begin{empfile}",
				"\t[${1:METAPOST file%file}]",
			"\\end{empfile}"
		],
		"luasnip": {"priority": 1000} 
	},


	"emp()":{
		"prefix":"emp",
		"body":[
			"\\begin{emp}",
				"\t(${1:width},${2:height})",
			"\\end{emp}"
		],
		"luasnip": {"priority": 3000} 
	},


	"emp[]()":{
		"prefix":"emp",
		"body":[
			"\\begin{emp}",
				"\t[${1:name}](${2:width},${3:height})",
			"\\end{emp}"
		],
		"luasnip": {"priority": 1000} 
	},


