{


	"liftarm{}{}{}":{
		"prefix":"liftarm",
		"body":[
			"\\liftarm{${1:point}}{${2:length%plain}}{${3:angle}}"
		]
	},


	"liftarmconstruct{}":{
		"prefix":"liftarmconstruct",
		"body":[
			"\\liftarmconstruct{${1:commands}}"
		]
	},


	"liftarmconstructclear":{
		"prefix":"liftarmconstructclear",
		"body":[
			"\\liftarmconstructclear"
		]
	},


	"liftarmanimate{}{}{}":{
		"prefix":"liftarmanimate",
		"body":[
			"\\liftarmanimate{${1:frame rate}}{${2:list}}{${3:commands}}"
		]
	},


	"liftarm[]{}{}{}":{
		"prefix":"liftarm",
		"body":[
			"\\liftarm[${4:options%keyvals}]{${1:point}}{${2:length%plain}}{${3:angle}}"
		]
	},


	"liftarmanimate[]{}{}{}":{
		"prefix":"liftarmanimate",
		"body":[
			"\\liftarmanimate[${4:options%keyvals}]{${1:frame rate}}{${2:list}}{${3:commands}}"
		]
	},


	"liftarmconnect":{
		"prefix":"liftarmconnect",
		"body":[
			"\\begin{liftarmconnect}",
				"\t$1",
			"\\end{liftarmconnect}"
		]
	},


	"liftarmconnect":{
		"prefix":"liftarmconnect",
		"body":[
			"\\begin{liftarmconnect}",
				"\t$1",
			"\\end{liftarmconnect}"
		]
	}
}
