			"\\theTSrunsilent"
		]
	},


	"lstinlinesafe":{
		"prefix":"lstinlinesafe",
		"body":[
			"\\lstinlinesafe|${1:code}|"
		]
	},


	"eval":{
		"prefix":"eval",
		"body":[
			"\\eval{${1:code%definition}}"
		]
	},


	"eval":{
		"prefix":"eval",
		"body":[
			"\\eval[${2:language}]{${1:code%definition}}"
		]
	},


	"sage":{
		"prefix":"sage",
		"body":[
			"\\sage{${1:code%definition}}"
		]
	},


