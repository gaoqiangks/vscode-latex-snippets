{


	"theTSeval":{
		"prefix":"theTSeval",
		"body":[
			"\\theTSeval"
		]
	},


	"theTSrun":{
		"prefix":"theTSrun",
		"body":[
			"\\theTSrun"
		]
	},


	"theTSrunsilent":{
