{


	"doibanner{}":{
		"prefix":"doibanner",
		"body":[
			"\\doibanner{${1:DOI}}"
		]
	},


	"doiurl{}":{
		"prefix":"doiurl",
		"body":[
			"\\doiurl{${1:DOI}}"
		]
	},


	"doibanner*{}":{
		"prefix":"*doibanner",
		"body":[
			"\\doibanner*{${1:DOI}}"
		]
	},


	"doibanner*[]{}":{
		"prefix":"*doibanner",
		"body":[
			"\\doibanner*[${2:options}]{${1:DOI}}"
		]
	},


	"doiurl*{}":{
		"prefix":"*doiurl",
		"body":[
			"\\doiurl*{${1:DOI}}"
		]
	},


	"doibanner[]{}":{
		"prefix":"doibanner",
		"body":[
			"\\doibanner[${2:options}]{${1:DOI}}"
		]
	}
}
