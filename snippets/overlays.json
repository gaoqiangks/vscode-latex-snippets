{


	"alert{}{}":{
		"prefix":"alert",
		"body":[
			"\\alert{${1:overlay specification}}{${2:content%text}}"
		]
	},


	"visible{}{}":{
		"prefix":"visible",
		"body":[
			"\\visible{${1:overlay specification}}{${2:content%text}}"
		]
	},


	"only{}{}":{
		"prefix":"only",
		"body":[
			"\\only{${1:overlay specification}}{${2:content%text}}"
		]
	},


	"savecounterbetweenoverlays{}":{
		"prefix":"savecounterbetweenoverlays",
		"body":[
			"\\savecounterbetweenoverlays{${1:counter}}"
		]
	},


	"saveseriesbetweenoverlays{}":{
		"prefix":"saveseriesbetweenoverlays",
		"body":[
			"\\saveseriesbetweenoverlays{${1:series name}}"
		]
	},


	"overlaysoff":{
		"prefix":"overlaysoff",
		"body":[
			"\\overlaysoff"
		]
	},


	"alertsoff":{
		"prefix":"alertsoff",
		"body":[
			"\\alertsoff"
		]
	},


	"psalert{}{}":{
		"prefix":"psalert",
		"body":[
			"\\psalert{${1:overlay specification}}{${2:content%text}}"
		]
	},


	"psvisible{}{}":{
		"prefix":"psvisible",
		"body":[
			"\\psvisible{${1:overlay specification}}{${2:content%text}}"
		]
	},


	"overlays":{
		"prefix":"overlays",
		"body":[
			"\\begin{overlays}",
				"\t$1",
			"\\end{overlays}"
		]
	},


	"fragileoverlays":{
		"prefix":"fragileoverlays",
		"body":[
			"\\begin{fragileoverlays}",
				"\t$1",
			"\\end{fragileoverlays}"
		]
	}
}
