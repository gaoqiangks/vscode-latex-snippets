{


	"cooltooltip{}{}{}{}{}":{
		"prefix":"cooltooltip",
		"body":[
			"\\cooltooltip{${1:subject%text}}{${2:message%text}}{${3:URL}}{${4:tooltip}}{${5:text}}"
		]
	},


	"cooltooltiptoggle{}":{
		"prefix":"cooltooltiptoggle",
		"body":[
			"\\cooltooltiptoggle{${1:text}}"
		]
	},


	"ifcoolpdf":{
		"prefix":"ifcoolpdf",
		"body":[
			"\\ifcoolpdf"
		]
	},


	"coolpdftrue":{
		"prefix":"coolpdftrue",
		"body":[
			"\\coolpdftrue"
		]
	},


	"coolpdffalse":{
		"prefix":"coolpdffalse",
		"body":[
			"\\coolpdffalse"
		]
	},


	"cooltooltip[]{}{}{}{}{}":{
		"prefix":"cooltooltip",
		"body":[
			"\\cooltooltip[${6:popup color}]{${1:subject%text}}{${2:message%text}}{${3:URL}}{${4:tooltip}}{${5:text}}"
		]
	},


	"cooltooltip[][]{}{}{}{}{}":{
		"prefix":"cooltooltip",
		"body":[
			"\\cooltooltip[${6:popup color}][${7:link color}]{${1:subject%text}}{${2:message%text}}{${3:URL}}{${4:tooltip}}{${5:text}}"
		]
	}
}
