{


	"extrafootnoterule":{
		"prefix":"extrafootnoterule",
		"body":[
			"\\extrafootnoterule"
		]
	},


	"defaultfootnoterule":{
		"prefix":"defaultfootnoterule",
		"body":[
			"\\defaultfootnoterule"
		]
	},


	"newfootnote{}":{
		"prefix":"newfootnote",
		"body":[
			"\\newfootnote{${1:suffix}}"
		]
	},


	"DeclareNewFootnote{}":{
		"prefix":"DeclareNewFootnote",
		"body":[
			"\\DeclareNewFootnote{${1:suffix}}"
		]
	},


	"SelectFootnoteRule{}":{
		"prefix":"SelectFootnoteRule",
		"body":[
			"\\SelectFootnoteRule{${1:rule-name}}"
		]
	},


	"footnoterulepriority":{
		"prefix":"footnoterulepriority",
		"body":[
			"\\footnoterulepriority"
		]
	},


	"SetFootnoteHook{}":{
		"prefix":"SetFootnoteHook",
		"body":[
			"\\SetFootnoteHook{${1:code}}"
		]
	},


	"SplitNote":{
		"prefix":"SplitNote",
		"body":[
			"\\SplitNote"
		]
	},


	"ExtraParaSkip{}":{
		"prefix":"ExtraParaSkip",
		"body":[
			"\\ExtraParaSkip{${1:space%l}}"
		]
	},


	"newfootnote[]{}":{
		"prefix":"newfootnote",
		"body":[
			"\\newfootnote[${2:style}]{${1:suffix}}"
		]
	},


	"DeclareNewFootnote[]{}[]":{
		"prefix":"DeclareNewFootnote",
		"body":[
			"\\DeclareNewFootnote[${2:footnote-style}]{${1:suffix}}[${3:enumeration-style}]"
		]
	},


	"SelectFootnoteRule[]{}[]":{
		"prefix":"SelectFootnoteRule",
		"body":[
			"\\SelectFootnoteRule[${2:priority}]{${1:rule-name}}[${3:action}]"
		]
	}
}
