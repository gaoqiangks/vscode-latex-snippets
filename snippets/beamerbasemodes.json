{


	"mode<mode specification>{}":{
		"prefix":"mode<mode specification>",
		"body":[
			"\\mode<mode specification>{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"mode{}":{
		"prefix":"mode",
		"body":[
			"\\mode{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"mode<mode specification>":{
		"prefix":"mode<mode specification>",
		"body":[
			"\\mode<mode specification>"
		],
		"luasnip": {"priority": 3000} 
	},


	"mode*":{
		"prefix":"*mode",
		"body":[
			"\\mode*"
		],
		"luasnip": {"priority": 2000} 
	},


	"includeslide{}":{
		"prefix":"includeslide",
		"body":[
			"\\includeslide{${1:label name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"includeslide[]{}":{
		"prefix":"includeslide",
		"body":[
			"\\includeslide[${2:options%keyvals}]{${1:label name}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"setjobnamebeamerversion{}":{
		"prefix":"setjobnamebeamerversion",
		"body":[
			"\\setjobnamebeamerversion{${1:filename without extension%file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"thebeamerpauses":{
		"prefix":"thebeamerpauses",
		"body":[
			"\\thebeamerpauses"
		],
		"luasnip": {"priority": 3000} 
	},


	"jobnamebeamerversion":{
		"prefix":"jobnamebeamerversion",
		"body":[
			"\\jobnamebeamerversion"
		],
		"luasnip": {"priority": 3000} 
	}
}
