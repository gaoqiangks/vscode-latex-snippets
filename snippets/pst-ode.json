{


	"pstODEsolve{}{}{}{}{}{}{}":{
		"prefix":"pstODEsolve",
		"body":[
			"\\pstODEsolve{${1:result}}{${2:output format}}{${3:t0}}{${4:t1}}{${5:N}}{${6:X_0}}{${7:function%definition}}"
		]
	},


	"pstODEsaveState{}":{
		"prefix":"pstODEsaveState",
		"body":[
			"\\pstODEsaveState{${1:state}}"
		]
	},


	"pstODErestoreState{}":{
		"prefix":"pstODErestoreState",
		"body":[
			"\\pstODErestoreState{${1:state}}"
		]
	},


	"pstODEsolve[]{}{}{}{}{}{}{}":{
		"prefix":"pstODEsolve",
		"body":[
			"\\pstODEsolve[${8:options%keyvals}]{${1:result}}{${2:output format}}{${3:t0}}{${4:t1}}{${5:N}}{${6:X_0}}{${7:function%definition}}"
		]
	}
}
