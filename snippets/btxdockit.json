{


	"fielditem{}{}":{
		"prefix":"fielditem",
		"body":[
			"\\fielditem{${1:field}}{${2:type}}"
		]
	},


	"listitem{}{}":{
		"prefix":"listitem",
		"body":[
			"\\listitem{${1:list}}{${2:type}}"
		]
	},


	"typeitem{}":{
		"prefix":"typeitem",
		"body":[
			"\\typeitem{${1:types}}"
		]
	},


	"reqitem{}":{
		"prefix":"reqitem",
		"body":[
			"\\reqitem{${1:types}}"
		]
	},


	"optitem{}":{
		"prefix":"optitem",
		"body":[
			"\\optitem{${1:types}}"
		]
	},


	"bibfield{}":{
		"prefix":"bibfield",
		"body":[
			"\\bibfield{${1:field}}"
		]
	},


	"bibtype{}":{
		"prefix":"bibtype",
		"body":[
			"\\bibtype{${1:type}}"
		]
	},


	"fieldlist":{
		"prefix":"fieldlist",
		"body":[
			"\\begin{fieldlist}",
				"\t$1",
			"\\end{fieldlist}"
		]
	},


	"typelist":{
		"prefix":"typelist",
		"body":[
			"\\begin{typelist}",
				"\t$1",
			"\\end{typelist}"
		]
	}
}
