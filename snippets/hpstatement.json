{


	"hpsetup{}":{
		"prefix":"hpsetup",
		"body":[
			"\\hpsetup{${1:options%keyvals}}"
		]
	},


	"hpnumber{}":{
		"prefix":"hpnumber",
		"body":[
			"\\hpnumber{${1:number}}"
		]
	},


	"hpstatement{}":{
		"prefix":"hpstatement",
		"body":[
			"\\hpstatement{${1:number}}"
		]
	},


	"hpnumber[]{}":{
		"prefix":"hpnumber",
		"body":[
			"\\hpnumber[${2:text1%text}]{${1:number}}"
		]
	},


	"hpnumber[][]{}":{
		"prefix":"hpnumber",
		"body":[
			"\\hpnumber[${2:text1%text}][${3:text2%text}]{${1:number}}"
		]
	},


	"hpstatement[]{}":{
		"prefix":"hpstatement",
		"body":[
			"\\hpstatement[${2:text1%text}]{${1:number}}"
		]
	},


	"hpstatement[][]{}":{
		"prefix":"hpstatement",
		"body":[
			"\\hpstatement[${2:text1%text}][${3:text2%text}]{${1:number}}"
		]
	}
}
