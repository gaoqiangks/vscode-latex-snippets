{


	"nameref{}":{
		"prefix":"nameref",
		"body":[
			"\\nameref{${1:label}}"
		]
	},


	"Nameref{}":{
		"prefix":"Nameref",
		"body":[
			"\\Nameref{${1:label}}"
		]
	},


	"Sectionformat{}{}":{
		"prefix":"Sectionformat",
		"body":[
			"\\Sectionformat{${1:arg1}}{${2:arg2}}"
		]
	},


	"nameref*{}":{
		"prefix":"*nameref",
		"body":[
			"\\nameref*{${1:label}}"
		]
	}
}
