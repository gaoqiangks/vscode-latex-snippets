{


	"iplot{}":{
		"prefix":"iplot",
		"body":[
			"\\iplot{${1:function%definition}}"
		]
	},


	"iplot[]{}":{
		"prefix":"iplot",
		"body":[
			"\\iplot[${2:options%keyvals}]{${1:function%definition}}"
		]
	},


	"iplotdd":{
		"prefix":"iplotdd",
		"body":[
			"\\begin{iplotdd}",
				"\t$1",
			"\\end{iplotdd}"
		]
	},


	"iplotddd":{
		"prefix":"iplotddd",
		"body":[
			"\\begin{iplotddd}",
				"\t$1",
			"\\end{iplotddd}"
		]
	},


	"iplotdd":{
		"prefix":"iplotdd",
		"body":[
			"\\begin{iplotdd}",
				"\t$1",
			"\\end{iplotdd}"
		]
	},


	"iplotddd":{
		"prefix":"iplotddd",
		"body":[
			"\\begin{iplotddd}",
				"\t$1",
			"\\end{iplotddd}"
		]
	}
}
