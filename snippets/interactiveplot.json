{


	"iplot{}":{
		"prefix":"iplot",
		"body":[
			"\\iplot{${1:function%definition}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"iplot[]{}":{
		"prefix":"iplot",
		"body":[
			"\\iplot[${2:options%keyvals}]{${1:function%definition}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"iplotdd":{
		"prefix":"iplotdd",
		"body":[
			"\\begin{iplotdd}",
				"\t$1",
			"\\end{iplotdd}"
		],
		"luasnip": {"priority": 3000} 
	},


	"iplotdd[]":{
		"prefix":"iplotdd",
		"body":[
			"\\begin{iplotdd}",
				"\t[${1:options%keyvals}]",
			"\\end{iplotdd}"
		],
		"luasnip": {"priority": 1000} 
	},


	"iplotddd":{
		"prefix":"iplotddd",
		"body":[
			"\\begin{iplotddd}",
				"\t$1",
			"\\end{iplotddd}"
		],
		"luasnip": {"priority": 3000} 
	},


	"iplotddd[]":{
		"prefix":"iplotddd",
		"body":[
			"\\begin{iplotddd}",
				"\t[${1:options%keyvals}]",
			"\\end{iplotddd}"
		],
		"luasnip": {"priority": 1000} 
	}
}
