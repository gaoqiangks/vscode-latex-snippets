{


	"handout{}":{
		"prefix":"handout",
		"body":[
			"\\handout{${1:file path%file}}"
		]
	},


	"thehandoutnumber":{
		"prefix":"thehandoutnumber",
		"body":[
			"\\thehandoutnumber"
		]
	},


	"handoutnumber{}":{
		"prefix":"handoutnumber",
		"body":[
			"\\handoutnumber{${1:arg}}"
		]
	},


	"handoutnumberintxt{}":{
		"prefix":"handoutnumberintxt",
		"body":[
			"\\handoutnumberintxt{${1:arg}}"
		]
	},


	"disablehandout":{
		"prefix":"disablehandout",
		"body":[
			"\\disablehandout"
		]
	},


	"enablehandout":{
		"prefix":"enablehandout",
		"body":[
			"\\enablehandout"
		]
	},


	"onlyhandout{}":{
		"prefix":"onlyhandout",
		"body":[
			"\\onlyhandout{${1:contents%text}}"
		]
	},


	"nothandout{}":{
		"prefix":"nothandout",
		"body":[
			"\\nothandout{${1:contents%text}}"
		]
	},


	"forhandout{}":{
		"prefix":"forhandout",
		"body":[
			"\\forhandout{${1:contents%text}}"
		]
	},


	"forhandout*{}":{
		"prefix":"*forhandout",
		"body":[
			"\\forhandout*{${1:contents%text}}"
		]
	}
}
