{


	"SetOptionsToPackage":{
		"prefix":"SetOptionsToPackage",
		"body":[
			"\\SetOptionsToPackage{${1:options list}}{${2:package}}"
		]
	},


	"SetOptionsToPackage*":{
		"prefix":"SetOptionsToPackage*",
		"body":[
			"\\SetOptionsToPackage*{${1:options list}}{${2:package}}"
		]
	},


	"SetOptionsToClass":{
		"prefix":"SetOptionsToClass",
		"body":[
			"\\SetOptionsToClass{${1:options list}}{${2:class}}"
		]
	},


	"SetOptionsToClass*":{
		"prefix":"SetOptionsToClass*",
		"body":[
			"\\SetOptionsToClass*{${1:options list}}{${2:class}}"
		]
	},


	"ClearPackageOptions":{
		"prefix":"ClearPackageOptions",
		"body":[
			"\\ClearPackageOptions{${1:package}}"
		]
	},


	"ClearClassOptions":{
		"prefix":"ClearClassOptions",
		"body":[
			"\\ClearClassOptions{${1:class}}"
		]
	}
}
