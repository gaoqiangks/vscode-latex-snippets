{


	"Vertex{}":{
		"prefix":"Vertex",
		"body":[
			"\\Vertex{${1:name}}"
		]
	},


	"Edge()()":{
		"prefix":"Edge",
		"body":[
			"\\Edge(${1:vertex1})(${2:vertex2})"
		]
	},


	"Text{}":{
		"prefix":"Text",
		"body":[
			"\\Text{${1:text}}"
		]
	},


	"Vertices{}":{
		"prefix":"Vertices",
		"body":[
			"\\Vertices{${1:file}}"
		]
	},


	"Edges{}":{
		"prefix":"Edges",
		"body":[
			"\\Edges{${1:file}}"
		]
	},


	"SetDefaultUnit{}":{
		"prefix":"SetDefaultUnit",
		"body":[
			"\\SetDefaultUnit{${1:unit}}"
		]
	},


	"SetDistanceScale{}":{
		"prefix":"SetDistanceScale",
		"body":[
			"\\SetDistanceScale{${1:number}}"
		]
	},


	"SetLayerDistance{}":{
		"prefix":"SetLayerDistance",
		"body":[
			"\\SetLayerDistance{${1:length}}"
		]
	},


	"EdgesNotInBG":{
		"prefix":"EdgesNotInBG",
		"body":[
			"\\EdgesNotInBG"
		]
	},


	"EdgesInBG":{
		"prefix":"EdgesInBG",
		"body":[
			"\\EdgesInBG"
		]
	},


	"SetPlaneWidth{}":{
		"prefix":"SetPlaneWidth",
		"body":[
			"\\SetPlaneWidth{${1:width}}"
		]
	},


	"SetPlaneHeight{}":{
		"prefix":"SetPlaneHeight",
		"body":[
			"\\SetPlaneHeight{${1:height}}"
		]
	},


	"Vertex[]{}":{
		"prefix":"Vertex",
		"body":[
			"\\Vertex[${2:options%keyvals}]{${1:name}}"
		]
	},


	"Edge[]()()":{
		"prefix":"Edge",
		"body":[
			"\\Edge[${1:options%keyvals}](${2:vertex1})(${3:vertex2})"
		]
	},


	"Text[]{}":{
		"prefix":"Text",
		"body":[
			"\\Text[${2:options%keyvals}]{${1:text}}"
		]
	},


	"Vertices[]{}":{
		"prefix":"Vertices",
		"body":[
			"\\Vertices[${2:options%keyvals}]{${1:file}}"
		]
	},


	"Edges[]{}":{
		"prefix":"Edges",
		"body":[
			"\\Edges[${2:options%keyvals}]{${1:file}}"
		]
	},


	"Plane[]":{
		"prefix":"Plane",
		"body":[
			"\\Plane[${1:options%keyvals}]"
		]
	},


	"SetCoordinates[]":{
		"prefix":"SetCoordinates",
		"body":[
			"\\SetCoordinates[${1:specs%keyvals}]"
		]
	},


	"SetVertexStyle[]":{
		"prefix":"SetVertexStyle",
		"body":[
			"\\SetVertexStyle[${1:options%keyvals}]"
		]
	},


	"SetEdgeStyle[]":{
		"prefix":"SetEdgeStyle",
		"body":[
			"\\SetEdgeStyle[${1:options%keyvals}]"
		]
	},


	"SetTextStyle[]":{
		"prefix":"SetTextStyle",
		"body":[
			"\\SetTextStyle[${1:options%keyvals}]"
		]
	},


	"SetPlaneStyle[]":{
		"prefix":"SetPlaneStyle",
		"body":[
			"\\SetPlaneStyle[${1:options%keyvals}]"
		]
	},


	"Layer":{
		"prefix":"Layer",
		"body":[
			"\\begin{Layer}",
				"\t$1",
			"\\end{Layer}"
		]
	}
}
