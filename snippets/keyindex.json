{


	"keyindexfile":{
		"prefix":"keyindexfile",
		"body":[
			"\\keyindexfile{${1:file}}"
		]
	},


	"keyindexformat":{
		"prefix":"keyindexformat",
		"body":[
			"\\keyindexformat{${1:text}}"
		]
	},


	"missingkeyindexformat":{
		"prefix":"missingkeyindexformat",
		"body":[
			"\\missingkeyindexformat{${1:text}}"
		]
	},


	"keyindexcommand":{
		"prefix":"keyindexcommand",
		"body":[
			"\\keyindexcommand{${1:text}}"
		]
	},


	"keyindexprint":{
		"prefix":"keyindexprint",
		"body":[
			"\\keyindexprint{${1:key%plain}}"
		]
	},


	"keyindexonly":{
		"prefix":"keyindexonly",
		"body":[
			"\\keyindexonly{${1:key%plain}}"
		]
	},


	"keyindexonly":{
		"prefix":"keyindexonly",
		"body":[
			"\\keyindexonly[${2:index option}]{${1:key%plain}}"
		]
	},


	"keyindex":{
		"prefix":"keyindex",
		"body":[
			"\\keyindex{${1:key%plain}}"
		]
	},


	"keyindex":{
		"prefix":"keyindex",
		"body":[
			"\\keyindex[${2:index option}]{${1:key%plain}}"
		]
	},


	"keyindexentry":{
		"prefix":"keyindexentry",
		"body":[
			"\\keyindexentry{${1:key%plain}}{${2:print text}}{${3:index text}}"
		]
	}
}
