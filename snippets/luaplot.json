{


	"luaplot{}":{
		"prefix":"luaplot",
		"body":[
			"\\luaplot{${1:function%definition}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"luaplot[]{}":{
		"prefix":"luaplot",
		"body":[
			"\\luaplot[${2:options%keyvals}]{${1:function%definition}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"luatikzpath{}{}{}{}":{
		"prefix":"luatikzpath",
		"body":[
			"\\luatikzpath{${1:function%definition}}{${2:xmin}}{${3:xmax}}{${4:plot points}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
