{


	"tsvtemplate{}{}":{
		"prefix":"tsvtemplate",
		"body":[
			"\\tsvtemplate{${1:template}}{${2:file}}"
		]
	},


	"applytemplate{}{}{}{}":{
		"prefix":"applytemplate",
		"body":[
			"\\applytemplate{${1:separator}}{${2:colnames}}{${3:template}}{${4:file}}"
		]
	},


	"tsvtemplate[]{}{}":{
		"prefix":"tsvtemplate",
		"body":[
			"\\tsvtemplate[${3:colname1,colname2,...}]{${1:template}}{${2:file}}"
		]
	},


	"tsv template":{
		"prefix":"tsv template",
		"body":[
			"\\begin{tsv template}",
				"\t$1",
			"\\end{tsv template}"
		]
	},


	"tsv template":{
		"prefix":"tsv template",
		"body":[
			"\\begin{tsv template}",
				"\t$1",
			"\\end{tsv template}"
		]
	}
}
