{


	"tsvtemplate{}{}":{
		"prefix":"tsvtemplate",
		"body":[
			"\\tsvtemplate{${1:template}}{${2:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tsvtemplate[]{}{}":{
		"prefix":"tsvtemplate",
		"body":[
			"\\tsvtemplate[${3:colname1,colname2,...}]{${1:template}}{${2:file}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"applytemplate{}{}{}{}":{
		"prefix":"applytemplate",
		"body":[
			"\\applytemplate{${1:separator}}{${2:colnames}}{${3:template}}{${4:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tsv template{}":{
		"prefix":"tsv template",
		"body":[
			"\\begin{tsv template}",
				"\t{${1:file}}",
			"\\end{tsv template}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tsv template[]{}":{
		"prefix":"tsv template",
		"body":[
			"\\begin{tsv template}",
				"\t[${2:colname1,colname2,...}]{${1:file}}",
			"\\end{tsv template}"
		],
		"luasnip": {"priority": 1000} 
	}
}
