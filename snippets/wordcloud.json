{


	"wordcloud{}":{
		"prefix":"wordcloud",
		"body":[
			"\\wordcloud{${1:(${2:word1},${3:weight1});(${4:word2},${5:weight2});...}}"
		]
	},


	"wordcloudFile{}{}":{
		"prefix":"wordcloudFile",
		"body":[
			"\\wordcloudFile{${1:file}}{${2:number of words}}"
		]
	},


	"wordcloudIgnoreWords{}":{
		"prefix":"wordcloudIgnoreWords",
		"body":[
			"\\wordcloudIgnoreWords{${1:word1,word2,...}}"
		]
	},


	"wordcloud[]{}":{
		"prefix":"wordcloud",
		"body":[
			"\\wordcloud[${2:options}]{${1:(${3:word1},${4:weight1});(${5:word2},${6:weight2});...}}"
		]
	},


	"wordcloudFile[]{}{}":{
		"prefix":"wordcloudFile",
		"body":[
			"\\wordcloudFile[${3:options%keyvals}]{${1:file}}{${2:number of words}}"
		]
	}
}
