{


	"setptextdefault{}":{
		"prefix":"setptextdefault",
		"body":[
			"\\setptextdefault{${1:range}}"
		]
	},


	"ptext":{
		"prefix":"ptext",
		"body":[
			"\\ptext"
		]
	},


	"ChangePtextPar":{
		"prefix":"ChangePtextPar",
		"body":[
			"\\ChangePtextPar"
		]
	},


	"ptext*":{
		"prefix":"*ptext",
		"body":[
			"\\ptext*"
		]
	},


	"ptext*[]":{
		"prefix":"*ptext",
		"body":[
			"\\ptext*[${1:range}]"
		]
	},


	"ptext[]":{
		"prefix":"ptext",
		"body":[
			"\\ptext[${1:range}]"
		]
	}
}
