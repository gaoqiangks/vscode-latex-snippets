{


	"pssavepath{}{}":{
		"prefix":"pssavepath",
		"body":[
			"\\pssavepath{${1:name}}{${2:commands}}"
		]
	},


	"psintersect{}{}":{
		"prefix":"psintersect",
		"body":[
			"\\psintersect{${1:curveA}}{${2:curveB}}"
		]
	},


	"pstracecurve{}":{
		"prefix":"pstracecurve",
		"body":[
			"\\pstracecurve{${1:curve name}}"
		]
	},


	"pstracecurve{}{}":{
		"prefix":"pstracecurve",
		"body":[
			"\\pstracecurve{${1:intersection}}{${2:curve name}}"
		]
	},


	"psGetCurvePoint{}{}":{
		"prefix":"psGetCurvePoint",
		"body":[
			"\\psGetCurvePoint{${1:curve name}}{${2:macro%cmd}}"
		]
	},


	"psGetIsectCenter{}{}{}":{
		"prefix":"psGetIsectCenter",
		"body":[
			"\\psGetIsectCenter{${1:intersection}}{${2:curve name}}{${3:macro%cmd}}"
		]
	},


	"pssavepath[]{}{}":{
		"prefix":"pssavepath",
		"body":[
			"\\pssavepath[${3:options%keyvals}]{${1:name}}{${2:commands}}"
		]
	},


	"psintersect[]{}{}":{
		"prefix":"psintersect",
		"body":[
			"\\psintersect[${3:options%keyvals}]{${1:curveA}}{${2:curveB}}"
		]
	},


	"pstracecurve[]{}":{
		"prefix":"pstracecurve",
		"body":[
			"\\pstracecurve[${2:options%keyvals}]{${1:curve name}}"
		]
	},


	"pstracecurve[]{}{}":{
		"prefix":"pstracecurve",
		"body":[
			"\\pstracecurve[${3:options%keyvals}]{${1:intersection}}{${2:curve name}}"
		]
	}
}
