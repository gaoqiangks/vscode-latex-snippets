{


	"stubs{}{}":{
		"prefix":"stubs",
		"body":[
			"\\stubs{${1:width}}{${2:text front%text}}"
		]
	},


	"stublmargin":{
		"prefix":"stublmargin",
		"body":[
			"\\stublmargin"
		]
	},


	"stubrmargin":{
		"prefix":"stubrmargin",
		"body":[
			"\\stubrmargin"
		]
	},


	"stubbmargin":{
		"prefix":"stubbmargin",
		"body":[
			"\\stubbmargin"
		]
	},


	"stubs{}[]{}":{
		"prefix":"stubs",
		"body":[
			"\\stubs{${1:width}}[${3:text back%text}]{${2:text front%text}}"
		]
	},


	"stubs[]{}{}":{
		"prefix":"stubs",
		"body":[
			"\\stubs[${3:number}]{${1:width}}{${2:text front%text}}"
		]
	},


	"stubs[]{}[]{}":{
		"prefix":"stubs",
		"body":[
			"\\stubs[${3:number}]{${1:width}}[${4:text back%text}]{${2:text front%text}}"
		]
	}
}
