{


	"readpsbardata{}{}":{
		"prefix":"readpsbardata",
		"body":[
			"\\readpsbardata{${1:macro%cmd}}{${2:filename%file}}"
		]
	},


	"psbarchart{}":{
		"prefix":"psbarchart",
		"body":[
			"\\psbarchart{${1:data macro}}"
		]
	},


	"newpsbarstyle{}{}":{
		"prefix":"newpsbarstyle",
		"body":[
			"\\newpsbarstyle{${1:name}}{${2:definition}}"
		]
	},


	"psbarlabel{}":{
		"prefix":"psbarlabel",
		"body":[
			"\\psbarlabel{${1:label}}"
		]
	},


	"psbarlabelsep":{
		"prefix":"psbarlabelsep",
		"body":[
			"\\psbarlabelsep"
		]
	},


	"psbarscale(){}":{
		"prefix":"psbarscale",
		"body":[
			"\\psbarscale(${2:scale}){${1:PostScript code}}"
		]
	},


	"setbarstyle{}":{
		"prefix":"setbarstyle",
		"body":[
			"\\setbarstyle{${1:style}}"
		]
	},


	"readpsbardata[]{}{}":{
		"prefix":"readpsbardata",
		"body":[
			"\\readpsbardata[${3:options%keyvals}]{${1:macro%cmd}}{${2:filename%file}}"
		]
	},


	"psbarchart[]{}":{
		"prefix":"psbarchart",
		"body":[
			"\\psbarchart[${2:options%keyvals}]{${1:data macro}}"
		]
	}
}
