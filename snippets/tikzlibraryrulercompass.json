{


	"thepointlabels":{
		"prefix":"thepointlabels",
		"body":[
			"\\thepointlabels"
		]
	},


	"point{}{}{}":{
		"prefix":"point",
		"body":[
			"\\point{${1:path1}}{${2:path2}}{${3:intersection}}"
		]
	},


	"ruler{}{}":{
		"prefix":"ruler",
		"body":[
			"\\ruler{${1:point1}}{${2:point2}}"
		]
	},


	"compass{}{}":{
		"prefix":"compass",
		"body":[
			"\\compass{${1:point1}}{${2:point2}}"
		]
	},


	"constrain":{
		"prefix":"constrain",
		"body":[
			"\\constrain"
		]
	},


	"point[]{}{}{}":{
		"prefix":"point",
		"body":[
			"\\point[${4:styles}]{${1:path1}}{${2:path2}}{${3:intersection}}"
		]
	},


	"ruler[]{}{}":{
		"prefix":"ruler",
		"body":[
			"\\ruler[${3:styles}]{${1:point1}}{${2:point2}}"
		]
	},


	"compass[]{}{}":{
		"prefix":"compass",
		"body":[
			"\\compass[${3:styles}]{${1:point1}}{${2:point2}}"
		]
	}
}
