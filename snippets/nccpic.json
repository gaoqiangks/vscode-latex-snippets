{


	"ipic{}":{
		"prefix":"ipic",
		"body":[
			"\\ipic{${1:imagefile}}"
		]
	},


	"draftgraphics":{
		"prefix":"draftgraphics",
		"body":[
			"\\draftgraphics"
		]
	},


	"finalgraphics":{
		"prefix":"finalgraphics",
		"body":[
			"\\finalgraphics"
		]
	},


	"putimage(){}":{
		"prefix":"putimage",
		"body":[
			"\\putimage(${2:x},${3:y}){${1:imagefile}}"
		]
	},


	"putimage()(){}":{
		"prefix":"putimage",
		"body":[
			"\\putimage(${2:x},${3:y})(${4:xshift},${5:yshift}){${1:imagefile}}"
		]
	},


	"putimage()[]{}":{
		"prefix":"putimage",
		"body":[
			"\\putimage(${3:x},${4:y})[${2:width,height}]{${1:imagefile}}"
		]
	},


	"putimage()[](){}":{
		"prefix":"putimage",
		"body":[
			"\\putimage(${3:x},${4:y})[${2:width,height}](${5:xshift},${6:yshift}){${1:imagefile}}"
		]
	}
}
