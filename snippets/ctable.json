{


	"setupctable":{
		"prefix":"setupctable",
		"body":[
			"\\setupctable{${1:options%keyvals}}"
		]
	},


	"ctable":{
		"prefix":"ctable",
		"body":[
			"\\ctable{${1:preamble}}{${2:tnote cmds}}{${3:table}}"
		]
	},


	"ctable":{
		"prefix":"ctable",
		"body":[
			"\\ctable[${4:options%keyvals}]{${1:preamble}}{${2:tnote cmds}}{${3:table}}"
		]
	},


	"tnote":{
		"prefix":"tnote",
		"body":[
			"\\tnote{${1:footnote text}}"
		]
	},


	"tnote":{
		"prefix":"tnote",
		"body":[
			"\\tnote[${2:label}]{${1:footnote text}}"
		]
	},


	"tmark":{
		"prefix":"tmark",
		"body":[
			"\\tmark"
		]
	},


	"tmark":{
		"prefix":"tmark",
		"body":[
			"\\tmark[${1:label}]"
		]
	},


	"NN":{
		"prefix":"NN",
		"body":[
			"\\NN"
		]
	},


	"FL":{
		"prefix":"FL",
		"body":[
			"\\FL"
		]
	},


	"ML":{
		"prefix":"ML",
		"body":[
			"\\ML"
		]
	},


	"LL":{
		"prefix":"LL",
		"body":[
			"\\LL"
		]
	}
}
