{


	"setupctable{}":{
		"prefix":"setupctable",
		"body":[
			"\\setupctable{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"ctable{}{}{}":{
		"prefix":"ctable",
		"body":[
			"\\ctable{${1:preamble}}{${2:tnote cmds}}{${3:table}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"ctable[]{}{}{}":{
		"prefix":"ctable",
		"body":[
			"\\ctable[${4:options%keyvals}]{${1:preamble}}{${2:tnote cmds}}{${3:table}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"tnote{}":{
		"prefix":"tnote",
		"body":[
			"\\tnote{${1:footnote text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tnote[]{}":{
		"prefix":"tnote",
		"body":[
			"\\tnote[${2:label}]{${1:footnote text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"tmark":{
		"prefix":"tmark",
		"body":[
			"\\tmark"
		],
		"luasnip": {"priority": 3000} 
	},


	"tmark[]":{
		"prefix":"tmark",
		"body":[
			"\\tmark[${1:label}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"NN":{
		"prefix":"NN",
		"body":[
			"\\NN"
		],
		"luasnip": {"priority": 3000} 
	},


	"FL":{
		"prefix":"FL",
		"body":[
			"\\FL"
		],
		"luasnip": {"priority": 3000} 
	},


	"ML":{
		"prefix":"ML",
		"body":[
			"\\ML"
		],
		"luasnip": {"priority": 3000} 
	},


	"LL":{
		"prefix":"LL",
		"body":[
			"\\LL"
		],
		"luasnip": {"priority": 3000} 
	}
}
