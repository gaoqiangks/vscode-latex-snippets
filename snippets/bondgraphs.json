{


	"bond":{
		"prefix":"bond",
		"body":[
			"\\bond"
		],
		"luasnip": {"priority": 3000} 
	},


	"bond[]":{
		"prefix":"bond",
		"body":[
			"\\bond[${1:options%keyvals}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"bgelement{}":{
		"prefix":"bgelement",
		"body":[
			"\\bgelement{${1:element}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"bgelement[]{}":{
		"prefix":"bgelement",
		"body":[
			"\\bgelement[${2:options%keyvals}]{${1:element}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"bondgraph":{
		"prefix":"bondgraph",
		"body":[
			"\\begin{bondgraph}",
				"\t$1",
			"\\end{bondgraph}"
		],
		"luasnip": {"priority": 3000} 
	},


	"bondgraph[]":{
		"prefix":"bondgraph",
		"body":[
			"\\begin{bondgraph}",
				"\t[${1:options%keyvals}]",
			"\\end{bondgraph}"
		],
		"luasnip": {"priority": 1000} 
	}
}
