{


	"HardWrap{}{}{}{}{}":{
		"prefix":"HardWrap",
		"body":[
			"\\HardWrap{${1:function}}{${2:width}}{${3:setup code}}{${4:newline}}{${5:text}}"
		]
	},


	"GenerateLogMacros{}{}":{
		"prefix":"GenerateLogMacros",
		"body":[
			"\\GenerateLogMacros{${1:type%keyvals}}{${2:name}}"
		]
	},


	"HardWrapSetup":{
		"prefix":"HardWrapSetup",
		"body":[
			"\\HardWrapSetup"
		]
	},


	"setmaxprintline{}":{
		"prefix":"setmaxprintline",
		"body":[
			"\\setmaxprintline{${1:integer}}"
		]
	},


	"GeneratePackageLogMacros{}":{
		"prefix":"GeneratePackageLogMacros",
		"body":[
			"\\GeneratePackageLogMacros{${1:name}}"
		]
	},


	"GenerateClassLogMacros{}":{
		"prefix":"GenerateClassLogMacros",
		"body":[
			"\\GenerateClassLogMacros{${1:name}}"
		]
	},


	"GenerateLogMacros{}[]{}":{
		"prefix":"GenerateLogMacros",
		"body":[
			"\\GenerateLogMacros{${1:type%keyvals}}[${3:prefix}]{${2:name}}"
		]
	},


	"GeneratePackageLogMacros[]{}":{
		"prefix":"GeneratePackageLogMacros",
		"body":[
			"\\GeneratePackageLogMacros[${2:prefix}]{${1:name}}"
		]
	},


	"GenerateClassLogMacros[]{}":{
		"prefix":"GenerateClassLogMacros",
		"body":[
			"\\GenerateClassLogMacros[${2:prefix}]{${1:name}}"
		]
	}
}
