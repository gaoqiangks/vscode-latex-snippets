{


	"newfloatsetup{}":{
		"prefix":"newfloatsetup",
		"body":[
			"\\newfloatsetup{${1:options%keyvals}}"
		]
	},


	"DeclareFloatingEnvironment{}":{
		"prefix":"DeclareFloatingEnvironment",
		"body":[
			"\\DeclareFloatingEnvironment{${1:envname}}"
		]
	},


	"SetupFloatingEnvironment{}{}":{
		"prefix":"SetupFloatingEnvironment",
		"body":[
			"\\SetupFloatingEnvironment{${1:envname}}{${2:options%keyvals}}"
		]
	},


	"ForEachFloatingEnvironment{}":{
		"prefix":"ForEachFloatingEnvironment",
		"body":[
			"\\ForEachFloatingEnvironment{${1:code}}"
		]
	},


	"PrepareListOf{}{}":{
		"prefix":"PrepareListOf",
		"body":[
			"\\PrepareListOf{${1:envname}}{${2:code}}"
		]
	},


	"DeclareFloatingEnvironment[]{}":{
		"prefix":"DeclareFloatingEnvironment",
		"body":[
			"\\DeclareFloatingEnvironment[${2:options%keyvals}]{${1:envname}}"
		]
	}
}
