{


	"bcbar{}":{
		"prefix":"bcbar",
		"body":[
			"\\bcbar{${1:number}}"
		]
	},


	"bcskip{}":{
		"prefix":"bcskip",
		"body":[
			"\\bcskip{${1:length}}"
		]
	},


	"bcxlabel{}":{
		"prefix":"bcxlabel",
		"body":[
			"\\bcxlabel{${1:label}}"
		]
	},


	"bclabel{}":{
		"prefix":"bclabel",
		"body":[
			"\\bclabel{${1:label}}"
		]
	},


	"bcfontstyle":{
		"prefix":"bcfontstyle",
		"body":[
			"\\bcfontstyle"
		]
	},


	"bcbar[]{}":{
		"prefix":"bcbar",
		"body":[
			"\\bcbar[${2:options%keyvals}]{${1:number}}"
		]
	},


	"bchart":{
		"prefix":"bchart",
		"body":[
			"\\begin{bchart}",
				"\t$1",
			"\\end{bchart}"
		]
	}
}
