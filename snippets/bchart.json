{


	"bcbar{}":{
		"prefix":"bcbar",
		"body":[
			"\\bcbar{${1:number}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"bcbar[]{}":{
		"prefix":"bcbar",
		"body":[
			"\\bcbar[${2:options%keyvals}]{${1:number}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"bcskip{}":{
		"prefix":"bcskip",
		"body":[
			"\\bcskip{${1:length}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"bcxlabel{}":{
		"prefix":"bcxlabel",
		"body":[
			"\\bcxlabel{${1:label}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"bclabel{}":{
		"prefix":"bclabel",
		"body":[
			"\\bclabel{${1:label}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"bcfontstyle":{
		"prefix":"bcfontstyle",
		"body":[
			"\\bcfontstyle"
		],
		"luasnip": {"priority": 3000} 
	},


	"bchart[]":{
