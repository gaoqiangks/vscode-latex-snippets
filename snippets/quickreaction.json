		],
		"luasnip": {"priority": 1000} 
	},


	"fixrow":{
		"prefix":"fixrow",
		"body":[
			"\\fixrow"
		],
		"luasnip": {"priority": 3000} 
	},


	"quickreaction":{
		"prefix":"quickreaction",
		"body":[
			"\\begin{quickreaction}",
				"\t$1",
			"\\end{quickreaction}"
		],
		"luasnip": {"priority": 3000} 
	},


	"quickreaction[]":{
		"prefix":"quickreaction",
		"body":[
			"\\begin{quickreaction}",
				"\t[${1:opt1}]",
