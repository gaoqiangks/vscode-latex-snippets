{


	"byoTwemoji{}":{
		"prefix":"byoTwemoji",
		"body":[
			"\\byoTwemoji{${1:elmt1;elmt2;...}}"
		]
	},


	"forElementInList{}{}":{
		"prefix":"forElementInList",
		"body":[
			"\\forElementInList{${1:macro}}{${2:list}}"
		]
	},


	"byoTwemojiShadowTransparency":{
		"prefix":"byoTwemojiShadowTransparency",
		"body":[
			"\\byoTwemojiShadowTransparency"
		]
	},


	"twemojiDefaultHeight":{
		"prefix":"twemojiDefaultHeight",
		"body":[
			"\\twemojiDefaultHeight"
		]
	},


	"defineByoTwemojiElement{}{}":{
		"prefix":"defineByoTwemojiElement",
		"body":[
			"\\defineByoTwemojiElement{${1:elmt name}}{${2:elmt TikZ paths}}"
		]
	},


	"byoTwemojiElement{}":{
		"prefix":"byoTwemojiElement",
		"body":[
			"\\byoTwemojiElement{${1:name!TikZ options}}"
		]
	},


	"byoTwemoji[]{}":{
		"prefix":"byoTwemoji",
		"body":[
			"\\byoTwemoji[${2:options%keyvals}]{${1:elmt1;elmt2;...}}"
		]
	}
}
