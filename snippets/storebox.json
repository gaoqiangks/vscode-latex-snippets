{


	"newstorebox{}":{
		"prefix":"newstorebox",
		"body":[
			"\\newstorebox{${1:boxname%cmd}}"
		]
	},


	"storebox{}{}":{
		"prefix":"storebox",
		"body":[
			"\\storebox{${1:boxname}}{${2:content%text}}"
		]
	},


	"usestorebox{}":{
		"prefix":"usestorebox",
		"body":[
			"\\usestorebox{${1:boxname}}"
		]
	},


	"ifstorebox":{
		"prefix":"ifstorebox",
		"body":[
			"\\ifstorebox"
		]
	},


	"storebox":{
		"prefix":"storebox",
		"body":[
			"\\begin{storebox}",
				"\t$1",
			"\\end{storebox}"
		]
	}
}
