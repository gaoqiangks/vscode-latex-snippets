{


	"newstorebox{}":{
		"prefix":"newstorebox",
		"body":[
			"\\newstorebox{${1:boxname%cmd}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"storebox{}{}":{
		"prefix":"storebox",
		"body":[
			"\\storebox{${1:boxname}}{${2:content%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"usestorebox{}":{
		"prefix":"usestorebox",
		"body":[
			"\\usestorebox{${1:boxname}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"ifstorebox":{
		"prefix":"ifstorebox",
		"body":[
			"\\ifstorebox"
		],
		"luasnip": {"priority": 3000} 
	},


	"storebox{}":{
		"prefix":"storebox",
		"body":[
			"\\begin{storebox}",
				"\t{${1:boxname}}",
			"\\end{storebox}"
		],
		"luasnip": {"priority": 3000} 
	}
}
