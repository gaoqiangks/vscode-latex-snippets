{


	"PDFManagementAdd{}{}{}":{
		"prefix":"PDFManagementAdd",
		"body":[
			"\\PDFManagementAdd{${1:resource path}}{${2:name}}{${3:value}}"
		]
	},


	"AddToDocumentProperties{}{}":{
		"prefix":"AddToDocumentProperties",
		"body":[
			"\\AddToDocumentProperties{${1:property}}{${2:value}}"
		]
	},


	"GetDocumentProperties{}":{
		"prefix":"GetDocumentProperties",
		"body":[
			"\\GetDocumentProperties{${1:label/property}}"
		]
	},


	"ShowDocumentProperties":{
		"prefix":"ShowDocumentProperties",
		"body":[
			"\\ShowDocumentProperties"
		]
	},


	"hrefurl{}{}":{
		"prefix":"hrefurl",
		"body":[
			"\\hrefurl{${1:URL}}{${2:text}}"
		]
	},


	"hrefpdf{}{}":{
		"prefix":"hrefpdf",
		"body":[
			"\\hrefpdf{${1:file}}{${2:text}}"
		]
	},


	"hrefrun{}{}":{
		"prefix":"hrefrun",
		"body":[
			"\\hrefrun{${1:launch link%URL}}{${2:text}}"
		]
	},


	"AddToDocumentProperties[]{}{}":{
		"prefix":"AddToDocumentProperties",
		"body":[
			"\\AddToDocumentProperties[${3:label}]{${1:property}}{${2:value}}"
		]
	},


	"url[]{}":{
		"prefix":"url",
		"body":[
			"\\url[${2:options%keyvals}]{${1:URL}}"
		]
	},


	"hrefurl[]{}{}":{
		"prefix":"hrefurl",
		"body":[
			"\\hrefurl[${3:options%keyvals}]{${1:URL}}{${2:text}}"
		]
	},


	"hrefpdf[]{}{}":{
		"prefix":"hrefpdf",
		"body":[
			"\\hrefpdf[${3:options%keyvals}]{${1:file}}{${2:text}}"
		]
	},


	"hrefrun[]{}{}":{
		"prefix":"hrefrun",
		"body":[
			"\\hrefrun[${3:options%keyvals}]{${1:launch link%URL}}{${2:text}}"
		]
	}
}
