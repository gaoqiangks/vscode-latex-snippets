{


	"dmntable{}{}{}{}{}":{
		"prefix":"dmntable",
		"body":[
			"\\dmntable{${1:title}}{${2:hit policy}}{${3:input}}{${4:output}}{${5:entries}}"
		]
	},


	"dmnoutputtable{}{}{}{}":{
		"prefix":"dmnoutputtable",
		"body":[
			"\\dmnoutputtable{${1:title}}{${2:hit policy}}{${3:output}}{${4:entries}}"
		]
	},


	"glossarytable{}{}{}":{
		"prefix":"glossarytable",
		"body":[
			"\\glossarytable{${1:title}}{${2:subtitles}}{${3:entries}}"
		]
	},


	"goaltable{}{}":{
		"prefix":"goaltable",
		"body":[
			"\\goaltable{${1:title}}{${2:entries}}"
		]
	},


	"pdmntable{}{}{}{}{}{}":{
		"prefix":"pdmntable",
		"body":[
			"\\pdmntable{${1:title}}{${2:hit policy}}{${3:inputs}}{${4:outputs}}{${5:output vals}}{${6:entries}}"
		]
	},


	"pdmnoutputtable{}{}{}{}{}":{
		"prefix":"pdmnoutputtable",
		"body":[
			"\\pdmnoutputtable{${1:title}}{${2:hit policy}}{${3:output}}{${4:output val}}{${5:probabilities}}"
		]
	}
}
