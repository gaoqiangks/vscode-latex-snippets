{


	"Nr{}":{
		"prefix":"Nr",
		"body":[
			"\\Nr{${1:Nummer}}"
		]
	},


	"Klausurdatum{}":{
		"prefix":"Klausurdatum",
		"body":[
			"\\Klausurdatum{${1:Datum}}"
		]
	},


	"Abschnitt{}":{
		"prefix":"Abschnitt",
		"body":[
			"\\Abschnitt{${1:Thema%text}}"
		]
	},


	"Unterabschnitt{}":{
		"prefix":"Unterabschnitt",
		"body":[
			"\\Unterabschnitt{${1:Thema%text}}"
		]
	},


	"Unterunterabschnitt{}{}":{
		"prefix":"Unterunterabschnitt",
		"body":[
			"\\Unterunterabschnitt{${1:Thema%text}}{${2:Kompetenzen%text}}"
		]
	},


	"Abschnitt[]{}":{
		"prefix":"Abschnitt",
		"body":[
			"\\Abschnitt[${2:Nummer}]{${1:Thema%text}}"
		]
	},


	"Unterabschnitt[]{}":{
		"prefix":"Unterabschnitt",
		"body":[
			"\\Unterabschnitt[${2:Nummer}]{${1:Thema%text}}"
		]
	},


	"Unterabschnitt{}[]":{
		"prefix":"Unterabschnitt",
		"body":[
			"\\Unterabschnitt{${1:Thema%text}}[${2:Kompetenzen%text}]"
		]
	},


	"Unterabschnitt[]{}[]":{
		"prefix":"Unterabschnitt",
		"body":[
			"\\Unterabschnitt[${2:Nummer}]{${1:Thema%text}}[${3:Kompetenzen%text}]"
		]
	},


	"Unterunterabschnitt[]{}{}":{
		"prefix":"Unterunterabschnitt",
		"body":[
			"\\Unterunterabschnitt[${3:Nummer}]{${1:Thema%text}}{${2:Kompetenzen%text}}"
		]
	}
}
