{


	"makeindex":{
		"prefix":"makeindex",
		"body":[
			"\\makeindex[${1:options%keyvals}]"
		]
	},


	"indexsetup":{
		"prefix":"indexsetup",
		"body":[
			"\\indexsetup{${1:options%keyvals}}"
		]
	},


	"splitindexoptions":{
		"prefix":"splitindexoptions",
		"body":[
			"\\splitindexoptions{${1:splitindex options}}"
		]
	},


	"index":{
		"prefix":"index",
		"body":[
			"\\index[${2:name}]{${1:entry}}"
		]
	},


	"indexprologue":{
		"prefix":"indexprologue",
		"body":[
			"\\indexprologue{${1:text}}"
		]
	},


	"indexprologue":{
		"prefix":"indexprologue",
		"body":[
			"\\indexprologue[${2:spacing}]{${1:text}}"
		]
	},


	"printindex":{
		"prefix":"printindex",
		"body":[
			"\\printindex"
		]
	},


	"printindex":{
		"prefix":"printindex",
		"body":[
			"\\printindex[${1:name}]"
		]
	}
}
