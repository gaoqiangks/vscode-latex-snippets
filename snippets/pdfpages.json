{


	"includepdf{}":{
		"prefix":"includepdf",
		"body":[
			"\\includepdf{${1:filename%file}}"
		]
	},


	"includepdfmerge{}":{
		"prefix":"includepdfmerge",
		"body":[
			"\\includepdfmerge{${1:file-list%definition}}"
		]
	},


	"includepdfset{}":{
		"prefix":"includepdfset",
		"body":[
			"\\includepdfset{${1:global options%keyvals}}"
		]
	},


	"threadinfodict":{
		"prefix":"threadinfodict",
		"body":[
			"\\threadinfodict"
		]
	},


	"AddToSurvey":{
		"prefix":"AddToSurvey",
		"body":[
			"\\AddToSurvey"
		]
	},


	"includepdf[]{}":{
		"prefix":"includepdf",
		"body":[
			"\\includepdf[${2:options%keyvals}]{${1:filename%file}}"
		]
	},


	"includepdfmerge[]{}":{
		"prefix":"includepdfmerge",
		"body":[
			"\\includepdfmerge[${2:options%keyvals}]{${1:file-list%definition}}"
		]
	}
}
