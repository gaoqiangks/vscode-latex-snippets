{


	"declarecompound{}":{
		"prefix":"declarecompound",
		"body":[
			"\\declarecompound{${1:label%specialDef}}"
		]
	},


	"compound{}":{
		"prefix":"compound",
		"body":[
			"\\compound{${1:label1,label2,...%keyvals}}"
		]
	},


	"compoundseparator":{
		"prefix":"compoundseparator",
		"body":[
			"\\compoundseparator"
		]
	},


	"compoundglobalprefix":{
		"prefix":"compoundglobalprefix",
		"body":[
			"\\compoundglobalprefix"
		]
	},


	"compoundglobalsuffix":{
		"prefix":"compoundglobalsuffix",
		"body":[
			"\\compoundglobalsuffix"
		]
	},


	"compoundprefix":{
		"prefix":"compoundprefix",
		"body":[
			"\\compoundprefix"
		]
	},


	"compoundsuffix":{
		"prefix":"compoundsuffix",
		"body":[
			"\\compoundsuffix"
		]
	},


	"compoundstyle":{
		"prefix":"compoundstyle",
		"body":[
			"\\compoundstyle"
		]
	},


	"printcompound{}":{
		"prefix":"printcompound",
		"body":[
			"\\printcompound{${1:text%plain}}"
		]
	},


	"compound*{}":{
		"prefix":"*compound",
		"body":[
			"\\compound*{${1:label1,label2,...%keyvals}}"
		]
	},


	"declarecompound[]{}":{
		"prefix":"declarecompound",
		"body":[
			"\\declarecompound[${2:name}]{${1:label}}"
		]
	}
}
