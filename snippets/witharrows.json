{


	"WithArrowsOptions{}":{
		"prefix":"WithArrowsOptions",
		"body":[
			"\\WithArrowsOptions{${1:options%keyvals}}"
		]
	},


	"Arrow{}":{
		"prefix":"Arrow",
		"body":[
			"\\Arrow{${1:label%plain}}"
		]
	},


	"MultiArrow{}{}":{
		"prefix":"MultiArrow",
		"body":[
			"\\MultiArrow{${1:row1,row2,...}}{${2:label%plain}}"
		]
	},


	"WithArrowsLastEnv":{
		"prefix":"WithArrowsLastEnv",
		"body":[
			"\\WithArrowsLastEnv"
		]
	},


	"tag{}":{
		"prefix":"tag",
		"body":[
			"\\tag{${1:label%plain}}"
		]
	},


	"notag":{
		"prefix":"notag",
		"body":[
			"\\notag"
		]
	},


	"tagnextline":{
		"prefix":"tagnextline",
		"body":[
			"\\tagnextline"
		]
	},


	"WithArrowsRightX":{
		"prefix":"WithArrowsRightX",
		"body":[
			"\\WithArrowsRightX"
		]
	},


	"WithArrowsNewStyle{}{}":{
		"prefix":"WithArrowsNewStyle",
		"body":[
			"\\WithArrowsNewStyle{${1:style name%specialDef}}{${2:options%keyvals}}"
		]
	},


	"WithArrowsNbLines":{
		"prefix":"WithArrowsNbLines",
		"body":[
			"\\WithArrowsNbLines"
		]
	},


	"tag*{}":{
		"prefix":"*tag",
		"body":[
			"\\tag*{${1:label%plain}}"
		]
	},


	"Arrow[]{}":{
		"prefix":"Arrow",
		"body":[
			"\\Arrow[${2:options%keyvals}]{${1:label%plain}}"
		]
	},


	"WithArrows":{
		"prefix":"WithArrows",
		"body":[
			"\\begin{WithArrows}",
				"\t$1",
			"\\end{WithArrows}"
		]
	},


	"DispWithArrows":{
		"prefix":"DispWithArrows",
		"body":[
			"\\begin{DispWithArrows}",
				"\t$1",
			"\\end{DispWithArrows}"
		]
	},


	"DispWithArrows*":{
		"prefix":"*DispWithArrows",
		"body":[
			"\\begin{DispWithArrows*}",
				"\t$1",
			"\\end{DispWithArrows*}"
		]
	},


	"WithArrows":{
		"prefix":"WithArrows",
		"body":[
			"\\begin{WithArrows}",
				"\t$1",
			"\\end{WithArrows}"
		]
	},


	"DispWithArrows":{
		"prefix":"DispWithArrows",
		"body":[
			"\\begin{DispWithArrows}",
				"\t$1",
			"\\end{DispWithArrows}"
		]
	},


	"DispWithArrows*":{
		"prefix":"DispWithArrows",
		"body":[
			"\\begin{DispWithArrows*}",
				"\t$1",
			"\\end{DispWithArrows*}"
		]
	}
}
