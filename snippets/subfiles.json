{


	"subfile":{
		"prefix":"subfile",
		"body":[
			"\\subfile{${1:file}}"
		]
	},


	"subfileinclude":{
		"prefix":"subfileinclude",
		"body":[
			"\\subfileinclude{${1:file}}"
		]
	},


	"subfix":{
		"prefix":"subfix",
		"body":[
			"\\subfix{${1:file}}"
		]
	},


	"ifSubfilesClassLoaded":{
		"prefix":"ifSubfilesClassLoaded",
		"body":[
			"\\ifSubfilesClassLoaded{${1:then}}{${2:else}}"
		]
	}
}
