{


	"barcode{}":{
		"prefix":"barcode",
		"body":[
			"\\barcode{${1:code}}"
		]
	},


	"HPlabel{}":{
		"prefix":"HPlabel",
		"body":[
			"\\HPlabel{${1:code}}"
		]
	},


	"ITFbarcode{}":{
		"prefix":"ITFbarcode",
		"body":[
			"\\ITFbarcode{${1:code}}"
		]
	},


	"BarcodeSanitize":{
		"prefix":"BarcodeSanitize",
		"body":[
			"\\BarcodeSanitize"
		]
	},


	"barcode[]{}":{
		"prefix":"barcode",
		"body":[
			"\\barcode[${2:options%keyvals}]{${1:code}}"
		]
	},


	"ITFbarcode[]{}":{
		"prefix":"ITFbarcode",
		"body":[
			"\\ITFbarcode[${2:options%keyvals}]{${1:code}}"
		]
	}
}
