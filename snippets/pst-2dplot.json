{


	"setpstgraph{}":{
		"prefix":"setpstgraph",
		"body":[
			"\\setpstgraph{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"pstlabel{}{}":{
		"prefix":"pstlabel",
		"body":[
			"\\pstlabel{${1:x-label}}{${2:y-label}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"pstfileplot{}":{
		"prefix":"pstfileplot",
		"body":[
			"\\pstfileplot{${1:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"pstfileplot[]{}":{
		"prefix":"pstfileplot",
		"body":[
			"\\pstfileplot[${2:pstricks options}]{${1:file}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"pstgraph()":{
		"prefix":"pstgraph",
		"body":[
			"\\begin{pstgraph}",
				"\t(${1:x0},${2:y0})",
			"\\end{pstgraph}"
		],
		"luasnip": {"priority": 3000} 
	},


	"pstgraph[]()":{
		"prefix":"pstgraph",
		"body":[
			"\\begin{pstgraph}",
				"\t[${1:options%keyvals}](${2:x0},${3:y0})",
			"\\end{pstgraph}"
		],
		"luasnip": {"priority": 1000} 
	}
}
