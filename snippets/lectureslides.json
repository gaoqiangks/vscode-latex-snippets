{


	"course{}":{
		"prefix":"course",
		"body":[
			"\\course{${1:title}}"
		]
	},


	"lecture{}{}":{
		"prefix":"lecture",
		"body":[
			"\\lecture{${1:title}}{${2:PDF file%file}}"
		]
	},


	"lecturetitle{}":{
		"prefix":"lecturetitle",
		"body":[
			"\\lecturetitle{${1:title}}"
		]
	},


	"lectureslides{}":{
		"prefix":"lectureslides",
		"body":[
			"\\lectureslides{${1:PDF file%file}}"
		]
	},


	"tocline{}":{
		"prefix":"tocline",
		"body":[
			"\\tocline{${1:arg}}"
		]
	},


	"createdAt{}":{
		"prefix":"createdAt",
		"body":[
			"\\createdAt{${1:text}}"
		]
	},


	"orientation":{
		"prefix":"orientation",
		"body":[
			"\\orientation"
		]
	},


	"course[]{}":{
		"prefix":"course",
		"body":[
			"\\course[${2:footnote%text}]{${1:title}}"
		]
	},


	"lecture{}[]{}":{
		"prefix":"lecture",
		"body":[
			"\\lecture{${1:title}}[${3:start}]{${2:PDF file%file}}"
		]
	},


	"lecture{}[][]{}":{
		"prefix":"lecture",
		"body":[
			"\\lecture{${1:title}}[${3:start}][${4:end}]{${2:PDF file%file}}"
		]
	},


	"lectureslides[]{}":{
		"prefix":"lectureslides",
		"body":[
			"\\lectureslides[${2:start}]{${1:PDF file%file}}"
		]
	},


	"lectureslides[][]{}":{
		"prefix":"lectureslides",
		"body":[
			"\\lectureslides[${2:start}][${3:end}]{${1:PDF file%file}}"
		]
	}
}
