{


	"readhanja{}":{
		"prefix":"readhanja",
		"body":[
			"\\readhanja{${1:hanja}}"
		]
	},


	"readhanjaword{}":{
		"prefix":"readhanjaword",
		"body":[
			"\\readhanjaword{${1:hanja}}"
		]
	},


	"rwhanja{}":{
		"prefix":"rwhanja",
		"body":[
			"\\rwhanja{${1:hanja}}"
		]
	},


	"AssignReading{}{}":{
		"prefix":"AssignReading",
		"body":[
			"\\AssignReading{${1:hanja}}{${2:reading}}"
		]
	},


	"rpSetReading{}":{
		"prefix":"rpSetReading",
		"body":[
			"\\rpSetReading{${1:hanja=reading}}"
		]
	},


	"grrwhanja{}":{
		"prefix":"grrwhanja",
		"body":[
			"\\grrwhanja{${1:hanja}}"
		]
	},


	"rwhanjachar{}":{
		"prefix":"rwhanjachar",
		"body":[
			"\\rwhanjachar{${1:hanja}}"
		]
	},


	"rpRead{}":{
		"prefix":"rpRead",
		"body":[
			"\\rpRead{${1:options}}"
		]
	},


	"viewCodePoint{}":{
		"prefix":"viewCodePoint",
		"body":[
			"\\viewCodePoint{${1:unicode char}}"
		]
	},


	"showReadings{}":{
		"prefix":"showReadings",
		"body":[
			"\\showReadings{${1:hanja}}"
		]
	},


	"viewCodePoint*{}":{
		"prefix":"*viewCodePoint",
		"body":[
			"\\viewCodePoint*{${1:unicode char}}"
		]
	},


	"readhanja{}[]":{
		"prefix":"readhanja",
		"body":[
			"\\readhanja{${1:hanja}}[${2:number}]"
		]
	},


	"rpRead{}[]":{
		"prefix":"rpRead",
		"body":[
			"\\rpRead{${1:options}}[${2:hanja=reading}]"
		]
	}
}
