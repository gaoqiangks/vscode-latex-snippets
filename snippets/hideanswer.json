{


	"sethideanswer":{
		"prefix":"sethideanswer",
		"body":[
			"\\sethideanswer"
		]
	},


	"unsethideanswer":{
		"prefix":"unsethideanswer",
		"body":[
			"\\unsethideanswer"
		]
	},


	"hideanswer{}":{
		"prefix":"hideanswer",
		"body":[
			"\\hideanswer{${1:text}}"
		]
	},


	"smashanswer{}":{
		"prefix":"smashanswer",
		"body":[
			"\\smashanswer{${1:text}}"
		]
	},


	"hidegraphics{}":{
		"prefix":"hidegraphics",
		"body":[
			"\\hidegraphics{${1:imagefile}}"
		]
	},


	"smashgraphics{}":{
		"prefix":"smashgraphics",
		"body":[
			"\\smashgraphics{${1:imagefile}}"
		]
	},


	"switchanswer{}{}":{
		"prefix":"switchanswer",
		"body":[
			"\\switchanswer{${1:sethide-text%text}}{${2:unsethide-text%text}}"
		]
	},


	"hideanswer*{}":{
		"prefix":"*hideanswer",
		"body":[
			"\\hideanswer*{${1:text}}"
		]
	},


	"smashanswer*{}":{
		"prefix":"*smashanswer",
		"body":[
			"\\smashanswer*{${1:text}}"
		]
	},


	"hidegraphics*{}":{
		"prefix":"*hidegraphics",
		"body":[
			"\\hidegraphics*{${1:imagefile}}"
		]
	},


	"hidegraphics*[]{}":{
		"prefix":"*hidegraphics",
		"body":[
			"\\hidegraphics*[${2:options%keyvals}]{${1:imagefile}}"
		]
	},


	"smashgraphics*{}":{
		"prefix":"*smashgraphics",
		"body":[
			"\\smashgraphics*{${1:imagefile}}"
		]
	},


	"smashgraphics*[]{}":{
		"prefix":"*smashgraphics",
		"body":[
			"\\smashgraphics*[${2:options%keyvals}]{${1:imagefile}}"
		]
	},


	"switchanswer*{}{}":{
		"prefix":"*switchanswer",
		"body":[
			"\\switchanswer*{${1:unsethide-text%text}}{${2:sethide-text%text}}"
		]
	},


	"hidegraphics[]{}":{
		"prefix":"hidegraphics",
		"body":[
			"\\hidegraphics[${2:options%keyvals}]{${1:imagefile}}"
		]
	},


	"smashgraphics[]{}":{
		"prefix":"smashgraphics",
		"body":[
			"\\smashgraphics[${2:options%keyvals}]{${1:imagefile}}"
		]
	},


	"hideanswerdiv":{
		"prefix":"hideanswerdiv",
		"body":[
			"\\begin{hideanswerdiv}",
				"\t$1",
			"\\end{hideanswerdiv}"
		]
	},


	"smashanswerdiv":{
		"prefix":"smashanswerdiv",
		"body":[
			"\\begin{smashanswerdiv}",
				"\t$1",
			"\\end{smashanswerdiv}"
		]
	},


	"hideanswerdiv*":{
		"prefix":"*hideanswerdiv",
		"body":[
			"\\begin{hideanswerdiv*}",
				"\t$1",
			"\\end{hideanswerdiv*}"
		]
	},


	"smashanswerdiv*":{
		"prefix":"*smashanswerdiv",
		"body":[
			"\\begin{smashanswerdiv*}",
				"\t$1",
			"\\end{smashanswerdiv*}"
		]
	}
}
