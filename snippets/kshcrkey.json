{


	"hcrkey{}":{
		"prefix":"hcrkey",
		"body":[
			"\\hcrkey{${1:keyname%keyvals}}"
		]
	},


	"hcrkeyfontname":{
		"prefix":"hcrkeyfontname",
		"body":[
			"\\hcrkeyfontname"
		]
	},


	"hcrkeys[]{}":{
		"prefix":"hcrkeys",
		"body":[
			"\\hcrkeys[${2:separator}]{${1:key, ...%keyvals}}"
		]
	}
}
