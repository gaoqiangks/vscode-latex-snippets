{


	"newverbcommand{}{}{}":{
		"prefix":"newverbcommand",
		"body":[
			"\\newverbcommand{${1:cmd}}{${2:code before}}{${3:code after}}"
		]
	},


	"renewverbcommand{}{}{}":{
		"prefix":"renewverbcommand",
		"body":[
			"\\renewverbcommand{${1:cmd}}{${2:code before}}{${3:code after}}"
		]
