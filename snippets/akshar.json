{


	"aksharStrLen{}":{
		"prefix":"aksharStrLen",
		"body":[
			"\\aksharStrLen{${1:tokens}}"
		]
	},


	"aksharStrHead{}":{
		"prefix":"aksharStrHead",
		"body":[
			"\\aksharStrHead{${1:tokens}}"
		]
	},


	"aksharStrTail{}":{
		"prefix":"aksharStrTail",
		"body":[
			"\\aksharStrTail{${1:tokens}}"
		]
	},


	"aksharStrChar{}{}":{
		"prefix":"aksharStrChar",
		"body":[
			"\\aksharStrChar{${1:tokens}}{${2:n}}"
		]
	},


	"aksharStrReplace{}{}{}":{
		"prefix":"aksharStrReplace",
		"body":[
			"\\aksharStrReplace{${1:tokens1}}{${2:tokens2}}{${3:tokens3}}"
		]
	},


	"aksharStrRemove{}{}":{
		"prefix":"aksharStrRemove",
		"body":[
			"\\aksharStrRemove{${1:tokens1}}{${2:tokens2}}"
		]
	},


	"aksharStrReplace*{}{}{}":{
		"prefix":"*aksharStrReplace",
		"body":[
			"\\aksharStrReplace*{${1:tokens1}}{${2:tokens2}}{${3:tokens3}}"
		]
	},


	"aksharStrRemove*{}{}":{
		"prefix":"*aksharStrRemove",
		"body":[
			"\\aksharStrRemove*{${1:tokens1}}{${2:tokens2}}"
		]
	}
}
