{


	"zlabel{}":{
		"prefix":"zlabel",
		"body":[
			"\\zlabel{${1:label}}"
		]
	},


	"zkvlabel{}{}":{
		"prefix":"zkvlabel",
		"body":[
			"\\zkvlabel{${1:keyvals}}{${2:label}}"
		]
	},


	"zref{}":{
		"prefix":"zref",
		"body":[
			"\\zref{${1:label}}"
		]
	},


	"zpageref{}":{
		"prefix":"zpageref",
		"body":[
			"\\zpageref{${1:label}}"
		]
	},


	"zrefused{}":{
		"prefix":"zrefused",
		"body":[
			"\\zrefused{${1:label}}"
		]
	},


	"zref[]{}":{
		"prefix":"zref",
		"body":[
			"\\zref[${2:propname%keyvals}]{${1:label}}"
		]
	}
}
