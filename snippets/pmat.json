{


	"pmatcross":{
		"prefix":"pmatcross",
		"body":[
			"\\pmatcross"
		]
	},


	"pmatnocross":{
		"prefix":"pmatnocross",
		"body":[
			"\\pmatnocross"
		]
	},


	"pmatget":{
		"prefix":"pmatget",
		"body":[
			"\\pmatget{${1:parameter 0-6}}"
		]
	},


	"pmatset":{
		"prefix":"pmatset",
		"body":[
			"\\pmatset{${1:parameter 0-6}}{${2:value}}"
		]
	},


	"pmat":{
		"prefix":"pmat",
		"body":[
			"\\begin{pmat}",
			"\t$1",
			"\\end{pmat}"
		]
	}
}
