{


	"pmatcross":{
		"prefix":"pmatcross",
		"body":[
			"\\pmatcross"
		],
		"luasnip": {"priority": 3000} 
	},


	"pmatnocross":{
		"prefix":"pmatnocross",
		"body":[
			"\\pmatnocross"
		],
		"luasnip": {"priority": 3000} 
	},


	"pmatget{}":{
		"prefix":"pmatget",
		"body":[
			"\\pmatget{${1:parameter 0-6}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"pmatset{}{}":{
		"prefix":"pmatset",
		"body":[
			"\\pmatset{${1:parameter 0-6}}{${2:value}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"pmat{}{}{}":{
		"prefix":"pmat",
		"body":[
			"\\begin{pmat}",
				"\t{${1:ldelim}}{${2:vline-spec}}{${3:rdelim}}",
			"\\end{pmat}"
		],
		"luasnip": {"priority": 3000} 
	}
}
