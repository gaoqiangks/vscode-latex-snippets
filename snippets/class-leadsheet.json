		],
		"luasnip": {"priority": 3000} 
	},


	"prechorus[]":{
		"prefix":"prechorus",
		"body":[
			"\\begin{prechorus}",
				"\t[${1:options%keyvals}]",
			"\\end{prechorus}"
		],
		"luasnip": {"priority": 1000} 
	}
