{


	"zhlipsum":{
		"prefix":"zhlipsum",
		"body":[
			"\\zhlipsum"
		]
	},


	"newzhlipsum{}{}":{
		"prefix":"newzhlipsum",
		"body":[
			"\\newzhlipsum{${1:name}}{${2:paragraphs list%text}}"
		]
	},


	"zhlipsum*":{
		"prefix":"*zhlipsum",
		"body":[
			"\\zhlipsum*"
		]
	},


	"zhlipsum*[]":{
		"prefix":"*zhlipsum",
		"body":[
			"\\zhlipsum*[${1:paragraph}]"
		]
	},


	"zhlipsum*[][]":{
		"prefix":"*zhlipsum",
		"body":[
			"\\zhlipsum*[${1:paragraph}][${2:keyvals}]"
		]
	},


	"zhlipsum[]":{
		"prefix":"zhlipsum",
		"body":[
			"\\zhlipsum[${1:paragraph}]"
		]
	},


	"zhlipsum[][]":{
		"prefix":"zhlipsum",
		"body":[
			"\\zhlipsum[${1:paragraph}][${2:keyvals}]"
		]
	}
}
