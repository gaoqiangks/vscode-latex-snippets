{


	"SetDOIAPIMail{}":{
		"prefix":"SetDOIAPIMail",
		"body":[
			"\\SetDOIAPIMail{${1:email%URL}}"
		]
	},


	"SetDOIAPICacheExpiration{}":{
		"prefix":"SetDOIAPICacheExpiration",
		"body":[
			"\\SetDOIAPICacheExpiration{${1:days}}"
		]
	},


	"IsOpenaccess{}{}{}":{
		"prefix":"IsOpenaccess",
		"body":[
			"\\IsOpenaccess{${1:DOI}}{${2:true}}{${3:false}}"
		]
	},


	"GetOpenaccessURLWrapped{}":{
		"prefix":"GetOpenaccessURLWrapped",
		"body":[
			"\\GetOpenaccessURLWrapped{${1:DOI}}"
		]
	},


	"OpenaccessURLisDOI{}{}{}":{
		"prefix":"OpenaccessURLisDOI",
		"body":[
			"\\OpenaccessURLisDOI{${1:DOI}}{${2:true}}{${3:false}}"
		]
	}
}
