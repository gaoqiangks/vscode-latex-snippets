{


	"py":{
		"prefix":"py",
		"body":[
			"\\py{${1:code%definition}}"
		]
	},


	"pyq":{
		"prefix":"pyq",
		"body":[
			"\\pyq{${1:code%definition}}"
		]
	},


	"pyc":{
		"prefix":"pyc",
		"body":[
			"\\pyc{${1:code%definition}}"
		]
	},


	"pycq":{
		"prefix":"pycq",
		"body":[
			"\\pycq{${1:code%definition}}"
		]
	},


	"pyfile":{
		"prefix":"pyfile",
		"body":[
			"\\pyfile{${1:file}}"
		]
	},


	"pyfileq":{
		"prefix":"pyfileq",
		"body":[
			"\\pyfileq{${1:file}}"
		]
	},


	"pyfilerepl":{
		"prefix":"pyfilerepl",
		"body":[
			"\\pyfilerepl{${1:file}}"
		]
	},


	"pysession":{
		"prefix":"pysession",
		"body":[
			"\\pysession{${1:session}}"
		]
	},


	"pyoption":{
		"prefix":"pyoption",
		"body":[
			"\\pyoption{${1:option%keyvals}}{${2:value}}"
		]
	},


	"pyif":{
		"prefix":"pyif",
		"body":[
			"\\pyif{${1:test}}{${2:then clause}}{${3:else clause}}"
		]
	},


	"PyLTVerbatimEnv":{
		"prefix":"PyLTVerbatimEnv",
		"body":[
			"\\PyLTVerbatimEnv"
		]
	},


	"pythonq":{
		"prefix":"pythonq",
		"body":[
			"\\begin{pythonq}",
			"\t$1",
			"\\end{pythonq}"
		]
	},


	"pythonrepl":{
		"prefix":"pythonrepl",
		"body":[
			"\\begin{pythonrepl}",
			"\t$1",
			"\\end{pythonrepl}"
		]
	},


	"python":{
		"prefix":"python",
		"body":[
			"\\begin{python}",
			"\t$1",
			"\\end{python}"
		]
	}
}
