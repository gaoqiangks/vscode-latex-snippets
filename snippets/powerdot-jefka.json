{


	"titleslide{}":{
		"prefix":"titleslide",
		"body":[
			"\\begin{titleslide}",
				"\t{${1:title}}",
			"\\end{titleslide}"
		],
		"luasnip": {"priority": 3000} 
	},


	"titleslide[]{}":{
		"prefix":"titleslide",
		"body":[
			"\\begin{titleslide}",
				"\t[${2:options%keyvals}]{${1:title}}",
			"\\end{titleslide}"
		],
		"luasnip": {"priority": 1000} 
	},


