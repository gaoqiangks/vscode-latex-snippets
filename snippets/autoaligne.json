{


	"autoaligne{}":{
		"prefix":"autoaligne",
		"body":[
			"\\autoaligne{${1:eq1\\\\eq2\\\\...%formula}}"
		]
	},


	"egaldevantmembrevide":{
		"prefix":"egaldevantmembrevide",
		"body":[
			"\\egaldevantmembrevide"
		]
	},


	"aavcoeff{}":{
		"prefix":"aavcoeff",
		"body":[
			"\\aavcoeff{${1:value}}"
		]
	},


	"definirseparateurs{}{}{}":{
		"prefix":"definirseparateurs",
		"body":[
			"\\definirseparateurs{${1:sep_line}}{${2:sep_members}}{${3:sep_terms}}"
		]
	},


	"definirespacements{}{}":{
		"prefix":"definirespacements",
		"body":[
			"\\definirespacements{${1:space_members}}{${2:space_terms}}"
		]
	},


	"autoaligne[]{}":{
		"prefix":"autoaligne",
		"body":[
			"\\autoaligne[${2:hor_align}]{${1:eq1\\\\eq2\\\\...%formula}}"
		]
	},


	"autoaligne[](){}":{
		"prefix":"autoaligne",
		"body":[
			"\\autoaligne[${2:hor_align}](${3:vert_align}){${1:eq1\\\\eq2\\\\...%formula}}"
		]
	},


	"definirespacements[]{}{}":{
		"prefix":"definirespacements",
		"body":[
			"\\definirespacements[${3:dimen1,dimen2}]{${1:space_members}}{${2:space_terms}}"
		]
	}
}
