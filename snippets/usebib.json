{


	"bibinput":{
		"prefix":"bibinput",
		"body":[
			"\\bibinput{${1:bib file}}"
		]
	},


	"newbibfield":{
		"prefix":"newbibfield",
		"body":[
			"\\newbibfield{${1:field}}"
		]
	},


	"newbibignore":{
		"prefix":"newbibignore",
		"body":[
			"\\newbibignore{${1:type}}"
		]
	},


	"usebibentry":{
		"prefix":"usebibentry",
		"body":[
			"\\usebibentry{${1:citekey}}{${2:field%keyvals}}"
		]
	},


	"usebibentryurl":{
		"prefix":"usebibentryurl",
		"body":[
			"\\usebibentryurl{${1:citekey}}"
		]
	},


	"usebibentryurl":{
		"prefix":"usebibentryurl",
		"body":[
			"\\usebibentryurl[${2:delim}]{${1:citekey}}"
		]
	}
}
