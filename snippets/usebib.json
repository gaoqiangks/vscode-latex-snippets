{


	"bibinput{}":{
		"prefix":"bibinput",
		"body":[
			"\\bibinput{${1:bib file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"newbibfield{}":{
		"prefix":"newbibfield",
		"body":[
			"\\newbibfield{${1:field}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"newbibignore{}":{
		"prefix":"newbibignore",
		"body":[
			"\\newbibignore{${1:type}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"usebibentry{}{}":{
		"prefix":"usebibentry",
		"body":[
			"\\usebibentry{${1:citekey}}{${2:field%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"usebibentryurl{}":{
		"prefix":"usebibentryurl",
		"body":[
			"\\usebibentryurl{${1:citekey}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"usebibentryurl[]{}":{
		"prefix":"usebibentryurl",
		"body":[
			"\\usebibentryurl[${2:delim}]{${1:citekey}}"
		],
		"luasnip": {"priority": 1000} 
	}
}
