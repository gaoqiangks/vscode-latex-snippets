{


	"pstr{}":{
		"prefix":"pstr",
		"body":[
			"\\pstr{${1:string spec%formula}}"
		]
	},


	"pstrSetLabelStyle{}":{
		"prefix":"pstrSetLabelStyle",
		"body":[
			"\\pstrSetLabelStyle{${1:style cmds}}"
		]
	},


	"pstrSetArrowColor{}":{
		"prefix":"pstrSetArrowColor",
		"body":[
			"\\pstrSetArrowColor{${1:color}}"
		]
	},


	"pstrSetArrowLineWidth{}":{
		"prefix":"pstrSetArrowLineWidth",
		"body":[
			"\\pstrSetArrowLineWidth{${1:width}}"
		]
	},


	"pstrSetArrowAngle{}":{
		"prefix":"pstrSetArrowAngle",
		"body":[
			"\\pstrSetArrowAngle{${1:angle}}"
		]
	},


	"pstrSetArrowLabel{}":{
		"prefix":"pstrSetArrowLabel",
		"body":[
			"\\pstrSetArrowLabel{${1:label}}"
		]
	},


	"pstrSetArrowLineStyle{}":{
		"prefix":"pstrSetArrowLineStyle",
		"body":[
			"\\pstrSetArrowLineStyle{${1:line style%keyvals}}"
		]
	},


	"Pstr{}":{
		"prefix":"Pstr",
		"body":[
			"\\Pstr{${1:string spec%formula}}"
		]
	},


	"nd":{
		"prefix":"nd",
		"body":[
			"\\nd"
		]
	},


	"arrow{}{}{}{}{}{}":{
		"prefix":"arrow",
		"body":[
			"\\arrow{${1:source}}{${2:target}}{${3:angle}}{${4:label}}{${5:linecolor}}{${6:linestyle}}"
		]
	},


	"txt{}":{
		"prefix":"txt",
		"body":[
			"\\txt{${1:text%plain}}"
		]
	},


	"pstr[]{}":{
		"prefix":"pstr",
		"body":[
			"\\pstr[${2:raise%l}]{${1:string spec%formula}}"
		]
	},


	"pstr[][]{}":{
		"prefix":"pstr",
		"body":[
			"\\pstr[${2:raise%l}][${3:nodesep%l}]{${1:string spec%formula}}"
		]
	},


	"Pstr[]{}":{
		"prefix":"Pstr",
		"body":[
			"\\Pstr[${2:raise%l}]{${1:string spec%formula}}"
		]
	},


	"Pstr[][]{}":{
		"prefix":"Pstr",
		"body":[
			"\\Pstr[${2:raise%l}][${3:nodesep%l}]{${1:string spec%formula}}"
		]
	}
}
