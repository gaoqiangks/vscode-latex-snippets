{


	"SetBNFLayout{}":{
		"prefix":"SetBNFLayout",
		"body":[
			"\\SetBNFLayout{${1:tblr options}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"SetBNFConfig{}":{
		"prefix":"SetBNFConfig",
		"body":[
			"\\SetBNFConfig{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"bnf":{
		"prefix":"bnf",
		"body":[
			"\\begin{bnf}",
				"\t$1",
			"\\end{bnf}"
		],
		"luasnip": {"priority": 3000} 
	},


	"bnf[]":{
		"prefix":"bnf",
		"body":[
			"\\begin{bnf}",
				"\t[${1:tblr options}]",
			"\\end{bnf}"
		],
		"luasnip": {"priority": 1000} 
	},


	"bnf()":{
		"prefix":"bnf",
		"body":[
			"\\begin{bnf}",
				"\t(${1:options%keyvals})",
			"\\end{bnf}"
		],
		"luasnip": {"priority": 3000} 
	},


	"bnf()[]":{
		"prefix":"bnf",
		"body":[
			"\\begin{bnf}",
				"\t(${2:options%keyvals})[${1:tblr options}]",
			"\\end{bnf}"
		],
		"luasnip": {"priority": 1000} 
	}
}
