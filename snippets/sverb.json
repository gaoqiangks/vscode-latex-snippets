{


	"listingsize":{
		"prefix":"listingsize",
		"body":[
			"\\listingsize"
		],
		"luasnip": {"priority": 3000} 
	},


	"listingindent":{
		"prefix":"listingindent",
		"body":[
			"\\listingindent"
		],
		"luasnip": {"priority": 3000} 
	},


	"verbinput{}":{
		"prefix":"verbinput",
		"body":[
			"\\verbinput{${1:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"matcher{}{}{}":{
		"prefix":"matcher",
		"body":[
			"\\matcher{${1:cmd}}{${2:target}}{${3:process-cmd}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"listing":{
		"prefix":"listing",
		"body":[
			"\\begin{listing}",
				"\t$1",
			"\\end{listing}"
		],
		"luasnip": {"priority": 3000} 
	},


	"listing*{}":{
		"prefix":"*listing",
		"body":[
			"\\begin{listing*}",
				"\t{${1:end string}}",
			"\\end{listing*}"
		],
		"luasnip": {"priority": 2000} 
	},


	"verbwrite{}":{
		"prefix":"verbwrite",
		"body":[
			"\\begin{verbwrite}",
				"\t{${1:file}}",
			"\\end{verbwrite}"
		],
		"luasnip": {"priority": 3000} 
	},


	"demo{}":{
		"prefix":"demo",
		"body":[
			"\\begin{demo}",
				"\t{${1:title}}",
			"\\end{demo}"
		],
		"luasnip": {"priority": 3000} 
	},


	"demo[]{}":{
		"prefix":"demo",
		"body":[
			"\\begin{demo}",
				"\t[${2:shape%keyvals}]{${1:title}}",
			"\\end{demo}"
		],
		"luasnip": {"priority": 1000} 
	},


	"ignore":{
		"prefix":"ignore",
		"body":[
			"\\begin{ignore}",
				"\t$1",
			"\\end{ignore}"
		],
		"luasnip": {"priority": 3000} 
	}
}
