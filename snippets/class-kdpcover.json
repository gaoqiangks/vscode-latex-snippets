{


	"putSpine{}":{
		"prefix":"putSpine",
		"body":[
			"\\putSpine{${1:text}}"
		]
	},


	"putPicture{}":{
		"prefix":"putPicture",
		"body":[
			"\\putPicture{${1:imagefile}}"
		]
	},


	"putVolume{}":{
		"prefix":"putVolume",
		"body":[
			"\\putVolume{${1:number}}"
		]
	},


	"putPrice{}":{
		"prefix":"putPrice",
		"body":[
			"\\putPrice{${1:price}}"
		]
	},


	"putBack{}":{
		"prefix":"putBack",
		"body":[
			"\\putBack{${1:text}}"
		]
	},


	"putTitle{}":{
		"prefix":"putTitle",
		"body":[
			"\\putTitle{${1:text}}"
		]
	},


	"putAuthor{}":{
		"prefix":"putAuthor",
		"body":[
			"\\putAuthor{${1:name}}"
		]
	},


	"putTLDR{}":{
		"prefix":"putTLDR",
		"body":[
			"\\putTLDR{${1:text}}"
		]
	},


	"putVersion{}":{
		"prefix":"putVersion",
		"body":[
			"\\putVersion{${1:version}}"
		]
	},


	"putCopyright{}{}":{
		"prefix":"putCopyright",
		"body":[
			"\\putCopyright{${1:year}}{${2:name}}"
		]
	},


	"putSpine[]{}":{
		"prefix":"putSpine",
		"body":[
			"\\putSpine[${2:imagefile}]{${1:text}}"
		]
	}
}
