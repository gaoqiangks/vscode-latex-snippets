{


	"Substitute":{
		"prefix":"Substitute",
		"body":[
			"\\Substitute{${1:input}}{${2:from}}{${3:to}}"
		]
	},


	"Substitute*":{
		"prefix":"Substitute*",
		"body":[
			"\\Substitute*{${1:input}}{${2:from}}{${3:to}}"
		]
	},


	"Substitute":{
		"prefix":"Substitute",
		"body":[
			"\\Substitute[${4:output}]{${1:input}}{${2:from}}{${3:to}}"
		]
	},


	"Substitute*":{
		"prefix":"Substitute*",
		"body":[
			"\\Substitute*[${4:output}]{${1:input}}{${2:from}}{${3:to}}"
		]
	},


	"ShowTokens":{
		"prefix":"ShowTokens",
		"body":[
			"\\ShowTokens{${1:token list}}"
		]
	},


	"ShowTokens*":{
		"prefix":"ShowTokens*",
		"body":[
			"\\ShowTokens*{${1:token list}}"
		]
	},


	"ShowTokensLogonly":{
		"prefix":"ShowTokensLogonly",
		"body":[
			"\\ShowTokensLogonly"
		]
	},


	"ShowTokensOnline":{
		"prefix":"ShowTokensOnline",
		"body":[
			"\\ShowTokensOnline"
		]
	}
}
