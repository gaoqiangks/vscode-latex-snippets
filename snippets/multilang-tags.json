{


	"SetTagFilter{}":{
		"prefix":"SetTagFilter",
		"body":[
			"\\SetTagFilter{${1:policy}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"SetTagFilter[]{}":{
		"prefix":"SetTagFilter",
		"body":[
			"\\SetTagFilter[${2:default}]{${1:policy}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"DefineTagFilter{}{}{}":{
		"prefix":"DefineTagFilter",
		"body":[
			"\\DefineTagFilter{${1:name}}{${2:default}}{${3:policy}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"UseTagFilter{}":{
		"prefix":"UseTagFilter",
		"body":[
			"\\UseTagFilter{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
