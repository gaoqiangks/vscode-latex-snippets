{


	"SetTagFilter":{
		"prefix":"SetTagFilter",
		"body":[
			"\\SetTagFilter{${1:policy}}"
		]
	},


	"SetTagFilter":{
		"prefix":"SetTagFilter",
		"body":[
			"\\SetTagFilter[${2:default}]{${1:policy}}"
		]
	},


	"DefineTagFilter":{
		"prefix":"DefineTagFilter",
		"body":[
			"\\DefineTagFilter{${1:name}}{${2:default}}{${3:policy}}"
		]
	},


	"UseTagFilter":{
		"prefix":"UseTagFilter",
		"body":[
			"\\UseTagFilter{${1:name}}"
		]
	}
}
