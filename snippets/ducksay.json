{


	"DefaultAnimal{}":{
		"prefix":"DefaultAnimal",
		"body":[
			"\\DefaultAnimal{${1:animal%keyvals}}"
		]
	},


	"DucksayOptions{}":{
		"prefix":"DucksayOptions",
		"body":[
			"\\DucksayOptions{${1:keyvals}}"
		]
	},


	"AnimalOptions{}{}":{
		"prefix":"AnimalOptions",
		"body":[
			"\\AnimalOptions{${1:animal}}{${2:options%keyvals}}"
		]
	},


	"ducksay{}":{
		"prefix":"ducksay",
		"body":[
			"\\ducksay{${1:message}}"
		]
	},


	"duckthink{}":{
		"prefix":"duckthink",
		"body":[
			"\\duckthink{${1:message}}"
		]
	},


	"AnimalOptions*{}{}":{
		"prefix":"*AnimalOptions",
		"body":[
			"\\AnimalOptions*{${1:animal}}{${2:options%keyvals}}"
		]
	},


	"ducksay[]{}":{
		"prefix":"ducksay",
		"body":[
			"\\ducksay[${2:options%keyvals}]{${1:message}}"
		]
	},


	"duckthink[]{}":{
		"prefix":"duckthink",
		"body":[
			"\\duckthink[${2:options%keyvals}]{${1:message}}"
		]
	}
}
