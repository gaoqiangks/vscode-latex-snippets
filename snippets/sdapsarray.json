{


	"sdapsnested{}":{
		"prefix":"sdapsnested",
		"body":[
			"\\sdapsnested{${1:content%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"sdapsarray":{
		"prefix":"sdapsarray",
		"body":[
			"\\begin{sdapsarray}",
				"\t$1",
			"\\end{sdapsarray}"
		],
		"luasnip": {"priority": 3000} 
	},


	"sdapsarray[]":{
		"prefix":"sdapsarray",
		"body":[
			"\\begin{sdapsarray}",
				"\t[${1:options%keyvals}]",
			"\\end{sdapsarray}"
		],
		"luasnip": {"priority": 1000} 
	}
}
