{


	"parshapelenout":{
		"prefix":"parshapelenout",
		"body":[
			"\\parshapelenout{${1:length}}"
		]
	},


	"parshapearrlenout":{
		"prefix":"parshapearrlenout",
		"body":[
			"\\parshapearrlenout{${1:array}}{${2:counter}}"
		]
	},


	"parshapeary":{
		"prefix":"parshapeary",
		"body":[
			"\\parshapeary{${1:array}}"
		]
	},


	"parshapearray":{
		"prefix":"parshapearray",
		"body":[
			"\\parshapearray{${1:array}}{${2:width}}"
		]
	},


	"Parshapearray":{
		"prefix":"Parshapearray",
		"body":[
			"\\Parshapearray{${1:array1}}{${2:array2}}"
		]
	},


	"parshapecollect":{
		"prefix":"parshapecollect",
		"body":[
			"\\begin{parshapecollect}",
			"\t$1",
			"\\end{parshapecollect}"
		]
	}
}
