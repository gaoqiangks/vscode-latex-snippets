{


	"py":{
		"prefix":"py",
		"body":[
			"\\py{${1:python expr%definition}}"
		]
	},


	"pyc":{
		"prefix":"pyc",
		"body":[
			"\\pyc{${1:python code%definition}}"
		]
	},


	"pycq":{
		"prefix":"pycq",
		"body":[
			"\\pycq{${1:python code%definition}}"
		]
	},


	"pyfile":{
		"prefix":"pyfile",
		"body":[
			"\\pyfile{${1:file}}"
		]
	},


	"pythonimmediatecontinue":{
		"prefix":"pythonimmediatecontinue",
		"body":[
			"\\pythonimmediatecontinue{${1:code}}"
		]
	},


	"pycodeq":{
		"prefix":"pycodeq",
		"body":[
			"\\begin{pycodeq}",
			"\t$1",
			"\\end{pycodeq}"
		]
	},


	"pycode":{
		"prefix":"pycode",
		"body":[
			"\\begin{pycode}",
			"\t$1",
			"\\end{pycode}"
		]
	}
}
