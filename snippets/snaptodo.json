{


	"snaptodo{}":{
		"prefix":"snaptodo",
		"body":[
			"\\snaptodo{${1:text%todo}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"snaptodo[]{}":{
		"prefix":"snaptodo",
		"body":[
			"\\snaptodo[${2:options%keyvals}]{${1:text%todo}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"snaptodoset{}":{
		"prefix":"snaptodoset",
		"body":[
			"\\snaptodoset{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
