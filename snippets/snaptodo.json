{


	"snaptodo{}":{
		"prefix":"snaptodo",
		"body":[
			"\\snaptodo{${1:text%todo}}"
		]
	},


	"snaptodoset{}":{
		"prefix":"snaptodoset",
		"body":[
			"\\snaptodoset{${1:options%keyvals}}"
		]
	},


	"snaptodo[]{}":{
		"prefix":"snaptodo",
		"body":[
			"\\snaptodo[${2:options%keyvals}]{${1:text%todo}}"
		]
	}
}
