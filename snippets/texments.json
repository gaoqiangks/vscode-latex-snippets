{


	"usestyle{}":{
		"prefix":"usestyle",
		"body":[
			"\\usestyle{${1:pygments style}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"pygment{}{}":{
		"prefix":"pygment",
		"body":[
			"\\pygment{${1:language}}{${2:code%definition}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"includecode{}":{
		"prefix":"includecode",
		"body":[
			"\\includecode{${1:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"includecode[]{}":{
		"prefix":"includecode",
		"body":[
			"\\includecode[${2:language}]{${1:file}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"proglang":{
		"prefix":"proglang",
		"body":[
			"\\proglang"
		],
		"luasnip": {"priority": 3000} 
	},


	"lexercommand{}":{
		"prefix":"lexercommand",
		"body":[
			"\\lexercommand{${1:arg}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"pygmented{}":{
		"prefix":"pygmented",
		"body":[
			"\\begin{pygmented}",
				"\t{${1:language}}",
			"\\end{pygmented}"
		],
		"luasnip": {"priority": 3000} 
	}
}
