{


	"usestyle":{
		"prefix":"usestyle",
		"body":[
			"\\usestyle{${1:pygments style}}"
		]
	},


	"pygment":{
		"prefix":"pygment",
		"body":[
			"\\pygment{${1:language}}{${2:code%definition}}"
		]
	},


	"includecode":{
		"prefix":"includecode",
		"body":[
			"\\includecode{${1:file}}"
		]
	},


	"includecode":{
		"prefix":"includecode",
		"body":[
			"\\includecode[${2:language}]{${1:file}}"
		]
	},


	"proglang":{
		"prefix":"proglang",
		"body":[
			"\\proglang"
		]
	},


	"lexercommand":{
		"prefix":"lexercommand",
		"body":[
			"\\lexercommand{${1:arg}}"
		]
	},


	"pygmented":{
		"prefix":"pygmented",
		"body":[
			"\\begin{pygmented}",
			"\t$1",
			"\\end{pygmented}"
		]
	}
}
