{


	"diagbox":{
		"prefix":"diagbox",
		"body":[
			"\\diagbox{${1:left%text}}{${2:right%text}}"
		]
	},


	"diagbox":{
		"prefix":"diagbox",
		"body":[
			"\\diagbox[${3:options%keyvals}]{${1:left%text}}{${2:right%text}}"
		]
	},


	"diagbox":{
		"prefix":"diagbox",
		"body":[
			"\\diagbox{${1:left%text}}{${2:middle%text}}{${3:right%text}}"
		]
	},


	"backslashbox":{
		"prefix":"backslashbox",
		"body":[
			"\\backslashbox{${1:left%text}}{${2:right%text}}"
		]
	},


	"backslashbox":{
		"prefix":"backslashbox",
		"body":[
			"\\backslashbox[${3:width}]{${1:left%text}}{${2:right%text}}"
		]
	},


	"backslashbox":{
		"prefix":"backslashbox",
		"body":[
			"\\backslashbox[${3:width}][${4:trim%keyvals}]{${1:left%text}}{${2:right%text}}"
		]
	},


	"slashbox":{
		"prefix":"slashbox",
		"body":[
			"\\slashbox{${1:left%text}}{${2:right%text}}"
		]
	},


	"slashbox":{
		"prefix":"slashbox",
		"body":[
			"\\slashbox[${3:width}]{${1:left%text}}{${2:right%text}}"
		]
	},


	"slashbox":{
		"prefix":"slashbox",
		"body":[
			"\\slashbox[${3:width}][${4:trim%keyvals}]{${1:left%text}}{${2:right%text}}"
		]
	}
