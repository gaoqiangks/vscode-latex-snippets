{


	"factor{}{}{}{}":{
		"prefix":"factor",
		"body":[
			"\\factor{${1:name}}{${2:caption%text}}{${3:inputs}}{${4:outputs}}"
		]
	},


	"plate{}{}{}":{
		"prefix":"plate",
		"body":[
			"\\plate{${1:name}}{${2:fitlist}}{${3:caption%text}}"
		]
	},


	"gate{}{}{}":{
		"prefix":"gate",
		"body":[
			"\\gate{${1:name}}{${2:fitlist}}{${3:inputs}}"
		]
	},


	"vgate{}{}{}{}{}{}":{
		"prefix":"vgate",
		"body":[
			"\\vgate{${1:name}}{${2:fitlist-left}}{${3:caption-left%text}}{${4:fitlist-right}}{${5:caption-right%text}}{${6:inputs}}"
		]
	},


	"hgate{}{}{}{}{}{}":{
		"prefix":"hgate",
		"body":[
			"\\hgate{${1:name}}{${2:fitlist-top}}{${3:caption-top%text}}{${4:fitlist-bottom}}{${5:caption-bottom%text}}{${6:inputs}}"
		]
	},


	"edge{}{}":{
		"prefix":"edge",
		"body":[
			"\\edge{${1:inputs}}{${2:outputs}}"
		]
	},


	"factoredge{}{}{}":{
		"prefix":"factoredge",
		"body":[
			"\\factoredge{${1:inputs}}{${2:factors}}{${3:outputs}}"
		]
	},


	"factor[]{}{}{}{}":{
		"prefix":"factor",
		"body":[
			"\\factor[${5:TikZ keys}]{${1:name}}{${2:caption%text}}{${3:inputs}}{${4:outputs}}"
		]
	},


