		]
	},


	"gate[]{}{}{}":{
		"prefix":"gate",
		"body":[
			"\\gate[${4:TikZ keys}]{${1:name}}{${2:fitlist}}{${3:inputs}}"
		]
	},


	"vgate[]{}{}{}{}{}{}":{
		"prefix":"vgate",
		"body":[
			"\\vgate[${7:TikZ keys}]{${1:name}}{${2:fitlist-left}}{${3:caption-left%text}}{${4:fitlist-right}}{${5:caption-right%text}}{${6:inputs}}"
