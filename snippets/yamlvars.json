{


	"yv{}":{
		"prefix":"yv",
		"body":[
			"\\yv{${1:var}}"
		]
	},


	"declareYAMLvarsFile{}":{
		"prefix":"declareYAMLvarsFile",
		"body":[
			"\\declareYAMLvarsFile{${1:file}}"
		]
	},


	"parseYAMLvarsFile{}":{
		"prefix":"parseYAMLvarsFile",
		"body":[
			"\\parseYAMLvarsFile{${1:file}}"
		]
	},


	"resetYAMLvarsspec":{
		"prefix":"resetYAMLvarsspec",
		"body":[
			"\\resetYAMLvarsspec"
		]
	},


	"AllowUndeclaredYV":{
		"prefix":"AllowUndeclaredYV",
		"body":[
			"\\AllowUndeclaredYV"
		]
	},


	"ForbidUndeclaredYV":{
		"prefix":"ForbidUndeclaredYV",
		"body":[
			"\\ForbidUndeclaredYV"
		]
	},


	"lowercasevarYVon":{
		"prefix":"lowercasevarYVon",
		"body":[
			"\\lowercasevarYVon"
		]
	},


	"lowercasevarYVoff":{
		"prefix":"lowercasevarYVoff",
		"body":[
			"\\lowercasevarYVoff"
		]
	},


	"declareYAMLvars":{
		"prefix":"declareYAMLvars",
		"body":[
			"\\begin{declareYAMLvars}",
				"\t$1",
			"\\end{declareYAMLvars}"
		]
	},


	"parseYAMLvars":{
		"prefix":"parseYAMLvars",
		"body":[
			"\\begin{parseYAMLvars}",
				"\t$1",
			"\\end{parseYAMLvars}"
		]
	},


	"parseYAMLpdfdata":{
		"prefix":"parseYAMLpdfdata",
		"body":[
			"\\begin{parseYAMLpdfdata}",
				"\t$1",
			"\\end{parseYAMLpdfdata}"
		]
	}
}
