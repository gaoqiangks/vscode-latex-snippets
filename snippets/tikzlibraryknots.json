{


	"flipcrossings{}":{
		"prefix":"flipcrossings",
		"body":[
			"\\flipcrossings{${1:list of crossings}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"redraw{}{}":{
		"prefix":"redraw",
		"body":[
			"\\redraw{${1:strand number}}{${2:point}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"knot":{
		"prefix":"knot",
		"body":[
			"\\begin{knot}",
				"\t$1",
			"\\end{knot}"
		],
		"luasnip": {"priority": 3000} 
	},


	"knot[]":{
		"prefix":"knot",
		"body":[
			"\\begin{knot}",
				"\t[${1:options%keyvals}]",
			"\\end{knot}"
		],
		"luasnip": {"priority": 1000} 
	}
}
