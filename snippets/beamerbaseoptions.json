{


	"ProcessOptionsBeamer":{
		"prefix":"ProcessOptionsBeamer",
		"body":[
			"\\ProcessOptionsBeamer"
		]
	},


	"ExecuteOptionsBeamer":{
		"prefix":"ExecuteOptionsBeamer",
		"body":[
			"\\ExecuteOptionsBeamer{${1:options}}"
		]
	},


	"DeclareOptionBeamer":{
		"prefix":"DeclareOptionBeamer",
		"body":[
			"\\DeclareOptionBeamer{${1:option}}{${2:definition}}"
		]
	},


	"DeclareOptionBeamer":{
		"prefix":"DeclareOptionBeamer",
		"body":[
			"\\DeclareOptionBeamer{${1:option}}[${3:default}]{${2:definition}}"
		]
	},


	"defbeameroption":{
		"prefix":"defbeameroption",
		"body":[
			"\\defbeameroption{${1:option%specialDef}}{${2:definition}}"
		]
	},


	"setbeameroption":{
		"prefix":"setbeameroption",
		"body":[
			"\\setbeameroption{${1:options%keyvals}}"
		]
	}
}
