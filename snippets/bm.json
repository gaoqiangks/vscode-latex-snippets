{


	"bm{}":{
		"prefix":"bm",
		"body":[
			"\\bm{${1:math expression}}"
		]
	},


	"hm{}":{
		"prefix":"hm",
		"body":[
			"\\hm{${1:math expression}}"
		]
	},


	"bmdefine{}{}":{
		"prefix":"bmdefine",
		"body":[
			"\\bmdefine{${1:cmd}}{${2:def}}"
		]
	},


	"hmdefine{}{}":{
		"prefix":"hmdefine",
		"body":[
			"\\hmdefine{${1:cmd}}{${2:def}}"
		]
	},


	"boldsymbol{}":{
		"prefix":"boldsymbol",
		"body":[
			"\\boldsymbol{${1:math expression}}"
		]
	},


	"heavysymbol{}":{
		"prefix":"heavysymbol",
		"body":[
			"\\heavysymbol{${1:math expression}}"
		]
	},


	"DeclareBoldMathCommand{}{}":{
		"prefix":"DeclareBoldMathCommand",
		"body":[
			"\\DeclareBoldMathCommand{${1:cmd}}{${2:def}}"
		]
	},


	"bmmax":{
		"prefix":"bmmax",
		"body":[
			"\\bmmax"
		]
	},


	"hmmax":{
		"prefix":"hmmax",
		"body":[
			"\\hmmax"
		]
	},


	"DeclareBoldMathCommand[]{}{}":{
		"prefix":"DeclareBoldMathCommand",
		"body":[
			"\\DeclareBoldMathCommand[${3:math version%keyvals}]{${1:cmd}}{${2:def}}"
		]
	}
}
