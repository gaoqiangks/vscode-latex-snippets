{


	"ksruby{}{}":{
		"prefix":"ksruby",
		"body":[
			"\\ksruby{${1:below}}{${2:above}}"
		]
	},


	"ruby{}{}":{
		"prefix":"ruby",
		"body":[
			"\\ruby{${1:below}}{${2:above}}"
		]
	},


	"ksrubycenterdefault{}":{
		"prefix":"ksrubycenterdefault",
		"body":[
			"\\ksrubycenterdefault{${1:options}}"
		]
	},


	"ksrubyheightdefault":{
		"prefix":"ksrubyheightdefault",
		"body":[
			"\\ksrubyheightdefault"
		]
	},


	"ksrubywidthdefault":{
		"prefix":"ksrubywidthdefault",
		"body":[
			"\\ksrubywidthdefault"
		]
	},


	"ksrubyeachchardefault":{
		"prefix":"ksrubyeachchardefault",
		"body":[
			"\\ksrubyeachchardefault"
		]
	},


	"ksrubysep{}":{
		"prefix":"ksrubysep",
		"body":[
			"\\ksrubysep{${1:dim%l}}"
		]
	},


	"ksrubysize{}":{
		"prefix":"ksrubysize",
		"body":[
			"\\ksrubysize{${1:size}}"
		]
	},


	"ksrubyextra{}":{
		"prefix":"ksrubyextra",
		"body":[
			"\\ksrubyextra{${1:cmds}}"
		]
	},


	"ksruby*{}{}":{
		"prefix":"*ksruby",
		"body":[
			"\\ksruby*{${1:below}}{${2:above}}"
		]
	},


	"ruby*{}{}":{
		"prefix":"*ruby",
		"body":[
			"\\ruby*{${1:below}}{${2:above}}"
		]
	},


	"ksrubyheightdefault[]":{
		"prefix":"ksrubyheightdefault",
		"body":[
			"\\ksrubyheightdefault[${1:false}]"
		]
	},


	"ksrubywidthdefault[]":{
		"prefix":"ksrubywidthdefault",
		"body":[
			"\\ksrubywidthdefault[${1:false}]"
		]
	},


	"ksrubyeachchardefault[]":{
		"prefix":"ksrubyeachchardefault",
		"body":[
			"\\ksrubyeachchardefault[${1:false}]"
		]
	}
}
