{


	"NewSpotColorSpace{}":{
		"prefix":"NewSpotColorSpace",
		"body":[
			"\\NewSpotColorSpace{${1:name}}"
		]
	},


	"AddSpotColor{}{}{}{}":{
		"prefix":"AddSpotColor",
		"body":[
			"\\AddSpotColor{${1:colorspace}}{${2:color}}{${3:pdf name}}{${4:specification}}"
		]
	},


	"SpotSpace":{
		"prefix":"SpotSpace",
		"body":[
			"\\SpotSpace"
		]
	},


	"SetPageColorSpace{}":{
		"prefix":"SetPageColorSpace",
		"body":[
			"\\SetPageColorSpace{${1:colorspace}}"
		]
	},


	"SpotColor{}{}":{
		"prefix":"SpotColor",
		"body":[
			"\\SpotColor{${1:color}}{${2:number}}"
		]
	},


	"SetPageColorResource{}":{
		"prefix":"SetPageColorResource",
		"body":[
			"\\SetPageColorResource{${1:resource}}"
		]
	}
}
