{


	"tensordelimiter":{
		"prefix":"tensordelimiter",
		"body":[
			"\\tensordelimiter{${1:symbol}}"
		]
	},


	"tensorformat":{
		"prefix":"tensorformat",
		"body":[
			"\\tensorformat{${1:format}}"
		]
	},


	"indexdot":{
		"prefix":"indexdot",
		"body":[
			"\\indexdot"
		]
	},


	"whenindex":{
		"prefix":"whenindex",
		"body":[
			"\\whenindex{${1:index}}{${2:new-index}}{${3:commands}}"
		]
	},


	"tensor":{
		"prefix":"tensor",
		"body":[
			"\\begin{tensor}",
			"\t$1",
			"\\end{tensor}"
		]
	}
}
