	},


	"tensorformat{}":{
		"prefix":"tensorformat",
		"body":[
			"\\tensorformat{${1:format}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"indexdot":{
		"prefix":"indexdot",
		"body":[
			"\\indexdot"
		],
		"luasnip": {"priority": 3000} 
	},


	"whenindex{}{}{}":{
		"prefix":"whenindex",
