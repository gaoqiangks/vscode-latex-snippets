{


	"embedsource":{
		"prefix":"embedsource",
		"body":[
			"\\embedsource"
		]
	},


	"embedinput{}":{
		"prefix":"embedinput",
		"body":[
			"\\embedinput{${1:file}}"
		]
	},


	"embedsource[]":{
		"prefix":"embedsource",
		"body":[
			"\\embedsource[${1:options%keyvals}]"
		]
	},


	"embedinput[]{}":{
		"prefix":"embedinput",
		"body":[
			"\\embedinput[${2:options%keyvals}]{${1:file}}"
		]
	}
}
