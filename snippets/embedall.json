{


	"embedsource":{
		"prefix":"embedsource",
		"body":[
			"\\embedsource"
		],
		"luasnip": {"priority": 3000} 
	},


	"embedsource[]":{
		"prefix":"embedsource",
		"body":[
			"\\embedsource[${1:options%keyvals}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"embedinput{}":{
		"prefix":"embedinput",
		"body":[
			"\\embedinput{${1:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"embedinput[]{}":{
		"prefix":"embedinput",
		"body":[
			"\\embedinput[${2:options%keyvals}]{${1:file}}"
		],
		"luasnip": {"priority": 1000} 
	}
}
