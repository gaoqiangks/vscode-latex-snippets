{


	"blackcase{}{}":{
		"prefix":"blackcase",
		"body":[
			"\\blackcase{${1:x}}{${2:y}}"
		]
	},


	"blackcases{}":{
		"prefix":"blackcases",
		"body":[
			"\\blackcases{${1:x1/y1,x2/y2,...}}"
		]
	},


	"word{}{}{}":{
		"prefix":"word",
		"body":[
			"\\word{${1:x}}{${2:y}}{${3:MOT}}"
		]
	},


	"words{}":{
		"prefix":"words",
		"body":[
			"\\words{${1:x1/y1/MOT1,x2/y2.MOT2,...}}"
		]
	},


	"gridcross{}":{
		"prefix":"gridcross",
		"body":[
			"\\gridcross{${1:MOT1*MOT2,MOT3*MOT4,...}}"
		]
	},


	"printDef{}{}":{
		"prefix":"printDef",
		"body":[
			"\\printDef{${1:h|v}}{${2:la définition%text}}"
		]
	},


	"symbsep":{
		"prefix":"symbsep",
		"body":[
			"\\symbsep"
		]
	},


	"symbnext":{
		"prefix":"symbnext",
		"body":[
			"\\symbnext"
		]
	},


	"cRM{}":{
		"prefix":"cRM",
		"body":[
			"\\cRM{${1:text}}"
		]
	},


	"newlist":{
		"prefix":"newlist",
		"body":[
			"\\newlist"
		]
	},


	"theL":{
		"prefix":"theL",
		"body":[
			"\\theL"
		]
	},


	"thecntdef":{
		"prefix":"thecntdef",
		"body":[
			"\\thecntdef"
		]
	},


	"word[]{}{}{}":{
		"prefix":"word",
		"body":[
			"\\word[${4:options%keyvals}]{${1:x}}{${2:y}}{${3:MOT}}"
		]
	},


	"words[]{}":{
		"prefix":"words",
		"body":[
			"\\words[${2:options%keyvals}]{${1:x1/y1/MOT1,x2/y2.MOT2,...}}"
		]
	},


	"printDef[]{}{}":{
		"prefix":"printDef",
		"body":[
			"\\printDef[${3:s}]{${1:h|v}}{${2:la définition}}"
		]
	},


	"crossgrid":{
		"prefix":"crossgrid",
		"body":[
			"\\begin{crossgrid}",
				"\t$1",
			"\\end{crossgrid}"
		]
	},


	"crossgrid":{
		"prefix":"crossgrid",
		"body":[
