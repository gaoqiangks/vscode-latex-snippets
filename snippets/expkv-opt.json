{


	"ekvoProcessOptions{}":{
		"prefix":"ekvoProcessOptions",
		"body":[
			"\\ekvoProcessOptions{${1:set}}"
		]
	},


	"ekvoProcessLocalOptions{}":{
		"prefix":"ekvoProcessLocalOptions",
		"body":[
			"\\ekvoProcessLocalOptions{${1:set}}"
		]
	},


	"ekvoProcessGlobalOptions{}":{
		"prefix":"ekvoProcessGlobalOptions",
		"body":[
			"\\ekvoProcessGlobalOptions{${1:set}}"
		]
	},


	"ekvoProcessFutureOptions{}":{
		"prefix":"ekvoProcessFutureOptions",
		"body":[
			"\\ekvoProcessFutureOptions{${1:set}}"
		]
	},


	"ekvoProcessOptionsList{}{}":{
		"prefix":"ekvoProcessOptionsList",
		"body":[
			"\\ekvoProcessOptionsList{${1:list macro}}{${2:set}}"
		]
	},


	"ekvoUseUnknownHandlers{}{}":{
		"prefix":"ekvoUseUnknownHandlers",
		"body":[
			"\\ekvoUseUnknownHandlers{${1:cmd1}}{${2:cmd2}}"
		]
	},


	"ekvoUseUnknownHandlers*":{
		"prefix":"*ekvoUseUnknownHandlers",
		"body":[
			"\\ekvoUseUnknownHandlers*"
		]
	}
}
