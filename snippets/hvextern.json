{


	"runExtCmd{}{}":{
		"prefix":"runExtCmd",
		"body":[
			"\\runExtCmd{${1:command with arguments%definition}}{${2:filename%file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"runExtCmd[]{}{}":{
		"prefix":"runExtCmd",
		"body":[
			"\\runExtCmd[${3:options%keyvals}]{${1:command with arguments%definition}}{${2:filename%file}}"
		],
		"luasnip": {"priority": 1000} 
	},


