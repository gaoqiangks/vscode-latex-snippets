{


	"DeclareGraphicsExtensions{}":{
		"prefix":"DeclareGraphicsExtensions",
		"body":[
			"\\DeclareGraphicsExtensions{${1:ext-list}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"DeclareGraphicsRule{}{}{}{}":{
		"prefix":"DeclareGraphicsRule",
		"body":[
			"\\DeclareGraphicsRule{${1:ext}}{${2:type}}{${3:read-file}}{${4:command%plain}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"graphicspath{}":{
		"prefix":"graphicspath",
		"body":[
			"\\graphicspath{${1:dir-list%definition}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"includegraphics*{}":{
		"prefix":"*includegraphics",
		"body":[
			"\\includegraphics*{${1:imagefile}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"includegraphics*[]{}":{
		"prefix":"*includegraphics",
		"body":[
			"\\includegraphics*[${2:keyvals}]{${1:imagefile}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"reflectbox{}":{
		"prefix":"reflectbox",
		"body":[
			"\\reflectbox{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"resizebox*{}{}{}":{
		"prefix":"*resizebox",
		"body":[
			"\\resizebox*{${1:h-length%l}}{${2:v-length%l}}{${3:text}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"resizebox{}{}{}":{
		"prefix":"resizebox",
		"body":[
			"\\resizebox{${1:h-length%l}}{${2:v-length%l}}{${3:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"rotatebox[]{}{}":{
		"prefix":"rotatebox",
		"body":[
			"\\rotatebox[${3:keyvals}]{${1:angle}}{${2:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"rotatebox{}{}":{
		"prefix":"rotatebox",
		"body":[
			"\\rotatebox{${1:angle}}{${2:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"scalebox{}{}":{
		"prefix":"scalebox",
		"body":[
			"\\scalebox{${1:h-scale}}{${2:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"scalebox{}[]{}":{
		"prefix":"scalebox",
		"body":[
			"\\scalebox{${1:h-scale}}[${3:v-scale}]{${2:text}}"
		],
		"luasnip": {"priority": 1000} 
	}
}
