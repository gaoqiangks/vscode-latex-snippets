{


	"addfont{}{}{}":{
		"prefix":"addfont",
		"body":[
			"\\addfont{${1:encoding}}{${2:font name}}{${3:macro%cmd}}"
		]
	},


	"addshape{}{}{}":{
		"prefix":"addshape",
		"body":[
			"\\addshape{${1:series}}{${2:shape}}{${3:font name}}"
		]
	},


	"addfont[]{}{}{}":{
		"prefix":"addfont",
		"body":[
			"\\addfont[${4:scale}]{${1:encoding}}{${2:font name}}{${3:macro%cmd}}"
		]
	},


	"addfont[][]{}{}{}":{
		"prefix":"addfont",
		"body":[
			"\\addfont[${4:scale}][${5:cmds}]{${1:encoding}}{${2:font name}}{${3:macro%cmd}}"
		]
	},


	"addshape[]{}{}{}":{
		"prefix":"addshape",
		"body":[
			"\\addshape[${4:cmds}]{${1:series}}{${2:shape}}{${3:font name}}"
		]
	}
}
