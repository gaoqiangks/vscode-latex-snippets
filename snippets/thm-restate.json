	"restatable[]{}{}":{
		"prefix":"restatable",
		"body":[
			"\\begin{restatable}",
				"\t[${3:options%keyvals}]{${1:envname}}{${2:cmd name}}",
			"\\end{restatable}"
		],
		"luasnip": {"priority": 1000} 
	},


