{


	"deffloat{}{}{}":{
		"prefix":"deffloat",
		"body":[
			"\\deffloat{${1:float type%envname}}{${2:placement}}{${3:ext}}"
		]
	},


	"providefloat{}{}{}":{
		"prefix":"providefloat",
		"body":[
			"\\providefloat{${1:float type%envname}}{${2:placement}}{${3:ext}}"
		]
	},


	"renewfloat{}{}{}":{
		"prefix":"renewfloat",
		"body":[
			"\\renewfloat{${1:float type}}{${2:placement}}{${3:ext}}"
		]
	},


	"deffloat{}{}{}[]":{
		"prefix":"deffloat",
		"body":[
			"\\deffloat{${1:float type%envname}}{${2:placement}}{${3:ext}}[${4:within}]"
		]
	},


	"providefloat{}{}{}[]":{
		"prefix":"providefloat",
		"body":[
			"\\providefloat{${1:float type%envname}}{${2:placement}}{${3:ext}}[${4:within}]"
		]
	},


	"renewfloat{}{}{}[]":{
		"prefix":"renewfloat",
		"body":[
			"\\renewfloat{${1:float type}}{${2:placement}}{${3:ext}}[${4:within}]"
		]
	}
}
