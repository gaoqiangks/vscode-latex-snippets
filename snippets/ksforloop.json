{


	"ksforloop":{
		"prefix":"ksforloop",
		"body":[
			"\\ksforloop{${1:counter=start+step}}{${2:end}}{${3:code}}"
		]
	},


	"ksforloop*":{
		"prefix":"ksforloop*",
		"body":[
			"\\ksforloop*{${1:counter=start+step}}{${2:end}}{${3:code}}"
		]
	},


	"ksforquit":{
		"prefix":"ksforquit",
		"body":[
			"\\ksforquit"
		]
	}
}
