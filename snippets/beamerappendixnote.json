{


	"appxnote{}{}":{
		"prefix":"appxnote",
		"body":[
			"\\appxnote{${1:title}}{${2:content%text}}"
		]
	},


	"printappxnotes":{
		"prefix":"printappxnotes",
		"body":[
			"\\printappxnotes"
		]
	},


	"appxnote[]{}{}":{
		"prefix":"appxnote",
		"body":[
			"\\appxnote[${3:frame options}]{${1:title}}{${2:content%text}}"
		]
	}
}
