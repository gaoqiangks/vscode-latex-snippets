{


	"appxnote{}{}":{
		"prefix":"appxnote",
		"body":[
			"\\appxnote{${1:title}}{${2:content%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"appxnote[]{}{}":{
		"prefix":"appxnote",
		"body":[
			"\\appxnote[${3:frame options}]{${1:title}}{${2:content%text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"printappxnotes":{
		"prefix":"printappxnotes",
		"body":[
			"\\printappxnotes"
		],
		"luasnip": {"priority": 3000} 
	}
}
