{


	"definespotcolor":{
		"prefix":"definespotcolor",
		"body":[
			"\\definespotcolor{${1:name%specialDef}}{${2:PDF name}}{${3:CMYK-equivalent}}"
		]
	},


	"definespotcolor":{
		"prefix":"definespotcolor",
		"body":[
			"\\definespotcolor{${1:name%specialDef}}{${2:PDF name}}[${4:model}]{${3:equivalent}}"
		]
	},


	"definespotcolor":{
		"prefix":"definespotcolor",
		"body":[
			"\\definespotcolor{${1:name}}{${2:PDF name}}[${4:alt=lab}]{${3:lab-values}}"
		]
	},


	"definecolorspace":{
		"prefix":"definecolorspace",
		"body":[
			"\\definecolorspace{${1:name}}{${2:mixed}}{${3:color-list}}"
		]
	},


	"definecolorspace*":{
		"prefix":"definecolorspace*",
		"body":[
			"\\definecolorspace*{${1:name}}{${2:iccbased}}{${3:icc-file}}"
		]
	},


	"pagecolorspace":{
		"prefix":"pagecolorspace",
		"body":[
			"\\pagecolorspace{${1:colorspace list}}"
		]
	},


	"resetpagecolorspace":{
		"prefix":"resetpagecolorspace",
		"body":[
			"\\resetpagecolorspace"
		]
	},


	"overprintstate":{
		"prefix":"overprintstate",
		"body":[
			"\\overprintstate{${1:state}}"
		]
	},


	"textoverprint":{
		"prefix":"textoverprint",
		"body":[
			"\\textoverprint{${1:text}}"
		]
	},


	"textoverprint":{
		"prefix":"textoverprint",
		"body":[
			"\\textoverprint[${2:state}]{${1:text}}"
		]
	}
