{


	"ulposdef{}{}":{
		"prefix":"ulposdef",
		"body":[
			"\\ulposdef{${1:cmd}}{${2:definition}}"
		]
	},


	"ifulstarttype{}{}{}":{
		"prefix":"ifulstarttype",
		"body":[
			"\\ifulstarttype{${1:type}}{${2:true}}{${3:false}}"
		]
	},


	"ifulendtype{}{}{}":{
		"prefix":"ifulendtype",
		"body":[
			"\\ifulendtype{${1:type}}{${2:true}}{${3:false}}"
		]
	},


	"ulstarttype":{
		"prefix":"ulstarttype",
		"body":[
			"\\ulstarttype"
		]
	},


	"ulendtype":{
		"prefix":"ulendtype",
		"body":[
			"\\ulendtype"
		]
	},


	"ulpostolerance":{
		"prefix":"ulpostolerance",
		"body":[
			"\\ulpostolerance"
		]
	},


	"ulwidth":{
		"prefix":"ulwidth",
		"body":[
			"\\ulwidth"
		]
	},


	"ulposdef{}[]{}":{
		"prefix":"ulposdef",
		"body":[
			"\\ulposdef{${1:cmd}}[${3:options%keyvals}]{${2:definition}}"
		]
	}
}
