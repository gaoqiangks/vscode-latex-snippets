{


	"pbox{}{}":{
		"prefix":"pbox",
		"body":[
			"\\pbox{${1:width}}{${2:contents%text}}"
		]
	},


	"settominwidth{}{}":{
		"prefix":"settominwidth",
		"body":[
			"\\settominwidth{${1:cmd}}{${2:text}}"
		]
	},


	"widthofpbox{}":{
		"prefix":"widthofpbox",
		"body":[
			"\\widthofpbox{${1:text}}"
		]
	},


	"pbox[]{}{}":{
		"prefix":"pbox",
		"body":[
			"\\pbox[${3:position}]{${1:width}}{${2:contents%text}}"
		]
	},


	"pbox[][]{}{}":{
		"prefix":"pbox",
		"body":[
			"\\pbox[${3:position}][${4:height}]{${1:width}}{${2:contents%text}}"
		]
	},


	"pbox[][][]{}{}":{
		"prefix":"pbox",
		"body":[
			"\\pbox[${3:position}][${4:height}][${5:inner-pos}]{${1:width}}{${2:contents%text}}"
		]
	},


	"settominwidth[]{}{}":{
		"prefix":"settominwidth",
		"body":[
			"\\settominwidth[${3:max-width}]{${1:cmd}}{${2:text}}"
		]
	}
}
