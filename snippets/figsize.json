{


	"SetFigLayout{}{}":{
		"prefix":"SetFigLayout",
		"body":[
			"\\SetFigLayout{${1:rows}}{${2:columns}}"
		]
	},


	"figheight":{
		"prefix":"figheight",
		"body":[
			"\\figheight"
		]
	},


	"figwidth":{
		"prefix":"figwidth",
		"body":[
			"\\figwidth"
		]
	},


	"SetFigLayout[]{}{}":{
		"prefix":"SetFigLayout",
		"body":[
			"\\SetFigLayout[${3:space}]{${1:rows}}{${2:columns}}"
		]
	}
}
