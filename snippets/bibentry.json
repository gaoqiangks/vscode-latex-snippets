{


	"nobibliography":{
		"prefix":"nobibliography",
		"body":[
			"\\nobibliography{${1:bib file}}"
		]
	},


	"nobibliography*":{
		"prefix":"nobibliography*",
		"body":[
			"\\nobibliography*"
		]
	},


	"bibentry":{
		"prefix":"bibentry",
		"body":[
			"\\bibentry{${1:keylist}}"
		]
	},


	"urlprefix":{
		"prefix":"urlprefix",
		"body":[
			"\\urlprefix"
		]
	},


	"url":{
		"prefix":"url",
		"body":[
			"\\url{${1:URL}}"
		]
	},


	"doi":{
		"prefix":"doi",
		"body":[
			"\\doi{${1:DOI}}"
		]
	}
}
