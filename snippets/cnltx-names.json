{


	"newfirstformat{}":{
		"prefix":"newfirstformat",
		"body":[
			"\\newfirstformat{${1:cmd}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"writename{}":{
		"prefix":"writename",
		"body":[
			"\\writename{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"defnameformat{}{}{}{}":{
		"prefix":"defnameformat",
		"body":[
			"\\defnameformat{${1:format name}}{${2:first}}{${3:middle}}{${4:last}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"writenameformat{}":{
		"prefix":"writenameformat",
		"body":[
			"\\writenameformat{${1:format name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"name{}":{
		"prefix":"name",
		"body":[
			"\\name{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"name*{}":{
		"prefix":"*name",
		"body":[
			"\\name*{${1:text}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"nameformat":{
		"prefix":"nameformat",
		"body":[
			"\\nameformat"
		],
		"luasnip": {"priority": 3000} 
	},


	"namefirstformat":{
		"prefix":"namefirstformat",
		"body":[
			"\\namefirstformat"
		],
		"luasnip": {"priority": 3000} 
	},


	"resetname":{
		"prefix":"resetname",
		"body":[
			"\\resetname"
		],
		"luasnip": {"priority": 3000} 
	},


	"newname{}{}":{
		"prefix":"newname",
		"body":[
			"\\newname{${1:cmd}}{${2:text}}"
		],
		"luasnip": {"priority": 3000} 
	}
}
