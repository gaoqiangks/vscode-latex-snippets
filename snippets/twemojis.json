{


	"twemoji{}":{
		"prefix":"twemoji",
		"body":[
			"\\twemoji{${1:emoji name}}"
		]
	},


	"texttwemoji{}":{
		"prefix":"texttwemoji",
		"body":[
			"\\texttwemoji{${1:emoji name}}"
		]
	},


	"twemojiDefaultHeight":{
		"prefix":"twemojiDefaultHeight",
		"body":[
			"\\twemojiDefaultHeight"
		]
	},


	"defineTwemoji{}{}":{
		"prefix":"defineTwemoji",
		"body":[
			"\\defineTwemoji{${1:number}}{${2:name}}"
		]
	},


	"twemoji[]{}":{
		"prefix":"twemoji",
		"body":[
			"\\twemoji[${2:options%keyvals}]{${1:emoji name}}"
		]
	}
}
