{


	"kvmapsetup":{
		"prefix":"kvmapsetup",
		"body":[
			"\\kvmapsetup{${1:options%keyvals}}"
		]
	},


	"kvlist":{
		"prefix":"kvlist",
		"body":[
			"\\kvlist{${1:width}}{${2:height}}{${3:elements%formula}}{${4:variables%formula}}"
		]
	},


	"bundle":{
		"prefix":"bundle",
		"body":[
			"\\bundle{${1:x1}}{${2:y1}}{${3:x2}}{${4:y2}}"
		]
	},


	"bundle":{
		"prefix":"bundle",
		"body":[
			"\\bundle[${5:options%keyvals}]{${1:x1}}{${2:y1}}{${3:x2}}{${4:y2}}"
		]
	},


	"kvmatrix":{
		"prefix":"kvmatrix",
		"body":[
			"\\begin{kvmatrix}",
			"\t$1",
			"\\end{kvmatrix}"
		]
	},


	"kvmap":{
		"prefix":"kvmap",
		"body":[
			"\\begin{kvmap}",
			"\t$1",
			"\\end{kvmap}"
		]
	}
}
