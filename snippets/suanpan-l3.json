{


	"rod{}{}":{
		"prefix":"rod",
		"body":[
			"\\rod{${1:num}}{${2:val}}"
		]
	},


	"rods{}":{
		"prefix":"rods",
		"body":[
			"\\rods{${1:val list}}"
		]
	},


	"bids{}{}{}":{
		"prefix":"bids",
		"body":[
			"\\bids{${1:num}}{${2:val}}{${3:color}}"
		]
	},


	"rodmark{}":{
		"prefix":"rodmark",
		"body":[
			"\\rodmark{${1:marker list}}"
		]
	},


	"mkframe":{
		"prefix":"mkframe",
		"body":[
			"\\mkframe"
		]
	},


	"lrframe{}{}":{
		"prefix":"lrframe",
		"body":[
			"\\lrframe{${1:first num}}{${2:last num}}"
		]
	},


	"suanpanset{}":{
		"prefix":"suanpanset",
		"body":[
			"\\suanpanset{${1:options%keyvals}}"
		]
	},


	"rod*{}{}":{
		"prefix":"*rod",
		"body":[
			"\\rod*{${1:num}}{${2:val}}"
		]
	},


	"bids*{}{}{}":{
		"prefix":"*bids",
		"body":[
			"\\bids*{${1:num}}{${2:val}}{${3:color}}"
		]
	},


	"bid{}{}{}[]":{
		"prefix":"bid",
		"body":[
			"\\bid{${1:num}}{${2:pos list}}{${3:fill color}}[${4:draw color}]"
		]
	},


	"rodmark[]{}":{
		"prefix":"rodmark",
		"body":[
			"\\rodmark[${2:start}]{${1:marker list}}"
		]
	},


	"suanpan":{
		"prefix":"suanpan",
		"body":[
			"\\begin{suanpan}",
				"\t$1",
			"\\end{suanpan}"
		]
	},


	"suanpan":{
		"prefix":"suanpan",
		"body":[
			"\\begin{suanpan}",
				"\t$1",
			"\\end{suanpan}"
		]
	}
}
