{


	"rod":{
		"prefix":"rod",
		"body":[
			"\\rod{${1:num}}{${2:val}}"
		]
	},


	"rod*":{
		"prefix":"rod*",
		"body":[
			"\\rod*{${1:num}}{${2:val}}"
		]
	},


	"rods":{
		"prefix":"rods",
		"body":[
			"\\rods{${1:val list}}"
		]
	},


	"bid":{
		"prefix":"bid",
		"body":[
			"\\bid{${1:num}}{${2:pos list}}{${3:fill color}}[${4:draw color}]"
		]
	},


	"bids":{
		"prefix":"bids",
		"body":[
			"\\bids{${1:num}}{${2:val}}{${3:color}}"
		]
	},


	"bids*":{
		"prefix":"bids*",
		"body":[
			"\\bids*{${1:num}}{${2:val}}{${3:color}}"
		]
	},


	"rodmark":{
		"prefix":"rodmark",
		"body":[
			"\\rodmark{${1:marker list}}"
		]
	},


	"rodmark":{
		"prefix":"rodmark",
		"body":[
			"\\rodmark[${2:start}]{${1:marker list}}"
		]
	},


	"mkframe":{
		"prefix":"mkframe",
		"body":[
			"\\mkframe"
		]
	},


	"lrframe":{
		"prefix":"lrframe",
		"body":[
			"\\lrframe{${1:first num}}{${2:last num}}"
		]
	},


	"suanpanset":{
		"prefix":"suanpanset",
		"body":[
			"\\suanpanset{${1:options%keyvals}}"
		]
	},


	"suanpan":{
		"prefix":"suanpan",
		"body":[
			"\\begin{suanpan}",
			"\t$1",
			"\\end{suanpan}"
		]
	}
}
