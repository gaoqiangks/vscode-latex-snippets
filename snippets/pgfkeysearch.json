{


	"pgfkeysearch":{
		"prefix":"pgfkeysearch",
		"body":[
			"\\pgfkeysearch{${1:path-list}}{${2:key%plain}}{${3:macro%cmd}}"
		]
	},


	"pgfkeysearchvalueof":{
		"prefix":"pgfkeysearchvalueof",
		"body":[
			"\\pgfkeysearchvalueof{${1:path-list}}{${2:key%plain}}{${3:macro%cmd}}"
		]
	},


	"pgfkeysearchTF":{
		"prefix":"pgfkeysearchTF",
		"body":[
			"\\pgfkeysearchTF{${1:path-list}}{${2:key%plain}}{${3:macro%cmd}}{${4:if-found}}{${5:if-not}}"
		]
	},


	"pgfkeysearchvalueofTF":{
		"prefix":"pgfkeysearchvalueofTF",
		"body":[
			"\\pgfkeysearchvalueofTF{${1:path-list}}{${2:key%plain}}{${3:macro%cmd}}{${4:if-found}}{${5:if-not}}"
		]
	}
}
