{


	"psTurtle(){}":{
		"prefix":"psTurtle",
		"body":[
			"\\psTurtle(${2:x},${3:y}){${1:turtle code}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"psTurtle[](){}":{
		"prefix":"psTurtle",
		"body":[
			"\\psTurtle[${2:options%keyvals}](${3:x},${4:y}){${1:turtle code}}"
		],
		"luasnip": {"priority": 1000} 
	}
}
