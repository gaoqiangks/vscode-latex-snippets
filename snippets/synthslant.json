{


	"synthslant":{
		"prefix":"synthslant",
		"body":[
			"\\synthslant"
		]
	},


	"synthnegslant":{
		"prefix":"synthnegslant",
		"body":[
			"\\synthnegslant"
		]
	},


	"textsynthslant{}":{
		"prefix":"textsynthslant",
		"body":[
			"\\textsynthslant{${1:text}}"
		]
	},


	"textsynthuprightitalic{}":{
		"prefix":"textsynthuprightitalic",
		"body":[
			"\\textsynthuprightitalic{${1:text}}"
		]
	},


	"synthslantbox{}{}":{
		"prefix":"synthslantbox",
		"body":[
			"\\synthslantbox{${1:slant}}{${2:text}}"
		]
	},


	"slantcontext":{
		"prefix":"slantcontext",
		"body":[
			"\\slantcontext"
		]
	},


	"negslantcontext":{
		"prefix":"negslantcontext",
		"body":[
			"\\negslantcontext"
		]
	},


	"slantenvironment":{
		"prefix":"slantenvironment",
		"body":[
			"\\begin{slantenvironment}",
				"\t$1",
			"\\end{slantenvironment}"
		]
	},


	"negslantenvironment":{
		"prefix":"negslantenvironment",
		"body":[
			"\\begin{negslantenvironment}",
				"\t$1",
			"\\end{negslantenvironment}"
		]
	}
}
