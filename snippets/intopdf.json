{


	"attachandlink{}{}{}":{
		"prefix":"attachandlink",
		"body":[
			"\\attachandlink{${1:file}}{${2:description%text}}{${3:link text%text}}"
		]
	},


	"attachandlink[]{}{}{}":{
		"prefix":"attachandlink",
		"body":[
			"\\attachandlink[${4:filespec}]{${1:file}}{${2:description%text}}{${3:link text%text}}"
		]
	},


	"attachandlink{}[]{}{}":{
		"prefix":"attachandlink",
		"body":[
			"\\attachandlink{${1:file}}[${4:mime-type}]{${2:description%text}}{${3:link text%text}}"
		]
	},


	"attachandlink[]{}[]{}{}":{
		"prefix":"attachandlink",
		"body":[
			"\\attachandlink[${4:filespec}]{${1:file}}[${5:mime-type}]{${2:description%text}}{${3:link text%text}}"
		]
	}
}
