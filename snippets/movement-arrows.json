{


	"mkword{}":{
		"prefix":"mkword",
		"body":[
			"\\mkword{${1:word%text}}"
		]
	},


	"mvarrow{}{}":{
		"prefix":"mvarrow",
		"body":[
			"\\mvarrow{${1:start}}{${2:end}}"
		]
	},


	"mvlink{}{}":{
		"prefix":"mvlink",
		"body":[
			"\\mvlink{${1:start}}{${2:end}}"
		]
	},


	"setarrowstyle{}":{
		"prefix":"setarrowstyle",
		"body":[
			"\\setarrowstyle{${1:style}}"
		]
	},


	"arrowheight":{
		"prefix":"arrowheight",
		"body":[
			"\\arrowheight"
		]
	},


	"extraexheight":{
		"prefix":"extraexheight",
		"body":[
			"\\extraexheight"
		]
	},


	"glarrowheight":{
		"prefix":"glarrowheight",
		"body":[
			"\\glarrowheight"
		]
	},


	"arrowstrut":{
		"prefix":"arrowstrut",
		"body":[
			"\\arrowstrut"
		]
	},


	"arrowgloss":{
		"prefix":"arrowgloss",
		"body":[
			"\\arrowgloss"
		]
	},


	"mvarrow*{}{}":{
		"prefix":"*mvarrow",
		"body":[
			"\\mvarrow*{${1:start}}{${2:end}}"
		]
	},


	"mvarrow*[]{}{}":{
		"prefix":"*mvarrow",
		"body":[
			"\\mvarrow*[${3:options}]{${1:start}}{${2:end}}"
		]
	},


	"mvlink*{}{}":{
		"prefix":"*mvlink",
		"body":[
			"\\mvlink*{${1:start}}{${2:end}}"
		]
	},


	"mvlink*[]{}{}":{
		"prefix":"*mvlink",
		"body":[
			"\\mvlink*[${3:options}]{${1:start}}{${2:end}}"
		]
	},


	"mkword[]{}":{
		"prefix":"mkword",
		"body":[
			"\\mkword[${2:name}]{${1:word%text}}"
		]
	},


	"mvarrow[]{}{}":{
		"prefix":"mvarrow",
		"body":[
			"\\mvarrow[${3:options}]{${1:start}}{${2:end}}"
		]
	},


	"mvlink[]{}{}":{
		"prefix":"mvlink",
		"body":[
			"\\mvlink[${3:options}]{${1:start}}{${2:end}}"
		]
	}
}
