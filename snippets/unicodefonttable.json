{


	"displayfonttable{}{}":{
		"prefix":"displayfonttable",
		"body":[
			"\\displayfonttable{${1:font name}}{${2:font features}}"
		]
	},


	"fonttablesetup{}":{
		"prefix":"fonttablesetup",
		"body":[
			"\\fonttablesetup{${1:options%keyvals}}"
		]
	},


	"fonttableglyphcount":{
		"prefix":"fonttableglyphcount",
		"body":[
			"\\fonttableglyphcount"
		]
	},


	"displayfonttable*{}{}":{
		"prefix":"*displayfonttable",
		"body":[
			"\\displayfonttable*{${1:font name}}{${2:font features}}"
		]
	},


	"displayfonttable*[]{}{}":{
		"prefix":"*displayfonttable",
		"body":[
			"\\displayfonttable*[${3:options%keyvals}]{${1:font name}}{${2:font features}}"
		]
	},


	"displayfonttable[]{}{}":{
		"prefix":"displayfonttable",
		"body":[
			"\\displayfonttable[${3:options%keyvals}]{${1:font name}}{${2:font features}}"
		]
	}
}
