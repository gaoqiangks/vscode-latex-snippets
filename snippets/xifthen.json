{


	"isnamedefined":{
		"prefix":"isnamedefined",
		"body":[
			"\\isnamedefined{${1:command name}}"
		]
	},


	"isempty":{
		"prefix":"isempty",
		"body":[
			"\\isempty{${1:content}}"
		]
	},


	"isequivalentto":{
		"prefix":"isequivalentto",
		"body":[
			"\\isequivalentto{${1:command1}}{${2:command2}}"
		]
	},


	"isin":{
		"prefix":"isin",
		"body":[
			"\\isin{${1:substring}}{${2:string}}"
		]
	},


	"endswith":{
		"prefix":"endswith",
		"body":[
			"\\endswith{${1:string}}{${2:substring}}"
		]
	},


	"cnttest":{
		"prefix":"cnttest",
		"body":[
			"\\cnttest{${1:counter-expr1}}{${2:comparison}}{${3:counter-expr2}}"
		]
	},


	"dimtest":{
		"prefix":"dimtest",
		"body":[
			"\\dimtest{${1:dimen-expr1}}{${2:comparison}}{${3:dimen-expr2}}"
		]
	},


	"newtest":{
		"prefix":"newtest",
		"body":[
			"\\newtest{${1:command}}{${2:test-expr}}"
		]
	},


	"newtest":{
		"prefix":"newtest",
		"body":[
			"\\newtest{${1:command}}[${3:args}]{${2:test-expr}}"
		]
	}
}
