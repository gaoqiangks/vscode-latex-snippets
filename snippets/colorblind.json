{


	"drawScheme":{
		"prefix":"drawScheme",
		"body":[
			"\\drawScheme{${1:scheme}}"
		]
	},


	"drawSchemeC":{
		"prefix":"drawSchemeC",
		"body":[
			"\\drawSchemeC{${1:scheme}}"
		]
	},


	"drawSchemeC":{
		"prefix":"drawSchemeC",
		"body":[
			"\\drawSchemeC[${2:width}]{${1:scheme}}"
		]
	}
}
