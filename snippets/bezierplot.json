{


	"bezierplot{}":{
		"prefix":"bezierplot",
		"body":[
			"\\bezierplot{${1:function%definition}}"
		]
	},


	"bezierplot[][]{}":{
		"prefix":"bezierplot",
		"body":[
			"\\bezierplot[${2:xmin}][${3:xmax}]{${1:function%definition}}"
		]
	},


	"bezierplot[][][][]{}":{
		"prefix":"bezierplot",
		"body":[
			"\\bezierplot[${2:xmin}][${3:xmax}][${4:ymin}][${5:ymax}]{${1:function%definition}}"
		]
	},


	"bezierplot[][][][][]{}":{
		"prefix":"bezierplot",
		"body":[
			"\\bezierplot[${2:xmin}][${3:xmax}][${4:ymin}][${5:ymax}][${6:samples}]{${1:function%definition}}"
		]
	}
}
