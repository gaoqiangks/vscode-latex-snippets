{


	"DeclareGraphicsExtensions{}":{
		"prefix":"DeclareGraphicsExtensions",
		"body":[
			"\\DeclareGraphicsExtensions{${1:ext-list}}"
		]
	},


	"DeclareGraphicsRule{}{}{}{}":{
		"prefix":"DeclareGraphicsRule",
		"body":[
			"\\DeclareGraphicsRule{${1:ext}}{${2:type}}{${3:read-file}}{${4:command%plain}}"
		]
	},


	"graphicspath{}":{
		"prefix":"graphicspath",
		"body":[
			"\\graphicspath{${1:dir-list%definition}}"
		]
	},


	"reflectbox{}":{
		"prefix":"reflectbox",
		"body":[
			"\\reflectbox{${1:text}}"
		]
	},


	"resizebox{}{}{}":{
		"prefix":"resizebox",
		"body":[
			"\\resizebox{${1:h-length%l}}{${2:v-length%l}}{${3:text}}"
		]
	},


	"rotatebox{}{}":{
		"prefix":"rotatebox",
		"body":[
			"\\rotatebox{${1:angle}}{${2:text}}"
		]
	},


	"scalebox{}{}":{
		"prefix":"scalebox",
		"body":[
			"\\scalebox{${1:h-scale}}{${2:text}}"
		]
	},


	"includegraphics*{}":{
		"prefix":"*includegraphics",
		"body":[
			"\\includegraphics*{${1:imagefile}}"
		]
	},


	"includegraphics*[]{}":{
		"prefix":"*includegraphics",
		"body":[
			"\\includegraphics*[${2:llx,lly}]{${1:imagefile}}"
		]
	},


	"includegraphics*[][]{}":{
		"prefix":"*includegraphics",
		"body":[
			"\\includegraphics*[${2:llx,lly}][${3:urx,ury}]{${1:imagefile}}"
		]
	},


	"resizebox*{}{}{}":{
		"prefix":"*resizebox",
		"body":[
			"\\resizebox*{${1:h-length%l}}{${2:v-length%l}}{${3:text}}"
		]
	},


	"includegraphics[][]{}":{
		"prefix":"includegraphics",
		"body":[
			"\\includegraphics[${2:llx,lly}][${3:urx,ury}]{${1:imagefile}}"
		]
	},


	"scalebox{}[]{}":{
		"prefix":"scalebox",
		"body":[
			"\\scalebox{${1:h-scale}}[${3:v-scale}]{${2:text}}"
		]
	}
}
