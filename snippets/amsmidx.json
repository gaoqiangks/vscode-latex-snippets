{


	"makeindex":{
		"prefix":"makeindex",
		"body":[
			"\\makeindex{${1:name}}"
		]
	},


	"printindex":{
		"prefix":"printindex",
		"body":[
			"\\printindex{${1:name}}"
		]
	},


	"Printindex":{
		"prefix":"Printindex",
		"body":[
			"\\Printindex{${1:name}}{${2:title}}"
		]
	},


	"indexcomment":{
		"prefix":"indexcomment",
		"body":[
			"\\indexcomment{${1:text}}"
		]
	},


	"theindexcomment":{
		"prefix":"theindexcomment",
		"body":[
			"\\theindexcomment"
		]
	}
}
