{


	"avm{}":{
		"prefix":"avm",
		"body":[
			"\\avm{${1:structure}}"
		]
	},


	"lframe":{
		"prefix":"lframe",
		"body":[
			"\\lframe"
		]
	},


	"rframe":{
		"prefix":"rframe",
		"body":[
			"\\rframe"
		]
	},


	"tag{}":{
		"prefix":"tag",
		"body":[
			"\\tag{${1:identifier}}"
		]
	},


	"type{}":{
		"prefix":"type",
		"body":[
			"\\type{${1:type}}"
		]
	},


	"id{}{}":{
		"prefix":"id",
		"body":[
			"\\id{${1:id}}{${2:structure}}"
		]
	},


	"punk{}{}":{
		"prefix":"punk",
		"body":[
			"\\punk{${1:attribute}}{${2:type}}"
		]
	},


	"shuffle":{
		"prefix":"shuffle",
		"body":[
			"\\shuffle"
		]
	},


	"avmsetup{}":{
		"prefix":"avmsetup",
		"body":[
			"\\avmsetup{${1:options%keyvals}}"
		]
	},


	"avmdefinestyle{}{}":{
		"prefix":"avmdefinestyle",
		"body":[
			"\\avmdefinestyle{${1:name}}{${2:settings%keyvals}}"
		]
	},


	"avmdefinecommand{}{}":{
		"prefix":"avmdefinecommand",
		"body":[
			"\\avmdefinecommand{${1:name}}{${2:settings%keyvals}}"
		]
	},


	"type*{}":{
		"prefix":"*type",
		"body":[
			"\\type*{${1:type}}"
		]
	},


	"avm[]{}":{
		"prefix":"avm",
		"body":[
			"\\avm[${2:options%keyvals}]{${1:structure}}"
		]
	},


	"avmdefinecommand{}[]{}":{
		"prefix":"avmdefinecommand",
		"body":[
			"\\avmdefinecommand{${1:name}}[${3:label}]{${2:settings%keyvals}}"
		]
	}
}
