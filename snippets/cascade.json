{


	"Cascade":{
		"prefix":"Cascade",
		"body":[
			"\\Cascade{${1:top_left}}{${2:top_right}}{${3:bot_left}}{${4:bot_right}}"
		]
	},


	"Cascade":{
		"prefix":"Cascade",
		"body":[
			"\\Cascade[${5:options%keyvals}]{${1:top_left}}{${2:top_right}}{${3:bot_left}}{${4:bot_right}}"
		]
	},


	"ShortCascade":{
		"prefix":"ShortCascade",
		"body":[
			"\\ShortCascade{${1:top}}{${2:bottom}}"
		]
	},


	"ShortCascade":{
		"prefix":"ShortCascade",
		"body":[
			"\\ShortCascade[${3:options%keyvals}]{${1:top}}{${2:bottom}}"
		]
	},


	"CascadeOptions":{
		"prefix":"CascadeOptions",
		"body":[
			"\\CascadeOptions{${1:keyvals}}"
		]
	},


	"Edacsac":{
		"prefix":"Edacsac",
		"body":[
			"\\Edacsac{${1:top_left}}{${2:top_right}}{${3:bot_left}}{${4:bot_right}}"
		]
	},


	"Edacsac":{
		"prefix":"Edacsac",
		"body":[
			"\\Edacsac[${5:options%keyvals}]{${1:top_left}}{${2:top_right}}{${3:bot_left}}{${4:bot_right}}"
		]
	},


	"ShortEdacsac":{
		"prefix":"ShortEdacsac",
		"body":[
			"\\ShortEdacsac{${1:top}}{${2:bottom}}"
		]
	},


	"ShortEdacsac":{
		"prefix":"ShortEdacsac",
		"body":[
			"\\ShortEdacsac[${3:options%keyvals}]{${1:top}}{${2:bottom}}"
		]
	}
}
