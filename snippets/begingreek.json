{


	"greekfontfamily":{
		"prefix":"greekfontfamily",
		"body":[
			"\\greekfontfamily"
		]
	},


	"greektxt{}":{
		"prefix":"greektxt",
		"body":[
			"\\greektxt{${1:greek text}}"
		]
	},


	"greektxt[]{}":{
		"prefix":"greektxt",
		"body":[
			"\\greektxt[${2:font family}]{${1:greek text}}"
		]
	},


	"greek":{
		"prefix":"greek",
		"body":[
			"\\begin{greek}",
				"\t$1",
			"\\end{greek}"
		]
	},


	"greek":{
		"prefix":"greek",
		"body":[
			"\\begin{greek}",
				"\t$1",
			"\\end{greek}"
		]
	}
}
