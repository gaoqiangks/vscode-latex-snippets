{


	"greekfontfamily":{
		"prefix":"greekfontfamily",
		"body":[
			"\\greekfontfamily"
		],
		"luasnip": {"priority": 3000} 
	},


	"greektxt{}":{
		"prefix":"greektxt",
		"body":[
			"\\greektxt{${1:greek text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"greektxt[]{}":{
		"prefix":"greektxt",
		"body":[
			"\\greektxt[${2:font family}]{${1:greek text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"greek":{
		"prefix":"greek",
		"body":[
			"\\begin{greek}",
				"\t$1",
			"\\end{greek}"
		],
		"luasnip": {"priority": 3000} 
	},


	"greek[]":{
		"prefix":"greek",
		"body":[
			"\\begin{greek}",
				"\t[${1:font family}]",
			"\\end{greek}"
		],
		"luasnip": {"priority": 1000} 
	}
}
