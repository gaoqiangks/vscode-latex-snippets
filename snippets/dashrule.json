{


	"hdashrule{}{}{}":{
		"prefix":"hdashrule",
		"body":[
			"\\hdashrule{${1:width}}{${2:height}}{${3:dash}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"hdashrule[]{}{}{}":{
		"prefix":"hdashrule",
		"body":[
			"\\hdashrule[${4:raise%l}]{${1:width}}{${2:height}}{${3:dash}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"hdashrule[][]{}{}{}":{
		"prefix":"hdashrule",
		"body":[
			"\\hdashrule[${4:raise%l}][${5:leaders%keyvals}]{${1:width}}{${2:height}}{${3:dash}}"
		],
		"luasnip": {"priority": 1000} 
	}
}
