{


	"bidicontourlength":{
		"prefix":"bidicontourlength",
		"body":[
			"\\bidicontourlength{${1:length}}"
		]
	},


	"bidicontournumber":{
		"prefix":"bidicontournumber",
		"body":[
			"\\bidicontournumber{${1:number}}"
		]
	},


	"bidicontour":{
		"prefix":"bidicontour",
		"body":[
			"\\bidicontour{${1:color}}{${2:length}}"
		]
	},


	"bidicontour":{
		"prefix":"bidicontour",
		"body":[
			"\\bidicontour[${3:number}]{${1:color}}{${2:length}}"
		]
	}
}
