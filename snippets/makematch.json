{


	"MakeMatcher":{
		"prefix":"MakeMatcher",
		"body":[
			"\\MakeMatcher{${1:cmd}}{${2:pattern}}"
		]
	},


	"MakeMatchTarget":{
		"prefix":"MakeMatchTarget",
		"body":[
			"\\MakeMatchTarget{${1:cmd}}{${2:keyword list}}"
		]
	},


	"MakeMatchTarget":{
		"prefix":"MakeMatchTarget",
		"body":[
			"\\MakeMatchTarget[${3:list separator}]{${1:cmd}}{${2:keyword list}}"
		]
	},


	"MatchedTarget":{
		"prefix":"MatchedTarget",
		"body":[
			"\\MatchedTarget"
		]
	},


	"RemoveMatched":{
		"prefix":"RemoveMatched",
		"body":[
			"\\RemoveMatched{${1:cmd}}"
		]
	}
}
