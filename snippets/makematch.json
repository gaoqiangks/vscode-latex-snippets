		],
		"luasnip": {"priority": 1000} 
	},


	"MatchedTarget":{
		"prefix":"MatchedTarget",
		"body":[
			"\\MatchedTarget"
		],
		"luasnip": {"priority": 3000} 
	},


	"RemoveMatched{}":{
		"prefix":"RemoveMatched",
		"body":[
			"\\RemoveMatched{${1:cmd}}"
		],
		"luasnip": {"priority": 3000} 
	}
