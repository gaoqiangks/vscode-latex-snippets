{


	"highlight{}":{
		"prefix":"highlight",
		"body":[
			"\\highlight{${1:text}}"
		]
	},


	"colheight":{
		"prefix":"colheight",
		"body":[
			"\\colheight"
		]
	},


	"colwidth":{
		"prefix":"colwidth",
		"body":[
			"\\colwidth"
		]
	},


	"coltextwidth":{
		"prefix":"coltextwidth",
		"body":[
			"\\coltextwidth"
		]
	},


	"colsep":{
		"prefix":"colsep",
		"body":[
			"\\colsep"
		]
	},


	"boxheight":{
		"prefix":"boxheight",
		"body":[
			"\\boxheight"
		]
	},


	"boxlinewidth":{
		"prefix":"boxlinewidth",
		"body":[
			"\\boxlinewidth"
		]
	},


	"posterbox{}{}":{
		"prefix":"posterbox",
		"body":[
			"\\posterbox{${1:title%text}}{${2:contents%text}}"
		]
	},


	"doubleposterbox{}{}{}{}":{
		"prefix":"doubleposterbox",
		"body":[
			"\\doubleposterbox{${1:title1%text}}{${2:contents1%text}}{${3:title2%text}}{${4:contents2%text}}"
		]
	},


	"email{}":{
		"prefix":"email",
		"body":[
			"\\email{${1:email%URL}}"
		]
	},


	"thenumcols":{
		"prefix":"thenumcols",
		"body":[
			"\\thenumcols"
		]
	},


	"doubleposterbox[]{}{}{}{}":{
		"prefix":"doubleposterbox",
		"body":[
			"\\doubleposterbox[${5:height}]{${1:title1%text}}{${2:contents1%text}}{${3:title2%text}}{${4:contents2%text}}"
		]
	},


	"postercolumn":{
		"prefix":"postercolumn",
		"body":[
			"\\begin{postercolumn}",
				"\t$1",
			"\\end{postercolumn}"
		]
	}
}
