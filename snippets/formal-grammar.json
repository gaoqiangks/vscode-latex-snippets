{


	"firstcase":{
		"prefix":"firstcase",
		"body":[
			"\\firstcase{${1:letter}}{${2:definition%formula}}{${3:explanation%text}}"
		]
	},


	"otherform":{
		"prefix":"otherform",
		"body":[
			"\\otherform{${1:definition%formula}}{${2:explanation%text}}"
		]
	},


	"nonterm":{
		"prefix":"nonterm",
		"body":[
			"\\nonterm{${1:text%plain}}"
		]
	},


	"gralt":{
		"prefix":"gralt",
		"body":[
			"\\gralt"
		]
	},


	"nontermsubtil":{
		"prefix":"nontermsubtil",
		"body":[
			"\\nontermsubtil{${1:arg}}"
		]
	},


	"firstcasesubtil":{
		"prefix":"firstcasesubtil",
		"body":[
			"\\firstcasesubtil{${1:arg}}{${2:definition%formula}}{${3:explanation%text}}"
		]
	},


	"downplay":{
		"prefix":"downplay",
		"body":[
			"\\downplay"
		]
	},


	"highlight":{
		"prefix":"highlight",
		"body":[
			"\\highlight"
		]
	},


	"lochighlight":{
		"prefix":"lochighlight",
		"body":[
			"\\lochighlight{${1:text}}"
		]
	},


	"rowstyle":{
		"prefix":"rowstyle",
		"body":[
			"\\rowstyle{${1:style code}}"
		]
	},


	"grammar":{
		"prefix":"grammar",
		"body":[
			"\\begin{grammar}",
			"\t$1",
			"\\end{grammar}"
		]
	},


	"floatgrammar":{
		"prefix":"floatgrammar",
		"body":[
			"\\begin{floatgrammar}",
			"\t$1",
			"\\end{floatgrammar}"
		]
	}
}
