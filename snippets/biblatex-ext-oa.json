		]
	},


	"DeclareOpenAccessEprintUrl[]{}{}":{
		"prefix":"DeclareOpenAccessEprintUrl",
		"body":[
			"\\DeclareOpenAccessEprintUrl[${3:openness}]{${1:eprint type}}{${2:URL spec%URL}}"
		]
