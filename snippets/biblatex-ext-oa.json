			"\\DeclareOpenAccessFieldUrl{${1:field}}{${2:URL spec%URL}}"
		]
	},


	"UndeclareOpenAccessFieldUrl{}":{
		"prefix":"UndeclareOpenAccessFieldUrl",
		"body":[
			"\\UndeclareOpenAccessFieldUrl{${1:field}}"
		]
	},


	"DeclareOpenAccessEprintUrl{}{}":{
		"prefix":"DeclareOpenAccessEprintUrl",
		"body":[
			"\\DeclareOpenAccessEprintUrl{${1:eprint type}}{${2:URL spec%URL}}"
		]
	},


	"DeclareOpenAccessEprintAlias{}{}":{
		"prefix":"DeclareOpenAccessEprintAlias",
		"body":[
			"\\DeclareOpenAccessEprintAlias{${1:alias}}{${2:eprint type}}"
		]
	},


	"UndeclareOpenAccessEprintUrl{}":{
		"prefix":"UndeclareOpenAccessEprintUrl",
		"body":[
			"\\UndeclareOpenAccessEprintUrl{${1:eprint type}}"
		]
	},


