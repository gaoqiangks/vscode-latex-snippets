{


	"line":{
		"prefix":"line",
		"body":[
			"\\line(${2:x},${3:y}){${1:length}}"
		]
	},


	"circle":{
		"prefix":"circle",
		"body":[
			"\\circle{${1:diameter}}"
		]
	},


	"circle*":{
		"prefix":"circle*",
		"body":[
			"\\circle*{${1:diameter}}"
		]
	},


	"oval":{
		"prefix":"oval",
		"body":[
			"\\oval(${1:width},${2:height})"
		]
	},


	"oval":{
		"prefix":"oval",
		"body":[
			"\\oval(${2:width},${3:height})[${1:portion}]"
		]
	},


	"maxovaldiam":{
		"prefix":"maxovaldiam",
		"body":[
			"\\maxovaldiam"
		]
	},


	"allinethickness":{
		"prefix":"allinethickness",
		"body":[
			"\\allinethickness{${1:dimen}}"
		]
	},


	"Thicklines":{
		"prefix":"Thicklines",
		"body":[
			"\\Thicklines"
		]
	},


	"ellipse":{
		"prefix":"ellipse",
		"body":[
			"\\ellipse{${1:x diameter}}{${2:y diameter}}"
		]
	},


	"ellipse*":{
		"prefix":"ellipse*",
		"body":[
			"\\ellipse*{${1:x diameter}}{${2:y diameter}}"
		]
	},


	"arc":{
		"prefix":"arc",
		"body":[
			"\\arc{${1:diameter}}{${2:start angle}}{${3:end angle}}"
		]
	},


	"filltype":{
		"prefix":"filltype",
		"body":[
			"\\filltype{${1:type%keyvals}}"
		]
	},


	"blacken":{
		"prefix":"blacken",
		"body":[
			"\\blacken"
		]
	},


	"whiten":{
		"prefix":"whiten",
		"body":[
			"\\whiten"
		]
	},


	"shade":{
		"prefix":"shade",
		"body":[
			"\\shade"
		]
	},


	"texture":{
		"prefix":"texture",
		"body":[
			"\\texture{${1:hex nums}}"
		]
	}
}
