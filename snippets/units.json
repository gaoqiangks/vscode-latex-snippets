{


	"unit":{
		"prefix":"unit",
		"body":[
			"\\unit[${2:value}]{${1:dimension}}"
		]
	},


	"unit":{
		"prefix":"unit",
		"body":[
			"\\unit{${1:dimension}}"
		]
	},


	"unitfrac":{
		"prefix":"unitfrac",
		"body":[
			"\\unitfrac[${3:value%:translatable}]{${1:num%:translatable}}{${2:den%:translatable}}"
		]
	},


	"unitfrac":{
		"prefix":"unitfrac",
		"body":[
			"\\unitfrac{${1:num%:translatable}}{${2:den%:translatable}}"
		]
	}
}
