{


	"autopdfoptions":{
		"prefix":"autopdfoptions",
		"body":[
			"\\autopdfoptions{${1:options%keyvals}}"
		]
	},


	"autopdfinclude":{
		"prefix":"autopdfinclude",
		"body":[
			"\\autopdfinclude"
		]
	},


	"autopdfendinclude":{
		"prefix":"autopdfendinclude",
		"body":[
			"\\autopdfendinclude"
		]
	},


	"autopdfpsfrag":{
		"prefix":"autopdfpsfrag",
		"body":[
			"\\autopdfpsfrag{${1:tag}}{${2:content%text}}"
		]
	},


	"autopdfpsfrag":{
		"prefix":"autopdfpsfrag",
		"body":[
			"\\autopdfpsfrag[${3:options%keyvals}]{${1:tag}}{${2:content%text}}"
		]
	},


	"autopdfpsfoptions":{
		"prefix":"autopdfpsfoptions",
		"body":[
			"\\autopdfpsfoptions{${1:options%keyvals}}"
		]
