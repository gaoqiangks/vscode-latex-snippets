{


	"tagged{}{}":{
		"prefix":"tagged",
		"body":[
			"\\tagged{${1:tag list}}{${2:source text}}"
		]
	},


	"untagged{}{}":{
		"prefix":"untagged",
		"body":[
			"\\untagged{${1:tag list}}{${2:alt text}}"
		]
	},


	"iftagged{}{}{}":{
		"prefix":"iftagged",
		"body":[
			"\\iftagged{${1:tag list}}{${2:source text}}{${3:alt text}}"
		]
	},


	"usetag{}":{
		"prefix":"usetag",
		"body":[
			"\\usetag{${1:tag list}}"
		]
	},


	"droptag{}":{
		"prefix":"droptag",
		"body":[
			"\\droptag{${1:tag list}}"
		]
	},


	"taggedy":{
		"prefix":"taggedy",
		"body":[
			"\\taggedy"
		]
	},


	"taggedn":{
		"prefix":"taggedn",
		"body":[
			"\\taggedn"
		]
	},


	"taggedblock":{
		"prefix":"taggedblock",
		"body":[
			"\\begin{taggedblock}",
				"\t$1",
			"\\end{taggedblock}"
		]
	},


	"untaggedblock":{
		"prefix":"untaggedblock",
		"body":[
			"\\begin{untaggedblock}",
				"\t$1",
			"\\end{untaggedblock}"
		]
	}
}
