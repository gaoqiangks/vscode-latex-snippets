{


	"MakeShortVerb{}":{
		"prefix":"MakeShortVerb",
		"body":[
			"\\MakeShortVerb{${1:\\character}}"
		]
	},


	"DeleteShortVerb{}":{
		"prefix":"DeleteShortVerb",
		"body":[
			"\\DeleteShortVerb{${1:\\character}}"
		]
	},


	"MakeShortVerb*{}":{
		"prefix":"*MakeShortVerb",
		"body":[
			"\\MakeShortVerb*{${1:\\character}}"
		]
	}
}
