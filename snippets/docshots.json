{


	"docshotOptions{}":{
		"prefix":"docshotOptions",
		"body":[
			"\\docshotOptions{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"docshotPrerequisite{}":{
		"prefix":"docshotPrerequisite",
		"body":[
			"\\docshotPrerequisite{${1:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"docshotAfter{}":{
		"prefix":"docshotAfter",
		"body":[
			"\\docshotAfter{${1:command line}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"docshot":{
		"prefix":"docshot",
		"body":[
			"\\begin{docshot}",
				"\t$1",
			"\\end{docshot}"
		],
		"luasnip": {"priority": 3000} 
	}
}
