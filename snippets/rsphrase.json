	},


	"rsphrase{}":{
		"prefix":"rsphrase",
		"body":[
			"\\rsphrase{${1:number}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"rsphrase[]{}":{
		"prefix":"rsphrase",
		"body":[
			"\\rsphrase[${2:text}]{${1:number}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"iflanguage{}{}{}":{
		"prefix":"iflanguage",
		"body":[
			"\\iflanguage{${1:language}}{${2:true}}{${3:false}}"
		],
