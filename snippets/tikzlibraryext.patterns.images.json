{


	"pgfsetupimageaspattern{}{}":{
		"prefix":"pgfsetupimageaspattern",
		"body":[
			"\\pgfsetupimageaspattern{${1:name}}{${2:imagefile}}"
		]
	},


	"pgfsetupimageaspattern[]{}{}":{
		"prefix":"pgfsetupimageaspattern",
		"body":[
			"\\pgfsetupimageaspattern[${3:options%keyvals}]{${1:name}}{${2:imagefile}}"
		]
	}
}
