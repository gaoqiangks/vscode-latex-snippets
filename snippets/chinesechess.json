{


	"cchessboard":{
		"prefix":"cchessboard",
		"body":[
			"\\cchessboard"
		]
	},


	"cchessman{}":{
		"prefix":"cchessman",
		"body":[
			"\\cchessman{${1:棋子列表}}"
		]
	},


	"init{}":{
		"prefix":"init",
		"body":[
			"\\init{${1:棋子列表}}"
		]
	},


	"set{}{}":{
		"prefix":"set",
		"body":[
			"\\set{${1:棋子位置}}{${2:棋子位置列表}}"
		]
	},


	"del{}":{
		"prefix":"del",
		"body":[
			"\\del{${1:棋盘坐标}}"
		]
	},


	"mov{}{}{}":{
		"prefix":"mov",
		"body":[
			"\\mov{${1:棋子编码}}{${2:原棋盘坐标}}{${3:新棋盘坐标}}"
		]
	},


	"printman{}":{
		"prefix":"printman",
		"body":[
			"\\printman{${1:棋子编码%ref}}"
		]
	},


	"getpiece{}":{
		"prefix":"getpiece",
		"body":[
			"\\getpiece{${1:棋子编码}}"
		]
	},


	"piecechar{}{}":{
		"prefix":"piecechar",
		"body":[
			"\\piecechar{${1:棋子编码}}{${2:棋子字符}}"
		]
	},


	"resetpiece":{
		"prefix":"resetpiece",
		"body":[
			"\\resetpiece"
		]
	},


	"cchessset{}":{
		"prefix":"cchessset",
		"body":[
			"\\cchessset{${1:外观选项%keyvals}}"
		]
	},


	"cchessboard*":{
		"prefix":"*cchessboard",
		"body":[
			"\\cchessboard*"
		]
	},


	"cchessboard*[]":{
		"prefix":"*cchessboard",
		"body":[
			"\\cchessboard*[${1:外观选项%keyvals}]"
		]
	},


	"cchessboard[]":{
		"prefix":"cchessboard",
		"body":[
			"\\cchessboard[${1:外观选项%keyvals}]"
		]
	},


	"cchessman[]{}":{
		"prefix":"cchessman",
		"body":[
			"\\cchessman[${2:外观选项%keyvals}]{${1:棋子列表}}"
		]
	},


	"printman[]{}":{
		"prefix":"printman",
		"body":[
			"\\printman[${2:外观选项%keyvals}]{${1:棋子编码%ref}}"
		]
	},


	"getpiece[]{}":{
		"prefix":"getpiece",
		"body":[
			"\\getpiece[${2:棋子字号}]{${1:棋子编码}}"
		]
	},


	"setcchessman":{
		"prefix":"setcchessman",
		"body":[
			"\\begin{setcchessman}",
				"\t$1",
			"\\end{setcchessman}"
		]
	},


	"setcchessman*":{
		"prefix":"*setcchessman",
		"body":[
			"\\begin{setcchessman*}",
				"\t$1",
			"\\end{setcchessman*}"
		]
	},


	"setcchessman":{
		"prefix":"setcchessman",
		"body":[
			"\\begin{setcchessman}",
				"\t$1",
			"\\end{setcchessman}"
		]
	},


	"setcchessman*":{
		"prefix":"setcchessman",
		"body":[
			"\\begin{setcchessman*}",
				"\t$1",
			"\\end{setcchessman*}"
		]
	}
}
