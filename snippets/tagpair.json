{


	"pairing{}{}":{
		"prefix":"pairing",
		"body":[
			"\\pairing{${1:upper text%text}}{${2:lower text%text}}"
		]
	},


	"bottomline{}":{
		"prefix":"bottomline",
		"body":[
			"\\bottomline{${1:bottom text%text}}"
		]
	},


	"narrowraggedleft":{
		"prefix":"narrowraggedleft",
		"body":[
			"\\narrowraggedleft"
		]
	},


	"pairingline":{
		"prefix":"pairingline",
		"body":[
			"\\begin{pairingline}",
				"\t$1",
			"\\end{pairingline}"
		]
	},


	"taggedline":{
		"prefix":"taggedline",
		"body":[
			"\\begin{taggedline}",
				"\t$1",
			"\\end{taggedline}"
		]
	}
}
