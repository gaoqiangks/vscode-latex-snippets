{


	"setbeamercolor{}{}":{
		"prefix":"setbeamercolor",
		"body":[
			"\\setbeamercolor{${1:beamer-color name}}{${2:options%keyvals}}"
		]
	},


	"ifbeamercolorempty{}{}{}":{
		"prefix":"ifbeamercolorempty",
		"body":[
			"\\ifbeamercolorempty{${1:beamer-color name}}{${2:if undefined}}{${3:if defined}}"
		]
	},


	"usebeamercolor{}":{
		"prefix":"usebeamercolor",
		"body":[
			"\\usebeamercolor{${1:beamer-color name}}"
		]
	},


	"donotcoloroutermaths":{
		"prefix":"donotcoloroutermaths",
		"body":[
			"\\donotcoloroutermaths"
		]
	},


	"donotcolorouterdisplaymaths":{
		"prefix":"donotcolorouterdisplaymaths",
		"body":[
			"\\donotcolorouterdisplaymaths"
		]
	},


	"setbeamercolor*{}{}":{
		"prefix":"*setbeamercolor",
		"body":[
			"\\setbeamercolor*{${1:beamer-color name}}{${2:options%keyvals}}"
		]
	},


	"usebeamercolor*{}":{
		"prefix":"*usebeamercolor",
		"body":[
			"\\usebeamercolor*{${1:beamer-color name}}"
		]
	},


	"usebeamercolor*[]{}":{
		"prefix":"*usebeamercolor",
		"body":[
			"\\usebeamercolor*[${2:fg or bg%keyvals}]{${1:beamer-color name}}"
		]
	},


	"ifbeamercolorempty[]{}{}{}":{
		"prefix":"ifbeamercolorempty",
		"body":[
			"\\ifbeamercolorempty[${4:fg or bg%keyvals}]{${1:beamer-color name}}{${2:if undefined}}{${3:if defined}}"
		]
	},


	"usebeamercolor[]{}":{
		"prefix":"usebeamercolor",
		"body":[
			"\\usebeamercolor[${2:fg or bg%keyvals}]{${1:beamer-color name}}"
		]
	},


	"beamercolorbox":{
		"prefix":"beamercolorbox",
		"body":[
			"\\begin{beamercolorbox}",
				"\t$1",
			"\\end{beamercolorbox}"
		]
	},


	"beamercolorbox":{
		"prefix":"beamercolorbox",
		"body":[
			"\\begin{beamercolorbox}",
				"\t$1",
			"\\end{beamercolorbox}"
		]
	}
}
