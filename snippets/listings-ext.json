{


	"lstdef{}{}{}":{
		"prefix":"lstdef",
		"body":[
			"\\lstdef{${1:identifier}}{${2:file}}{${3:range}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"lstcheck{}{}":{
		"prefix":"lstcheck",
		"body":[
			"\\lstcheck{${1:identifier}}{${2:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"lstuse{}":{
		"prefix":"lstuse",
		"body":[
			"\\lstuse{${1:identifier}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"lstuse[]{}":{
		"prefix":"lstuse",
		"body":[
			"\\lstuse[${2:options%keyvals}]{${1:identifier}}"
		],
		"luasnip": {"priority": 1000} 
	}
}
