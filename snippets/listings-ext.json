{


	"lstdef":{
		"prefix":"lstdef",
		"body":[
			"\\lstdef{${1:identifier}}{${2:file}}{${3:range}}"
		]
	},


	"lstcheck":{
		"prefix":"lstcheck",
		"body":[
			"\\lstcheck{${1:identifier}}{${2:file}}"
		]
	},


	"lstuse":{
		"prefix":"lstuse",
		"body":[
			"\\lstuse{${1:identifier}}"
		]
	},


	"lstuse":{
		"prefix":"lstuse",
		"body":[
			"\\lstuse[${2:options%keyvals}]{${1:identifier}}"
		]
	}
}
