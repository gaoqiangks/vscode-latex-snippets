{


	"PstGanttChart":{
		"prefix":"PstGanttChart",
		"body":[
			"\\PstGanttChart{${1:vertical units}}{${2:horizontal units}}"
		]
	},


	"PstGanttChart":{
		"prefix":"PstGanttChart",
		"body":[
			"\\PstGanttChart[${3:options%keyvals}]{${1:vertical units}}{${2:horizontal units}}"
		]
	},


	"endPstGanttChart":{
		"prefix":"endPstGanttChart",
		"body":[
			"\\endPstGanttChart"
		]
	},


	"PstGanttTask":{
		"prefix":"PstGanttTask",
		"body":[
			"\\PstGanttTask{${1:start}}{${2:end}}"
		]
	},


	"PstGanttTask":{
		"prefix":"PstGanttTask",
		"body":[
			"\\PstGanttTask[${3:options%keyvals}]{${1:start}}{${2:end}}"
		]
	},


	"PstGanttChart":{
		"prefix":"PstGanttChart",
		"body":[
			"\\begin{PstGanttChart}",
			"\t$1",
			"\\end{PstGanttChart}"
		]
	}
}
