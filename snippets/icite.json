{


	"SetTitleStyle{}":{
		"prefix":"SetTitleStyle",
		"body":[
			"\\SetTitleStyle{${1:style code}}"
		]
	},


	"AuthorTitleDelim{}":{
		"prefix":"AuthorTitleDelim",
		"body":[
			"\\AuthorTitleDelim{${1:delim}}"
		]
	},


	"TitlePageDelim{}":{
		"prefix":"TitlePageDelim",
		"body":[
			"\\TitlePageDelim{${1:delim}}"
		]
	},


	"icite{}":{
		"prefix":"icite",
		"body":[
			"\\icite{${1:bibid}}"
		]
	},


	"IndexSubtypeAs{}{}":{
		"prefix":"IndexSubtypeAs",
		"body":[
			"\\IndexSubtypeAs{${1:subtype}}{${2:index name}}"
		]
	},


	"icite[]{}":{
		"prefix":"icite",
		"body":[
			"\\icite[${2:pre}]{${1:bibid}}"
		]
	},


	"icite[][]{}":{
		"prefix":"icite",
		"body":[
			"\\icite[${2:pre}][${3:post}]{${1:bibid}}"
		]
	},


	"icite[][]{}[]":{
		"prefix":"icite",
		"body":[
			"\\icite[${2:pre}][${3:post}]{${1:bibid}}[${4:csname}]"
		]
	}
}
