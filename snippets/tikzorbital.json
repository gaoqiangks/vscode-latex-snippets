{


	"setOrbitalDrawing{}":{
		"prefix":"setOrbitalDrawing",
		"body":[
			"\\setOrbitalDrawing{${1:TikZ options}}"
		]
	},


	"drawLevel[]{}":{
		"prefix":"drawLevel",
		"body":[
			"\\drawLevel[${2:options%keyvals}]{${1:name}}"
		]
	},


	"orbital[]{}":{
		"prefix":"orbital",
		"body":[
			"\\orbital[${2:options%keyvals}]{${1:type}}"
		]
	},


	"satom[]{}":{
		"prefix":"satom",
		"body":[
			"\\satom[${2:options}]{${1:color/rotation-angle/anchor/number of electrons/scale}}"
		]
	},


	"atom[]{}":{
		"prefix":"atom",
		"body":[
			"\\atom[${2:options}]{${1:color/rotation-angle/anchor/number of electrons/scale}}"
		]
	}
}
