{


	"captcont{}":{
		"prefix":"captcont",
		"body":[
			"\\captcont{${1:text}}"
		]
	},


	"caption*{}":{
		"prefix":"*caption",
		"body":[
			"\\caption*{${1:text}}"
		]
	},


	"caption*[]{}":{
		"prefix":"*caption",
		"body":[
			"\\caption*[${2:short text%text}]{${1:text}}"
		]
	},


	"captcont*{}":{
		"prefix":"*captcont",
		"body":[
			"\\captcont*{${1:text}}"
		]
	},


	"captcont*[]{}":{
		"prefix":"*captcont",
		"body":[
			"\\captcont*[${2:short text%text}]{${1:text}}"
		]
	},


	"captcont[]{}":{
		"prefix":"captcont",
		"body":[
			"\\captcont[${2:short text%text}]{${1:text}}"
		]
	}
}
