{


	"newmultienvironment{}{}":{
		"prefix":"newmultienvironment",
		"body":[
			"\\newmultienvironment{${1:envname}}{${2:env,env=arg,...}}"
		]
	},


	"renewmultienvironment{}{}":{
		"prefix":"renewmultienvironment",
		"body":[
			"\\renewmultienvironment{${1:envname}}{${2:env,env=arg,...}}"
		]
	},


	"providemultienvironment{}{}":{
		"prefix":"providemultienvironment",
		"body":[
			"\\providemultienvironment{${1:envname}}{${2:env,env=arg,...}}"
		]
	},


	"newmultienvironment{}[]{}":{
		"prefix":"newmultienvironment",
		"body":[
			"\\newmultienvironment{${1:envname}}[${3:args}]{${2:env,env=arg,...}}"
		]
	},


	"newmultienvironment{}[][]{}":{
		"prefix":"newmultienvironment",
		"body":[
			"\\newmultienvironment{${1:envname}}[${3:args}][${4:default}]{${2:env,env=arg,...}}"
		]
	},


	"renewmultienvironment{}[]{}":{
		"prefix":"renewmultienvironment",
		"body":[
			"\\renewmultienvironment{${1:envname}}[${3:args}]{${2:env,env=arg,...}}"
		]
	},


	"renewmultienvironment{}[][]{}":{
		"prefix":"renewmultienvironment",
		"body":[
			"\\renewmultienvironment{${1:envname}}[${3:args}][${4:default}]{${2:env,env=arg,...}}"
		]
	},


	"providemultienvironment{}[]{}":{
		"prefix":"providemultienvironment",
		"body":[
			"\\providemultienvironment{${1:envname}}[${3:args}]{${2:env,env=arg,...}}"
		]
	},


	"providemultienvironment{}[][]{}":{
		"prefix":"providemultienvironment",
		"body":[
			"\\providemultienvironment{${1:envname}}[${3:args}][${4:default}]{${2:env,env=arg,...}}"
		]
	},


	"multienv":{
		"prefix":"multienv",
		"body":[
			"\\begin{multienv}",
				"\t$1",
			"\\end{multienv}"
		]
	},


	"multienv*":{
		"prefix":"multienv",
		"body":[
			"\\begin{multienv*}",
				"\t$1",
			"\\end{multienv*}"
		]
	}
}
