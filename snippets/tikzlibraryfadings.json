{


	"tikzfading[]":{
		"prefix":"tikzfading",
		"body":[
			"\\tikzfading[${1:options%keyvals}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"tikzfadingfrompicture":{
		"prefix":"tikzfadingfrompicture",
		"body":[
			"\\begin{tikzfadingfrompicture}",
				"\t$1",
			"\\end{tikzfadingfrompicture}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tikzfadingfrompicture[]":{
		"prefix":"tikzfadingfrompicture",
		"body":[
			"\\begin{tikzfadingfrompicture}",
				"\t[${1:options%keyvals}]",
			"\\end{tikzfadingfrompicture}"
		],
		"luasnip": {"priority": 1000} 
	}
}
