{


	"colorcurrentmixin":{
		"prefix":"colorcurrentmixin",
		"body":[
			"\\colorcurrentmixin"
		]
	},


	"newcolormixin{}":{
		"prefix":"newcolormixin",
		"body":[
			"\\newcolormixin{${1:cmd}}"
		]
	},


	"applycolormixins{}":{
		"prefix":"applycolormixins",
		"body":[
			"\\applycolormixins{${1:color}}"
		]
	},


	"colormixin":{
		"prefix":"colormixin",
		"body":[
			"\\begin{colormixin}",
				"\t$1",
			"\\end{colormixin}"
		]
	}
}
