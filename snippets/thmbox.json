{


	"thmboxoptions":{
		"prefix":"thmboxoptions",
		"body":[
			"\\thmboxoptions{${1:options%keyvals}}"
		]
	},


	"newtheorem":{
		"prefix":"newtheorem",
		"body":[
			"\\newtheorem[${3:options%keyvals}]{${1:envname}}{${2:caption}}"
		]
	},


	"newtheorem":{
		"prefix":"newtheorem",
		"body":[
			"\\newtheorem[${3:options%keyvals}]{${1:envname}}{${2:caption}}[${4:within}]"
		]
	},


	"examplename":{
		"prefix":"examplename",
		"body":[
			"\\examplename"
		]
	},


	"proofname":{
		"prefix":"proofname",
		"body":[
			"\\proofname"
		]
	},


	"leftbar":{
		"prefix":"leftbar",
		"body":[
			"\\begin{leftbar}",
			"\t$1",
			"\\end{leftbar}"
		]
	},


	"thmbox":{
		"prefix":"thmbox",
		"body":[
			"\\begin{thmbox}",
			"\t$1",
			"\\end{thmbox}"
		]
	},


	"example":{
		"prefix":"example",
		"body":[
