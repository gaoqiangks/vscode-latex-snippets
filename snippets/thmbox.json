{


	"thmboxoptions{}":{
		"prefix":"thmboxoptions",
		"body":[
			"\\thmboxoptions{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"newtheorem[]{}{}":{
		"prefix":"newtheorem",
		"body":[
			"\\newtheorem[${3:options%keyvals}]{${1:envname}}{${2:caption}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"newtheorem[]{}{}[]":{
		"prefix":"newtheorem",
		"body":[
			"\\newtheorem[${3:options%keyvals}]{${1:envname}}{${2:caption}}[${4:within}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"examplename":{
		"prefix":"examplename",
		"body":[
			"\\examplename"
		],
		"luasnip": {"priority": 3000} 
	},


	"proofname":{
		"prefix":"proofname",
		"body":[
			"\\proofname"
		],
		"luasnip": {"priority": 3000} 
	},


	"thmbox{}":{
		"prefix":"thmbox",
		"body":[
			"\\begin{thmbox}",
				"\t{${1:title%text}}",
			"\\end{thmbox}"
		],
		"luasnip": {"priority": 3000} 
	},


	"thmbox[]{}":{
		"prefix":"thmbox",
		"body":[
			"\\begin{thmbox}",
				"\t[${2:options%keyvals}]{${1:title%text}}",
			"\\end{thmbox}"
		],
		"luasnip": {"priority": 1000} 
	},


	"leftbar":{
		"prefix":"leftbar",
		"body":[
			"\\begin{leftbar}",
				"\t$1",
			"\\end{leftbar}"
		],
		"luasnip": {"priority": 3000} 
	},


	"leftbar[]":{
		"prefix":"leftbar",
		"body":[
			"\\begin{leftbar}",
				"\t[${1:options%keyvals}]",
			"\\end{leftbar}"
		],
		"luasnip": {"priority": 1000} 
	},


	"proof":{
		"prefix":"proof",
		"body":[
			"\\begin{proof}",
				"\t$1",
			"\\end{proof}"
		],
		"luasnip": {"priority": 3000} 
	},


	"proof[]":{
		"prefix":"proof",
		"body":[
			"\\begin{proof}",
				"\t[${1:postfix}]",
			"\\end{proof}"
		],
		"luasnip": {"priority": 1000} 
	},


	"example":{
		"prefix":"example",
		"body":[
			"\\begin{example}",
				"\t$1",
			"\\end{example}"
		],
		"luasnip": {"priority": 3000} 
	},


	"example[]":{
		"prefix":"example",
		"body":[
			"\\begin{example}",
				"\t[${1:label text}]",
			"\\end{example}"
		],
		"luasnip": {"priority": 1000} 
	}
}
