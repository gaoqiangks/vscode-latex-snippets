{


	"JobnameSuffix":{
		"prefix":"JobnameSuffix",
		"body":[
			"\\JobnameSuffix"
		]
	},


	"IfSuffixTF{}{}":{
		"prefix":"IfSuffixTF",
		"body":[
			"\\IfSuffixTF{${1:true condition}}{${2:false condition}}"
		]
	},


	"IfSuffixT{}":{
		"prefix":"IfSuffixT",
		"body":[
			"\\IfSuffixT{${1:true condition}}"
		]
	},


	"IfSuffixF{}":{
		"prefix":"IfSuffixF",
		"body":[
			"\\IfSuffixF{${1:false condition}}"
		]
	},


	"OverrideSuffix{}":{
		"prefix":"OverrideSuffix",
		"body":[
			"\\OverrideSuffix{${1:suffix}}"
		]
	},


	"IfSuffixTF[]{}{}":{
		"prefix":"IfSuffixTF",
		"body":[
			"\\IfSuffixTF[${3:suffixes}]{${1:true condition}}{${2:false condition}}"
		]
	},


	"IfSuffixT[]{}":{
		"prefix":"IfSuffixT",
		"body":[
			"\\IfSuffixT[${2:suffixes}]{${1:true condition}}"
		]
	},


	"IfSuffixF[]{}":{
		"prefix":"IfSuffixF",
		"body":[
			"\\IfSuffixF[${2:suffixes}]{${1:false condition}}"
		]
	},


	"IfSuffix":{
		"prefix":"IfSuffix",
		"body":[
			"\\begin{IfSuffix}",
				"\t$1",
			"\\end{IfSuffix}"
		]
	},


	"IfSuffix":{
		"prefix":"IfSuffix",
		"body":[
			"\\begin{IfSuffix}",
				"\t$1",
			"\\end{IfSuffix}"
		]
	}
}
