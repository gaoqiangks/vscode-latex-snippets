{


	"inputpygmented":{
		"prefix":"inputpygmented",
		"body":[
			"\\inputpygmented{${1:file}}"
		]
	},


	"inputpygmented":{
		"prefix":"inputpygmented",
		"body":[
			"\\inputpygmented[${2:options%keyvals}]{${1:file}}"
		]
	},


	"pyginline":{
		"prefix":"pyginline",
		"body":[
			"\\pyginline|${1:code}|"
		]
	},


	"pyginline":{
		"prefix":"pyginline",
		"body":[
			"\\pyginline[${1:options}]|${2:code}|"
		]
	},


	"setpygmented":{
		"prefix":"setpygmented",
		"body":[
			"\\setpygmented{${1:options%keyvals}}"
		]
	},


	"widest":{
		"prefix":"widest",
		"body":[
			"\\widest(${3:string list}){${1:code}}{${2:cmd}}"
		]
	},


	"VerbatimOutAppend":{
		"prefix":"VerbatimOutAppend",
		"body":[
			"\\VerbatimOutAppend"
		]
	},


	"FormatLineNumber":{
		"prefix":"FormatLineNumber",
		"body":[
			"\\FormatLineNumber{${1:arg}}"
		]
	},


	"VerbatimOutAppend":{
		"prefix":"VerbatimOutAppend",
		"body":[
			"\\begin{VerbatimOutAppend}",
			"\t$1",
			"\\end{VerbatimOutAppend}"
		]
	},


	"pygmented":{
		"prefix":"pygmented",
		"body":[
			"\\begin{pygmented}",
			"\t$1",
			"\\end{pygmented}"
		]
	}
}
