{


	"newscrambledenv{}":{
		"prefix":"newscrambledenv",
		"body":[
			"\\newscrambledenv{${1:envname}}"
		]
	},


	"defaultlabelfont{}":{
		"prefix":"defaultlabelfont",
		"body":[
			"\\defaultlabelfont{${1:font cmds}}"
		]
	},


	"defaultrefprefix{}":{
		"prefix":"defaultrefprefix",
		"body":[
			"\\defaultrefprefix{${1:prefix}}"
		]
	},


	"defaultreffont{}":{
		"prefix":"defaultreffont",
		"body":[
			"\\defaultreffont{${1:font cmds}}"
		]
	},


	"defaultprintenv{}{}":{
		"prefix":"defaultprintenv",
		"body":[
			"\\defaultprintenv{${1:open env code}}{${2:close env code}}"
		]
	},


	"defaultprintitem{}":{
		"prefix":"defaultprintitem",
		"body":[
			"\\defaultprintitem{${1:item cmds}}"
		]
	}
}
