{


	"wrapstuffset{}":{
		"prefix":"wrapstuffset",
		"body":[
			"\\wrapstuffset{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"wrapstuffclear":{
		"prefix":"wrapstuffclear",
		"body":[
			"\\wrapstuffclear"
		],
		"luasnip": {"priority": 3000} 
	},


	"wrapstuff":{
		"prefix":"wrapstuff",
		"body":[
			"\\begin{wrapstuff}",
				"\t$1",
			"\\end{wrapstuff}"
		],
		"luasnip": {"priority": 3000} 
	},


	"wrapstuff[]":{
		"prefix":"wrapstuff",
		"body":[
			"\\begin{wrapstuff}",
				"\t[${1:options%keyvals}]",
			"\\end{wrapstuff}"
		],
		"luasnip": {"priority": 1000} 
	}
}
