	"wrapstuffset{}":{
		"prefix":"wrapstuffset",
		"body":[
			"\\wrapstuffset{${1:options%keyvals}}"
		]
	},


	"wrapstuffclear":{
		"prefix":"wrapstuffclear",
		"body":[
			"\\wrapstuffclear"
		]
	},


	"wrapstuff":{
		"prefix":"wrapstuff",
		"body":[
			"\\begin{wrapstuff}",
				"\t$1",
			"\\end{wrapstuff}"
		]
	},


	"wrapstuff":{
		"prefix":"wrapstuff",
		"body":[
			"\\begin{wrapstuff}",
				"\t$1",
			"\\end{wrapstuff}"
		]
	}
}
