{


	"tableur{}":{
		"prefix":"tableur",
		"body":[
			"\\tableur{${1:colonnes}}"
		]
	},


	"tabcolwidth{}":{
		"prefix":"tabcolwidth",
		"body":[
			"\\tabcolwidth{${1:width}}"
		]
	},


	"tabnumlinewidth{}":{
		"prefix":"tabnumlinewidth",
		"body":[
			"\\tabnumlinewidth{${1:width}}"
		]
	},


	"tablineheight{}":{
		"prefix":"tablineheight",
		"body":[
			"\\tablineheight{${1:height}}"
		]
	},


	"helvbx":{
		"prefix":"helvbx",
		"body":[
			"\\helvbx"
		]
	},


	"celtxt{}{}{}":{
		"prefix":"celtxt",
		"body":[
			"\\celtxt{${1:colonne}}{${2:ligne}}{${3:texte}}"
		]
	},


	"selecCell{}{}":{
		"prefix":"selecCell",
		"body":[
			"\\selecCell{${1:colonne}}{${2:ligne}}"
		]
	},


	"multiSelec{}{}":{
		"prefix":"multiSelec",
		"body":[
			"\\multiSelec{${1:colonnes}}{${2:lignes}}"
		]
	},


	"multimultiSelec{}{}{}":{
		"prefix":"multimultiSelec",
		"body":[
			"\\multimultiSelec{${1:ligne1}}{${2:ligne2}}{${3:ligne3}}"
		]
	},


	"tableur*{}":{
		"prefix":"*tableur",
		"body":[
			"\\tableur*{${1:col1/dim1,col2/dim2,...}}"
		]
	},


	"tableur*[]{}":{
		"prefix":"*tableur",
		"body":[
			"\\tableur*[${2:nombre de lignes}]{${1:col1/dim1,col2/dim2,...}}"
		]
	},


	"celtxt*{}{}{}":{
		"prefix":"*celtxt",
		"body":[
			"\\celtxt*{${1:colonne}}{${2:ligne}}{${3:texte}}"
		]
	},


	"celtxt*[]{}{}{}":{
		"prefix":"*celtxt",
		"body":[
			"\\celtxt*[${4:options%keyvals}]{${1:colonne}}{${2:ligne}}{${3:texte}}"
		]
	},


	"tableur[]{}":{
		"prefix":"tableur",
		"body":[
			"\\tableur[${2:nombre de lignes}]{${1:colonnes}}"
		]
	},


	"celtxt[]{}{}{}":{
		"prefix":"celtxt",
		"body":[
			"\\celtxt[${4:options%keyvals}]{${1:colonne}}{${2:ligne}}{${3:texte}}"
		]
	}
}
