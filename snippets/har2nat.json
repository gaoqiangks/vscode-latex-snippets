{


	"cite*":{
		"prefix":"cite*",
		"body":[
			"\\cite*{${1:keylist}}"
		]
	},


	"citeaffixed":{
		"prefix":"citeaffixed",
		"body":[
			"\\citeaffixed{${1:prefix}}{${2:keylist}}"
		]
	},


	"citeasnoun":{
		"prefix":"citeasnoun",
		"body":[
			"\\citeasnoun{${1:keylist}}"
		]
	},


	"citeasnoun":{
		"prefix":"citeasnoun",
		"body":[
			"\\citeasnoun[${2:postfix}]{${1:keylist}}"
		]
	},


	"citeasnoun*":{
		"prefix":"citeasnoun*",
		"body":[
			"\\citeasnoun*{${1:keylist}}"
		]
	},


	"citename":{
		"prefix":"citename",
		"body":[
			"\\citename{${1:keylist}}"
		]
	},


	"citename*":{
		"prefix":"citename*",
		"body":[
			"\\citename*{${1:keylist}}"
		]
	},


	"citeyear*":{
		"prefix":"citeyear*",
		"body":[
			"\\citeyear*{${1:keylist}}"
		]
	}
}
