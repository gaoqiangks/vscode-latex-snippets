{


	"cite*{}":{
		"prefix":"*cite",
		"body":[
			"\\cite*{${1:keylist}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"citeaffixed{}{}":{
		"prefix":"citeaffixed",
		"body":[
			"\\citeaffixed{${1:prefix}}{${2:keylist}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"citeasnoun{}":{
		"prefix":"citeasnoun",
		"body":[
			"\\citeasnoun{${1:keylist}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"citeasnoun[]{}":{
		"prefix":"citeasnoun",
		"body":[
			"\\citeasnoun[${2:postfix}]{${1:keylist}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"citeasnoun*{}":{
		"prefix":"*citeasnoun",
		"body":[
			"\\citeasnoun*{${1:keylist}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"citename{}":{
