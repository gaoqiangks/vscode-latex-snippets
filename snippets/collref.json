{


	"collectsep{}":{
		"prefix":"collectsep",
		"body":[
			"\\collectsep{${1:separator}}"
		]
	},


	"nocollect{}":{
		"prefix":"nocollect",
		"body":[
			"\\nocollect{${1:key}}"
		]
	},


	"collectsep[]{}":{
		"prefix":"collectsep",
		"body":[
			"\\collectsep[${2:punctuation}]{${1:separator}}"
		]
	}
}
