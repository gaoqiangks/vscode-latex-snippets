{


	"itemLabel{}":{
		"prefix":"itemLabel",
		"body":[
			"\\itemLabel{${1:label}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"itemLabel{}[]":{
		"prefix":"itemLabel",
		"body":[
			"\\itemLabel{${1:label}}[${2:skipA}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"itemLabel{}[][]":{
		"prefix":"itemLabel",
		"body":[
			"\\itemLabel{${1:label}}[${2:skipA}][${3:skipB}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"easylist":{
		"prefix":"easylist",
		"body":[
			"\\begin{easylist}",
				"\t$1",
			"\\end{easylist}"
		],
		"luasnip": {"priority": 3000} 
	},


	"easylist[]":{
		"prefix":"easylist",
		"body":[
			"\\begin{easylist}",
				"\t[${1:style%keyvals}]",
			"\\end{easylist}"
		],
		"luasnip": {"priority": 1000} 
	}
}
