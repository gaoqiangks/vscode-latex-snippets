{


	"itemLabel{}":{
		"prefix":"itemLabel",
		"body":[
			"\\itemLabel{${1:label}}"
		]
	},


	"itemLabel{}[]":{
		"prefix":"itemLabel",
		"body":[
			"\\itemLabel{${1:label}}[${2:skipA}]"
		]
	},


	"itemLabel{}[][]":{
		"prefix":"itemLabel",
		"body":[
			"\\itemLabel{${1:label}}[${2:skipA}][${3:skipB}]"
		]
	},


	"easylist":{
		"prefix":"easylist",
		"body":[
			"\\begin{easylist}",
				"\t$1",
			"\\end{easylist}"
		]
	},


	"easylist":{
		"prefix":"easylist",
		"body":[
			"\\begin{easylist}",
				"\t$1",
			"\\end{easylist}"
		]
	}
}
