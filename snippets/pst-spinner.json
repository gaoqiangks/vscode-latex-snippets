{


	"psFidgetSpinner":{
		"prefix":"psFidgetSpinner",
		"body":[
			"\\psFidgetSpinner"
		]
	},


	"psFidgetSpinner()":{
		"prefix":"psFidgetSpinner",
		"body":[
			"\\psFidgetSpinner(${1:x},${2:y})"
		]
	},


	"psFidgetSpinner[]":{
		"prefix":"psFidgetSpinner",
		"body":[
			"\\psFidgetSpinner[${1:options%keyvals}]"
		]
	},


	"psFidgetSpinner[]()":{
		"prefix":"psFidgetSpinner",
		"body":[
			"\\psFidgetSpinner[${1:options%keyvals}](${2:x},${3:y})"
		]
	}
}
