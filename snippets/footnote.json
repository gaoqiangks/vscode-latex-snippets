{


	"savenotes":{
		"prefix":"savenotes",
		"body":[
			"\\savenotes"
		],
		"luasnip": {"priority": 3000} 
	},


	"spewnotes":{
		"prefix":"spewnotes",
		"body":[
			"\\spewnotes"
		],
		"luasnip": {"priority": 3000} 
	},


	"makesavenoteenv{}":{
		"prefix":"makesavenoteenv",
		"body":[
			"\\makesavenoteenv{${1:environment}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"makesavenoteenv[]{}":{
		"prefix":"makesavenoteenv",
		"body":[
			"\\makesavenoteenv[${2:new_envname%envname}]{${1:environment}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"savenotes":{
		"prefix":"savenotes",
		"body":[
			"\\begin{savenotes}",
				"\t$1",
			"\\end{savenotes}"
		],
		"luasnip": {"priority": 3000} 
	},


	"minipage*{}":{
		"prefix":"*minipage",
		"body":[
			"\\begin{minipage*}",
				"\t{${1:width}}",
			"\\end{minipage*}"
		],
		"luasnip": {"priority": 2000} 
	},


	"minipage*[]{}":{
		"prefix":"*minipage",
		"body":[
			"\\begin{minipage*}",
				"\t[${2:position%keyvals}]{${1:width}}",
			"\\end{minipage*}"
		],
		"luasnip": {"priority": 2000} 
	},


	"minipage*[][][]{}":{
		"prefix":"*minipage",
		"body":[
			"\\begin{minipage*}",
				"\t[${2:position%keyvals}][${3:height}][${4:inner-pos}]{${1:width}}",
			"\\end{minipage*}"
		],
		"luasnip": {"priority": 2000} 
	},


	"footnote":{
		"prefix":"footnote",
		"body":[
			"\\begin{footnote}",
				"\t$1",
			"\\end{footnote}"
		],
		"luasnip": {"priority": 3000} 
	},


	"footnote[]":{
		"prefix":"footnote",
		"body":[
			"\\begin{footnote}",
				"\t[${1:number}]",
			"\\end{footnote}"
		],
		"luasnip": {"priority": 1000} 
	},


	"footnotetext":{
		"prefix":"footnotetext",
		"body":[
			"\\begin{footnotetext}",
				"\t$1",
			"\\end{footnotetext}"
		],
		"luasnip": {"priority": 3000} 
	}
}
