{


	"pstSetRelativeOrigin":{
		"prefix":"pstSetRelativeOrigin",
		"body":[
			"\\pstSetRelativeOrigin(${1:x},${2:y})"
		]
	},


	"pstSetAbsoluteOrigin":{
		"prefix":"pstSetAbsoluteOrigin",
		"body":[
			"\\pstSetAbsoluteOrigin"
		]
	},


	"pstSetPostScriptOrigin":{
		"prefix":"pstSetPostScriptOrigin",
		"body":[
			"\\pstSetPostScriptOrigin"
		]
	},


	"pstPutAbs":{
		"prefix":"pstPutAbs",
		"body":[
			"\\pstPutAbs(${2:x},${3:y}){${1:stuff}}"
		]
	},


	"pstPutAbs":{
		"prefix":"pstPutAbs",
		"body":[
			"\\pstPutAbs[${2:options%keyvals}](${3:x},${4:y}){${1:stuff}}"
		]
	}
}
