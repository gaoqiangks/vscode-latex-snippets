{


	"makecommand{}{}":{
		"prefix":"makecommand",
		"body":[
			"\\makecommand{${1:cmd}}{${2:def}}"
		]
	},


	"provideenvironment{}{}{}":{
		"prefix":"provideenvironment",
		"body":[
			"\\provideenvironment{${1:envname}}{${2:begdef}}{${3:enddef}}"
		]
	},


	"makeenvironment{}{}{}":{
