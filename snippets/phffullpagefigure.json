		"body":[
			"\\figplacement{${1:position%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"figcapmaxheight{}":{
		"prefix":"figcapmaxheight",
		"body":[
			"\\figcapmaxheight{${1:length}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"fullpagefigurecaptionfmt":{
