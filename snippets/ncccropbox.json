{


	"cropbox{}":{
		"prefix":"cropbox",
		"body":[
			"\\cropbox{${1:content%text}}"
		]
	},


	"cropboxsep{}":{
		"prefix":"cropboxsep",
		"body":[
			"\\cropboxsep{${1:length}}"
		]
	},


	"croplinewidth{}":{
		"prefix":"croplinewidth",
		"body":[
			"\\croplinewidth{${1:length}}"
		]
	},


	"croplinelength{}":{
		"prefix":"croplinelength",
		"body":[
			"\\croplinelength{${1:length}}"
		]
	},


	"cropbox[]{}":{
		"prefix":"cropbox",
		"body":[
			"\\cropbox[${2:width}]{${1:content%text}}"
		]
	},


	"cropbox[][]{}":{
		"prefix":"cropbox",
		"body":[
			"\\cropbox[${2:width}][${3:height}]{${1:content%text}}"
		]
	}
}
