{


	"defaultgobble":{
		"prefix":"defaultgobble",
		"body":[
			"\\defaultgobble"
		],
		"luasnip": {"priority": 3000} 
	},


	"updatehighlight{}":{
		"prefix":"updatehighlight",
		"body":[
			"\\updatehighlight{${1:highlight rules%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"useblock{}":{
		"prefix":"useblock",
		"body":[
			"\\useblock{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"consumeblock{}":{
		"prefix":"consumeblock",
		"body":[
			"\\consumeblock{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"highlightblock":{
		"prefix":"highlightblock",
		"body":[
			"\\begin{highlightblock}",
				"\t$1",
			"\\end{highlightblock}"
		],
		"luasnip": {"priority": 3000} 
	},


	"highlightblock[]":{
		"prefix":"highlightblock",
		"body":[
			"\\begin{highlightblock}",
				"\t[${1:options%keyvals}]",
			"\\end{highlightblock}"
		],
		"luasnip": {"priority": 1000} 
	},


	"saveblock{}":{
		"prefix":"saveblock",
		"body":[
			"\\begin{saveblock}",
				"\t{${1:name}}",
			"\\end{saveblock}"
		],
		"luasnip": {"priority": 3000} 
	}
}
