{


	"defaultgobble":{
		"prefix":"defaultgobble",
		"body":[
			"\\defaultgobble"
		]
	},


	"updatehighlight{}":{
		"prefix":"updatehighlight",
		"body":[
			"\\updatehighlight{${1:highlight rules%keyvals}}"
		]
	},


	"useblock{}":{
		"prefix":"useblock",
		"body":[
			"\\useblock{${1:name}}"
		]
	},


	"consumeblock{}":{
		"prefix":"consumeblock",
		"body":[
			"\\consumeblock{${1:name}}"
		]
	},


	"highlightblock":{
		"prefix":"highlightblock",
		"body":[
			"\\begin{highlightblock}",
				"\t$1",
			"\\end{highlightblock}"
		]
	},


	"saveblock":{
		"prefix":"saveblock",
		"body":[
			"\\begin{saveblock}",
				"\t$1",
			"\\end{saveblock}"
		]
	},


	"highlightblock":{
		"prefix":"highlightblock",
		"body":[
			"\\begin{highlightblock}",
				"\t$1",
			"\\end{highlightblock}"
		]
	}
}
