{


	"njustsetup{}":{
		"prefix":"njustsetup",
		"body":[
			"\\njustsetup{${1:options%keyvals}}"
		]
	},


	"copyrightpage":{
		"prefix":"copyrightpage",
		"body":[
			"\\copyrightpage"
		]
	},


	"reviewpage":{
		"prefix":"reviewpage",
		"body":[
			"\\reviewpage"
		]
	},


	"njustwhole":{
		"prefix":"njustwhole",
		"body":[
			"\\njustwhole"
		]
	},


	"njustwhole[]":{
		"prefix":"njustwhole",
		"body":[
			"\\njustwhole[${1:color}]"
		]
	},


	"njustwhole[][]":{
		"prefix":"njustwhole",
		"body":[
			"\\njustwhole[${1:color}][${2:scale}]"
		]
	},


	"abstract":{
		"prefix":"abstract",
		"body":[
			"\\begin{abstract}",
				"\t$1",
			"\\end{abstract}"
		]
	},


	"acknowledgement":{
		"prefix":"acknowledgement",
		"body":[
			"\\begin{acknowledgement}",
				"\t$1",
			"\\end{acknowledgement}"
		]
	},


	"keyword":{
		"prefix":"keyword",
		"body":[
			"\\begin{keyword}",
				"\t$1",
			"\\end{keyword}"
		]
	},


	"abstract*":{
		"prefix":"*abstract",
		"body":[
			"\\begin{abstract*}",
				"\t$1",
			"\\end{abstract*}"
		]
	},


	"keyword*":{
		"prefix":"*keyword",
		"body":[
			"\\begin{keyword*}",
				"\t$1",
			"\\end{keyword*}"
		]
	}
}
