{


	"sidenote":{
		"prefix":"sidenote",
		"body":[
			"\\sidenote{${1:text}}"
		]
	},


	"sidenote":{
		"prefix":"sidenote",
		"body":[
			"\\sidenote[${2:mark}]{${1:text}}"
		]
	},


	"sidenote":{
		"prefix":"sidenote",
		"body":[
			"\\sidenote(${2:offset}){${1:text}}"
		]
	},


	"sidenote":{
		"prefix":"sidenote",
		"body":[
			"\\sidenote(${3:offset})[${2:mark}]{${1:text}}"
		]
	},


	"sidenote":{
		"prefix":"sidenote",
		"body":[
			"\\sidenote[${2:offset}][${3:mark}]{${1:text}}"
		]
	},


	"sidenotemark":{
		"prefix":"sidenotemark",
		"body":[
			"\\sidenotemark"
		]
	},


	"sidenotemark":{
		"prefix":"sidenotemark",
		"body":[
			"\\sidenotemark[${1:mark}]"
		]
	},


	"sidenotetext":{
		"prefix":"sidenotetext",
		"body":[
			"\\sidenotetext{${1:text}}"
		]
	},


	"sidenotetext":{
		"prefix":"sidenotetext",
		"body":[
			"\\sidenotetext[${2:mark}]{${1:text}}"
		]
	},


	"newsnotezfloat":{
		"prefix":"newsnotezfloat",
		"body":[
			"\\newsnotezfloat{${1:name}}"
		]
	},


	"newsnotezfloat":{
		"prefix":"newsnotezfloat",
		"body":[
			"\\newsnotezfloat[${2:code}]{${1:name}}"
		]
	},


	"setsidenotes":{
		"prefix":"setsidenotes",
		"body":[
			"\\setsidenotes{${1:options%keyvals}}"
		]
	},


	"sidefigure":{
		"prefix":"sidefigure",
		"body":[
			"\\begin{sidefigure}",
			"\t$1",
			"\\end{sidefigure}"
		]
	},


	"sidetable":{
		"prefix":"sidetable",
		"body":[
			"\\begin{sidetable}",
			"\t$1",
			"\\end{sidetable}"
		]
	}
}
