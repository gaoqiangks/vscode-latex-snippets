{


	"ToCenter":{
		"prefix":"ToCenter",
		"body":[
			"\\ToCenter{${1:width}}{${2:height}}"
		]
	},


	"ToCenter":{
		"prefix":"ToCenter",
		"body":[
			"\\ToCenter[${3:hfm}]{${1:width}}{${2:height}}"
		]
	},


	"ToCenter*":{
		"prefix":"ToCenter*",
		"body":[
			"\\ToCenter*{${1:width}}{${2:height}}"
		]
	},


	"FromMargins":{
		"prefix":"FromMargins",
		"body":[
			"\\FromMargins{${1:left%l}}{${2:right%l}}{${3:top%l}}{${4:bottom%l}}"
		]
	},


	"FromMargins":{
		"prefix":"FromMargins",
		"body":[
			"\\FromMargins[${5:hfm}]{${1:left%l}}{${2:right%l}}{${3:top%l}}{${4:bottom%l}}"
		]
	},


	"FromMargins*":{
		"prefix":"FromMargins*",
		"body":[
			"\\FromMargins*{${1:left%l}}{${2:right%l}}{${3:top%l}}{${4:bottom%l}}"
		]
	}
}
