{


	"doi{}":{
		"prefix":"doi",
		"body":[
			"\\doi{${1:ref number}}"
		]
	},


	"pubmed{}":{
		"prefix":"pubmed",
		"body":[
			"\\pubmed{${1:ref number}}"
		]
	},


	"citeurl{}":{
		"prefix":"citeurl",
		"body":[
			"\\citeurl{${1:URL}}"
		]
	},


	"href{}{}":{
		"prefix":"href",
		"body":[
			"\\href{${1:URL}}{${2:text}}"
		]
	},


	"doitext{}":{
		"prefix":"doitext",
		"body":[
			"\\doitext{${1:doi ref}}"
		]
	},


	"pubmedtext{}":{
		"prefix":"pubmedtext",
		"body":[
			"\\pubmedtext{${1:pub med code}}"
		]
	}
}
