{


	"luaModularMult{}":{
		"prefix":"luaModularMult",
		"body":[
			"\\luaModularMult{${1:integer}}"
		]
	},


	"luaModularAdd{}":{
		"prefix":"luaModularAdd",
		"body":[
			"\\luaModularAdd{${1:integer}}"
		]
	},


	"luaModularMult[]{}":{
		"prefix":"luaModularMult",
		"body":[
			"\\luaModularMult[${2:options%keyvals}]{${1:integer}}"
		]
	},


	"luaModularAdd[]{}":{
		"prefix":"luaModularAdd",
		"body":[
			"\\luaModularAdd[${2:options%keyvals}]{${1:integer}}"
		]
	}
}
