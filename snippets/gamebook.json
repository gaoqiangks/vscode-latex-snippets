{


	"gbsection{}":{
		"prefix":"gbsection",
		"body":[
			"\\gbsection{${1:id%labeldef}}"
		]
	},


	"gbturn{}":{
		"prefix":"gbturn",
		"body":[
			"\\gbturn{${1:id%ref}}"
		]
	},


	"gbturntext":{
		"prefix":"gbturntext",
		"body":[
			"\\gbturntext"
		]
	},


	"gbitem{}{}":{
		"prefix":"gbitem",
		"body":[
			"\\gbitem{${1:text}}{${2:id%ref}}"
		]
	},


	"gbvillain{}{}{}{}{}":{
		"prefix":"gbvillain",
		"body":[
			"\\gbvillain{${1:name}}{${2:skill%text}}{${3:skill level%text}}{${4:life%text}}{${5:life level%text}}"
		]
	},


	"gbheader":{
		"prefix":"gbheader",
		"body":[
			"\\gbheader"
		]
	},


	"gbheadtext":{
		"prefix":"gbheadtext",
		"body":[
			"\\gbheadtext"
		]
	},


	"gbdebugx{}":{
		"prefix":"gbdebugx",
		"body":[
			"\\gbdebugx{${1:arg}}"
		]
	},


	"gbdebug{}":{
		"prefix":"gbdebug",
		"body":[
			"\\gbdebug{${1:arg}}"
		]
	},


	"gbturnoptions":{
		"prefix":"gbturnoptions",
		"body":[
			"\\begin{gbturnoptions}",
				"\t$1",
			"\\end{gbturnoptions}"
		]
	},


	"gbtabbing":{
		"prefix":"gbtabbing",
		"body":[
			"\\begin{gbtabbing}",
				"\t$1",
			"\\end{gbtabbing}"
		]
	},


	"gbturnoptions":{
		"prefix":"gbturnoptions",
		"body":[
			"\\begin{gbturnoptions}",
				"\t$1",
			"\\end{gbturnoptions}"
		]
	}
}
