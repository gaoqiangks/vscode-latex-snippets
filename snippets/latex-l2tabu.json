{


	"centerline{}":{
		"prefix":"centerline",
		"body":[
			"\\centerline{${1:text}}"
		]
	},


	"fussy":{
		"prefix":"fussy",
		"body":[
			"\\fussy"
		]
	},


	"sloppy":{
		"prefix":"sloppy",
		"body":[
			"\\sloppy"
		]
	},


	"appendix":{
		"prefix":"appendix",
		"body":[
			"\\begin{appendix}",
				"\t$1",
			"\\end{appendix}"
		]
	},


	"eqnarray":{
		"prefix":"eqnarray",
		"body":[
			"\\begin{eqnarray}",
				"\t$1",
			"\\end{eqnarray}"
		]
	},


	"fussypar":{
		"prefix":"fussypar",
		"body":[
			"\\begin{fussypar}",
				"\t$1",
			"\\end{fussypar}"
		]
	},


	"sloppypar":{
		"prefix":"sloppypar",
		"body":[
			"\\begin{sloppypar}",
				"\t$1",
			"\\end{sloppypar}"
		]
	},


	"eqnarray*":{
		"prefix":"*eqnarray",
		"body":[
			"\\begin{eqnarray*}",
				"\t$1",
			"\\end{eqnarray*}"
		]
	}
}
