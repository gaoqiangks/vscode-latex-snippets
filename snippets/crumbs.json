{


	"crumbs":{
		"prefix":"crumbs",
		"body":[
			"\\crumbs"
		]
	},


	"subcrumbs":{
		"prefix":"subcrumbs",
		"body":[
			"\\subcrumbs"
		]
	},


	"appendwrite":{
		"prefix":"appendwrite",
		"body":[
			"\\appendwrite"
		]
	},


	"appendtofile{}{}":{
		"prefix":"appendtofile",
		"body":[
			"\\appendtofile{${1:file}}{${2:content}}"
		]
	},


	"crumbection{}":{
		"prefix":"crumbection",
		"body":[
			"\\crumbection{${1:title}}"
		]
	},


	"subcrumbection{}":{
		"prefix":"subcrumbection",
		"body":[
			"\\subcrumbection{${1:title}}"
		]
	},


	"crumb{}{}":{
		"prefix":"crumb",
		"body":[
			"\\crumb{${1:arg1}}{${2:arg2}}"
		]
	},


	"subcrumb{}{}":{
		"prefix":"subcrumb",
		"body":[
			"\\subcrumb{${1:arg1}}{${2:arg2}}"
		]
	},


	"thecrumbi":{
		"prefix":"thecrumbi",
		"body":[
			"\\thecrumbi"
		]
	},


	"thesubcrumbi":{
		"prefix":"thesubcrumbi",
		"body":[
			"\\thesubcrumbi"
		]
	},


	"crumbection[]{}":{
		"prefix":"crumbection",
		"body":[
			"\\crumbection[${2:short title}]{${1:title}}"
		]
	},


	"subcrumbection[]{}":{
		"prefix":"subcrumbection",
		"body":[
			"\\subcrumbection[${2:short title}]{${1:title}}"
		]
	}
}
