{


	"speccons":{
		"prefix":"speccons",
		"body":[
			"\\speccons{${1:name}}{${2:prototype}}"
		]
	},


	"speccons":{
		"prefix":"speccons",
		"body":[
			"\\speccons[${3:modifiers}]{${1:name}}{${2:prototype}}"
		]
	},


	"specget":{
		"prefix":"specget",
		"body":[
			"\\specget{${1:return type}}{${2:name}}"
		]
	},


	"specget":{
		"prefix":"specget",
		"body":[
			"\\specget[${3:modifiers}]{${1:return type}}{${2:name}}"
		]
	},


	"specset":{
		"prefix":"specset",
		"body":[
			"\\specset{${1:name}}{${2:prototype}}"
		]
	},


	"specset":{
		"prefix":"specset",
		"body":[
			"\\specset[${3:modifiers}]{${1:name}}{${2:prototype}}"
		]
	},


	"specfunc":{
		"prefix":"specfunc",
		"body":[
			"\\specfunc{${1:return type}}{${2:name}}{${3:prototype}}"
		]
	},


	"specfunc":{
		"prefix":"specfunc",
		"body":[
			"\\specfunc[${4:modifiers}]{${1:return type}}{${2:name}}{${3:prototype}}"
		]
	},


	"specreturn":{
		"prefix":"specreturn",
		"body":[
			"\\specreturn{${1:description%text}}"
		]
	},


	"specglobalreturn":{
		"prefix":"specglobalreturn",
		"body":[
			"\\specglobalreturn{${1:description%text}}"
		]
	},


	"specparam":{
		"prefix":"specparam",
		"body":[
			"\\specparam{${1:name}}{${2:description%text}}"
		]
	},


	"specparam":{
		"prefix":"specparam",
		"body":[
			"\\specparam[${3:in|${4:inout}|out}]{${1:name}}{${2:description%text}}"
		]
	},


	"specendhline":{
		"prefix":"specendhline",
		"body":[
			"\\specendhline"
		]
	},


	"specstarthline":{
		"prefix":"specstarthline",
		"body":[
			"\\specstarthline"
		]
	},


	"detailspec*":{
		"prefix":"detailspec*",
		"body":[
			"\\begin{detailspec*}",
			"\t$1",
			"\\end{detailspec*}"
		]
	},


	"detailspec":{
		"prefix":"detailspec",
		"body":[
			"\\begin{detailspec}",
			"\t$1",
			"\\end{detailspec}"
		]
	}
}
