{


	"collectbox{}{}":{
		"prefix":"collectbox",
		"body":[
			"\\collectbox{${1:code}}{${2:content%text}}"
		]
	},


	"collectboxto{}{}{}":{
		"prefix":"collectboxto",
		"body":[
			"\\collectboxto{${1:register}}{${2:code}}{${3:content%text}}"
		]
	},


	"collectboxcheckenv{}":{
		"prefix":"collectboxcheckenv",
		"body":[
			"\\collectboxcheckenv{${1:name}}"
		]
	},


	"collectedbox":{
		"prefix":"collectedbox",
		"body":[
			"\\collectedbox"
		]
	},


	"BOXCONTENT":{
		"prefix":"BOXCONTENT",
		"body":[
			"\\BOXCONTENT"
		]
	},


	"ifcollectboxenv":{
		"prefix":"ifcollectboxenv",
		"body":[
			"\\ifcollectboxenv"
		]
	},


	"collectboxenvtrue":{
		"prefix":"collectboxenvtrue",
		"body":[
			"\\collectboxenvtrue"
		]
	},


	"collectboxenvfalse":{
		"prefix":"collectboxenvfalse",
		"body":[
			"\\collectboxenvfalse"
		]
	},


	"collectboxcheckenv{}":{
		"prefix":"collectboxcheckenv",
		"body":[
			"\\collectboxcheckenv{${1:arg}}"
		]
	},


	"collectboxenvend":{
		"prefix":"collectboxenvend",
		"body":[
			"\\collectboxenvend"
		]
	},


	"collectbox*{}{}":{
		"prefix":"*collectbox",
		"body":[
			"\\collectbox*{${1:code}}{${2:content%text}}"
		]
	},


	"collectbox*[]{}[]{}":{
		"prefix":"*collectbox",
		"body":[
			"\\collectbox*[${3:begin code}]{${1:code}}[${4:end code}]{${2:content%text}}"
		]
	},


	"collectbox[]{}[]{}":{
		"prefix":"collectbox",
		"body":[
			"\\collectbox[${3:begin code}]{${1:code}}[${4:end code}]{${2:content%text}}"
		]
	},


	"collectboxto{}[]{}[]{}":{
		"prefix":"collectboxto",
		"body":[
			"\\collectboxto{${1:register}}[${4:begin code}]{${2:code}}[${5:end code}]{${3:content%text}}"
		]
	}
}
