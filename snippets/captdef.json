{


	"DeclareCaption{}{}":{
		"prefix":"DeclareCaption",
		"body":[
			"\\DeclareCaption{${1:command}}{${2:counter}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"figcaption{}":{
		"prefix":"figcaption",
		"body":[
			"\\figcaption{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"figcaption[]{}":{
		"prefix":"figcaption",
		"body":[
			"\\figcaption[${2:short text%text}]{${1:text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"tabcaption{}":{
		"prefix":"tabcaption",
		"body":[
			"\\tabcaption{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"tabcaption[]{}":{
		"prefix":"tabcaption",
		"body":[
			"\\tabcaption[${2:short text%text}]{${1:text}}"
		],
		"luasnip": {"priority": 1000} 
	}
}
