{


	"DeclareCaption":{
		"prefix":"DeclareCaption",
		"body":[
			"\\DeclareCaption{${1:command}}{${2:counter}}"
		]
	},


	"figcaption":{
		"prefix":"figcaption",
		"body":[
			"\\figcaption{${1:text}}"
		]
	},


	"figcaption":{
		"prefix":"figcaption",
		"body":[
			"\\figcaption[${2:short text%text}]{${1:text}}"
		]
	},


	"tabcaption":{
		"prefix":"tabcaption",
		"body":[
			"\\tabcaption{${1:text}}"
		]
	},


	"tabcaption":{
		"prefix":"tabcaption",
		"body":[
			"\\tabcaption[${2:short text%text}]{${1:text}}"
		]
	}
}
