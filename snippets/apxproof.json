{


	"newtheoremrep{}{}":{
		"prefix":"newtheoremrep",
		"body":[
			"\\newtheoremrep{${1:envname}}{${2:title%text}}"
		]
	},


	"mainbodyrepeatedtheorem":{
		"prefix":"mainbodyrepeatedtheorem",
		"body":[
			"\\mainbodyrepeatedtheorem"
		]
	},


	"appendixsectionformat{}{}":{
		"prefix":"appendixsectionformat",
		"body":[
			"\\appendixsectionformat{${1:number}}{${2:title%text}}"
		]
	},


	"appendixrefname":{
		"prefix":"appendixrefname",
		"body":[
			"\\appendixrefname"
		]
	},


	"appendixbibliographystyle":{
		"prefix":"appendixbibliographystyle",
		"body":[
			"\\appendixbibliographystyle"
		]
	},


	"appendixbibliographyprelim":{
		"prefix":"appendixbibliographyprelim",
		"body":[
			"\\appendixbibliographyprelim"
		]
	},


	"appendixprelim":{
		"prefix":"appendixprelim",
		"body":[
			"\\appendixprelim"
		]
	},


	"noproofinappendix":{
		"prefix":"noproofinappendix",
		"body":[
			"\\noproofinappendix"
		]
	},


	"nosectionappendix":{
		"prefix":"nosectionappendix",
		"body":[
			"\\nosectionappendix"
		]
	},


	"newtheoremrep{}[]{}":{
		"prefix":"newtheoremrep",
		"body":[
			"\\newtheoremrep{${1:envname}}[${3:counter}]{${2:title%text}}"
		]
	},


	"newtheoremrep{}{}[]":{
		"prefix":"newtheoremrep",
		"body":[
			"\\newtheoremrep{${1:envname}}{${2:title%text}}[${3:countersec}]"
		]
	},


	"newtheoremrep{}[]{}[]":{
		"prefix":"newtheoremrep",
		"body":[
			"\\newtheoremrep{${1:envname}}[${3:counter}]{${2:title%text}}[${4:countersec}]"
		]
	},


	"toappendix":{
		"prefix":"toappendix",
		"body":[
			"\\begin{toappendix}",
				"\t$1",
			"\\end{toappendix}"
		]
	},


	"appendixproof":{
		"prefix":"appendixproof",
		"body":[
			"\\begin{appendixproof}",
				"\t$1",
			"\\end{appendixproof}"
		]
	},


	"proofsketch":{
		"prefix":"proofsketch",
		"body":[
			"\\begin{proofsketch}",
				"\t$1",
			"\\end{proofsketch}"
		]
	},


	"inlineproof":{
		"prefix":"inlineproof",
		"body":[
			"\\begin{inlineproof}",
				"\t$1",
			"\\end{inlineproof}"
		]
	},


	"nestedproof":{
		"prefix":"nestedproof",
		"body":[
			"\\begin{nestedproof}",
				"\t$1",
			"\\end{nestedproof}"
		]
	}
}
