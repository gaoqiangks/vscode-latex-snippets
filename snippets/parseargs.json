{


	"parseOpt{}{}{}{}":{
		"prefix":"parseOpt",
		"body":[
			"\\parseOpt{${1:openchar}}{${2:closechar}}{${3:yes}}{${4:no}}"
		]
	},


	"parseMand{}":{
		"prefix":"parseMand",
		"body":[
			"\\parseMand{${1:code}}"
		]
	},


	"parseFlag{}{}{}":{
		"prefix":"parseFlag",
		"body":[
			"\\parseFlag{${1:flagchar}}{${2:yes}}{${3:no}}"
		]
	}
}
