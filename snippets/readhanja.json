{


	"readhanjahangulfont{}":{
		"prefix":"readhanjahangulfont",
		"body":[
			"\\readhanjahangulfont{${1:font}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"readhanjahangulfont[]{}":{
		"prefix":"readhanjahangulfont",
		"body":[
			"\\readhanjahangulfont[${2:font features}]{${1:font}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"readhanjahangulfont{}[]":{
		"prefix":"readhanjahangulfont",
		"body":[
			"\\readhanjahangulfont{${1:font}}[${2:font features%keyvals}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"readhanjahangulfont[]{}[]":{
		"prefix":"readhanjahangulfont",
		"body":[
			"\\readhanjahangulfont[${2:font features}]{${1:font}}[${3:font features%keyvals}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"readhanjaraise{}":{
