{


	"ENsetup{}":{
		"prefix":"ENsetup",
		"body":[
			"\\ENsetup{${1:options%keyvals}}"
		]
	},


	"ENcurve{}":{
		"prefix":"ENcurve",
		"body":[
			"\\ENcurve{${1:level1,level2,level3,...}}"
		]
	},


	"ShowNiveaus":{
		"prefix":"ShowNiveaus",
		"body":[
			"\\ShowNiveaus"
		]
	},


	"ShowGain":{
		"prefix":"ShowGain",
		"body":[
			"\\ShowGain"
		]
	},


	"ShowEa":{
		"prefix":"ShowEa",
		"body":[
			"\\ShowEa"
		]
	},


	"MakeOrigin":{
		"prefix":"MakeOrigin",
		"body":[
			"\\MakeOrigin"
		]
	},


	"AddAxisLabel*{}":{
		"prefix":"*AddAxisLabel",
		"body":[
			"\\AddAxisLabel*{${1:level1[${2:label1}];level2;...}}"
		]
	},


	"AddAxisLabel*[]{}":{
		"prefix":"*AddAxisLabel",
		"body":[
			"\\AddAxisLabel*[${2:options}]{${1:level1[${3:label1}];level2;...}}"
		]
	},


	"ENsetup[]{}":{
		"prefix":"ENsetup",
		"body":[
			"\\ENsetup[${2:module}]{${1:options%keyvals}}"
		]
	},


	"ENcurve[]{}":{
		"prefix":"ENcurve",
		"body":[
			"\\ENcurve[${2:options%keyvals}]{${1:level1,level2,level3,...}}"
		]
	},


	"ShowNiveaus[]":{
		"prefix":"ShowNiveaus",
		"body":[
			"\\ShowNiveaus[${1:options%keyvals}]"
		]
	},


	"ShowGain[]":{
		"prefix":"ShowGain",
		"body":[
			"\\ShowGain[${1:options%keyvals}]"
		]
	},


	"ShowEa[]":{
		"prefix":"ShowEa",
		"body":[
			"\\ShowEa[${1:options%keyvals}]"
		]
	},


	"AddAxisLabel{}":{
		"prefix":"AddAxisLabel",
		"body":[
			"\\AddAxisLabel{${1:(${3:point1})[${2:label1}];(${4:point2});...}}"
		]
	},


	"AddAxisLabel[]{}":{
		"prefix":"AddAxisLabel",
		"body":[
			"\\AddAxisLabel[${2:options}]{${1:(${4:point1})[${3:label1}];(${5:point2});...}}"
		]
	},


	"endiagram":{
		"prefix":"endiagram",
		"body":[
			"\\begin{endiagram}",
				"\t$1",
			"\\end{endiagram}"
		]
	},


	"endiagram":{
		"prefix":"endiagram",
		"body":[
			"\\begin{endiagram}",
				"\t$1",
			"\\end{endiagram}"
		]
	}
}
