{


	"singlespacing":{
		"prefix":"singlespacing",
		"body":[
			"\\singlespacing"
		]
	},


	"onehalfspacing":{
		"prefix":"onehalfspacing",
		"body":[
			"\\onehalfspacing"
		]
	},


	"doublespacing":{
		"prefix":"doublespacing",
		"body":[
			"\\doublespacing"
		]
	},


	"SetSinglespace{}":{
		"prefix":"SetSinglespace",
		"body":[
			"\\SetSinglespace{${1:factor}}"
		]
	},


	"setstretch{}":{
		"prefix":"setstretch",
		"body":[
			"\\setstretch{${1:factor}}"
		]
	},


	"displayskipstretch":{
		"prefix":"displayskipstretch",
		"body":[
			"\\displayskipstretch"
		]
	},


	"setdisplayskipstretch{}":{
		"prefix":"setdisplayskipstretch",
		"body":[
			"\\setdisplayskipstretch{${1:factor}}"
		]
	},


	"singlespace":{
		"prefix":"singlespace",
		"body":[
			"\\begin{singlespace}",
				"\t$1",
			"\\end{singlespace}"
		]
	},


	"doublespace":{
		"prefix":"doublespace",
		"body":[
			"\\begin{doublespace}",
				"\t$1",
			"\\end{doublespace}"
		]
	},


	"onehalfspace":{
		"prefix":"onehalfspace",
		"body":[
			"\\begin{onehalfspace}",
				"\t$1",
			"\\end{onehalfspace}"
		]
	},


	"spacing":{
		"prefix":"spacing",
		"body":[
			"\\begin{spacing}",
				"\t$1",
			"\\end{spacing}"
		]
	},


	"singlespace*":{
		"prefix":"*singlespace",
		"body":[
			"\\begin{singlespace*}",
				"\t$1",
			"\\end{singlespace*}"
		]
	}
}
