{


	"cellGetText":{
		"prefix":"cellGetText",
		"body":[
			"\\cellGetText{${1:rownum}}{${2:colnum}}"
		]
	},


	"cellSetText":{
		"prefix":"cellSetText",
		"body":[
			"\\cellSetText{${1:rownum}}{${2:colnum}}{${3:text}}"
		]
	},


	"cellSetStyle":{
		"prefix":"cellSetStyle",
		"body":[
			"\\cellSetStyle{${1:rownum}}{${2:colnum}}{${3:style}}"
		]
	},


	"rowSetStyle":{
		"prefix":"rowSetStyle",
		"body":[
			"\\rowSetStyle{${1:rownum}}{${2:style}}"
		]
	},


	"columnSetStyle":{
		"prefix":"columnSetStyle",
		"body":[
			"\\columnSetStyle{${1:style}}"
		]
	}
}
