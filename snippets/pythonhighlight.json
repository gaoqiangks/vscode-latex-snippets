{


	"framemargin":{
		"prefix":"framemargin",
		"body":[
			"\\framemargin"
		],
		"luasnip": {"priority": 3000} 
	},


	"pythonprompt":{
		"prefix":"pythonprompt",
		"body":[
			"\\pythonprompt"
		],
		"luasnip": {"priority": 3000} 
	},


	"literatecolour{}":{
		"prefix":"literatecolour",
		"body":[
			"\\literatecolour{${1:text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"inputpythonfile{}":{
		"prefix":"inputpythonfile",
		"body":[
			"\\inputpythonfile{${1:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"inputpythonfile{}[]":{
		"prefix":"inputpythonfile",
		"body":[
			"\\inputpythonfile{${1:file}}[${2:start num}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"inputpythonfile{}[][]":{
		"prefix":"inputpythonfile",
		"body":[
			"\\inputpythonfile{${1:file}}[${2:start num}][${3:end num}]"
		],
		"luasnip": {"priority": 1000} 
	},


	"pyth||":{
		"prefix":"pyth",
		"body":[
			"\\pyth|${1:text}|"
		],
		"luasnip": {"priority": 3000} 
	},


	"python":{
		"prefix":"python",
		"body":[
			"\\begin{python}",
				"\t$1",
			"\\end{python}"
		],
		"luasnip": {"priority": 3000} 
	},


	"python[]":{
		"prefix":"python",
		"body":[
			"\\begin{python}",
				"\t[${1:options}]",
			"\\end{python}"
		],
		"luasnip": {"priority": 1000} 
	}
}
