{


	"psRQBCmasse()()(){}":{
		"prefix":"psRQBCmasse",
		"body":[
			"\\psRQBCmasse(${2:x0},${3:y0})(${4:x1},${5:y1})(${6:x2},${7:y2}){${1:w0,w1,w2}}"
		]
	},


	"pscalculate{}":{
		"prefix":"pscalculate",
		"body":[
			"\\pscalculate{${1:expr%definition}}"
		]
	},


	"psRQBCmasse[]()()(){}":{
		"prefix":"psRQBCmasse",
		"body":[
			"\\psRQBCmasse[${2:options%keyvals}](${3:x0},${4:y0})(${5:x1},${6:y1})(${7:x2},${8:y2}){${1:w0,w1,w2}}"
		]
	},


	"defopt{}{}[]args{}":{
		"prefix":"defopt",
		"body":[
			"\\defopt{${1:macro%cmd}}{${2:default}}[${4:optargs}]args{${3:definition}}"
		]
	}
}
