{


	"gitdirectory":{
		"prefix":"gitdirectory",
		"body":[
			"\\gitdirectory{${1:path%definition}}"
		]
	},


	"gitunsetdirectory":{
		"prefix":"gitunsetdirectory",
		"body":[
			"\\gitunsetdirectory"
		]
	},


	"gitversion":{
		"prefix":"gitversion",
		"body":[
			"\\gitversion"
		]
	},


	"ifgitdirty":{
		"prefix":"ifgitdirty",
		"body":[
			"\\ifgitdirty{${1:true code}}{${2:false code}}"
		]
	},


	"gitdate":{
		"prefix":"gitdate",
		"body":[
			"\\gitdate"
		]
	},


	"gitauthor":{
		"prefix":"gitauthor",
		"body":[
			"\\gitauthor"
		]
	},


	"gitemail":{
		"prefix":"gitemail",
		"body":[
			"\\gitemail"
		]
	},


	"dogitauthors":{
		"prefix":"dogitauthors",
		"body":[
			"\\dogitauthors"
		]
	},


	"dogitauthors":{
		"prefix":"dogitauthors",
		"body":[
			"\\dogitauthors[${1:conjunction}]"
		]
	},


	"forgitauthors":{
		"prefix":"forgitauthors",
		"body":[
			"\\forgitauthors{${1:csname}}"
		]
	},


	"forgitauthors":{
		"prefix":"forgitauthors",
		"body":[
			"\\forgitauthors[${2:conjunction}]{${1:csname}}"
		]
	},


	"gitcommit":{
		"prefix":"gitcommit",
		"body":[
			"\\gitcommit{${1:csname}}{${2:revision}}"
		]
	},


	"gitcommit":{
		"prefix":"gitcommit",
		"body":[
			"\\gitcommit[${3:format}]{${1:csname}}{${2:revision}}"
		]
	},


	"forgitcommit":{
		"prefix":"forgitcommit",
		"body":[
			"\\forgitcommit{${1:csname}}{${2:rev-spec}}"
		]
	},


	"forgitcommit":{
		"prefix":"forgitcommit",
		"body":[
			"\\forgitcommit[${3:format}]{${1:csname}}{${2:rev-spec}}"
		]
	},


	"forgittag":{
		"prefix":"forgittag",
		"body":[
			"\\forgittag{${1:csname}}"
		]
	},


	"forgittag":{
		"prefix":"forgittag",
		"body":[
			"\\forgittag[${2:format}]{${1:csname}}"
		]
	},


	"forgittagseq":{
		"prefix":"forgittagseq",
		"body":[
			"\\forgittagseq{${1:csname}}"
		]
	},


	"gittag":{
		"prefix":"gittag",
		"body":[
			"\\gittag{${1:csname}}{${2:tag}}"
		]
	},


	"gittag":{
		"prefix":"gittag",
		"body":[
			"\\gittag[${3:format}]{${1:csname}}{${2:tag}}"
		]
	}
}
