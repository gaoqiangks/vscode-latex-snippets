{


	"fullwidthsetup{}":{
		"prefix":"fullwidthsetup",
		"body":[
			"\\fullwidthsetup{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"fullwidth":{
		"prefix":"fullwidth",
		"body":[
			"\\begin{fullwidth}",
				"\t$1",
			"\\end{fullwidth}"
		],
		"luasnip": {"priority": 3000} 
	},


	"fullwidth[]":{
		"prefix":"fullwidth",
		"body":[
			"\\begin{fullwidth}",
				"\t[${1:options%keyvals}]",
			"\\end{fullwidth}"
		],
		"luasnip": {"priority": 1000} 
	}
}
