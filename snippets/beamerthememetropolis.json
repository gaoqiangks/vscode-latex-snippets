{


	"metroset{}":{
		"prefix":"metroset",
		"body":[
			"\\metroset{${1:options%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"plain{}":{
		"prefix":"plain",
		"body":[
			"\\plain{${1:title%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"plain[]{}":{
		"prefix":"plain",
		"body":[
			"\\plain[${2:format%keyvals}]{${1:title%text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"mreducelistspacing":{
		"prefix":"mreducelistspacing",
		"body":[
			"\\mreducelistspacing"
		],
		"luasnip": {"priority": 3000} 
	},


	"thefontsnotfound":{
		"prefix":"thefontsnotfound",
		"body":[
			"\\thefontsnotfound"
		],
		"luasnip": {"priority": 3000} 
	},


	"checkfont{}":{
		"prefix":"checkfont",
		"body":[
			"\\checkfont{${1:font name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"iffontsavailable{}{}{}":{
		"prefix":"iffontsavailable",
		"body":[
			"\\iffontsavailable{${1:font list}}{${2:true}}{${3:false}}"
		],
		"luasnip": {"priority": 3000} 
	}
