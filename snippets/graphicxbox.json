{


	"graphicxbox":{
		"prefix":"graphicxbox",
		"body":[
			"\\graphicxbox{${1:imagefile}}{${2:box content%text}}"
		]
	},


	"graphicxbox":{
		"prefix":"graphicxbox",
		"body":[
			"\\graphicxbox[${3:graphicx options}]{${1:imagefile}}{${2:box content%text}}"
		]
	},


	"fgraphicxbox":{
		"prefix":"fgraphicxbox",
		"body":[
			"\\fgraphicxbox{${1:color}}{${2:imagefile}}{${3:box content%text}}"
		]
	},


	"fgraphicxbox":{
		"prefix":"fgraphicxbox",
		"body":[
			"\\fgraphicxbox{${1:color}}[${4:graphicx options}]{${2:imagefile}}{${3:box content%text}}"
		]
	},


	"fgraphicxbox":{
		"prefix":"fgraphicxbox",
		"body":[
			"\\fgraphicxbox[${4:model}]{${1:color spec}}{${2:imagefile}}{${3:box content%text}}"
		]
	},


	"fgraphicxbox":{
		"prefix":"fgraphicxbox",
		"body":[
			"\\fgraphicxbox[${4:model}]{${1:color spec}}[${5:graphicx options}]{${2:imagefile}}{${3:box content%text}}"
		]
	}
}
