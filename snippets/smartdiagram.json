{


	"smartdiagram":{
		"prefix":"smartdiagram",
		"body":[
			"\\smartdiagram[${2:type%keyvals}]{${1:list of items}}"
		]
	},


	"smartdiagramanimated":{
		"prefix":"smartdiagramanimated",
		"body":[
			"\\smartdiagramanimated[${2:type%keyvals}]{${1:list of items}}"
		]
	},


	"usesmartdiagramlibrary":{
		"prefix":"usesmartdiagramlibrary",
		"body":[
			"\\usesmartdiagramlibrary{${1:library}}"
		]
	},


	"smartdiagramadd":{
		"prefix":"smartdiagramadd",
		"body":[
			"\\smartdiagramadd[${3:type%keyvals}]{${1:list of items}}{${2:list of additions}}"
		]
	},


	"smartdiagramconnect":{
		"prefix":"smartdiagramconnect",
		"body":[
			"\\smartdiagramconnect{${1:arrow options}}{${2:start module/end module}}"
		]
	},


	"smartdiagramset":{
		"prefix":"smartdiagramset",
		"body":[
			"\\smartdiagramset{${1:options%keyvals}}"
		]
	}
}
