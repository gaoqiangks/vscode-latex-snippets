{


	"import{}{}":{
		"prefix":"import",
		"body":[
			"\\import{${1:full path%definition}}{${2:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"inputfrom{}{}":{
		"prefix":"inputfrom",
		"body":[
			"\\inputfrom{${1:full path%definition}}{${2:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"subimport{}{}":{
		"prefix":"subimport",
		"body":[
			"\\subimport{${1:relative path%definition}}{${2:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"subinputfrom{}{}":{
		"prefix":"subinputfrom",
		"body":[
			"\\subinputfrom{${1:relative path%definition}}{${2:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"includefrom{}{}":{
		"prefix":"includefrom",
		"body":[
			"\\includefrom{${1:full path%definition}}{${2:file}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"subincludefrom*{}{}":{
		"prefix":"*subincludefrom",
		"body":[
