{


	"bohr{}{}":{
		"prefix":"bohr",
		"body":[
			"\\bohr{${1:num of electrons}}{${2:atom name}}"
		]
	},


	"setbohr{}":{
		"prefix":"setbohr",
		"body":[
			"\\setbohr{${1:options%keyvals}}"
		]
	},


	"bohr[]{}{}":{
		"prefix":"bohr",
		"body":[
			"\\bohr[${3:num of shells}]{${1:num of electrons}}{${2:atom name}}"
		]
	}
}
