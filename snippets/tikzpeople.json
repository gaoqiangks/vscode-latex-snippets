{


	"alltikzpeople":{
		"prefix":"alltikzpeople",
		"body":[
			"\\alltikzpeople{${1:width}}{${2:node options}}"
		]
	},


	"alltikzpeople*":{
		"prefix":"alltikzpeople*",
		"body":[
			"\\alltikzpeople*{${1:width}}{${2:node options}}"
		]
	},


	"tikzpeoplecolors":{
		"prefix":"tikzpeoplecolors",
		"body":[
			"\\tikzpeoplecolors{${1:shape name}}"
		]
	}
}
