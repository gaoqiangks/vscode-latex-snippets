{


	"xsbox{}{}":{
		"prefix":"xsbox",
		"body":[
			"\\xsbox{${1:name}}{${2:content%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"xsavebox{}{}":{
		"prefix":"xsavebox",
		"body":[
			"\\xsavebox{${1:name}}{${2:content%text}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"xsavebox{}[]{}":{
		"prefix":"xsavebox",
		"body":[
			"\\xsavebox{${1:name}}[${3:width}]{${2:content%text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"xsavebox{}[][]{}":{
		"prefix":"xsavebox",
		"body":[
			"\\xsavebox{${1:name}}[${3:width}][${4:position}]{${2:content%text}}"
		],
		"luasnip": {"priority": 1000} 
	},


	"xsavebox*{}{}":{
		"prefix":"*xsavebox",
		"body":[
			"\\xsavebox*{${1:name}}{${2:content%text}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"xsavebox*{}[]{}":{
		"prefix":"*xsavebox",
		"body":[
			"\\xsavebox*{${1:name}}[${3:width}]{${2:content%text}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"xsavebox*{}[][]{}":{
		"prefix":"*xsavebox",
		"body":[
			"\\xsavebox*{${1:name}}[${3:width}][${4:position}]{${2:content%text}}"
		],
		"luasnip": {"priority": 2000} 
	},


	"xusebox{}":{
		"prefix":"xusebox",
		"body":[
			"\\xusebox{${1:name}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"xlrbox{}":{
		"prefix":"xlrbox",
		"body":[
			"\\begin{xlrbox}",
				"\t{${1:name}}",
			"\\end{xlrbox}"
		],
		"luasnip": {"priority": 3000} 
	},


	"xlrbox*{}":{
		"prefix":"*xlrbox",
		"body":[
			"\\begin{xlrbox*}",
