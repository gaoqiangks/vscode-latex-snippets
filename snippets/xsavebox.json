{


	"xsbox{}{}":{
		"prefix":"xsbox",
		"body":[
			"\\xsbox{${1:name}}{${2:content%text}}"
		]
	},


	"xsavebox{}{}":{
		"prefix":"xsavebox",
		"body":[
			"\\xsavebox{${1:name}}{${2:content%text}}"
		]
	},


	"xusebox{}":{
		"prefix":"xusebox",
		"body":[
			"\\xusebox{${1:name}}"
		]
	},


	"xsavebox*{}{}":{
		"prefix":"*xsavebox",
		"body":[
			"\\xsavebox*{${1:name}}{${2:content%text}}"
		]
	},


	"xsavebox*{}[]{}":{
		"prefix":"*xsavebox",
		"body":[
			"\\xsavebox*{${1:name}}[${3:width}]{${2:content%text}}"
		]
	},


	"xsavebox*{}[][]{}":{
		"prefix":"*xsavebox",
		"body":[
			"\\xsavebox*{${1:name}}[${3:width}][${4:position}]{${2:content%text}}"
		]
	},


	"xsavebox{}[]{}":{
		"prefix":"xsavebox",
		"body":[
			"\\xsavebox{${1:name}}[${3:width}]{${2:content%text}}"
		]
	},


	"xsavebox{}[][]{}":{
		"prefix":"xsavebox",
		"body":[
			"\\xsavebox{${1:name}}[${3:width}][${4:position}]{${2:content%text}}"
		]
	},


	"xlrbox":{
		"prefix":"xlrbox",
		"body":[
			"\\begin{xlrbox}",
				"\t$1",
			"\\end{xlrbox}"
		]
	},


	"xlrbox*":{
		"prefix":"xlrbox",
		"body":[
			"\\begin{xlrbox*}",
				"\t$1",
			"\\end{xlrbox*}"
		]
	}
}
