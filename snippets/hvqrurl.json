{


	"hvqrset{}":{
		"prefix":"hvqrset",
		"body":[
			"\\hvqrset{${1:options%keyvals}}"
		]
	},


	"hvqrurl{}":{
		"prefix":"hvqrurl",
		"body":[
			"\\hvqrurl{${1:URL}}"
		]
	},


	"hvqrurl*{}":{
		"prefix":"*hvqrurl",
		"body":[
			"\\hvqrurl*{${1:URL}}"
		]
	},


	"hvqrurl*[]{}":{
		"prefix":"*hvqrurl",
		"body":[
			"\\hvqrurl*[${2:options%keyvals}]{${1:URL}}"
		]
	},


	"hvqrurl[]{}":{
		"prefix":"hvqrurl",
		"body":[
			"\\hvqrurl[${2:options%keyvals}]{${1:URL}}"
		]
	}
}
