{


	"DefineLCDchar{}{}":{
		"prefix":"DefineLCDchar",
		"body":[
			"\\DefineLCDchar{${1:name}}{${2:binary code}}"
		]
	},


	"LCDcolors{}{}":{
		"prefix":"LCDcolors",
		"body":[
			"\\LCDcolors{${1:textcolor}}{${2:bgcolor}}"
		]
	},


	"LCDframe":{
		"prefix":"LCDframe",
		"body":[
			"\\LCDframe"
		]
	},


	"LCDnoframe":{
		"prefix":"LCDnoframe",
		"body":[
			"\\LCDnoframe"
		]
	},


	"textLCD{}||":{
		"prefix":"textLCD",
		"body":[
			"\\textLCD{${1:num of chars}}|${2:text}|"
		]
	},


	"textLCDcorr":{
		"prefix":"textLCDcorr",
		"body":[
			"\\textLCDcorr"
		]
	},


	"LCDunitlength":{
		"prefix":"LCDunitlength",
		"body":[
			"\\LCDunitlength"
		]
	},


	"LCDcolors[]{}{}":{
		"prefix":"LCDcolors",
		"body":[
			"\\LCDcolors[${3:framecolor}]{${1:textcolor}}{${2:bgcolor}}"
		]
	},


	"textLCD[]{}||":{
		"prefix":"textLCD",
		"body":[
			"\\textLCD[${2:shift}]{${1:num of chars}}|${3:text}|"
		]
	}
}
