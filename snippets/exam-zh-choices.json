{


	"setchoices{}":{
		"prefix":"setchoices",
		"body":[
			"\\setchoices{${1:键值列表%keyvals}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"AddChoicesCounter{}{}":{
		"prefix":"AddChoicesCounter",
		"body":[
			"\\AddChoicesCounter{${1:LaTeX command%cmd}}{${2:internal command%definition}}"
		],
		"luasnip": {"priority": 3000} 
	},


	"choices":{
		"prefix":"choices",
		"body":[
			"\\begin{choices}",
				"\t$1",
			"\\end{choices}"
		],
		"luasnip": {"priority": 3000} 
	},


	"choices[]":{
		"prefix":"choices",
		"body":[
			"\\begin{choices}",
				"\t[${1:键值列表%keyvals}]",
			"\\end{choices}"
		],
		"luasnip": {"priority": 1000} 
	}
}
