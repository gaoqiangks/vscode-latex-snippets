{


	"bnfprod{}{}":{
		"prefix":"bnfprod",
		"body":[
			"\\bnfprod{${1:name}}{${2:definition}}"
		]
	},


	"bnfmore{}":{
		"prefix":"bnfmore",
		"body":[
			"\\bnfmore{${1:definition}}"
		]
	},


	"bnfpn{}":{
		"prefix":"bnfpn",
		"body":[
			"\\bnfpn{${1:name}}"
		]
	},


	"bnfts{}":{
		"prefix":"bnfts",
		"body":[
			"\\bnfts{${1:string}}"
		]
	},


	"bnftd{}":{
		"prefix":"bnftd",
		"body":[
			"\\bnftd{${1:phrase}}"
		]
	},


	"bnfes":{
		"prefix":"bnfes",
		"body":[
			"\\bnfes"
		]
	},


	"bnfsk":{
		"prefix":"bnfsk",
		"body":[
			"\\bnfsk"
		]
	},


	"bnfor":{
		"prefix":"bnfor",
		"body":[
			"\\bnfor"
		]
	},


	"bnfsp":{
		"prefix":"bnfsp",
		"body":[
			"\\bnfsp"
		]
	},


	"bnfpo":{
		"prefix":"bnfpo",
		"body":[
			"\\bnfpo"
		]
	},


	"bnfprod*{}{}":{
		"prefix":"*bnfprod",
		"body":[
			"\\bnfprod*{${1:name}}{${2:definition}}"
		]
	},


	"bnfmore*{}":{
		"prefix":"*bnfmore",
		"body":[
			"\\bnfmore*{${1:definition}}"
		]
	},


	"bnf":{
		"prefix":"bnf",
		"body":[
			"\\begin{bnf}",
				"\t$1",
			"\\end{bnf}"
		]
	},


	"bnf*":{
		"prefix":"*bnf",
		"body":[
			"\\begin{bnf*}",
				"\t$1",
			"\\end{bnf*}"
		]
	}
}
