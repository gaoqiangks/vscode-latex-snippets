{


	"bash":{
		"prefix":"bash",
		"body":[
			"\\bash[${1:options%keyvals}]"
		]
	},


	"END":{
		"prefix":"END",
		"body":[
			"\\END"
		]
	},


	"bashStdout":{
		"prefix":"bashStdout",
		"body":[
			"\\bashStdout"
		]
	},


	"bashStderr":{
		"prefix":"bashStderr",
		"body":[
			"\\bashStderr"
		]
	},


	"splice":{
		"prefix":"splice",
		"body":[
			"\\splice{${1:shell command%definition}}"
		]
	},


	"bashI":{
		"prefix":"bashI",
		"body":[
			"\\bashI[${1:options}]"
		]
	},


	"bashII":{
		"prefix":"bashII",
		"body":[
			"\\bashII{${1:arg}}"
		]
	},


	"bashIII":{
		"prefix":"bashIII",
		"body":[
			"\\bashIII{${1:arg}}"
		]
	},


	"bashIV":{
		"prefix":"bashIV",
		"body":[
			"\\bashIV{${1:shell command%definition}}"
		]
	},


	"bashV":{
		"prefix":"bashV",
		"body":[
			"\\bashV{${1:arg}}"
		]
	},


	"logBL":{
		"prefix":"logBL",
		"body":[
			"\\logBL{${1:message%text}}"
		]
	}
}
